[
	{
		"id": 1,
		"start": 122.91,
		"end": 132.91,
		"text": "♪♪～"
	},
	{
		"id": 2,
		"start": 168.17657400000004,
		"end": 172.18057400000006,
		"text": "＜ワンピースを狙う<br />全ての海賊たちが目指す→"
	},
	{
		"id": 3,
		"start": 172.18057400000006,
		"end": 178.18057400000006,
		"text": "グランドライン後半の海<br />人呼んで　新世界＞"
	},
	{
		"id": 4,
		"start": 180.18857400000005,
		"end": 183.19157400000003,
		"text": "＜しかし<br />かつて　その海を制したのは→"
	},
	{
		"id": 5,
		"start": 183.19157400000003,
		"end": 187.19557400000005,
		"text": "海賊王　ゴールド・ロジャー<br />ただ　一人＞"
	},
	{
		"id": 6,
		"start": 187.19557400000005,
		"end": 194.20257400000006,
		"text": "＜新世界は　幾多の海賊たちの<br />夢と野望を阻んできた＞"
	},
	{
		"id": 7,
		"start": 194.20257400000006,
		"end": 199.20757400000005,
		"text": "＜その海に今　ゴムゴムの実を食べゴム人間となった少年→"
	},
	{
		"id": 8,
		"start": 199.20757400000005,
		"end": 203.21157400000007,
		"text": "モンキー・Ｄ・ルフィと<br />その仲間たちが挑む！＞"
	},
	{
		"id": 9,
		"start": 203.21157400000007,
		"end": 207.21157400000007,
		"text": "（ルフィ）海賊王に　俺は　なる！"
	},
	{
		"id": 10,
		"start": 210.21857400000002,
		"end": 213.22157400000006,
		"text": "＜新世界を支配する<br />四皇の一角を崩すため→"
	},
	{
		"id": 11,
		"start": 213.22157400000006,
		"end": 216.22457400000005,
		"text": "トラファルガー・ローと<br />海賊同盟を組んだ　ルフィたちは→"
	},
	{
		"id": 12,
		"start": 216.22457400000005,
		"end": 219.22757400000003,
		"text": "ドレスローザに乗り込むも→"
	},
	{
		"id": 13,
		"start": 219.22757400000003,
		"end": 223.23157400000005,
		"text": "現国王　ドフラミンゴの<br />計略によって→"
	},
	{
		"id": 14,
		"start": 223.23157400000005,
		"end": 226.16757400000003,
		"text": "島ごと閉じ込められてしまう＞"
	},
	{
		"id": 15,
		"start": 226.16757400000003,
		"end": 229.17057400000002,
		"text": "＜一味が　ドンキホーテファミリーと<br />激しい戦いを繰り広げる中→"
	},
	{
		"id": 16,
		"start": 229.17057400000002,
		"end": 236.17757400000002,
		"text": "ルフィとローは　王宮で<br />ついに　ドフラミンゴと激突した＞"
	},
	{
		"id": 17,
		"start": 236.17757400000002,
		"end": 239.18057400000006,
		"text": "（キュロス）<br />ドレスローザに降り注いだ悲劇。→"
	},
	{
		"id": 18,
		"start": 239.18057400000006,
		"end": 245.18657400000004,
		"text": "その数に比べれば　こんなもの<br />痛みですらない！→"
	},
	{
		"id": 19,
		"start": 245.18657400000004,
		"end": 252.19357400000004,
		"text": "私は<br />この国の怒りの塊となって→"
	},
	{
		"id": 20,
		"start": 252.19357400000004,
		"end": 255.19657400000003,
		"text": "貴様を討つ！"
	},
	{
		"id": 21,
		"start": 255.19657400000003,
		"end": 260.1965739999998,
		"text": "（ディアマンテ）半月グレイブ！"
	},
	{
		"id": 22,
		"start": 263.20457399999987,
		"end": 268.20457399999987,
		"text": "（キュロス）雷の破壊剣！"
	},
	{
		"id": 23,
		"start": 271.21257399999985,
		"end": 274.21257399999985,
		"text": "あ～！！"
	},
	{
		"id": 24,
		"start": 279.22057399999983,
		"end": 281.22257399999984,
		"text": "（レベッカ）キャー！<br />（キュロス）うおっ！"
	},
	{
		"id": 25,
		"start": 281.22257399999984,
		"end": 285.22257399999984,
		"text": "（ロビン）<br />ゾロが止めていたはずのピーカ！"
	},
	{
		"id": 26,
		"start": 290.1655739999998,
		"end": 297.1725739999998,
		"text": "（ピーカ）ディアマンテ　ファミリーの最高幹部が情けない姿だ。"
	},
	{
		"id": 27,
		"start": 297.1725739999998,
		"end": 311.1725739999998,
		"text": "♪♪～"
	},
	{
		"id": 28,
		"start": 320.19557399999985,
		"end": 326.19557399999985,
		"text": "（壁にぶつかる音）"
	},
	{
		"id": 29,
		"start": 333.2085739999998,
		"end": 337.2125739999998,
		"text": "やめろ　ベラミー！<br />本当に　ぶっ壊れちまうぞ！"
	},
	{
		"id": 30,
		"start": 337.2125739999998,
		"end": 341.2165739999998,
		"text": "（ベラミー）くっ。　麦わら<br />てめえ　まだ言うか！"
	},
	{
		"id": 31,
		"start": 341.2165739999998,
		"end": 345.2365739999998,
		"text": "何度でも言うさ。<br />もう　お前の体は限界だ。"
	},
	{
		"id": 32,
		"start": 345.2365739999998,
		"end": 348.2365739999998,
		"text": "戦うのは…。<br />とりゃあ！"
	},
	{
		"id": 33,
		"start": 350.1585739999998,
		"end": 352.1585739999998,
		"text": "ぐあっ…。"
	},
	{
		"id": 34,
		"start": 356.1645739999998,
		"end": 359.1645739999998,
		"text": "ベラミー！　いいかげんにしろ！"
	},
	{
		"id": 35,
		"start": 361.1695739999999,
		"end": 366.1745739999999,
		"text": "黙ってろ！　てめえの死に場所は<br />てめえで決める！"
	},
	{
		"id": 36,
		"start": 366.1745739999999,
		"end": 370.1785739999998,
		"text": "お前も海賊王になるんだろうが！"
	},
	{
		"id": 37,
		"start": 370.1785739999998,
		"end": 375.1785739999998,
		"text": "こんなもんに<br />足止めを食らってる場合かよ！"
	},
	{
		"id": 38,
		"start": 377.18557399999986,
		"end": 382.18557399999986,
		"text": "ぐはっ。<br />麦わらぁ！"
	},
	{
		"id": 39,
		"start": 394.2025739999998,
		"end": 396.20457399999987,
		"text": "（キュロス）うっ。<br />（レベッカ）お父さま！"
	},
	{
		"id": 40,
		"start": 396.20457399999987,
		"end": 399.2075739999998,
		"text": "（キュロス）当然だな。→"
	},
	{
		"id": 41,
		"start": 399.2075739999998,
		"end": 403.2115739999998,
		"text": "何も　差しの試合を<br />しにきたわけじゃない。→"
	},
	{
		"id": 42,
		"start": 403.2115739999998,
		"end": 407.1485739999998,
		"text": "敵がいる限り　戦いは終わらない。"
	},
	{
		"id": 43,
		"start": 407.1485739999998,
		"end": 409.1505739999999,
		"text": "うっ。"
	},
	{
		"id": 44,
		"start": 409.1505739999999,
		"end": 411.15257399999985,
		"text": "（レベッカ）無茶よ　お父さま。<br />もう動けない。"
	},
	{
		"id": 45,
		"start": 411.15257399999985,
		"end": 413.1545739999998,
		"text": "（キュロス）下がれ　レベッカ！"
	},
	{
		"id": 46,
		"start": 413.1545739999998,
		"end": 418.1595739999998,
		"text": "命に代えても　必ず<br />こいつらを討ち倒し→"
	},
	{
		"id": 47,
		"start": 418.1595739999998,
		"end": 425.16657399999986,
		"text": "リク王様の元に<br />この国を取り戻すんだ。"
	},
	{
		"id": 48,
		"start": 425.16657399999986,
		"end": 431.1725739999998,
		"text": "（ピーカ）聞くが　お前たちにとってリク王とは何だ？"
	},
	{
		"id": 49,
		"start": 431.1725739999998,
		"end": 438.1795739999999,
		"text": "レベッカ　お前は<br />やつのせいで罵声を浴び続けた。"
	},
	{
		"id": 50,
		"start": 438.1795739999999,
		"end": 442.1835739999998,
		"text": "キュロス　お前は<br />やつに仕えたせいで→"
	},
	{
		"id": 51,
		"start": 442.1835739999998,
		"end": 447.1885739999998,
		"text": "片足を失い　オモチャとして<br />生きることになった。"
	},
	{
		"id": 52,
		"start": 447.1885739999998,
		"end": 449.19057399999986,
		"text": "やつさえ　いなければ。"
	},
	{
		"id": 53,
		"start": 449.19057399999986,
		"end": 452.1935739999999,
		"text": "（レベッカ）ふざけないで！<br />あなたたちが陥れたくせに！"
	},
	{
		"id": 54,
		"start": 452.1935739999999,
		"end": 456.1975739999998,
		"text": "リク王様は<br />私にとって義父であり→"
	},
	{
		"id": 55,
		"start": 456.1975739999998,
		"end": 460.2015739999998,
		"text": "悪童のころより世話をかけた<br />大恩人。→"
	},
	{
		"id": 56,
		"start": 460.2015739999998,
		"end": 467.1415739999999,
		"text": "世界にあっても　リク王家は<br />平和の真意を知る尊い一族。→"
	},
	{
		"id": 57,
		"start": 467.1415739999999,
		"end": 472.1465739999999,
		"text": "今ほどに富もなく<br />栄えてはいなかった。→"
	},
	{
		"id": 58,
		"start": 472.1465739999999,
		"end": 475.1495739999998,
		"text": "だからといって<br />不幸であったわけではない！"
	},
	{
		"id": 59,
		"start": 475.1495739999998,
		"end": 479.1495739999998,
		"text": "う～ん…　あ～。"
	},
	{
		"id": 60,
		"start": 481.1555739999999,
		"end": 485.1595739999998,
		"text": "（キュロス）空は高く　花が咲き誇り→"
	},
	{
		"id": 61,
		"start": 485.1595739999998,
		"end": 488.16257399999984,
		"text": "心あふれる　素晴らしき国。→"
	},
	{
		"id": 62,
		"start": 488.16257399999984,
		"end": 491.1655739999999,
		"text": "それが真のドレスローザだ。→"
	},
	{
		"id": 63,
		"start": 491.1655739999999,
		"end": 496.1705739999999,
		"text": "金や宝石なぞ<br />石ころに心は宿らない。→"
	},
	{
		"id": 64,
		"start": 496.1705739999999,
		"end": 502.17657399999985,
		"text": "命の営みこそが<br />本当の幸せだと教えてくれる。"
	},
	{
		"id": 65,
		"start": 502.17657399999985,
		"end": 505.1795739999999,
		"text": "リク王様は　そういうお方だ。→"
	},
	{
		"id": 66,
		"start": 505.1795739999999,
		"end": 507.18157399999984,
		"text": "貴様らを追い出し→"
	},
	{
		"id": 67,
		"start": 507.18157399999984,
		"end": 512.1815739999998,
		"text": "彼が再び王座に就くことを<br />今　国中が望んでいる。"
	},
	{
		"id": 68,
		"start": 518.1925739999998,
		"end": 520.1925739999998,
		"text": "（ピーカ）了解した。"
	},
	{
		"id": 69,
		"start": 522.1965739999998,
		"end": 526.1345739999998,
		"text": "（レベッカ）あっ　消えた？"
	},
	{
		"id": 70,
		"start": 526.1345739999998,
		"end": 529.1345739999998,
		"text": "（ロビン）どういうこと？"
	},
	{
		"id": 71,
		"start": 533.1415739999999,
		"end": 536.1445739999998,
		"text": "（レベッカ・キュロス）あっ。"
	},
	{
		"id": 72,
		"start": 536.1445739999998,
		"end": 539.1475739999998,
		"text": "（ピーカ）舞踏石！"
	},
	{
		"id": 73,
		"start": 539.1475739999998,
		"end": 544.1525739999998,
		"text": "（バルトロメオ）うわ～！<br />なっ　何だベ！？　ビビった！"
	},
	{
		"id": 74,
		"start": 544.1525739999998,
		"end": 546.1525739999998,
		"text": "今度は　３段目に。"
	},
	{
		"id": 75,
		"start": 548.1565739999999,
		"end": 550.1585739999998,
		"text": "（ゾロ）くっそ！　あの野郎。"
	},
	{
		"id": 76,
		"start": 550.1585739999998,
		"end": 554.1585739999998,
		"text": "（ゾロ）相手は俺だってのに<br />戦う気あんのか！"
	},
	{
		"id": 77,
		"start": 560.1685739999998,
		"end": 563.1715739999999,
		"text": "いずれにせよ　台地の中を<br />うろうろ移動されたんじゃ→"
	},
	{
		"id": 78,
		"start": 563.1715739999999,
		"end": 565.1735739999998,
		"text": "仕留められねえ。"
	},
	{
		"id": 79,
		"start": 565.1735739999998,
		"end": 568.1765739999998,
		"text": "次　射程距離内に入ったら<br />覚悟しろ！"
	},
	{
		"id": 80,
		"start": 568.1765739999998,
		"end": 571.1795739999999,
		"text": "≪（ピーカ）舞踏石！"
	},
	{
		"id": 81,
		"start": 571.1795739999999,
		"end": 573.1815739999998,
		"text": "ピーカさま　おやめください！<br />うおっ！"
	},
	{
		"id": 82,
		"start": 573.1815739999998,
		"end": 576.1815739999998,
		"text": "ピーカさま！<br />（部下）助けて！"
	},
	{
		"id": 83,
		"start": 582.1905739999999,
		"end": 585.2095739999999,
		"text": "あの野郎　負傷者を！"
	},
	{
		"id": 84,
		"start": 585.2095739999999,
		"end": 588.1295739999998,
		"text": "（サイ）来るぞ　気を付けろ！<br />（ベビー５）はい　あなた。"
	},
	{
		"id": 85,
		"start": 588.1295739999998,
		"end": 590.1315739999999,
		"text": "くっつくな！<br />（ベビー５）あっ。　もう→"
	},
	{
		"id": 86,
		"start": 590.1315739999999,
		"end": 594.1355739999998,
		"text": "夫婦になるんだから<br />そんなに照れなくったって。"
	},
	{
		"id": 87,
		"start": 594.1355739999998,
		"end": 597.1385739999998,
		"text": "あっ！<br />（ピーカ）裏切ったな　ベビー５。→"
	},
	{
		"id": 88,
		"start": 597.1385739999998,
		"end": 600.1385739999998,
		"text": "石押！<br />（サイ）危ねえ！"
	},
	{
		"id": 89,
		"start": 603.1445739999998,
		"end": 605.1465739999999,
		"text": "（ベビー５）ああっ。<br />（チンジャオ）サイ！"
	},
	{
		"id": 90,
		"start": 605.1465739999999,
		"end": 607.1485739999998,
		"text": "あなた！"
	},
	{
		"id": 91,
		"start": 607.1485739999998,
		"end": 609.1485739999998,
		"text": "（サイ）ああっ…。"
	},
	{
		"id": 92,
		"start": 611.1525739999998,
		"end": 613.1545739999998,
		"text": "（サイ）おおおお…！"
	},
	{
		"id": 93,
		"start": 613.1545739999998,
		"end": 616.1575739999998,
		"text": "おお　無事か。　今　助けるぞ。"
	},
	{
		"id": 94,
		"start": 616.1575739999998,
		"end": 621.1625739999998,
		"text": "バカ野郎！　じじい　後ろだ！<br />（チンジャオ）うん？"
	},
	{
		"id": 95,
		"start": 621.1625739999998,
		"end": 623.8315739999998,
		"text": "ごみどもが！　舞踏石！"
	},
	{
		"id": 96,
		"start": 623.8315739999998,
		"end": 626.1315739999999,
		"text": "（ベビー５）ああっ！　あっ。"
	},
	{
		"id": 97,
		"start": 629.0705739999999,
		"end": 631.0725739999998,
		"text": "（サイ）でりゃっ！→"
	},
	{
		"id": 98,
		"start": 631.0725739999998,
		"end": 636.0725739999998,
		"text": "ハァ　ハァ…　くそったれが。"
	},
	{
		"id": 99,
		"start": 638.0795739999999,
		"end": 641.0825739999998,
		"text": "石の野郎<br />いいかげんにしろ　てめえ！"
	},
	{
		"id": 100,
		"start": 641.0825739999998,
		"end": 644.0855739999998,
		"text": "弱ったやつらに<br />手ぇ出しやがって！"
	},
	{
		"id": 101,
		"start": 644.0855739999998,
		"end": 647.0215739999999,
		"text": "ロロノア。　噛石！"
	},
	{
		"id": 102,
		"start": 647.0215739999999,
		"end": 650.0245739999998,
		"text": "一刀流　馬鬼！"
	},
	{
		"id": 103,
		"start": 650.0245739999998,
		"end": 652.0265739999999,
		"text": "なあ　ロロノア・ゾロ→"
	},
	{
		"id": 104,
		"start": 652.0265739999999,
		"end": 657.0315739999999,
		"text": "旧　王の台地に<br />誰がいるか知ってるか？"
	},
	{
		"id": 105,
		"start": 657.0315739999999,
		"end": 660.0345739999998,
		"text": "この国の王だろ？<br />（ピーカ）「元」だ。"
	},
	{
		"id": 106,
		"start": 660.0345739999998,
		"end": 666.0405739999999,
		"text": "何が言いてえんだ　てめえ。<br />おかしいと思わないか？"
	},
	{
		"id": 107,
		"start": 666.0405739999999,
		"end": 670.0445739999999,
		"text": "何が！<br />侵略は世の常だ。→"
	},
	{
		"id": 108,
		"start": 670.0445739999999,
		"end": 675.0495739999999,
		"text": "なぜ　ドフィーだけが責められる。"
	},
	{
		"id": 109,
		"start": 675.0495739999999,
		"end": 678.0495739999999,
		"text": "ごちゃごちゃ　うるせえんだよ。"
	},
	{
		"id": 110,
		"start": 681.0555739999999,
		"end": 687.0615739999998,
		"text": "（ピーカ）旧　王の台地には<br />昔のこの国の要人たちに加え→"
	},
	{
		"id": 111,
		"start": 687.0615739999998,
		"end": 691.0655739999999,
		"text": "侍たちに<br />ゴッド・ウソップもいる。→"
	},
	{
		"id": 112,
		"start": 691.0655739999999,
		"end": 697.0715739999998,
		"text": "国中の人間たちが　リク王の復古を望んでいるそうだ。"
	},
	{
		"id": 113,
		"start": 697.0715739999998,
		"end": 700.0745739999999,
		"text": "それが　どうした！<br />（ピーカ）目障りじゃないか。→"
	},
	{
		"id": 114,
		"start": 700.0745739999999,
		"end": 703.0745739999999,
		"text": "王は　ドフィー１人だ。"
	},
	{
		"id": 115,
		"start": 712.0195739999998,
		"end": 714.0195739999998,
		"text": "うん？"
	},
	{
		"id": 116,
		"start": 725.0325739999998,
		"end": 727.0325739999998,
		"text": "あっ。"
	},
	{
		"id": 117,
		"start": 731.0385739999998,
		"end": 733.0405739999999,
		"text": "何やい！？"
	},
	{
		"id": 118,
		"start": 733.0405739999999,
		"end": 735.0425739999998,
		"text": "（ロビン）何が起こっているの？"
	},
	{
		"id": 119,
		"start": 735.0425739999998,
		"end": 737.0445739999999,
		"text": "（キュロス）あれを見ろ！"
	},
	{
		"id": 120,
		"start": 737.0445739999999,
		"end": 757.0645739999999,
		"text": "♪♪～"
	},
	{
		"id": 121,
		"start": 757.0645739999999,
		"end": 772.0795739999999,
		"text": "♪♪～"
	},
	{
		"id": 122,
		"start": 772.0795739999999,
		"end": 774.0815739999998,
		"text": "やめろ。"
	},
	{
		"id": 123,
		"start": 774.0815739999998,
		"end": 794.1015739999998,
		"text": "♪♪～"
	},
	{
		"id": 124,
		"start": 794.1015739999998,
		"end": 804.1115739999998,
		"text": "♪♪～"
	},
	{
		"id": 125,
		"start": 804.1115739999998,
		"end": 807.1145739999998,
		"text": "また動き始めたぞ。"
	},
	{
		"id": 126,
		"start": 807.1145739999998,
		"end": 809.1145739999998,
		"text": "ど…　どうするつもりだ。"
	},
	{
		"id": 127,
		"start": 816.1235739999998,
		"end": 820.1235739999998,
		"text": "ここは　ドンキホーテの国だ。"
	},
	{
		"id": 128,
		"start": 844.0855739999998,
		"end": 847.0885739999999,
		"text": "（海兵）歩くぞ　逃げろ！"
	},
	{
		"id": 129,
		"start": 847.0885739999999,
		"end": 851.0925739999998,
		"text": "石像が動きだした　逃げろ！<br />逃げろ！"
	},
	{
		"id": 130,
		"start": 851.0925739999998,
		"end": 864.1055739999999,
		"text": "♪♪～"
	},
	{
		"id": 131,
		"start": 864.1055739999999,
		"end": 866.1055739999999,
		"text": "おいおい　まさか…。"
	},
	{
		"id": 132,
		"start": 871.1125739999998,
		"end": 874.1155739999999,
		"text": "（ウソップ）あ～。"
	},
	{
		"id": 133,
		"start": 874.1155739999999,
		"end": 878.1195739999998,
		"text": "な…　何なんだ！？<br />何しようってんだ。"
	},
	{
		"id": 134,
		"start": 878.1195739999998,
		"end": 892.1665739999999,
		"text": "♪♪～"
	},
	{
		"id": 135,
		"start": 892.1665739999999,
		"end": 894.1685739999998,
		"text": "あの野郎！"
	},
	{
		"id": 136,
		"start": 894.1685739999998,
		"end": 897.1685739999998,
		"text": "王を消す気か！？"
	},
	{
		"id": 137,
		"start": 913.8437509999785,
		"end": 923.8537509999783,
		"text": "♪♪～"
	},
	{
		"id": 138,
		"start": 923.8537509999783,
		"end": 938.8687509999895,
		"text": "♪♪～"
	},
	{
		"id": 139,
		"start": 938.8687509999895,
		"end": 941.8717509999926,
		"text": "（男性）逃げろ！<br />（男性）つぶされるぞ！"
	},
	{
		"id": 140,
		"start": 941.8717509999926,
		"end": 961.8917510000153,
		"text": "♪♪～"
	},
	{
		"id": 141,
		"start": 961.8917510000153,
		"end": 981.8447510000381,
		"text": "♪♪～"
	},
	{
		"id": 142,
		"start": 981.8447510000381,
		"end": 984.8477510000416,
		"text": "♪♪～"
	},
	{
		"id": 143,
		"start": 984.8477510000416,
		"end": 988.8517510000461,
		"text": "（将校）砲撃用意！→"
	},
	{
		"id": 144,
		"start": 988.8517510000461,
		"end": 990.8517510000484,
		"text": "撃て！"
	},
	{
		"id": 145,
		"start": 993.8567510000519,
		"end": 995.8567510000541,
		"text": "撃て　撃て！"
	},
	{
		"id": 146,
		"start": 1008.8717510000688,
		"end": 1013.8767510000746,
		"text": "野郎！　それで　俺を<br />こんな端まで追い込んだのか。"
	},
	{
		"id": 147,
		"start": 1013.8767510000746,
		"end": 1016.876751000078,
		"text": "くっ　追い付けねえ！"
	},
	{
		"id": 148,
		"start": 1019.8827510000813,
		"end": 1021.884751000084,
		"text": "まんまと　わなにはまった。"
	},
	{
		"id": 149,
		"start": 1021.884751000084,
		"end": 1024.8877510000862,
		"text": "まずいな　このままじゃ。"
	},
	{
		"id": 150,
		"start": 1024.8877510000862,
		"end": 1029.8267510000865,
		"text": "あいつのパンチなら<br />王の台地は一発で粉々だ。"
	},
	{
		"id": 151,
		"start": 1029.8267510000865,
		"end": 1034.8317510000861,
		"text": "ウソップも王も　つぶされて即死。<br />何か　いい手はねえか。"
	},
	{
		"id": 152,
		"start": 1034.8317510000861,
		"end": 1040.8377510000864,
		"text": "（部下）海賊狩り！　これ以上<br />ピーカさまの邪魔はさせないぞ！"
	},
	{
		"id": 153,
		"start": 1040.8377510000864,
		"end": 1042.8397510000864,
		"text": "（部下たち）あ～！"
	},
	{
		"id": 154,
		"start": 1042.8397510000864,
		"end": 1044.8397510000864,
		"text": "うっとうしいんだよ。"
	},
	{
		"id": 155,
		"start": 1046.8437510000863,
		"end": 1049.8467510000864,
		"text": "（ロビン）ゾロ。<br />（キュロス）まずいぞ！→"
	},
	{
		"id": 156,
		"start": 1049.8467510000864,
		"end": 1052.8467510000864,
		"text": "あいつを行かせては。"
	},
	{
		"id": 157,
		"start": 1054.851751000086,
		"end": 1058.851751000086,
		"text": "だが　あんなもの<br />止める手段がない。"
	},
	{
		"id": 158,
		"start": 1062.8597510000864,
		"end": 1064.8597510000864,
		"text": "おじいさま。<br />（ロビン）ウソップ。"
	},
	{
		"id": 159,
		"start": 1066.8637510000863,
		"end": 1071.8637510000863,
		"text": "あー！　あんの野郎！"
	},
	{
		"id": 160,
		"start": 1086.8167510000862,
		"end": 1091.8167510000862,
		"text": "いや　遠過ぎて威力不足。<br />仕留めきれねえ。"
	},
	{
		"id": 161,
		"start": 1101.8317510000861,
		"end": 1105.8317510000861,
		"text": "どりゃあ～！"
	},
	{
		"id": 162,
		"start": 1112.842751000086,
		"end": 1114.842751000086,
		"text": "《無理だな》"
	},
	{
		"id": 163,
		"start": 1126.8567510000862,
		"end": 1131.8567510000862,
		"text": "ウソップ～！"
	},
	{
		"id": 164,
		"start": 1136.8667510000864,
		"end": 1140.8667510000864,
		"text": "聞こえるわけねえか　この距離。"
	},
	{
		"id": 165,
		"start": 1158.8217510000864,
		"end": 1161.824751000086,
		"text": "ああ　持ってねえな。"
	},
	{
		"id": 166,
		"start": 1161.824751000086,
		"end": 1167.824751000086,
		"text": "ギャ～！　こっち来んな～！"
	},
	{
		"id": 167,
		"start": 1173.8367510000862,
		"end": 1179.842751000086,
		"text": "どのみち　それを知らせても<br />全員で逃げ出す時間がねえ。"
	},
	{
		"id": 168,
		"start": 1179.842751000086,
		"end": 1181.842751000086,
		"text": "（海兵たち）うわ～！"
	},
	{
		"id": 169,
		"start": 1187.8507510000863,
		"end": 1191.8547510000863,
		"text": "やっちゃえ！<br />（部下）ピーカさま！"
	},
	{
		"id": 170,
		"start": 1191.8547510000863,
		"end": 1194.8547510000863,
		"text": "リク王を討つ気だわ。"
	},
	{
		"id": 171,
		"start": 1196.8597510000864,
		"end": 1199.862751000086,
		"text": "（サイ）俺らにゃ関係ねえが<br />解せねえ。"
	},
	{
		"id": 172,
		"start": 1199.862751000086,
		"end": 1202.8657510000862,
		"text": "（オオロンブス）<br />いったい　どうしたものか。"
	},
	{
		"id": 173,
		"start": 1202.8657510000862,
		"end": 1206.8027510000861,
		"text": "やめれ～　石の怪物！"
	},
	{
		"id": 174,
		"start": 1206.8027510000861,
		"end": 1209.8057510000863,
		"text": "そっちの台地にゃ<br />そげキング先輩が！"
	},
	{
		"id": 175,
		"start": 1209.8057510000863,
		"end": 1215.8057510000863,
		"text": "石！　戻ってこい！<br />俺が相手だ～！"
	},
	{
		"id": 176,
		"start": 1219.815751000086,
		"end": 1223.8197510000864,
		"text": "（ウソップ）い～や～！"
	},
	{
		"id": 177,
		"start": 1223.8197510000864,
		"end": 1226.8197510000864,
		"text": "（３人）うわ～！"
	},
	{
		"id": 178,
		"start": 1230.8267510000865,
		"end": 1234.8307510000864,
		"text": "（エリザベロー）<br />なるほど　海賊狩りの言うとおり→"
	},
	{
		"id": 179,
		"start": 1234.8307510000864,
		"end": 1238.8307510000864,
		"text": "このキング・パンチ<br />取って置いてよかった。"
	},
	{
		"id": 180,
		"start": 1243.8397510000864,
		"end": 1246.842751000086,
		"text": "今　救うぞ<br />わが友　リク王。"
	},
	{
		"id": 181,
		"start": 1246.842751000086,
		"end": 1250.8467510000864,
		"text": "≪ちょっと待て！　どっかの王。<br />「どっかの」！？"
	},
	{
		"id": 182,
		"start": 1250.8467510000864,
		"end": 1254.8507510000863,
		"text": "私は　プロデンスの王<br />エリザベローだ　海賊狩り。"
	},
	{
		"id": 183,
		"start": 1254.8507510000863,
		"end": 1257.853751000086,
		"text": "ただ破壊しても無駄と言ったろ。"
	},
	{
		"id": 184,
		"start": 1257.853751000086,
		"end": 1261.8577510000864,
		"text": "本体を倒さなきゃ<br />何度でも復活する。"
	},
	{
		"id": 185,
		"start": 1261.8577510000864,
		"end": 1263.8597510000864,
		"text": "ならば　どうする？"
	},
	{
		"id": 186,
		"start": 1263.8597510000864,
		"end": 1283.813751000086,
		"text": "♪♪～"
	},
	{
		"id": 187,
		"start": 1283.813751000086,
		"end": 1291.8217510000864,
		"text": "♪♪～"
	},
	{
		"id": 188,
		"start": 1291.8217510000864,
		"end": 1294.824751000086,
		"text": "もう時間はなさそうだ。"
	},
	{
		"id": 189,
		"start": 1294.824751000086,
		"end": 1304.8347510000863,
		"text": "♪♪～"
	},
	{
		"id": 190,
		"start": 1304.8347510000863,
		"end": 1307.8347510000863,
		"text": "助けて～！"
	},
	{
		"id": 191,
		"start": 1309.8397510000864,
		"end": 1311.8417510000863,
		"text": "（エリザベロー）海賊狩り！"
	},
	{
		"id": 192,
		"start": 1311.8417510000863,
		"end": 1323.853751000086,
		"text": "♪♪～"
	},
	{
		"id": 193,
		"start": 1323.853751000086,
		"end": 1325.871751000086,
		"text": "うん？"
	},
	{
		"id": 194,
		"start": 1325.871751000086,
		"end": 1327.871751000086,
		"text": "フッ。"
	},
	{
		"id": 195,
		"start": 1337.8007510000862,
		"end": 1340.8037510000863,
		"text": "（エリザベロー）おい。<br />２分　待て。"
	},
	{
		"id": 196,
		"start": 1340.8037510000863,
		"end": 1360.8237510000863,
		"text": "♪♪～"
	},
	{
		"id": 197,
		"start": 1360.8237510000863,
		"end": 1365.8287510000864,
		"text": "♪♪～"
	},
	{
		"id": 198,
		"start": 1365.8287510000864,
		"end": 1367.8307510000864,
		"text": "（ピーカ）リク王はいるか？"
	},
	{
		"id": 199,
		"start": 1367.8307510000864,
		"end": 1369.8327510000863,
		"text": "（ウソップ）あ～！"
	},
	{
		"id": 200,
		"start": 1369.8327510000863,
		"end": 1372.8327510000863,
		"text": "あー　あー　あー　あー　あ～！"
	},
	{
		"id": 201,
		"start": 1392.7887510000864,
		"end": 1395.7887510000864,
		"text": "これでいこう。"
	},
	{
		"id": 202,
		"start": 1403.7997510000864,
		"end": 1406.8027510000861,
		"text": "＜天を覆うほど巨大な<br />ピーカの拳が→"
	},
	{
		"id": 203,
		"start": 1406.8027510000861,
		"end": 1408.804751000086,
		"text": "リク王　目がけ　落ちてくる＞"
	},
	{
		"id": 204,
		"start": 1408.804751000086,
		"end": 1411.8077510000862,
		"text": "＜ドレスローザにともった<br />希望の光が押しつぶされ→"
	},
	{
		"id": 205,
		"start": 1411.8077510000862,
		"end": 1415.8117510000861,
		"text": "再び　力の支配の時代が<br />到来しようとした　その時→"
	},
	{
		"id": 206,
		"start": 1415.8117510000861,
		"end": 1418.8147510000863,
		"text": "ゾロとピーカとの<br />最後の戦いが始まる！＞"
	},
	{
		"id": 207,
		"start": 1418.8147510000863,
		"end": 1420.8147510000863,
		"text": "次回　『ワンピース』"
	},
	{
		"id": 208,
		"start": 1423.8197510000864,
		"end": 1426.8197510000864,
		"text": "海賊王に　俺は　なる！"
	},
	{
		"id": 209,
		"start": 1438.2677510000863,
		"end": 1439.7527510000864,
		"text": "（山﨑・東野）おはようございます。<br />（東野）『ワイドナショー』のお時間です。"
	},
	{
		"id": 210,
		"start": 1439.7527510000864,
		"end": 1442.371751000086,
		"text": "さあ　松本さん　ちょっと<br />お疲れな様子でございますけど。"
	},
	{
		"id": 211,
		"start": 1442.371751000086,
		"end": 1444.373751000086,
		"text": "（松本）そんなことは<br />全然ないですよ。"
	},
	{
		"id": 212,
		"start": 1444.373751000086,
		"end": 1447.3767510000862,
		"text": "そんなことは　全然ないんですが<br />今日もね→"
	},
	{
		"id": 213,
		"start": 1447.3767510000862,
		"end": 1450.6467510000862,
		"text": "頭のいい人と　バカな人と<br />色々　いますよね。"
	},
	{
		"id": 214,
		"start": 1450.6467510000862,
		"end": 1453.4327510000862,
		"text": "エキセントリックな人とかね<br />いますから。"
	},
	{
		"id": 215,
		"start": 1453.4327510000862,
		"end": 1455.3847510000865,
		"text": "よろしくお願いします。<br />さあ　堀　潤さん。"
	},
	{
		"id": 216,
		"start": 1455.3847510000865,
		"end": 1457.3867510000864,
		"text": "２回目でございます。<br />よろしくお願いします。"
	},
	{
		"id": 217,
		"start": 1457.3867510000864,
		"end": 1459.9557510000864,
		"text": "（堀）前回も<br />ありがとうございました。"
	},
	{
		"id": 218,
		"start": 1459.9557510000864,
		"end": 1462.5087510000862,
		"text": "色々　反響も大きくて。<br />ちょっと　何かありましたね。"
	}
]