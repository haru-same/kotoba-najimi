[
	{
		"id": 1,
		"start": 123.839,
		"end": 133.839,
		"text": "♪♪～"
	},
	{
		"id": 2,
		"start": 344.793,
		"end": 348.797,
		"text": "＜ワンピースを狙う<br />全ての海賊たちが目指す→"
	},
	{
		"id": 3,
		"start": 348.797,
		"end": 355.804,
		"text": "グランドライン後半の海<br />人呼んで　新世界＞"
	},
	{
		"id": 4,
		"start": 355.804,
		"end": 359.808,
		"text": "＜しかし<br />かつて　その海を制したのは→"
	},
	{
		"id": 5,
		"start": 359.808,
		"end": 363.812,
		"text": "海賊王　ゴールド・ロジャー<br />ただ　一人＞"
	},
	{
		"id": 6,
		"start": 363.812,
		"end": 370.819,
		"text": "＜新世界は　幾多の海賊たちの<br />夢と野望を阻んできた＞"
	},
	{
		"id": 7,
		"start": 370.819,
		"end": 374.823,
		"text": "＜その海に今　ゴムゴムの実を食べゴム人間となった少年→"
	},
	{
		"id": 8,
		"start": 374.823,
		"end": 378.827,
		"text": "モンキー・Ｄ・ルフィと<br />その仲間たちが挑む！＞"
	},
	{
		"id": 9,
		"start": 378.827,
		"end": 383.827,
		"text": "（ルフィ）海賊王に　俺は　なる！"
	},
	{
		"id": 10,
		"start": 386.835,
		"end": 389.838,
		"text": "＜新世界を支配する<br />四皇の一角を崩すため→"
	},
	{
		"id": 11,
		"start": 389.838,
		"end": 392.841,
		"text": "トラファルガー・ローと<br />海賊同盟を組んだ　ルフィたちは→"
	},
	{
		"id": 12,
		"start": 392.841,
		"end": 395.844,
		"text": "ドレスローザに乗り込むも→"
	},
	{
		"id": 13,
		"start": 395.844,
		"end": 399.848,
		"text": "現国王　ドフラミンゴの<br />計略によって→"
	},
	{
		"id": 14,
		"start": 399.848,
		"end": 401.866,
		"text": "島ごと閉じ込められてしまう＞"
	},
	{
		"id": 15,
		"start": 401.866,
		"end": 405.787,
		"text": "＜一味が　ドンキホーテファミリーと<br />激しい戦いを繰り広げる中→"
	},
	{
		"id": 16,
		"start": 405.787,
		"end": 410.792,
		"text": "ルフィとローは　王宮で<br />ついに　ドフラミンゴと激突した＞"
	},
	{
		"id": 17,
		"start": 410.792,
		"end": 414.796,
		"text": "＜そして　ゾロと　ピーカも＞"
	},
	{
		"id": 18,
		"start": 414.796,
		"end": 418.796,
		"text": "（ピーカ）<br />ここは　ドンキホーテの国だ。"
	},
	{
		"id": 19,
		"start": 420.802,
		"end": 422.804,
		"text": "（ゾロ）あの野郎！"
	},
	{
		"id": 20,
		"start": 422.804,
		"end": 424.806,
		"text": "王を消す気か！？"
	},
	{
		"id": 21,
		"start": 424.806,
		"end": 441.823,
		"text": "♪♪～"
	},
	{
		"id": 22,
		"start": 441.823,
		"end": 443.825,
		"text": "（エリザベロー）今　救うぞ<br />わが友　リク王。"
	},
	{
		"id": 23,
		"start": 443.825,
		"end": 446.825,
		"text": "≪ちょっと待て！　どっかの王。"
	},
	{
		"id": 24,
		"start": 449.831,
		"end": 453.831,
		"text": "（エリザベロー）おい。<br />２分　待て。"
	},
	{
		"id": 25,
		"start": 461.81,
		"end": 463.678,
		"text": "（ピーカ）リク王はいるか？"
	},
	{
		"id": 26,
		"start": 463.678,
		"end": 467.678,
		"text": "（ウソップ）<br />あー　あー　あー　あー　あ～！"
	},
	{
		"id": 27,
		"start": 471.686,
		"end": 478.693,
		"text": "《作戦５<br />空を飛び　やつを　ぶった切る！》"
	},
	{
		"id": 28,
		"start": 478.693,
		"end": 481.693,
		"text": "これでいこう。"
	},
	{
		"id": 29,
		"start": 492.04,
		"end": 508.723,
		"text": "♪♪～"
	},
	{
		"id": 30,
		"start": 508.723,
		"end": 511.726,
		"text": "（一同）うわ～！"
	},
	{
		"id": 31,
		"start": 511.726,
		"end": 531.779,
		"text": "♪♪～"
	},
	{
		"id": 32,
		"start": 531.779,
		"end": 533.781,
		"text": "ひぇ～！"
	},
	{
		"id": 33,
		"start": 533.781,
		"end": 536.781,
		"text": "（カン十郎・錦えもん）うわ～！"
	},
	{
		"id": 34,
		"start": 539.787,
		"end": 541.787,
		"text": "（ピーカ）ん～。"
	},
	{
		"id": 35,
		"start": 546.794,
		"end": 548.796,
		"text": "（キュロス）クッ。　このままでは！"
	},
	{
		"id": 36,
		"start": 548.796,
		"end": 550.798,
		"text": "（レベッカ）おじいさまたちが。"
	},
	{
		"id": 37,
		"start": 550.798,
		"end": 554.798,
		"text": "（バルトロメオ）そげキング先輩<br />逃げるっぺ！"
	},
	{
		"id": 38,
		"start": 560.808,
		"end": 564.812,
		"text": "（エリザベロー）<br />シュッ　シュッ　シュッ…。"
	},
	{
		"id": 39,
		"start": 564.812,
		"end": 568.816,
		"text": "海賊狩り。　いつまで待たせる。"
	},
	{
		"id": 40,
		"start": 568.816,
		"end": 570.818,
		"text": "おい　カントク！"
	},
	{
		"id": 41,
		"start": 570.818,
		"end": 573.821,
		"text": "（オオロンブス）んっ？<br />提督なら　われであるが？"
	},
	{
		"id": 42,
		"start": 573.821,
		"end": 577.825,
		"text": "どっちでもいい。　さっき<br />弾丸みてえに　人を投げてたな。"
	},
	{
		"id": 43,
		"start": 577.825,
		"end": 579.825,
		"text": "（オオロンブス）んっ？<br />フッ。"
	},
	{
		"id": 44,
		"start": 581.813,
		"end": 586.768,
		"text": "覚悟しろ　リク王！<br />（リク王）クッ。"
	},
	{
		"id": 45,
		"start": 586.768,
		"end": 591.773,
		"text": "フフフ。<br />（リク王）んっ…。"
	},
	{
		"id": 46,
		"start": 591.773,
		"end": 594.776,
		"text": "（部下たち）お～　ピーカさま！"
	},
	{
		"id": 47,
		"start": 594.776,
		"end": 597.779,
		"text": "（部下）そのまま<br />リク王と　ゴッド・ウソップを→"
	},
	{
		"id": 48,
		"start": 597.779,
		"end": 600.779,
		"text": "ぶっつぶしてやってください！"
	},
	{
		"id": 49,
		"start": 602.784,
		"end": 606.784,
		"text": "させるか！<br />（海兵）撃て　撃て！"
	},
	{
		"id": 50,
		"start": 611.793,
		"end": 614.793,
		"text": "んっ？<br />（海兵たち）あっ…。"
	},
	{
		"id": 51,
		"start": 616.798,
		"end": 620.798,
		"text": "（海兵）たっ　退避～！<br />（海兵）逃げろ～！"
	},
	{
		"id": 52,
		"start": 622.804,
		"end": 627.804,
		"text": "（一同）うわ～！"
	},
	{
		"id": 53,
		"start": 642.757,
		"end": 645.757,
		"text": "（リク王）やめろ！！<br />（ピーカ）んっ？"
	},
	{
		"id": 54,
		"start": 648.763,
		"end": 655.763,
		"text": "狙いは　私だろ！<br />これ以上　暴れないでくれ！"
	},
	{
		"id": 55,
		"start": 658.773,
		"end": 660.775,
		"text": "タンク。　皆を逃がすんだ。"
	},
	{
		"id": 56,
		"start": 660.775,
		"end": 664.779,
		"text": "（レパント）はっ。<br />皆の者　早く逃げろ！"
	},
	{
		"id": 57,
		"start": 664.779,
		"end": 667.779,
		"text": "（レパント）この台地から下りるんだ。"
	},
	{
		"id": 58,
		"start": 674.789,
		"end": 677.792,
		"text": "（一同）うわ～！"
	},
	{
		"id": 59,
		"start": 677.792,
		"end": 694.809,
		"text": "♪♪～"
	},
	{
		"id": 60,
		"start": 694.809,
		"end": 697.809,
		"text": "リク王様も　早く　お逃げください。"
	},
	{
		"id": 61,
		"start": 701.816,
		"end": 703.751,
		"text": "（ヴィオラ）早く　お父さま！"
	},
	{
		"id": 62,
		"start": 703.751,
		"end": 707.755,
		"text": "ヴィオラ　タンク！<br />皆を連れて　下へ早く！"
	},
	{
		"id": 63,
		"start": 707.755,
		"end": 710.758,
		"text": "やつの狙いは　私だ。　行け！"
	},
	{
		"id": 64,
		"start": 710.758,
		"end": 714.762,
		"text": "だから　逃げるのです　リク王様！<br />お早く！"
	},
	{
		"id": 65,
		"start": 714.762,
		"end": 722.762,
		"text": "いや　やつが私を狙っているうちに皆が逃げられるなら　それで　いい。"
	},
	{
		"id": 66,
		"start": 725.773,
		"end": 728.773,
		"text": "国王様…。"
	},
	{
		"id": 67,
		"start": 730.778,
		"end": 735.783,
		"text": "（ピーカ）そうか。<br />じゃあ　望みどおりにしてやろう。"
	},
	{
		"id": 68,
		"start": 735.783,
		"end": 738.783,
		"text": "クッ…。<br />（ピーカ）フフフッ。"
	},
	{
		"id": 69,
		"start": 745.793,
		"end": 751.793,
		"text": "あ～！<br />手のひらが　町の広さだ～！"
	},
	{
		"id": 70,
		"start": 760.808,
		"end": 762.808,
		"text": "（ハック・ヴィオラ）あ～…。"
	},
	{
		"id": 71,
		"start": 772.754,
		"end": 775.757,
		"text": "（オオロンブス）ん～。<br />（ベビー５・サイ）んっ？"
	},
	{
		"id": 72,
		"start": 775.757,
		"end": 778.76,
		"text": "（サイ）何やい。<br />海賊狩り　オオロンブス。→"
	},
	{
		"id": 73,
		"start": 778.76,
		"end": 780.762,
		"text": "何する気やい。"
	},
	{
		"id": 74,
		"start": 780.762,
		"end": 782.764,
		"text": "本当に　いいんだな？"
	},
	{
		"id": 75,
		"start": 782.764,
		"end": 786.768,
		"text": "人を安全に投げるための技では<br />ないぞ！"
	},
	{
		"id": 76,
		"start": 786.768,
		"end": 788.77,
		"text": "さっさとやれ。　時間がねえ。"
	},
	{
		"id": 77,
		"start": 788.77,
		"end": 790.772,
		"text": "ピッキャララララ。"
	},
	{
		"id": 78,
		"start": 790.772,
		"end": 796.778,
		"text": "この王の台地ごと<br />ぶっつぶしてやる。"
	},
	{
		"id": 79,
		"start": 796.778,
		"end": 801.778,
		"text": "（３人）うわ～！"
	},
	{
		"id": 80,
		"start": 808.79,
		"end": 812.794,
		"text": "あの野郎　戦意のねえやつらばっか狙いやがって！"
	},
	{
		"id": 81,
		"start": 812.794,
		"end": 815.794,
		"text": "いくぞ！<br />おう！"
	},
	{
		"id": 82,
		"start": 819.801,
		"end": 824.801,
		"text": "う～お～！"
	},
	{
		"id": 83,
		"start": 827.742,
		"end": 831.746,
		"text": "（ピーカ）リク王。<br />お前は　王じゃない。→"
	},
	{
		"id": 84,
		"start": 831.746,
		"end": 835.75,
		"text": "王の器でもない。"
	},
	{
		"id": 85,
		"start": 835.75,
		"end": 840.75,
		"text": "提督…。"
	},
	{
		"id": 86,
		"start": 851.766,
		"end": 853.766,
		"text": "んっ？"
	},
	{
		"id": 87,
		"start": 857.772,
		"end": 862.777,
		"text": "国王様が逃げないなら<br />われわれも逃げません！"
	},
	{
		"id": 88,
		"start": 862.777,
		"end": 866.781,
		"text": "（一同）<br />国王様！　国王様！　国王様！→"
	},
	{
		"id": 89,
		"start": 866.781,
		"end": 872.787,
		"text": "国王様！　リク王様！　国王様！<br />国王様！　国王様！"
	},
	{
		"id": 90,
		"start": 872.787,
		"end": 875.79,
		"text": "あっ…。"
	},
	{
		"id": 91,
		"start": 875.79,
		"end": 877.792,
		"text": "（ピーカ）リク王。→"
	},
	{
		"id": 92,
		"start": 877.792,
		"end": 884.732,
		"text": "１０年の誤解が解け<br />人望が回復したか？"
	},
	{
		"id": 93,
		"start": 884.732,
		"end": 890.738,
		"text": "（リク王）なぜ戻ってきた。<br />早く逃げるんだ！"
	},
	{
		"id": 94,
		"start": 890.738,
		"end": 893.741,
		"text": "（男性）リク王様を置いて<br />逃げるわけになどいきません！"
	},
	{
		"id": 95,
		"start": 893.741,
		"end": 895.743,
		"text": "（男性）国王様！"
	},
	{
		"id": 96,
		"start": 895.743,
		"end": 899.747,
		"text": "ピッキャララララ…。<br />くだらねえ。→"
	},
	{
		"id": 97,
		"start": 899.747,
		"end": 902.75,
		"text": "貴様のような平和主義者では→"
	},
	{
		"id": 98,
		"start": 902.75,
		"end": 907.755,
		"text": "国は守れなかったという事実を<br />忘れたか？"
	},
	{
		"id": 99,
		"start": 907.755,
		"end": 909.757,
		"text": "国とは武力だ。"
	},
	{
		"id": 100,
		"start": 909.757,
		"end": 917.765,
		"text": "敵を滅ぼせねえやつに<br />王になる資格は　ねえ！"
	},
	{
		"id": 101,
		"start": 917.765,
		"end": 926.774,
		"text": "くだらない理想とともに<br />つぶれろ！　リク王！！"
	},
	{
		"id": 102,
		"start": 926.774,
		"end": 931.774,
		"text": "ひぇ～！！<br />まっ　町が落ちてくる～！"
	},
	{
		"id": 103,
		"start": 936.784,
		"end": 941.784,
		"text": "（オオロンブス）キラーボウリング！"
	},
	{
		"id": 104,
		"start": 972.82,
		"end": 976.824,
		"text": "（リク王）確かに<br />国を守れなかった私は無能。"
	},
	{
		"id": 105,
		"start": 976.824,
		"end": 979.827,
		"text": "もう二度と<br />王座につく気は　ない！"
	},
	{
		"id": 106,
		"start": 979.827,
		"end": 982.827,
		"text": "（一同）<br />リク王様！　リク王様！"
	},
	{
		"id": 107,
		"start": 985.833,
		"end": 990.833,
		"text": "（リク王）だが<br />人間であるための努力をした！"
	},
	{
		"id": 108,
		"start": 999.847,
		"end": 1004.785,
		"text": "九山八海　一世界。"
	},
	{
		"id": 109,
		"start": 1004.785,
		"end": 1008.789,
		"text": "千　集まって　小千世界。"
	},
	{
		"id": 110,
		"start": 1008.789,
		"end": 1013.789,
		"text": "三乗　結んで　斬れぬ物なし！"
	},
	{
		"id": 111,
		"start": 1015.796,
		"end": 1019.8,
		"text": "三刀流奥義…。"
	},
	{
		"id": 112,
		"start": 1019.8,
		"end": 1022.803,
		"text": "人間であるため？"
	},
	{
		"id": 113,
		"start": 1022.803,
		"end": 1026.807,
		"text": "殺人を犯さなければ<br />生きてはいかん　というのなら→"
	},
	{
		"id": 114,
		"start": 1026.807,
		"end": 1029.81,
		"text": "私は進んで死を選ぶ。→"
	},
	{
		"id": 115,
		"start": 1029.81,
		"end": 1033.814,
		"text": "殺りく国家に　未来などない！"
	},
	{
		"id": 116,
		"start": 1033.814,
		"end": 1037.818,
		"text": "国王様！<br />われわれの王は　あなただけです！"
	},
	{
		"id": 117,
		"start": 1037.818,
		"end": 1039.82,
		"text": "ピッキャララララ…。"
	},
	{
		"id": 118,
		"start": 1039.82,
		"end": 1044.825,
		"text": "だから<br />善人は　歴史に名を残せない。→"
	},
	{
		"id": 119,
		"start": 1044.825,
		"end": 1048.829,
		"text": "お前のように　死んでいくからだ。"
	},
	{
		"id": 120,
		"start": 1048.829,
		"end": 1052.833,
		"text": "あ～！！"
	},
	{
		"id": 121,
		"start": 1052.833,
		"end": 1069.784,
		"text": "♪♪～"
	},
	{
		"id": 122,
		"start": 1069.784,
		"end": 1076.784,
		"text": "一大・三千・大千・世界！！"
	},
	{
		"id": 123,
		"start": 1088.803,
		"end": 1091.806,
		"text": "ひぇ～！"
	},
	{
		"id": 124,
		"start": 1091.806,
		"end": 1094.809,
		"text": "（オオロンブス・エリザベロー）あっ…。<br />（３人）あっ…。"
	},
	{
		"id": 125,
		"start": 1094.809,
		"end": 1096.809,
		"text": "（レベッカ・キュロス）あっ…。"
	},
	{
		"id": 126,
		"start": 1343.857,
		"end": 1345.857,
		"text": "んっ！？"
	},
	{
		"id": 127,
		"start": 1351.865,
		"end": 1354.868,
		"text": "《よし　上半身へ逃げた》"
	},
	{
		"id": 128,
		"start": 1354.868,
		"end": 1356.868,
		"text": "ロロノア！"
	},
	{
		"id": 129,
		"start": 1366.88,
		"end": 1370.884,
		"text": "（ピーカ）あ～！"
	},
	{
		"id": 130,
		"start": 1370.884,
		"end": 1372.886,
		"text": "あっ！"
	},
	{
		"id": 131,
		"start": 1372.886,
		"end": 1375.886,
		"text": "（レパント・リク王）あっ！"
	},
	{
		"id": 132,
		"start": 1378.892,
		"end": 1383.892,
		"text": "うお～！！　ゾロ～！！"
	},
	{
		"id": 133,
		"start": 1390.904,
		"end": 1392.906,
		"text": "《死んだ石》"
	},
	{
		"id": 134,
		"start": 1392.906,
		"end": 1394.842,
		"text": "《動く石》"
	},
	{
		"id": 135,
		"start": 1394.842,
		"end": 1400.842,
		"text": "《幾つに分かれようと<br />本体は必ず　一個体にいる》"
	},
	{
		"id": 136,
		"start": 1402.85,
		"end": 1405.853,
		"text": "《分かれることは　ない！》"
	},
	{
		"id": 137,
		"start": 1405.853,
		"end": 1422.87,
		"text": "♪♪～"
	},
	{
		"id": 138,
		"start": 1422.87,
		"end": 1425.87,
		"text": "（カン十郎・錦えもん）え～！"
	},
	{
		"id": 139,
		"start": 1434.882,
		"end": 1436.882,
		"text": "んっ！"
	},
	{
		"id": 140,
		"start": 1439.887,
		"end": 1443.887,
		"text": "《空中なら<br />逃げ込む地面は　ねえ》"
	},
	{
		"id": 141,
		"start": 1445.893,
		"end": 1447.893,
		"text": "出てこい　ピーカ！"
	},
	{
		"id": 142,
		"start": 1454.835,
		"end": 1456.835,
		"text": "クッ。"
	},
	{
		"id": 143,
		"start": 1459.84,
		"end": 1462.843,
		"text": "いつまで隠れてるつもりだ！"
	},
	{
		"id": 144,
		"start": 1462.843,
		"end": 1467.848,
		"text": "≪（ピーカ）グッ。　追い詰めて…。<br />んっ！"
	},
	{
		"id": 145,
		"start": 1467.848,
		"end": 1470.848,
		"text": "（ピーカ）勝ったつもりか！？"
	},
	{
		"id": 146,
		"start": 1476.857,
		"end": 1482.863,
		"text": "石さえ切れれば　俺に勝てると<br />思っちゃいないか？→"
	},
	{
		"id": 147,
		"start": 1482.863,
		"end": 1485.866,
		"text": "覇気をまとえば<br />切られやしねえ。→"
	},
	{
		"id": 148,
		"start": 1485.866,
		"end": 1487.866,
		"text": "はっ！"
	},
	{
		"id": 149,
		"start": 1490.871,
		"end": 1493.874,
		"text": "覇気か。<br />たたき落とせば→"
	},
	{
		"id": 150,
		"start": 1493.874,
		"end": 1496.874,
		"text": "お前の負けだろう。"
	},
	{
		"id": 151,
		"start": 1498.879,
		"end": 1503.884,
		"text": "フッ。　てめえの覇気が<br />俺の覇気を上回ってたらな。"
	},
	{
		"id": 152,
		"start": 1503.884,
		"end": 1511.892,
		"text": "クッ。　当然　上回っている。<br />お前など　俺の敵では　ない。"
	},
	{
		"id": 153,
		"start": 1511.892,
		"end": 1513.911,
		"text": "フフッ。"
	},
	{
		"id": 154,
		"start": 1513.911,
		"end": 1515.911,
		"text": "（ピーカ）フッ。"
	},
	{
		"id": 155,
		"start": 1517.831,
		"end": 1521.835,
		"text": "そういうのを<br />何ていうか知ってるか？"
	},
	{
		"id": 156,
		"start": 1521.835,
		"end": 1523.835,
		"text": "ああ？"
	},
	{
		"id": 157,
		"start": 1525.839,
		"end": 1528.842,
		"text": "（ミホーク）<br />《井の中の　吠えし　かわずよ》→"
	},
	{
		"id": 158,
		"start": 1528.842,
		"end": 1531.845,
		"text": "《世の広さを知るがいい》"
	},
	{
		"id": 159,
		"start": 1531.845,
		"end": 1534.845,
		"text": "《鬼斬り！》"
	},
	{
		"id": 160,
		"start": 1537.851,
		"end": 1542.856,
		"text": "《こんなに遠いわけねえ》"
	},
	{
		"id": 161,
		"start": 1542.856,
		"end": 1544.858,
		"text": "《世界が…》"
	},
	{
		"id": 162,
		"start": 1544.858,
		"end": 1548.862,
		"text": "《うっ…》"
	},
	{
		"id": 163,
		"start": 1548.862,
		"end": 1550.862,
		"text": "（ミホーク）《なぜ　引かん？》"
	},
	{
		"id": 164,
		"start": 1553.867,
		"end": 1556.87,
		"text": "《死んだ方が　ましだ》"
	},
	{
		"id": 165,
		"start": 1556.87,
		"end": 1560.87,
		"text": "《小僧。　名乗ってみよ》"
	},
	{
		"id": 166,
		"start": 1562.876,
		"end": 1564.876,
		"text": "《ロロノア・ゾロ！》"
	},
	{
		"id": 167,
		"start": 1568.882,
		"end": 1573.854,
		"text": "《覚えておく<br />久しく見ぬ　強き者よ》→"
	},
	{
		"id": 168,
		"start": 1573.854,
		"end": 1576.723,
		"text": "《そして　剣士たる礼儀をもって→"
	},
	{
		"id": 169,
		"start": 1576.723,
		"end": 1581.723,
		"text": "世界最強の　この　黒刀で<br />沈めてやる》"
	},
	{
		"id": 170,
		"start": 1590.737,
		"end": 1594.741,
		"text": "《三刀流奥義！》"
	},
	{
		"id": 171,
		"start": 1594.741,
		"end": 1597.741,
		"text": "《三・千・世・界！！》"
	},
	{
		"id": 172,
		"start": 1602.749,
		"end": 1604.749,
		"text": "《負けた》"
	},
	{
		"id": 173,
		"start": 1610.757,
		"end": 1615.757,
		"text": "《俺が負けるなんて<br />考えたこともなかった》"
	},
	{
		"id": 174,
		"start": 1618.765,
		"end": 1622.769,
		"text": "（ミホーク）《貴様が死ぬには<br />まだ早い！》"
	},
	{
		"id": 175,
		"start": 1622.769,
		"end": 1626.773,
		"text": "《わが名は<br />ジュラキュール・ミホーク！》→"
	},
	{
		"id": 176,
		"start": 1626.773,
		"end": 1631.778,
		"text": "《己を知り　世界を知り<br />強くなれ！》→"
	},
	{
		"id": 177,
		"start": 1631.778,
		"end": 1636.817,
		"text": "《この俺を超えてみよ<br />ロロノア・ゾロ！》"
	},
	{
		"id": 178,
		"start": 1636.817,
		"end": 1640.821,
		"text": "ここで　お前なんかに<br />つまずいてる暇は　ねえんだよ！"
	},
	{
		"id": 179,
		"start": 1640.821,
		"end": 1642.823,
		"text": "ほざくな～！"
	},
	{
		"id": 180,
		"start": 1642.823,
		"end": 1645.826,
		"text": "ピッキャー！"
	},
	{
		"id": 181,
		"start": 1645.826,
		"end": 1649.83,
		"text": "九山八海　斬れぬ物なし。"
	},
	{
		"id": 182,
		"start": 1649.83,
		"end": 1653.834,
		"text": "（ピーカ）死ね～<br />ロロノア・ゾロ！"
	},
	{
		"id": 183,
		"start": 1653.834,
		"end": 1656.837,
		"text": "三・千・世・界！"
	},
	{
		"id": 184,
		"start": 1656.837,
		"end": 1658.839,
		"text": "（ピーカ）うわ～！"
	},
	{
		"id": 185,
		"start": 1658.839,
		"end": 1660.841,
		"text": "グヘッ！"
	},
	{
		"id": 186,
		"start": 1660.841,
		"end": 1664.845,
		"text": "堅気に　迷惑　掛けてんじゃねえよ。"
	},
	{
		"id": 187,
		"start": 1664.845,
		"end": 1666.847,
		"text": "ゾロ先輩！"
	},
	{
		"id": 188,
		"start": 1666.847,
		"end": 1669.85,
		"text": "うお～！　ドロ～！"
	},
	{
		"id": 189,
		"start": 1669.85,
		"end": 1671.85,
		"text": "（レベッカ・キュロス）あっ…。"
	},
	{
		"id": 190,
		"start": 1676.857,
		"end": 1678.859,
		"text": "（ハック・リク王）あっ…。"
	},
	{
		"id": 191,
		"start": 1678.859,
		"end": 1680.861,
		"text": "（レパント・ヴィオラ）あっ…。"
	},
	{
		"id": 192,
		"start": 1680.861,
		"end": 1682.863,
		"text": "（３人）あっ…。"
	},
	{
		"id": 193,
		"start": 1682.863,
		"end": 1684.865,
		"text": "あっ…。"
	},
	{
		"id": 194,
		"start": 1684.865,
		"end": 1686.867,
		"text": "（カン十郎・錦えもん）うお～！"
	},
	{
		"id": 195,
		"start": 1686.867,
		"end": 1692.867,
		"text": "海賊狩りが<br />わが友　リク王を救った。"
	},
	{
		"id": 196,
		"start": 1695.809,
		"end": 1698.812,
		"text": "えっ？<br />ぶぇ～！→"
	},
	{
		"id": 197,
		"start": 1698.812,
		"end": 1706.82,
		"text": "で　結局　落ちてくる！　死ぬ～！！"
	},
	{
		"id": 198,
		"start": 1706.82,
		"end": 1710.82,
		"text": "助けぺ～　ボロ～！"
	},
	{
		"id": 199,
		"start": 1714.828,
		"end": 1719.833,
		"text": "あ～！！"
	},
	{
		"id": 200,
		"start": 1719.833,
		"end": 1721.833,
		"text": "出番だ。"
	},
	{
		"id": 201,
		"start": 1725.839,
		"end": 1730.839,
		"text": "キング・パンチ！！"
	},
	{
		"id": 202,
		"start": 1758.805,
		"end": 1760.807,
		"text": "あっ…。"
	},
	{
		"id": 203,
		"start": 1760.807,
		"end": 1763.81,
		"text": "ハァ…。　たっ　助かった。"
	},
	{
		"id": 204,
		"start": 1763.81,
		"end": 1767.814,
		"text": "ひぃ～！"
	},
	{
		"id": 205,
		"start": 1767.814,
		"end": 1770.817,
		"text": "ハァ　ハァ…。"
	},
	{
		"id": 206,
		"start": 1770.817,
		"end": 1777.824,
		"text": "あの　がき　俺の　キング・パンチを<br />石掃除に使うとは生意気な！→"
	},
	{
		"id": 207,
		"start": 1777.824,
		"end": 1781.824,
		"text": "覚えておれ。<br />（オオロンブス）ハハハ…。"
	},
	{
		"id": 208,
		"start": 1786.833,
		"end": 1788.833,
		"text": "上出来だ。"
	},
	{
		"id": 209,
		"start": 1796.843,
		"end": 1798.845,
		"text": "＜ルフィと　ベラミー＞"
	},
	{
		"id": 210,
		"start": 1798.845,
		"end": 1800.847,
		"text": "＜かつて<br />敵として戦った　２人が→"
	},
	{
		"id": 211,
		"start": 1800.847,
		"end": 1803.85,
		"text": "万感の思いを込め<br />再び　激突する！＞"
	},
	{
		"id": 212,
		"start": 1803.85,
		"end": 1805.85,
		"text": "次回　『ワンピース』"
	},
	{
		"id": 213,
		"start": 1807.854,
		"end": 1810.854,
		"text": "海賊王に　俺は　なる！"
	},
	{
		"id": 214,
		"start": 1813.877,
		"end": 1815.796,
		"text": "＜完全新作！＞"
	},
	{
		"id": 215,
		"start": 1815.796,
		"end": 1817.798,
		"text": "＜かつてない<br />麦わらの一味の大冒険が→"
	},
	{
		"id": 216,
		"start": 1817.798,
		"end": 1819.798,
		"text": "今ここに始まる！＞"
	},
	{
		"id": 217,
		"start": 1834.881,
		"end": 1836.6,
		"text": "（東野）さあ　松本さん。<br />けさはですね→"
	},
	{
		"id": 218,
		"start": 1836.6,
		"end": 1838.401,
		"text": "いつものメンバーといいますか。<br />一茂さん　ヒロミさん　山口先生。"
	},
	{
		"id": 219,
		"start": 1838.401,
		"end": 1841.471,
		"text": "（松本）わりと<br />クラシックメンバーですよね。"
	},
	{
		"id": 220,
		"start": 1841.471,
		"end": 1844.524,
		"text": "山口先生は<br />ずっと　『Ｂ面』の方で→"
	},
	{
		"id": 221,
		"start": 1844.524,
		"end": 1846.526,
		"text": "ロケばっかり<br />担当してましたけれども。"
	},
	{
		"id": 222,
		"start": 1846.526,
		"end": 1848.445,
		"text": "（長嶋）よく　ロケ　最近<br />行かれてね　見ましたけれども。"
	},
	{
		"id": 223,
		"start": 1848.445,
		"end": 1850.464,
		"text": "最近　どうなんですか？　街の方で<br />声とか掛けられるんですか？"
	},
	{
		"id": 224,
		"start": 1850.464,
		"end": 1854.468,
		"text": "（山口）はい。<br />『ワイドナショー』　見てますと。"
	},
	{
		"id": 225,
		"start": 1854.468,
		"end": 1856.319,
		"text": "（秋元）普段も　お着物<br />着られてるんですか？"
	},
	{
		"id": 226,
		"start": 1856.319,
		"end": 1859.389,
		"text": "普段は着ませんよっ！"
	}
]