[
	{
		"id": 1,
		"start": 151.048,
		"end": 155.035,
		"text": "あたし 月野うさぎ 14歳 中２"
	},
	{
		"id": 2,
		"start": 155.035,
		"end": 160.04,
		"text": "性格は ちょっと<br />おっちょこちょいで 泣き虫かな"
	},
	{
		"id": 3,
		"start": 160.04,
		"end": 163.043,
		"text": "うさぎ～ ８時 過ぎたわよ！"
	},
	{
		"id": 4,
		"start": 163.043,
		"end": 165.062,
		"text": "う～ん…"
	},
	{
		"id": 5,
		"start": 165.062,
		"end": 167.064,
		"text": "はっ！"
	},
	{
		"id": 6,
		"start": 167.064,
		"end": 170.064,
		"text": "きゃ～ わああ～！"
	},
	{
		"id": 7,
		"start": 172.052,
		"end": 177.024,
		"text": "もう もっと早く起こしてよ<br />ママのバカ！"
	},
	{
		"id": 8,
		"start": 177.024,
		"end": 179.076,
		"text": "何度も起こしたわよ"
	},
	{
		"id": 9,
		"start": 179.076,
		"end": 181.044,
		"text": "そのたんびに<br />ちゃんと返事したじゃない"
	},
	{
		"id": 10,
		"start": 181.044,
		"end": 183.063,
		"text": "知らないもん"
	},
	{
		"id": 11,
		"start": 183.063,
		"end": 185.032,
		"text": "あっ うさぎ！<br />な～に 急いでんの！"
	},
	{
		"id": 12,
		"start": 185.032,
		"end": 187.032,
		"text": "お弁当いらないの？"
	},
	{
		"id": 13,
		"start": 189.036,
		"end": 191.036,
		"text": "いる…"
	},
	{
		"id": 14,
		"start": 193.056,
		"end": 195.056,
		"text": "いってきま～す"
	},
	{
		"id": 15,
		"start": 196.043,
		"end": 200.043,
		"text": "は～ 朝ってなんで来るの<br />もう 眠いなぁ"
	},
	{
		"id": 16,
		"start": 201.031,
		"end": 203.031,
		"text": "え～い や～！<br />あっ…"
	},
	{
		"id": 17,
		"start": 205.052,
		"end": 208.055,
		"text": "あっ！ こら～っ！<br />やめなさ～い！"
	},
	{
		"id": 18,
		"start": 208.055,
		"end": 210.023,
		"text": "あっ ヤバ～い"
	},
	{
		"id": 19,
		"start": 210.023,
		"end": 212.075,
		"text": "まったく もう…"
	},
	{
		"id": 20,
		"start": 212.075,
		"end": 214.044,
		"text": "かわいそうにね よしよし"
	},
	{
		"id": 21,
		"start": 214.044,
		"end": 217.044,
		"text": "あら ばんそうこう<br />貼られちゃったの？"
	},
	{
		"id": 22,
		"start": 218.048,
		"end": 220.048,
		"text": "はがしてほしいのね はいはい"
	},
	{
		"id": 23,
		"start": 221.068,
		"end": 223.068,
		"text": "でも 結構 似合ってるじゃん"
	},
	{
		"id": 24,
		"start": 227.074,
		"end": 229.074,
		"text": "あっ 三日月ハゲ！"
	},
	{
		"id": 25,
		"start": 238.051,
		"end": 240.051,
		"text": "アハ へへ…"
	},
	{
		"id": 26,
		"start": 247.06,
		"end": 249.029,
		"text": "ああっ！ あ～っ"
	},
	{
		"id": 27,
		"start": 249.029,
		"end": 252.029,
		"text": "こんなことしてる場合じゃ<br />なかった～！"
	},
	{
		"id": 28,
		"start": 254.051,
		"end": 257.051,
		"text": "月野うさぎさん！ また遅刻なの？"
	},
	{
		"id": 29,
		"start": 259.056,
		"end": 263.056,
		"text": "ひど～い か弱い女の子を<br />廊下に立たせるなんて"
	},
	{
		"id": 30,
		"start": 264.027,
		"end": 267.027,
		"text": "はあ おなかすいた"
	},
	{
		"id": 31,
		"start": 272.052,
		"end": 276.039,
		"text": "へへ… 朝食抜きだもんね<br />いっただきま… えっ…"
	},
	{
		"id": 32,
		"start": 276.039,
		"end": 279.042,
		"text": "月野さん 何してんの？"
	},
	{
		"id": 33,
		"start": 279.042,
		"end": 282.029,
		"text": "そんなことだから<br />赤点を取るんですよ"
	},
	{
		"id": 34,
		"start": 282.029,
		"end": 285.032,
		"text": "ええ～っ 30点！"
	},
	{
		"id": 35,
		"start": 285.032,
		"end": 287.032,
		"text": "そう"
	},
	{
		"id": 36,
		"start": 288.035,
		"end": 291.038,
		"text": "もう うさぎったら信じらんない"
	},
	{
		"id": 37,
		"start": 291.038,
		"end": 293.04,
		"text": "女の子のくせに早弁なんてさ"
	},
	{
		"id": 38,
		"start": 293.04,
		"end": 297.06,
		"text": "だって 育ち盛りなんだもん<br />親友なら 私の気持ち"
	},
	{
		"id": 39,
		"start": 297.06,
		"end": 300.06,
		"text": "分かってくれるでしょ？<br />なるちゃん"
	},
	{
		"id": 40,
		"start": 301.064,
		"end": 303.066,
		"text": "う～ん…"
	},
	{
		"id": 41,
		"start": 303.066,
		"end": 305.085,
		"text": "うさぎさん テスト どうでした？"
	},
	{
		"id": 42,
		"start": 305.085,
		"end": 307.054,
		"text": "う 海野…"
	},
	{
		"id": 43,
		"start": 307.054,
		"end": 309.072,
		"text": "この落ちこみを見りゃ<br />分かるでしょ？"
	},
	{
		"id": 44,
		"start": 309.072,
		"end": 312.075,
		"text": "ダメだったに決まってるじゃない"
	},
	{
		"id": 45,
		"start": 312.075,
		"end": 315.062,
		"text": "く～っ…<br />あっ 傷ついた？ ごめん"
	},
	{
		"id": 46,
		"start": 315.062,
		"end": 317.047,
		"text": "そんなに 落ちこむことないですよ"
	},
	{
		"id": 47,
		"start": 317.047,
		"end": 321.068,
		"text": "ぼくも今回 手を抜いたもんで<br />100点取れなくて"
	},
	{
		"id": 48,
		"start": 321.068,
		"end": 323.053,
		"text": "ま テストなんて ゲームですよ"
	},
	{
		"id": 49,
		"start": 323.053,
		"end": 325.055,
		"text": "やなヤツね<br />ううう…"
	},
	{
		"id": 50,
		"start": 325.055,
		"end": 327.04,
		"text": "あっ ねえ ねえ"
	},
	{
		"id": 51,
		"start": 327.04,
		"end": 331.04,
		"text": "セーラーＶがさ<br />また 現れたんだって？ 聞いた？"
	},
	{
		"id": 52,
		"start": 332.079,
		"end": 334.064,
		"text": "｢セーラーＶ｣？<br />そ そう"
	},
	{
		"id": 53,
		"start": 334.064,
		"end": 337.067,
		"text": "宝石強盗の犯人を<br />捕まえちゃったのよ"
	},
	{
		"id": 54,
		"start": 337.067,
		"end": 340.053,
		"text": "へえ すごい！<br />すごいよね～"
	},
	{
		"id": 55,
		"start": 340.053,
		"end": 344.057,
		"text": "｢セーラーＶ｣って何？<br />知らないのに感心しないでよ！"
	},
	{
		"id": 56,
		"start": 344.057,
		"end": 346.059,
		"text": "セーラーＶとは…"
	},
	{
		"id": 57,
		"start": 346.059,
		"end": 350.046,
		"text": "今 ちまたをにぎわせている<br />セーラー服の正義の味方"
	},
	{
		"id": 58,
		"start": 350.046,
		"end": 354.034,
		"text": "セー… ラー… Ｖ！"
	},
	{
		"id": 59,
		"start": 354.034,
		"end": 359.055,
		"text": "実は 警視庁の特捜班って<br />うわさもあるんです"
	},
	{
		"id": 60,
		"start": 359.055,
		"end": 363.076,
		"text": "ふ～ん… そんなのが現れたんだ<br />世紀末ね"
	},
	{
		"id": 61,
		"start": 363.076,
		"end": 366.079,
		"text": "でもさ 宝石ってキレイだもん"
	},
	{
		"id": 62,
		"start": 366.079,
		"end": 368.081,
		"text": "強盗しちゃう気持ち分かるわ～"
	},
	{
		"id": 63,
		"start": 368.081,
		"end": 372.068,
		"text": "そうよね あたし<br />ダイヤのリングが欲しい"
	},
	{
		"id": 64,
		"start": 372.068,
		"end": 374.087,
		"text": "あたし ルビーのピアス"
	},
	{
		"id": 65,
		"start": 374.087,
		"end": 376.056,
		"text": "アハハ～ あこがれちゃう～"
	},
	{
		"id": 66,
		"start": 376.056,
		"end": 379.059,
		"text": "そうだ うさぎ ウチの店でも"
	},
	{
		"id": 67,
		"start": 379.059,
		"end": 382.062,
		"text": "昨日から 宝石のバーゲン<br />やってんのよ 来ない？"
	},
	{
		"id": 68,
		"start": 382.062,
		"end": 384.08,
		"text": "宝石のバーゲン？"
	},
	{
		"id": 69,
		"start": 384.08,
		"end": 386.049,
		"text": "あたしたちに買える<br />指輪とかあるのよ"
	},
	{
		"id": 70,
		"start": 386.049,
		"end": 388.068,
		"text": "ウソ～！ 行く 行く"
	},
	{
		"id": 71,
		"start": 388.068,
		"end": 391.071,
		"text": "じゃ 帰りにおいでよ<br />うん 絶対 行く！"
	},
	{
		"id": 72,
		"start": 391.071,
		"end": 393.039,
		"text": "ああ 早く学校終わらないかな<br />ハハッ…"
	},
	{
		"id": 73,
		"start": 393.039,
		"end": 396.039,
		"text": "アハハハ… うさぎさん…"
	},
	{
		"id": 74,
		"start": 416.079,
		"end": 420.079,
		"text": "幻の銀水晶は<br />まだ見つからぬのか？"
	},
	{
		"id": 75,
		"start": 421.067,
		"end": 423.067,
		"text": "はい"
	},
	{
		"id": 76,
		"start": 425.055,
		"end": 427.073,
		"text": "大いなる わが支配者は"
	},
	{
		"id": 77,
		"start": 427.073,
		"end": 430.06,
		"text": "十分なエナジーを求めておられる"
	},
	{
		"id": 78,
		"start": 430.06,
		"end": 433.063,
		"text": "銀水晶が手に入らぬのなら"
	},
	{
		"id": 79,
		"start": 433.063,
		"end": 437.063,
		"text": "まずは 人間どものエナジーを<br />ささげよう"
	},
	{
		"id": 80,
		"start": 438.051,
		"end": 441.051,
		"text": "クイン･ベリルさま<br />うん？"
	},
	{
		"id": 81,
		"start": 444.04,
		"end": 447.04,
		"text": "その役目<br />このジェダイトにお任せを"
	},
	{
		"id": 82,
		"start": 449.062,
		"end": 452.065,
		"text": "すでに わが配下の妖魔モルガが"
	},
	{
		"id": 83,
		"start": 452.065,
		"end": 455.085,
		"text": "人間どものエナジーを<br />集めております"
	},
	{
		"id": 84,
		"start": 455.085,
		"end": 459.085,
		"text": "お前に任そう ジェダイト"
	},
	{
		"id": 85,
		"start": 460.073,
		"end": 462.073,
		"text": "はっ"
	},
	{
		"id": 86,
		"start": 476.089,
		"end": 479.092,
		"text": "わあ… すごい人だね"
	},
	{
		"id": 87,
		"start": 479.092,
		"end": 482.095,
		"text": "毎度 ごひいきに<br />ありがとうございます"
	},
	{
		"id": 88,
		"start": 482.095,
		"end": 484.064,
		"text": "いらっしゃいませ<br />いらっしゃいませ！ どうぞ…"
	},
	{
		"id": 89,
		"start": 484.064,
		"end": 487.067,
		"text": "ママったら<br />やけに張り切ってるわね"
	},
	{
		"id": 90,
		"start": 487.067,
		"end": 489.052,
		"text": "商売に目覚めたんじゃない？"
	},
	{
		"id": 91,
		"start": 489.052,
		"end": 491.087,
		"text": "さあ いらっしゃいませ"
	},
	{
		"id": 92,
		"start": 491.087,
		"end": 494.087,
		"text": "どれも お安くなってますよ～"
	},
	{
		"id": 93,
		"start": 496.076,
		"end": 501.076,
		"text": "フフフ…"
	},
	{
		"id": 94,
		"start": 503.099,
		"end": 506.099,
		"text": "愚かな人間ども"
	},
	{
		"id": 95,
		"start": 511.074,
		"end": 513.059,
		"text": "われらが大いなる支配者に"
	},
	{
		"id": 96,
		"start": 513.059,
		"end": 517.059,
		"text": "エナジーのすべてを<br />ささげるがよい"
	},
	{
		"id": 97,
		"start": 534.08,
		"end": 538.068,
		"text": "フフフフ… 集まってくる…"
	},
	{
		"id": 98,
		"start": 538.068,
		"end": 543.089,
		"text": "宝石の輝きに魅せられた<br />愚かな人間どものエナジー"
	},
	{
		"id": 99,
		"start": 543.089,
		"end": 547.077,
		"text": "クイン･ベリルさまも<br />さぞかし お喜びになるであろう"
	},
	{
		"id": 100,
		"start": 547.077,
		"end": 549.079,
		"text": "フフフ…"
	},
	{
		"id": 101,
		"start": 549.079,
		"end": 551.064,
		"text": "ハハハハハ…"
	},
	{
		"id": 102,
		"start": 551.064,
		"end": 555.051,
		"text": "モルガよ<br />さらに多くのエナジーを集めよ"
	},
	{
		"id": 103,
		"start": 555.051,
		"end": 557.103,
		"text": "はい"
	},
	{
		"id": 104,
		"start": 557.103,
		"end": 559.072,
		"text": "ママ！"
	},
	{
		"id": 105,
		"start": 559.072,
		"end": 563.059,
		"text": "あっ あら！ おかえり なるちゃん"
	},
	{
		"id": 106,
		"start": 563.059,
		"end": 565.078,
		"text": "お友達 連れてきたのよ"
	},
	{
		"id": 107,
		"start": 565.078,
		"end": 567.063,
		"text": "こんにちは～！"
	},
	{
		"id": 108,
		"start": 567.063,
		"end": 569.099,
		"text": "まあ よくいらしてくださったわ"
	},
	{
		"id": 109,
		"start": 569.099,
		"end": 573.053,
		"text": "混んでますけど<br />ゆっくり見ていってちょうだいね"
	},
	{
		"id": 110,
		"start": 573.053,
		"end": 575.071,
		"text": "は～い"
	},
	{
		"id": 111,
		"start": 575.071,
		"end": 578.091,
		"text": "そうだわ なるちゃんのお友達なら<br />さらに おまけしちゃうわ"
	},
	{
		"id": 112,
		"start": 578.091,
		"end": 581.091,
		"text": "ホントですか？<br />やった～！ ラッキー"
	},
	{
		"id": 113,
		"start": 582.062,
		"end": 584.097,
		"text": "ダイヤなら これなんかどう？"
	},
	{
		"id": 114,
		"start": 584.097,
		"end": 588.084,
		"text": "50万円のお品なんだけど<br />50万円？"
	},
	{
		"id": 115,
		"start": 588.084,
		"end": 591.087,
		"text": "でも 思い切って<br />３万円に まけちゃうわ"
	},
	{
		"id": 116,
		"start": 591.087,
		"end": 593.106,
		"text": "50万円が３万円？"
	},
	{
		"id": 117,
		"start": 593.106,
		"end": 595.106,
		"text": "買った～！"
	},
	{
		"id": 118,
		"start": 600.063,
		"end": 602.063,
		"text": "はい はい…"
	},
	{
		"id": 119,
		"start": 603.083,
		"end": 605.101,
		"text": "すごいわね まったく"
	},
	{
		"id": 120,
		"start": 605.101,
		"end": 608.071,
		"text": "50万円が３万円か… 欲しいな…"
	},
	{
		"id": 121,
		"start": 608.071,
		"end": 610.073,
		"text": "パパに おねだりしちゃいなよ"
	},
	{
		"id": 122,
		"start": 610.073,
		"end": 612.092,
		"text": "う～ん でも"
	},
	{
		"id": 123,
		"start": 612.092,
		"end": 615.095,
		"text": "英語 赤点取っちゃったしね<br />あっ そっか"
	},
	{
		"id": 124,
		"start": 615.095,
		"end": 619.082,
		"text": "今月のおこづかいも もう ないし<br />はう…"
	},
	{
		"id": 125,
		"start": 619.082,
		"end": 622.082,
		"text": "強く生きるのよ うさぎ"
	},
	{
		"id": 126,
		"start": 629.109,
		"end": 632.109,
		"text": "もう少し 勉強しとけばなぁ"
	},
	{
		"id": 127,
		"start": 633.063,
		"end": 636.063,
		"text": "え～い 抹殺～！"
	},
	{
		"id": 128,
		"start": 638.068,
		"end": 641.071,
		"text": "痛いじゃないか おだんご頭！<br />あっ ごめんなさい"
	},
	{
		"id": 129,
		"start": 641.071,
		"end": 644.107,
		"text": "30点？<br />わああ…"
	},
	{
		"id": 130,
		"start": 644.107,
		"end": 648.107,
		"text": "もっと勉強しろ おだんご頭！"
	},
	{
		"id": 131,
		"start": 649.079,
		"end": 651.097,
		"text": "よ よけいなお世話よ！"
	},
	{
		"id": 132,
		"start": 651.097,
		"end": 653.066,
		"text": "ビ～ッだ！"
	},
	{
		"id": 133,
		"start": 653.066,
		"end": 656.066,
		"text": "何だってのよ まったく もう…"
	},
	{
		"id": 134,
		"start": 661.091,
		"end": 663.091,
		"text": "変なヤツ"
	},
	{
		"id": 135,
		"start": 676.089,
		"end": 678.091,
		"text": "あっ セーラーＶ？"
	},
	{
		"id": 136,
		"start": 678.091,
		"end": 682.091,
		"text": "もう ゲームになっちゃってるの<br />ふ～ん…"
	},
	{
		"id": 137,
		"start": 693.072,
		"end": 695.108,
		"text": "あ～あ いいなぁ"
	},
	{
		"id": 138,
		"start": 695.108,
		"end": 698.111,
		"text": "セーラーＶちゃんなんてさ<br />テストも関係ないし"
	},
	{
		"id": 139,
		"start": 698.111,
		"end": 701.111,
		"text": "悪者退治なんて<br />スカッとしそうだし"
	},
	{
		"id": 140,
		"start": 706.085,
		"end": 711.074,
		"text": "それに比べて あたしは…<br />あ～あ だぁ…"
	},
	{
		"id": 141,
		"start": 711.074,
		"end": 714.074,
		"text": "こんなテスト 持って帰りたくない"
	},
	{
		"id": 142,
		"start": 716.079,
		"end": 719.082,
		"text": "ああ～ん"
	},
	{
		"id": 143,
		"start": 719.082,
		"end": 721.084,
		"text": "ううう…"
	},
	{
		"id": 144,
		"start": 721.084,
		"end": 724.084,
		"text": "くうう…"
	},
	{
		"id": 145,
		"start": 726.072,
		"end": 729.072,
		"text": "月野うさぎ<br />とうとう見つけたわよ"
	},
	{
		"id": 146,
		"start": 753.099,
		"end": 756.069,
		"text": "た～だいま～<br />おかえりなさい"
	},
	{
		"id": 147,
		"start": 756.069,
		"end": 758.087,
		"text": "遅かったじゃないの"
	},
	{
		"id": 148,
		"start": 758.087,
		"end": 760.089,
		"text": "うん ちょっとね"
	},
	{
		"id": 149,
		"start": 760.089,
		"end": 763.092,
		"text": "さっき そこで<br />海野君に会ったわよ"
	},
	{
		"id": 150,
		"start": 763.092,
		"end": 765.094,
		"text": "テスト 返してもらったんだって？"
	},
	{
		"id": 151,
		"start": 765.094,
		"end": 767.08,
		"text": "いっ…"
	},
	{
		"id": 152,
		"start": 767.08,
		"end": 769.098,
		"text": "海野君 95点だったってね"
	},
	{
		"id": 153,
		"start": 769.098,
		"end": 773.086,
		"text": "ああ そう ハハハ… すごいね～"
	},
	{
		"id": 154,
		"start": 773.086,
		"end": 777.106,
		"text": "海野のヤツ<br />ベラベラしゃべんなよ"
	},
	{
		"id": 155,
		"start": 777.106,
		"end": 779.075,
		"text": "で？ うさぎは何点だったの？"
	},
	{
		"id": 156,
		"start": 779.075,
		"end": 782.111,
		"text": "うっ いや～ あの～ それが…"
	},
	{
		"id": 157,
		"start": 782.111,
		"end": 784.097,
		"text": "実は なんだ… その～"
	},
	{
		"id": 158,
		"start": 784.097,
		"end": 786.097,
		"text": "見せて"
	},
	{
		"id": 159,
		"start": 789.102,
		"end": 791.102,
		"text": "はい"
	},
	{
		"id": 160,
		"start": 794.09,
		"end": 796.092,
		"text": "うさぎ～！"
	},
	{
		"id": 161,
		"start": 796.092,
		"end": 798.077,
		"text": "はい"
	},
	{
		"id": 162,
		"start": 798.077,
		"end": 802.098,
		"text": "こんな点 取ってきて もう<br />おうちに入れてあげないわ！"
	},
	{
		"id": 163,
		"start": 802.098,
		"end": 804.1,
		"text": "きゃ～っ"
	},
	{
		"id": 164,
		"start": 804.1,
		"end": 807.103,
		"text": "ママ～<br />知りません！"
	},
	{
		"id": 165,
		"start": 807.103,
		"end": 810.106,
		"text": "何 やってんだよ バカうさぎ"
	},
	{
		"id": 166,
		"start": 810.106,
		"end": 812.091,
		"text": "また 閉め出しくってやんの"
	},
	{
		"id": 167,
		"start": 812.091,
		"end": 815.078,
		"text": "おれは<br />もう少し出来のいい姉が欲しいね"
	},
	{
		"id": 168,
		"start": 815.078,
		"end": 818.081,
		"text": "あんた 弟のくせになによ！"
	},
	{
		"id": 169,
		"start": 818.081,
		"end": 820.099,
		"text": "ベロベロ～っ"
	},
	{
		"id": 170,
		"start": 820.099,
		"end": 823.099,
		"text": "セーラーＶ キ～ック！"
	},
	{
		"id": 171,
		"start": 824.12,
		"end": 827.12,
		"text": "あ あ… 痛～い！"
	},
	{
		"id": 172,
		"start": 828.074,
		"end": 831.074,
		"text": "ママ 入れてよ～"
	},
	{
		"id": 173,
		"start": 837.083,
		"end": 841.104,
		"text": "変ね なんだか めまいが ああ…"
	},
	{
		"id": 174,
		"start": 841.104,
		"end": 843.106,
		"text": "はあ…"
	},
	{
		"id": 175,
		"start": 843.106,
		"end": 846.106,
		"text": "ああ 力が抜ける～"
	},
	{
		"id": 176,
		"start": 848.111,
		"end": 850.111,
		"text": "ああ…"
	},
	{
		"id": 177,
		"start": 852.081,
		"end": 855.084,
		"text": "あっ… みんな 急に<br />どうしちゃったのかしら？"
	},
	{
		"id": 178,
		"start": 855.084,
		"end": 859.084,
		"text": "ねえ ママ… ママ？"
	},
	{
		"id": 179,
		"start": 860.089,
		"end": 864.089,
		"text": "フフ… だいぶエナジーが<br />集まったわ"
	},
	{
		"id": 180,
		"start": 865.094,
		"end": 867.094,
		"text": "ママ…"
	},
	{
		"id": 181,
		"start": 870.083,
		"end": 873.102,
		"text": "フフフフ…"
	},
	{
		"id": 182,
		"start": 873.102,
		"end": 875.088,
		"text": "はっ！"
	},
	{
		"id": 183,
		"start": 875.088,
		"end": 879.075,
		"text": "あ～あ やっと入れてもらえた"
	},
	{
		"id": 184,
		"start": 879.075,
		"end": 881.127,
		"text": "いっぱい泣いたから疲れちゃった"
	},
	{
		"id": 185,
		"start": 881.127,
		"end": 885.114,
		"text": "ママも あんなに怒んなくたって<br />いいのに…"
	},
	{
		"id": 186,
		"start": 885.114,
		"end": 890.119,
		"text": "あ～ 宿題もやる気しないし<br />ちょっと寝ちゃおっと"
	},
	{
		"id": 187,
		"start": 890.119,
		"end": 893.119,
		"text": "こういうとき 寝つきがいいと<br />便利よね"
	},
	{
		"id": 188,
		"start": 919.098,
		"end": 922.101,
		"text": "はっ！ 三日月ハゲのネコ！"
	},
	{
		"id": 189,
		"start": 922.101,
		"end": 925.104,
		"text": "ハゲじゃないわよ 失礼ね"
	},
	{
		"id": 190,
		"start": 925.104,
		"end": 929.125,
		"text": "ああっ しゃべった！<br />ネコが ネコがしゃべった"
	},
	{
		"id": 191,
		"start": 929.125,
		"end": 933.112,
		"text": "オホン… あたしの名前はルナ"
	},
	{
		"id": 192,
		"start": 933.112,
		"end": 936.112,
		"text": "あなたを捜していたのよ<br />うさぎちゃん"
	},
	{
		"id": 193,
		"start": 938.117,
		"end": 941.12,
		"text": "ああ そうそう 初めに<br />お礼を言わなくちゃね"
	},
	{
		"id": 194,
		"start": 941.12,
		"end": 943.106,
		"text": "どうもありがとう"
	},
	{
		"id": 195,
		"start": 943.106,
		"end": 946.109,
		"text": "ばんそうこう<br />はがしてくれて助かったわ"
	},
	{
		"id": 196,
		"start": 946.109,
		"end": 950.129,
		"text": "ああされてると しゃべれないし<br />探知能力は鈍るし"
	},
	{
		"id": 197,
		"start": 950.129,
		"end": 952.098,
		"text": "いたずらされて困ってたのよ"
	},
	{
		"id": 198,
		"start": 952.098,
		"end": 957.12,
		"text": "でも おかげで やっと会えたわ<br />イヒヒッ… イヒヒヒ…"
	},
	{
		"id": 199,
		"start": 957.12,
		"end": 959.122,
		"text": "おやすみ！"
	},
	{
		"id": 200,
		"start": 959.122,
		"end": 963.109,
		"text": "うさぎちゃん<br />これは夢じゃないのよ"
	},
	{
		"id": 201,
		"start": 963.109,
		"end": 966.109,
		"text": "う～ん うさぎちゃんってば～！"
	},
	{
		"id": 202,
		"start": 967.113,
		"end": 970.113,
		"text": "いいわ じゃ<br />夢を覚まさせてあげる"
	},
	{
		"id": 203,
		"start": 971.1,
		"end": 973.1,
		"text": "えいっ！"
	},
	{
		"id": 204,
		"start": 981.11,
		"end": 983.096,
		"text": "あなたにプレゼントよ<br />うさぎちゃん"
	},
	{
		"id": 205,
		"start": 983.096,
		"end": 986.099,
		"text": "ウソ～ あたし<br />これもらっていいの？"
	},
	{
		"id": 206,
		"start": 986.099,
		"end": 988.101,
		"text": "うさぎちゃん<br />今 東京には変な事件が…"
	},
	{
		"id": 207,
		"start": 988.101,
		"end": 990.086,
		"text": "やった～ ありがとう"
	},
	{
		"id": 208,
		"start": 990.086,
		"end": 992.121,
		"text": "うさぎちゃん！ ちゃんと聞いて"
	},
	{
		"id": 209,
		"start": 992.121,
		"end": 995.124,
		"text": "今 東京には変な事件が<br />続発しているの"
	},
	{
		"id": 210,
		"start": 995.124,
		"end": 998.127,
		"text": "警察には<br />手に負えない事件ばかりよ"
	},
	{
		"id": 211,
		"start": 998.127,
		"end": 1003.116,
		"text": "敵が現れたの うさぎちゃん<br />その敵をあなたが倒すのよ"
	},
	{
		"id": 212,
		"start": 1003.116,
		"end": 1006.102,
		"text": "あなたは選ばれた戦士なの"
	},
	{
		"id": 213,
		"start": 1006.102,
		"end": 1008.121,
		"text": "そして もう１つの使命は"
	},
	{
		"id": 214,
		"start": 1008.121,
		"end": 1012.108,
		"text": "仲間と 一緒に あたしたちの<br />プリンセスを捜し出すこと"
	},
	{
		"id": 215,
		"start": 1012.108,
		"end": 1014.093,
		"text": "なんだか カッコいい～"
	},
	{
		"id": 216,
		"start": 1014.093,
		"end": 1018.114,
		"text": "まだ あたしの言ってることが<br />信じられないのね"
	},
	{
		"id": 217,
		"start": 1018.114,
		"end": 1020.114,
		"text": "ううん 信じる信じる"
	},
	{
		"id": 218,
		"start": 1021.117,
		"end": 1025.138,
		"text": "そう？ だったらね<br />こう叫んでみて"
	},
	{
		"id": 219,
		"start": 1025.138,
		"end": 1027.106,
		"text": "｢ムーン プリズムパワー<br />メイクアップ｣"
	},
	{
		"id": 220,
		"start": 1027.106,
		"end": 1032.106,
		"text": "はっ ｢ムーン プリズムパワー<br />メイクアップ｣！"
	},
	{
		"id": 221,
		"start": 1073.136,
		"end": 1076.105,
		"text": "えっ ウッソ～！ 何 これ？"
	},
	{
		"id": 222,
		"start": 1076.105,
		"end": 1078.107,
		"text": "あたし 変身しちゃった"
	},
	{
		"id": 223,
		"start": 1078.107,
		"end": 1082.107,
		"text": "何なのよ ね どういうこと？<br />教えて～！"
	},
	{
		"id": 224,
		"start": 1083.129,
		"end": 1086.132,
		"text": "助けて 誰か… 助けて～"
	},
	{
		"id": 225,
		"start": 1086.132,
		"end": 1088.134,
		"text": "はっ…<br />助けて ママが…"
	},
	{
		"id": 226,
		"start": 1088.134,
		"end": 1090.119,
		"text": "これは なるちゃんの声"
	},
	{
		"id": 227,
		"start": 1090.119,
		"end": 1093.122,
		"text": "どういうこと？<br />やっと 信用した？"
	},
	{
		"id": 228,
		"start": 1093.122,
		"end": 1095.091,
		"text": "よく 分かんないけど"
	},
	{
		"id": 229,
		"start": 1095.091,
		"end": 1097.143,
		"text": "なるちゃんが危ないみたいなの"
	},
	{
		"id": 230,
		"start": 1097.143,
		"end": 1100.129,
		"text": "うさぎちゃん あなたは<br />セーラームーンになったの"
	},
	{
		"id": 231,
		"start": 1100.129,
		"end": 1103.129,
		"text": "さあ 早く助けに行くのよ"
	},
	{
		"id": 232,
		"start": 1104.1,
		"end": 1106.1,
		"text": "えっ？"
	},
	{
		"id": 233,
		"start": 1107.119,
		"end": 1109.119,
		"text": "助けて～"
	},
	{
		"id": 234,
		"start": 1112.141,
		"end": 1114.11,
		"text": "苦しい… ううう…"
	},
	{
		"id": 235,
		"start": 1114.11,
		"end": 1116.128,
		"text": "うう… やめて ママ！"
	},
	{
		"id": 236,
		"start": 1116.128,
		"end": 1120.128,
		"text": "フッフッフ… 私は<br />お前のママなんかじゃないよ"
	},
	{
		"id": 237,
		"start": 1125.104,
		"end": 1127.106,
		"text": "うわっ！"
	},
	{
		"id": 238,
		"start": 1127.106,
		"end": 1131.093,
		"text": "お前のママは<br />地下室に閉じ込めてあるのさ"
	},
	{
		"id": 239,
		"start": 1131.093,
		"end": 1135.093,
		"text": "お前を殺した後で 一緒に<br />あの世へ送ってやろうね"
	},
	{
		"id": 240,
		"start": 1136.132,
		"end": 1138.132,
		"text": "なるちゃんから 離れなさい！"
	},
	{
		"id": 241,
		"start": 1145.141,
		"end": 1147.126,
		"text": "誰だ お前は？"
	},
	{
		"id": 242,
		"start": 1147.126,
		"end": 1150.112,
		"text": "えっ？ えっと～ あたしは～"
	},
	{
		"id": 243,
		"start": 1150.112,
		"end": 1152.131,
		"text": "愛と正義の…"
	},
	{
		"id": 244,
		"start": 1152.131,
		"end": 1155.131,
		"text": "セーラー服美少女戦士"
	},
	{
		"id": 245,
		"start": 1159.121,
		"end": 1161.123,
		"text": "セーラームーン！"
	},
	{
		"id": 246,
		"start": 1161.123,
		"end": 1165.127,
		"text": "月にかわって おしおきよ！"
	},
	{
		"id": 247,
		"start": 1165.127,
		"end": 1168.114,
		"text": "｢セーラームーン｣？"
	},
	{
		"id": 248,
		"start": 1168.114,
		"end": 1170.132,
		"text": "聞いたこともないわ そんなもの"
	},
	{
		"id": 249,
		"start": 1170.132,
		"end": 1172.134,
		"text": "さあ 目覚めよ"
	},
	{
		"id": 250,
		"start": 1172.134,
		"end": 1177.134,
		"text": "わが大いなる支配者に<br />エナジーをささげし奴隷どもよ"
	},
	{
		"id": 251,
		"start": 1180.126,
		"end": 1184.126,
		"text": "う うわあ 何よ あんたたち"
	},
	{
		"id": 252,
		"start": 1185.097,
		"end": 1187.149,
		"text": "わっ！"
	},
	{
		"id": 253,
		"start": 1187.149,
		"end": 1190.152,
		"text": "ああ～っ！ はああっ！"
	},
	{
		"id": 254,
		"start": 1190.152,
		"end": 1192.152,
		"text": "うっ あ…"
	},
	{
		"id": 255,
		"start": 1193.122,
		"end": 1195.124,
		"text": "ああ～っ！ うっ…"
	},
	{
		"id": 256,
		"start": 1195.124,
		"end": 1198.127,
		"text": "きゃあ 何これ 血が出てる！"
	},
	{
		"id": 257,
		"start": 1198.127,
		"end": 1201.13,
		"text": "何やってんの！ セーラームーン"
	},
	{
		"id": 258,
		"start": 1201.13,
		"end": 1204.13,
		"text": "戦うのよ<br />あの妖魔をやっつけるのよ"
	},
	{
		"id": 259,
		"start": 1208.154,
		"end": 1210.122,
		"text": "だって どうして あたしが"
	},
	{
		"id": 260,
		"start": 1210.122,
		"end": 1214.143,
		"text": "こんな目に遭わなくちゃ<br />いけないの～？"
	},
	{
		"id": 261,
		"start": 1214.143,
		"end": 1217.146,
		"text": "あ～ん おうちに帰る～"
	},
	{
		"id": 262,
		"start": 1217.146,
		"end": 1220.146,
		"text": "うう～っ とどめだ～"
	},
	{
		"id": 263,
		"start": 1223.152,
		"end": 1225.152,
		"text": "ああっ！"
	},
	{
		"id": 264,
		"start": 1226.138,
		"end": 1228.138,
		"text": "あ～っ！"
	},
	{
		"id": 265,
		"start": 1231.127,
		"end": 1235.114,
		"text": "誰だ？"
	},
	{
		"id": 266,
		"start": 1235.114,
		"end": 1237.133,
		"text": "私はタキシード仮面"
	},
	{
		"id": 267,
		"start": 1237.133,
		"end": 1241.12,
		"text": "泣いているばかりでは<br />何も解決しないぞ セーラームーン"
	},
	{
		"id": 268,
		"start": 1241.12,
		"end": 1244.12,
		"text": "そんなこと 言ったって…"
	},
	{
		"id": 269,
		"start": 1246.125,
		"end": 1248.125,
		"text": "ああ～ん…"
	},
	{
		"id": 270,
		"start": 1250.112,
		"end": 1253.112,
		"text": "ああ…"
	},
	{
		"id": 271,
		"start": 1254.133,
		"end": 1256.133,
		"text": "泣くな～"
	},
	{
		"id": 272,
		"start": 1257.103,
		"end": 1259.155,
		"text": "ティアラを取って"
	},
	{
		"id": 273,
		"start": 1259.155,
		"end": 1261.123,
		"text": "｢ムーン ティアラ アクション｣<br />と叫んで 投げて！"
	},
	{
		"id": 274,
		"start": 1261.123,
		"end": 1264.126,
		"text": "なんで そんなこと<br />しなきゃいけないの？"
	},
	{
		"id": 275,
		"start": 1264.126,
		"end": 1266.126,
		"text": "いいから 早く！"
	},
	{
		"id": 276,
		"start": 1281.11,
		"end": 1286.11,
		"text": "ムーン ティアラ アクション！"
	},
	{
		"id": 277,
		"start": 1289.151,
		"end": 1291.151,
		"text": "ワアア～"
	},
	{
		"id": 278,
		"start": 1296.142,
		"end": 1298.142,
		"text": "あっ ウッソ～"
	},
	{
		"id": 279,
		"start": 1308.137,
		"end": 1312.137,
		"text": "モルガ しくじったな バカめ"
	},
	{
		"id": 280,
		"start": 1321.133,
		"end": 1323.135,
		"text": "ああ…"
	},
	{
		"id": 281,
		"start": 1323.135,
		"end": 1325.121,
		"text": "見事だ セーラームーン"
	},
	{
		"id": 282,
		"start": 1325.121,
		"end": 1329.121,
		"text": "今夜のこと 覚えておこう"
	},
	{
		"id": 283,
		"start": 1330.142,
		"end": 1332.142,
		"text": "ああ…"
	},
	{
		"id": 284,
		"start": 1333.129,
		"end": 1335.114,
		"text": "よくやったわ セーラームーン"
	},
	{
		"id": 285,
		"start": 1335.114,
		"end": 1338.114,
		"text": "ステキ…"
	},
	{
		"id": 286,
		"start": 1342.138,
		"end": 1345.141,
		"text": "ねえ ゆうべ ステキな夢を見たの"
	},
	{
		"id": 287,
		"start": 1345.141,
		"end": 1349.161,
		"text": "セーラームーンっていう<br />正義の戦士が現れてね"
	},
	{
		"id": 288,
		"start": 1349.161,
		"end": 1351.13,
		"text": "怪物をやっつけるの"
	},
	{
		"id": 289,
		"start": 1351.13,
		"end": 1353.149,
		"text": "ええっ？ あたしも<br />おんなじ夢 見たわ"
	},
	{
		"id": 290,
		"start": 1353.149,
		"end": 1356.118,
		"text": "ああ あたしも！<br />ええ？ 不思議"
	},
	{
		"id": 291,
		"start": 1356.118,
		"end": 1360.139,
		"text": "ねえ うさぎ… うさぎ！<br />聞いてよ！"
	},
	{
		"id": 292,
		"start": 1360.139,
		"end": 1362.141,
		"text": "う～ん うるさいなぁ"
	},
	{
		"id": 293,
		"start": 1362.141,
		"end": 1367.163,
		"text": "あたしったら 疲れてるんだから<br />もう少し寝かせてよ"
	},
	{
		"id": 294,
		"start": 1367.163,
		"end": 1370.163,
		"text": "ふわあ… おやすみ…"
	},
	{
		"id": 295,
		"start": 1466.161,
		"end": 1469.164,
		"text": "ルナ 占ってあげよっか"
	},
	{
		"id": 296,
		"start": 1469.164,
		"end": 1472.167,
		"text": "うさぎちゃん 占いもいいけど<br />使命を忘れちゃダメよ"
	},
	{
		"id": 297,
		"start": 1472.167,
		"end": 1474.153,
		"text": "もう ルナってば うるさい<br />あたしだって"
	},
	{
		"id": 298,
		"start": 1474.153,
		"end": 1477.139,
		"text": "ちゃんと正義の味方してんのよ！<br />そうかしら？"
	},
	{
		"id": 299,
		"start": 1477.139,
		"end": 1480.142,
		"text": "実はね 占いハウスに行った<br />みんなの様子がおかしいの"
	},
	{
		"id": 300,
		"start": 1480.142,
		"end": 1483.145,
		"text": "これって もしかして…<br />事件よ うさぎちゃん！"
	},
	{
		"id": 301,
		"start": 1483.145,
		"end": 1485.145,
		"text": "｢美少女戦士 セーラームーン｣"
	},
	{
		"id": 302,
		"start": 1488.15,
		"end": 1491.15,
		"text": "月の光は愛のメッセージ"
	},
	{
		"id": 303,
		"start": 1524.136,
		"end": 1526.136,
		"text": "テレ朝チャンネル１"
	},
	{
		"id": 304,
		"start": 1533.045,
		"end": 1535.045,
		"text": "うわ～ 好きなだけ!?"
	},
	{
		"id": 305,
		"start": 1536.048,
		"end": 1538.067,
		"text": "ドラえもんのテレビシリーズが"
	},
	{
		"id": 306,
		"start": 1538.067,
		"end": 1541.07,
		"text": "いよいよ ＣＳで<br />レギュラー放送開始！"
	},
	{
		"id": 307,
		"start": 1541.07,
		"end": 1544.056,
		"text": "2005年から放送してきた<br />ハイビジョン作品を"
	},
	{
		"id": 308,
		"start": 1544.056,
		"end": 1546.056,
		"text": "300本以上 お届けします｡"
	},
	{
		"id": 309,
		"start": 1548.027,
		"end": 1550.027,
		"text": "あの２人に また会える！"
	},
	{
		"id": 310,
		"start": 1551.03,
		"end": 1553.03,
		"text": "ふたりはプリキュア！"
	},
	{
		"id": 311,
		"start": 1554.033,
		"end": 1556.018,
		"text": "なぎさとほのかが悪と戦う！"
	},
	{
		"id": 312,
		"start": 1556.018,
		"end": 1559.021,
		"text": "懐かしのプリキュア<br />ファーストシリーズをお届け！"
	},
	{
		"id": 313,
		"start": 1559.021,
		"end": 1561.021,
		"text": "ずーっと会いたかったメポ｡"
	},
	{
		"id": 314,
		"start": 1563.075,
		"end": 1566.078,
		"text": "お好み焼屋の看板娘 やっこと"
	},
	{
		"id": 315,
		"start": 1566.078,
		"end": 1570.082,
		"text": "人気バンド ビーハイブのボーカル<br />剛の乙女チックなラブロマンス"
	},
	{
		"id": 316,
		"start": 1570.082,
		"end": 1572.067,
		"text": "愛してナイト｡"
	},
	{
		"id": 317,
		"start": 1572.067,
		"end": 1575.07,
		"text": "里美ってやつを 見かけだけで<br />判断しちゃいけないぜ｡"
	},
	{
		"id": 318,
		"start": 1575.07,
		"end": 1578.07,
		"text": "愛とロックが青春の全て｡"
	},
	{
		"id": 319,
		"start": 1580.059,
		"end": 1584.079,
		"text": "多田かおる原作の<br />女の子たちの恋のバイブルが"
	},
	{
		"id": 320,
		"start": 1584.079,
		"end": 1587.079,
		"text": "テレ朝チャンネル１で<br />よみがえります｡"
	},
	{
		"id": 321,
		"start": 1589.051,
		"end": 1591.051,
		"text": "すてきな人に愛されたい…｡"
	},
	{
		"id": 322,
		"start": 1593.055,
		"end": 1595.04,
		"text": "ニン ニン ニン ニン…！"
	},
	{
		"id": 323,
		"start": 1595.04,
		"end": 1598.06,
		"text": "来たな 伊賀者め…｡<br />ケーン！ ケン ケン ケーン！"
	},
	{
		"id": 324,
		"start": 1598.06,
		"end": 1601.03,
		"text": "ニンニン！<br />どんぐりまなこに への字口｡"
	},
	{
		"id": 325,
		"start": 1601.03,
		"end": 1604.033,
		"text": "忍者の里 伊賀からやって来た<br />ハットリくんは"
	},
	{
		"id": 326,
		"start": 1604.033,
		"end": 1607.069,
		"text": "東京の三葉家に居候｡"
	},
	{
		"id": 327,
		"start": 1607.069,
		"end": 1611.069,
		"text": "悪い奴をこらしめたり<br />愉快な騒動を巻き起こします｡"
	},
	{
		"id": 328,
		"start": 1612.074,
		"end": 1615.044,
		"text": "今見ても 色あせない<br />藤子不二雄の傑作アニメ"
	},
	{
		"id": 329,
		"start": 1615.044,
		"end": 1618.08,
		"text": "忍者ハットリくんで<br />大笑いしてください！"
	},
	{
		"id": 330,
		"start": 1618.08,
		"end": 1621.08,
		"text": "見事な 忍法ぬけがら<br />敵も なかなかやるでござるな｡"
	},
	{
		"id": 331,
		"start": 1623.102,
		"end": 1625.104,
		"text": "オーケー！ 僕 やるよ！"
	},
	{
		"id": 332,
		"start": 1625.104,
		"end": 1628.073,
		"text": "藤子･Ｆ･不二雄<br />藤子不二雄原作の"
	},
	{
		"id": 333,
		"start": 1628.073,
		"end": 1631.073,
		"text": "大ヒットギャグアニメ<br />オバケのＱ太郎｡"
	},
	{
		"id": 334,
		"start": 1632.094,
		"end": 1634.079,
		"text": "Ｑちゃん… どこ行くんだよ!?"
	},
	{
		"id": 335,
		"start": 1634.079,
		"end": 1638.1,
		"text": "ちっとも怖くない<br />オバケのＱちゃんが巻き起こす"
	},
	{
		"id": 336,
		"start": 1638.1,
		"end": 1642.087,
		"text": "思わず笑ってしまう話や<br />心温まるストーリー｡"
	},
	{
		"id": 337,
		"start": 1642.087,
		"end": 1645.124,
		"text": "ぐわあぁ～ ぐわあぁ～！<br />ぐわあぁ～！"
	},
	{
		"id": 338,
		"start": 1645.124,
		"end": 1647.092,
		"text": "逃げろ－！"
	},
	{
		"id": 339,
		"start": 1647.092,
		"end": 1651.092,
		"text": "昭和を代表するアニメ<br />オバＱをお見逃しなく！"
	},
	{
		"id": 340,
		"start": 1658.921,
		"end": 1660.921,
		"text": "テレ朝チャンネル１"
	},
	{
		"id": 341,
		"start": 1667.946,
		"end": 1670.949,
		"text": "地上波で<br />カリスマ的な人気を集める"
	},
	{
		"id": 342,
		"start": 1670.949,
		"end": 1673.952,
		"text": "アメトーークが<br />ついに ＣＳに登場｡"
	},
	{
		"id": 343,
		"start": 1673.952,
		"end": 1677.94,
		"text": "過去10年の中で<br />特に反響が大きかった放送回を"
	},
	{
		"id": 344,
		"start": 1677.94,
		"end": 1679.94,
		"text": "厳選して お届け！"
	},
	{
		"id": 345,
		"start": 1682.978,
		"end": 1685.978,
		"text": "あの ナイナイナが帰ってきた！"
	},
	{
		"id": 346,
		"start": 1686.965,
		"end": 1690.002,
		"text": "20代の 若きナインティナインが<br />繰り広げる"
	},
	{
		"id": 347,
		"start": 1690.002,
		"end": 1691.97,
		"text": "体当たり企画満載！"
	},
	{
		"id": 348,
		"start": 1691.97,
		"end": 1693.956,
		"text": "今も色あせない<br />伝説のバラエティーを"
	},
	{
		"id": 349,
		"start": 1693.956,
		"end": 1695.956,
		"text": "お楽しみください！"
	},
	{
		"id": 350,
		"start": 1697.96,
		"end": 1699.978,
		"text": "あっ 美味しい！"
	},
	{
		"id": 351,
		"start": 1699.978,
		"end": 1702.978,
		"text": "テレビ朝日の人気番組の<br />スピンオフが登場！ その名も…｡"
	},
	{
		"id": 352,
		"start": 1705.984,
		"end": 1707.986,
		"text": "美食アカデミーの完全版など"
	},
	{
		"id": 353,
		"start": 1707.986,
		"end": 1710.986,
		"text": "ＣＳオリジナルの内容で<br />お届けします｡"
	},
	{
		"id": 354,
		"start": 1712.891,
		"end": 1716.895,
		"text": "劇団ひとりと土田晃之が<br />怪しいマニアの行動や"
	},
	{
		"id": 355,
		"start": 1716.895,
		"end": 1719.898,
		"text": "謎の団体の活動を<br />徹底調査！"
	},
	{
		"id": 356,
		"start": 1719.898,
		"end": 1721.884,
		"text": "青 いっとこうかな…｡"
	},
	{
		"id": 357,
		"start": 1721.884,
		"end": 1724.884,
		"text": "シンプルでいいですね｡<br />そうですね…｡"
	},
	{
		"id": 358,
		"start": 1725.871,
		"end": 1727.89,
		"text": "あぁ…！"
	},
	{
		"id": 359,
		"start": 1727.89,
		"end": 1729.892,
		"text": "専用のゴムが…！"
	},
	{
		"id": 360,
		"start": 1729.892,
		"end": 1732.861,
		"text": "マニア界の衝撃の事実が<br />次々と明らかに…！"
	},
	{
		"id": 361,
		"start": 1732.861,
		"end": 1734.861,
		"text": "トリガーを引くと<br />レーザーサイトが…｡"
	},
	{
		"id": 362,
		"start": 1737.9,
		"end": 1740.9,
		"text": "マニアの気持ちを探る<br />あっちマニア｡"
	},
	{
		"id": 363,
		"start": 1742.938,
		"end": 1745.941,
		"text": "アイドル界の頂点を目指す"
	},
	{
		"id": 364,
		"start": 1745.941,
		"end": 1748.944,
		"text": "ももいろクローバーＺの<br />リアルな姿を配信する"
	},
	{
		"id": 365,
		"start": 1748.944,
		"end": 1751.944,
		"text": "テレ朝動画のオリジナル番組<br />ももクロＣｈａｎ｡"
	},
	{
		"id": 366,
		"start": 1752.931,
		"end": 1756.952,
		"text": "その未放送部分も<br />特別にプラスして お届けするのが"
	},
	{
		"id": 367,
		"start": 1756.952,
		"end": 1758.952,
		"text": "別冊ももクロＣｈａｎ｡"
	},
	{
		"id": 368,
		"start": 1760.939,
		"end": 1764.939,
		"text": "ももクロの全てが見られるのは<br />テレ朝チャンネル１だけ！"
	},
	{
		"id": 369,
		"start": 1768.914,
		"end": 1770.914,
		"text": "別冊ももクロＣｈａｎ！"
	},
	{
		"id": 370,
		"start": 1773.001,
		"end": 1776.001,
		"text": "詳しくは<br />｢テレ朝チャンネル｣で検索！"
	},
	{
		"id": 371,
		"start": 1850.028,
		"end": 1854.016,
		"text": "若貴人気に嫉妬する<br />おじさんたちよりも"
	},
	{
		"id": 372,
		"start": 1854.016,
		"end": 1859.016,
		"text": "もっともっと悪いのは<br />人の心を何とも思わないヤツ"
	}
]