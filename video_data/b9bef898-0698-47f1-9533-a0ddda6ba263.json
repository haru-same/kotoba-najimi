[
	{
		"id": 1,
		"start": 2.760999999999999,
		"end": 4.722000000000002,
		"text": "（佐藤和真(さとうカズマ)）よし …っと"
	},
	{
		"id": 2,
		"start": 12.688000000000002,
		"end": 14.898000000000003,
		"text": "クリエイト･ウォーター！"
	},
	{
		"id": 3,
		"start": 24.19899999999999,
		"end": 25.36699999999999,
		"text": "フッ"
	},
	{
		"id": 4,
		"start": 27.41099999999999,
		"end": 28.61999999999999,
		"text": "ハア～ッ"
	},
	{
		"id": 5,
		"start": 29.07899999999999,
		"end": 32.24899999999999,
		"text": "まっ 初級魔法じゃ<br />この程度だろう"
	},
	{
		"id": 6,
		"start": 32.37399999999999,
		"end": 35.54399999999999,
		"text": "スキルも覚えて<br />俺も冒険者らしくなってきたかな"
	},
	{
		"id": 7,
		"start": 35.66899999999999,
		"end": 36.87899999999999,
		"text": "（ダクネス）カズマ"
	},
	{
		"id": 8,
		"start": 37.00399999999999,
		"end": 40.71599999999999,
		"text": "見てくれ<br />キャベツの報酬で鎧(よろい)を直したのだが"
	},
	{
		"id": 9,
		"start": 40.84099999999999,
		"end": 43.76099999999999,
		"text": "こんなにピカピカになった<br />どう思う？"
	},
	{
		"id": 10,
		"start": 43.88599999999999,
		"end": 47.76499999999999,
		"text": "なんか成金趣味の貴族のボンボンが<br />着けてる鎧みたい"
	},
	{
		"id": 11,
		"start": 49.05799999999999,
		"end": 52.51899999999999,
		"text": "私だって素直に褒めてもらいたい<br />ときも あるのだが…"
	},
	{
		"id": 12,
		"start": 53.18699999999999,
		"end": 56.77399999999999,
		"text": "カズマは どんなときでも<br />容赦ないな  ヘヘッ…"
	},
	{
		"id": 13,
		"start": 56.89899999999999,
		"end": 58.98399999999999,
		"text": "今は構ってやる余裕はないぞ"
	},
	{
		"id": 14,
		"start": 59.48499999999999,
		"end": 62.36299999999999,
		"text": "お前を超えそうな勢いの<br />そこの変態を何とかしろよ"
	},
	{
		"id": 15,
		"start": 62.48799999999999,
		"end": 65.94900000000004,
		"text": "（めぐみん）<br />魔力あふれるマナタイト製のつえの<br />この色つや"
	},
	{
		"id": 16,
		"start": 66.07500000000005,
		"end": 68.53500000000004,
		"text": "ハアッ ハアアッ…"
	},
	{
		"id": 17,
		"start": 68.66000000000004,
		"end": 71.91400000000004,
		"text": "（アクア）なんですって？<br />ちょっと あんた どういうことよ！"
	},
	{
		"id": 18,
		"start": 72.03900000000004,
		"end": 73.62400000000004,
		"text": "どれだけ キャベツ捕まえたと<br />思ってんの？"
	},
	{
		"id": 19,
		"start": 73.74900000000004,
		"end": 75.16700000000004,
		"text": "（ルナ）そ… それが"
	},
	{
		"id": 20,
		"start": 75.29200000000004,
		"end": 76.29300000000003,
		"text": "（アクア）何よ？"
	},
	{
		"id": 21,
		"start": 76.79400000000004,
		"end": 80.79800000000004,
		"text": "アクアさんの捕まえてきたのは<br />ほとんどがレタスでして"
	},
	{
		"id": 22,
		"start": 80.92300000000004,
		"end": 84.09300000000005,
		"text": "なんでレタスが混じってんのよ！"
	},
	{
		"id": 23,
		"start": 84.55100000000004,
		"end": 87.34600000000005,
		"text": "確かにレタスの換金率は低いな"
	},
	{
		"id": 24,
		"start": 87.47100000000005,
		"end": 89.47300000000004,
		"text": "よく分からんが そうなのか？"
	},
	{
		"id": 25,
		"start": 91.68300000000004,
		"end": 94.18600000000004,
		"text": "（カズマ）ああ こっち来る<br />イヤだなあ"
	},
	{
		"id": 26,
		"start": 94.31100000000004,
		"end": 96.93900000000004,
		"text": "カーズーマーさん"
	},
	{
		"id": 27,
		"start": 98.02300000000004,
		"end": 100.81800000000004,
		"text": "今回のクエストの報酬は<br />おいくら万円？"
	},
	{
		"id": 28,
		"start": 101.98500000000004,
		"end": 103.19500000000004,
		"text": "百万ちょい"
	},
	{
		"id": 29,
		"start": 103.32000000000004,
		"end": 104.61300000000004,
		"text": "（アクアたち）ひゃっ…"
	},
	{
		"id": 30,
		"start": 105.61400000000003,
		"end": 106.86500000000004,
		"text": "（カズマ）そう"
	},
	{
		"id": 31,
		"start": 106.99000000000004,
		"end": 112.24600000000004,
		"text": "俺は降ってわいた突発クエストで<br />いきなり小金持ちになりました"
	},
	{
		"id": 32,
		"start": 112.78800000000004,
		"end": 119.75300000000004,
		"text": "♪～"
	},
	{
		"id": 33,
		"start": 195.03700000000003,
		"end": 201.96000000000004,
		"text": "～♪"
	},
	{
		"id": 34,
		"start": 203.29500000000004,
		"end": 204.54600000000005,
		"text": "（３人）百万…"
	},
	{
		"id": 35,
		"start": 205.17200000000005,
		"end": 206.88200000000003,
		"text": "（カズマ）<br />俺の収穫したキャベツは"
	},
	{
		"id": 36,
		"start": 207.42400000000004,
		"end": 210.30200000000005,
		"text": "たくさん経験値が詰まったものが<br />多かったそうだ"
	},
	{
		"id": 37,
		"start": 210.88600000000005,
		"end": 212.76300000000003,
		"text": "これも幸運度の差というやつか"
	},
	{
		"id": 38,
		"start": 213.88900000000004,
		"end": 215.01500000000004,
		"text": "ウフッ"
	},
	{
		"id": 39,
		"start": 215.14000000000004,
		"end": 217.10000000000005,
		"text": "カ… カズマ様"
	},
	{
		"id": 40,
		"start": 217.22600000000003,
		"end": 220.56200000000004,
		"text": "前から思ってたんだけど<br />あなたって その…"
	},
	{
		"id": 41,
		"start": 220.89600000000004,
		"end": 223.31500000000003,
		"text": "そこはかとなく いい感じよね！"
	},
	{
		"id": 42,
		"start": 223.44000000000003,
		"end": 226.27600000000004,
		"text": "特に褒めるところが<br />思い浮かばないなら 無理すんな"
	},
	{
		"id": 43,
		"start": 228.32000000000005,
		"end": 230.32200000000003,
		"text": "カ… カズマさん！"
	},
	{
		"id": 44,
		"start": 230.44700000000003,
		"end": 233.70000000000005,
		"text": "私 今回の報酬が<br />相当な額になるって踏んで―"
	},
	{
		"id": 45,
		"start": 233.82500000000005,
		"end": 236.07800000000003,
		"text": "持ってたお金 全部<br />使っちゃったんですけど！"
	},
	{
		"id": 46,
		"start": 236.20300000000003,
		"end": 238.45500000000004,
		"text": "…ていうか 大金入ってくるって<br />見込んで―"
	},
	{
		"id": 47,
		"start": 238.58000000000004,
		"end": 241.20800000000003,
		"text": "この酒場に10万近いツケまで<br />あるんですけど！"
	},
	{
		"id": 48,
		"start": 241.33300000000003,
		"end": 242.54300000000003,
		"text": "（カズマ）知るか！"
	},
	{
		"id": 49,
		"start": 242.66800000000003,
		"end": 245.75400000000005,
		"text": "今回の報酬は それぞれの者にって<br />言いだしたのは お前だろ"
	},
	{
		"id": 50,
		"start": 245.87900000000005,
		"end": 248.96600000000004,
		"text": "だって 私だけ大もうけできると<br />思ったのよ"
	},
	{
		"id": 51,
		"start": 249.09100000000004,
		"end": 250.09200000000004,
		"text": "（カズマ）最低だな"
	},
	{
		"id": 52,
		"start": 250.21700000000004,
		"end": 253.84500000000003,
		"text": "お願いよ お金貸して<br />ツケ払う分だけでいいから！"
	},
	{
		"id": 53,
		"start": 253.97100000000003,
		"end": 255.43,
		"text": "うるさい 駄女神(ダメがみ)！"
	},
	{
		"id": 54,
		"start": 255.55599999999995,
		"end": 259.47599999999983,
		"text": "…というか いいかげん この金で<br />馬小屋生活を脱出するんだよ"
	},
	{
		"id": 55,
		"start": 259.60099999999983,
		"end": 261.85299999999984,
		"text": "そりゃあ カズマも男の子だし"
	},
	{
		"id": 56,
		"start": 261.9789999999998,
		"end": 264.52299999999985,
		"text": "たまに夜中 隣で<br />ゴソゴソしてるの知ってるから―"
	},
	{
		"id": 57,
		"start": 264.64799999999985,
		"end": 267.40099999999984,
		"text": "早くプライベートな空間が<br />欲しいのは分かるけど"
	},
	{
		"id": 58,
		"start": 267.52599999999984,
		"end": 269.2359999999998,
		"text": "分かった！<br />よし 分かった"
	},
	{
		"id": 59,
		"start": 269.3609999999998,
		"end": 271.23799999999983,
		"text": "貸してやるから黙ろうか！"
	},
	{
		"id": 60,
		"start": 273.11499999999984,
		"end": 277.36899999999986,
		"text": "カズマ 仲間っていいわね<br />私たち最高のパーティーだわ"
	},
	{
		"id": 61,
		"start": 277.49399999999986,
		"end": 279.37099999999987,
		"text": "アッハハハ　ウフフフッ"
	},
	{
		"id": 62,
		"start": 279.49599999999987,
		"end": 281.0389999999998,
		"text": "（取り立て屋）ああ どうも まいど<br />（アクア）は～い"
	},
	{
		"id": 63,
		"start": 281.1639999999998,
		"end": 282.49899999999985,
		"text": "（カズマ）この野郎…"
	},
	{
		"id": 64,
		"start": 291.29999999999984,
		"end": 294.4689999999998,
		"text": "カズマが ちゃんとした<br />冒険者みたいに見えるのです"
	},
	{
		"id": 65,
		"start": 294.59499999999986,
		"end": 297.84799999999984,
		"text": "ジャージのままじゃ<br />ファンタジー感 ぶち壊しだものね"
	},
	{
		"id": 66,
		"start": 297.97299999999984,
		"end": 299.01599999999985,
		"text": "ファンタジー感？"
	},
	{
		"id": 67,
		"start": 300.1829999999998,
		"end": 303.47799999999984,
		"text": "初級とはいえ<br />魔法スキルを習得したからな"
	},
	{
		"id": 68,
		"start": 303.6039999999998,
		"end": 307.27399999999983,
		"text": "盾は持たずに魔法剣士みたいな<br />スタイルでいこうと思う"
	},
	{
		"id": 69,
		"start": 307.39899999999983,
		"end": 309.77599999999984,
		"text": "言うことだけは 一丁前よね"
	},
	{
		"id": 70,
		"start": 310.23499999999984,
		"end": 312.11199999999985,
		"text": "では早速 討伐に行きましょう"
	},
	{
		"id": 71,
		"start": 312.23699999999985,
		"end": 315.6159999999998,
		"text": "それも たくさんの<br />雑魚モンスターがいるやつです"
	},
	{
		"id": 72,
		"start": 316.1159999999998,
		"end": 318.3679999999998,
		"text": "新調した つえの威力を<br />試すのです"
	},
	{
		"id": 73,
		"start": 318.4929999999998,
		"end": 323.3729999999998,
		"text": "いや  一撃が重くて気持ちいい<br />すごく強いモンスターを！"
	},
	{
		"id": 74,
		"start": 323.8319999999998,
		"end": 326.33499999999987,
		"text": "いいえ<br />お金になるクエストをやりましょう"
	},
	{
		"id": 75,
		"start": 326.45999999999987,
		"end": 328.87899999999985,
		"text": "ツケを払ったから<br />今日のごはん代もないの！"
	},
	{
		"id": 76,
		"start": 329.2959999999998,
		"end": 330.63099999999986,
		"text": "（カズマ）まとまりがねえ"
	},
	{
		"id": 77,
		"start": 331.0889999999998,
		"end": 334.1759999999998,
		"text": "（カズマ）<br />じゃあ ジャイアント･トードが<br />繁殖期に入っていて―"
	},
	{
		"id": 78,
		"start": 334.3009999999998,
		"end": 336.26099999999985,
		"text": "街の近場に<br />出没しているらしいから…"
	},
	{
		"id": 79,
		"start": 336.38599999999985,
		"end": 338.34699999999987,
		"text": "（２人）<br />カエルは やめましょう！"
	},
	{
		"id": 80,
		"start": 338.47199999999987,
		"end": 339.38899999999984,
		"text": "なぜだ？"
	},
	{
		"id": 81,
		"start": 339.51399999999984,
		"end": 343.64299999999986,
		"text": "（カズマ）ああ　この２人は<br />カエルがトラウマになってるんだ"
	},
	{
		"id": 82,
		"start": 343.76899999999983,
		"end": 347.1469999999998,
		"text": "頭からパックリいかれて<br />粘液まみれにされたからな"
	},
	{
		"id": 83,
		"start": 347.2719999999998,
		"end": 349.39899999999983,
		"text": "ハッ！ 粘液まみれ…"
	},
	{
		"id": 84,
		"start": 349.52399999999983,
		"end": 351.7769999999998,
		"text": "お前 今 興奮したろ"
	},
	{
		"id": 85,
		"start": 352.3609999999998,
		"end": 353.61199999999985,
		"text": "してない"
	},
	{
		"id": 86,
		"start": 356.44799999999987,
		"end": 359.70099999999985,
		"text": "（カズマ）あれ？ 何だ これ<br />依頼がほとんど ないじゃないか"
	},
	{
		"id": 87,
		"start": 359.82599999999985,
		"end": 361.74499999999983,
		"text": "（ダクネス）<br />カズマ　これだ これにしよう！"
	},
	{
		"id": 88,
		"start": 362.24499999999983,
		"end": 364.7479999999998,
		"text": "ブラックファングと呼ばれる<br />巨大熊討伐を"
	},
	{
		"id": 89,
		"start": 364.8729999999998,
		"end": 365.99899999999985,
		"text": "（カズマ）却下だ 却下！"
	},
	{
		"id": 90,
		"start": 366.83299999999986,
		"end": 370.9619999999998,
		"text": "おい 何だよ これ　高難易度の<br />クエストしか残ってないぞ"
	},
	{
		"id": 91,
		"start": 371.46299999999985,
		"end": 372.75599999999986,
		"text": "申し訳ありません"
	},
	{
		"id": 92,
		"start": 373.71499999999986,
		"end": 378.3029999999998,
		"text": "実は最近 魔王の幹部らしき者が<br />街の近くに住み着きまして"
	},
	{
		"id": 93,
		"start": 378.4279999999998,
		"end": 379.26199999999983,
		"text": "（カズマ）え？"
	},
	{
		"id": 94,
		"start": 379.38799999999986,
		"end": 383.5579999999998,
		"text": "（ルナ）その影響か この近辺の<br />弱いモンスターは隠れてしまい―"
	},
	{
		"id": 95,
		"start": 383.6829999999998,
		"end": 386.06099999999986,
		"text": "仕事が激減しておりまして"
	},
	{
		"id": 96,
		"start": 386.18599999999986,
		"end": 388.1879999999998,
		"text": "ええ～っ"
	},
	{
		"id": 97,
		"start": 389.31399999999985,
		"end": 392.27499999999986,
		"text": "（アクア）まったく<br />幹部だか何だか知らないけど―"
	},
	{
		"id": 98,
		"start": 392.40099999999984,
		"end": 394.77799999999985,
		"text": "もしアンデッドなら見てなさいよ"
	},
	{
		"id": 99,
		"start": 394.90299999999985,
		"end": 400.3249999999998,
		"text": "つまり 腕利きの冒険者や騎士が<br />王都から派遣されてくるまでは―"
	},
	{
		"id": 100,
		"start": 400.4499999999998,
		"end": 402.91099999999983,
		"text": "まともな仕事が<br />できないってことか"
	},
	{
		"id": 101,
		"start": 403.74499999999983,
		"end": 406.4979999999998,
		"text": "見て見て すごい！<br />会心の出来"
	},
	{
		"id": 102,
		"start": 406.6229999999998,
		"end": 409.08399999999983,
		"text": "買い取り単価<br />上がらないかしら？"
	},
	{
		"id": 103,
		"start": 409.66799999999984,
		"end": 412.5459999999998,
		"text": "（カズマ）<br />異世界で胸躍る冒険か"
	},
	{
		"id": 104,
		"start": 418.71799999999985,
		"end": 421.80499999999984,
		"text": "なあ 魔法の訓練なら<br />一人で行けばいいだろ？"
	},
	{
		"id": 105,
		"start": 422.30499999999984,
		"end": 425.1419999999998,
		"text": "そうしたら いったい誰が<br />私をおぶって帰るんですか？"
	},
	{
		"id": 106,
		"start": 425.2669999999998,
		"end": 426.10099999999983,
		"text": "（カズマ）え？"
	},
	{
		"id": 107,
		"start": 426.22599999999983,
		"end": 428.31099999999986,
		"text": "１発 撃ったら<br />動けなくなるんですよ"
	},
	{
		"id": 108,
		"start": 428.8539999999998,
		"end": 430.5219999999998,
		"text": "（カズマ）ひょっとして…"
	},
	{
		"id": 109,
		"start": 430.9809999999998,
		"end": 434.9429999999998,
		"text": "俺は これから毎日<br />これに つきあわされるのか？"
	},
	{
		"id": 110,
		"start": 435.60999999999984,
		"end": 439.78099999999984,
		"text": "もう この辺でいいだろ？<br />適当に魔法 撃って帰ろうぜ"
	},
	{
		"id": 111,
		"start": 439.90599999999984,
		"end": 441.1159999999998,
		"text": "ダメなのです"
	},
	{
		"id": 112,
		"start": 441.2409999999998,
		"end": 445.07799999999986,
		"text": "街から離れた所じゃないと<br />また守衛さんに叱られます"
	},
	{
		"id": 113,
		"start": 445.61999999999983,
		"end": 447.83099999999985,
		"text": "今 お前“また”って言ったな"
	},
	{
		"id": 114,
		"start": 447.95599999999985,
		"end": 450.20799999999986,
		"text": "音がうるさいとか迷惑だって<br />怒られたのか？"
	},
	{
		"id": 115,
		"start": 451.29299999999984,
		"end": 452.96099999999984,
		"text": "（めぐみん）う… うん"
	},
	{
		"id": 116,
		"start": 454.00399999999985,
		"end": 455.5889999999998,
		"text": "しかたねえな"
	},
	{
		"id": 117,
		"start": 458.79999999999984,
		"end": 461.4279999999998,
		"text": "（めぐみん）<br />あれは廃城でしょうか？"
	},
	{
		"id": 118,
		"start": 461.9279999999998,
		"end": 463.63799999999986,
		"text": "（カズマ）薄気味悪いな"
	},
	{
		"id": 119,
		"start": 463.76399999999984,
		"end": 466.64099999999985,
		"text": "（めぐみん）あれにしましょう<br />あれなら盛大に破壊しても―"
	},
	{
		"id": 120,
		"start": 466.7669999999998,
		"end": 468.35099999999983,
		"text": "誰も文句は言わないでしょう"
	},
	{
		"id": 121,
		"start": 468.47699999999986,
		"end": 469.93599999999986,
		"text": "（カズマ）あっ うん…"
	},
	{
		"id": 122,
		"start": 470.89599999999984,
		"end": 475.52499999999986,
		"text": "（めぐみん）紅(あか)き黒炎 万界の王<br />天地の法を敷延すれど―"
	},
	{
		"id": 123,
		"start": 475.64999999999986,
		"end": 477.81899999999985,
		"text": "我は万象照応のことわり"
	},
	{
		"id": 124,
		"start": 478.5699999999998,
		"end": 480.44699999999983,
		"text": "崩壊 破壊の別名なり"
	},
	{
		"id": 125,
		"start": 481.03099999999984,
		"end": 484.20099999999985,
		"text": "永劫(えいごう)の鉄槌(てっつい)は我がもとに下れ"
	},
	{
		"id": 126,
		"start": 484.6589999999998,
		"end": 487.53699999999986,
		"text": "エクスプロージョン！"
	},
	{
		"id": 127,
		"start": 497.50599999999986,
		"end": 498.38099999999986,
		"text": "（カズマ）あっ"
	},
	{
		"id": 128,
		"start": 498.50699999999983,
		"end": 501.34299999999985,
		"text": "燃え尽きろ 紅蓮(ぐれん)の中で"
	},
	{
		"id": 129,
		"start": 502.7189999999998,
		"end": 503.80399999999986,
		"text": "ハア"
	},
	{
		"id": 130,
		"start": 504.17899999999986,
		"end": 506.26399999999984,
		"text": "最高… です"
	},
	{
		"id": 131,
		"start": 507.51599999999985,
		"end": 509.18399999999986,
		"text": "（カズマ）こうして"
	},
	{
		"id": 132,
		"start": 509.68399999999986,
		"end": 512.9789999999998,
		"text": "俺と めぐみんの<br />新しい日課が始まった"
	},
	{
		"id": 133,
		"start": 513.6879999999999,
		"end": 516.5659999999998,
		"text": "（アクア）安いよ 安いよ<br />新鮮なキャベツですよ！"
	},
	{
		"id": 134,
		"start": 517.3169999999999,
		"end": 520.3199999999998,
		"text": "（カズマ）文無しのアクアは<br />毎日アルバイトに励んでる"
	},
	{
		"id": 135,
		"start": 521.9469999999999,
		"end": 525.9509999999998,
		"text": "ダクネスは しばらく実家で<br />筋トレをしてくると言っていた"
	},
	{
		"id": 136,
		"start": 526.6599999999999,
		"end": 530.9139999999999,
		"text": "特にやることのない めぐみんは<br />その廃城のそばへと毎日 通い―"
	},
	{
		"id": 137,
		"start": 531.0389999999999,
		"end": 533.1239999999998,
		"text": "爆裂魔法を放ち続けた"
	},
	{
		"id": 138,
		"start": 535.5019999999998,
		"end": 538.2129999999999,
		"text": "それは寒い 氷雨の降る夕方"
	},
	{
		"id": 139,
		"start": 538.3379999999999,
		"end": 539.7559999999999,
		"text": "（めぐみん）…ロージョン！"
	},
	{
		"id": 140,
		"start": 541.0489999999999,
		"end": 543.5519999999998,
		"text": "（カズマ）<br />それは穏やかな食後の昼下がり"
	},
	{
		"id": 141,
		"start": 543.6769999999998,
		"end": 545.1779999999999,
		"text": "（めぐみん）…ジョン！"
	},
	{
		"id": 142,
		"start": 547.0549999999998,
		"end": 550.8919999999998,
		"text": "（カズマ）それは<br />早朝の爽やかな散歩のついでに"
	},
	{
		"id": 143,
		"start": 551.0169999999998,
		"end": 552.2269999999999,
		"text": "（めぐみん）…ン！"
	},
	{
		"id": 144,
		"start": 554.3959999999998,
		"end": 555.3969999999998,
		"text": "あう…"
	},
	{
		"id": 145,
		"start": 555.5219999999998,
		"end": 559.2759999999998,
		"text": "60点か？<br />音圧が もの足りない"
	},
	{
		"id": 146,
		"start": 559.4009999999998,
		"end": 560.6519999999998,
		"text": "くっ…"
	},
	{
		"id": 147,
		"start": 561.6109999999999,
		"end": 566.7829999999998,
		"text": "（カズマ）<br />どんなときでも めぐみんは<br />毎日その廃城に魔法を放ち…"
	},
	{
		"id": 148,
		"start": 569.3689999999998,
		"end": 572.5389999999999,
		"text": "めぐみんの傍らで<br />魔法を見続けていた俺は―"
	},
	{
		"id": 149,
		"start": 573.5819999999999,
		"end": 576.0419999999998,
		"text": "その日の爆裂魔法の出来が―"
	},
	{
		"id": 150,
		"start": 578.0439999999999,
		"end": 579.5459999999998,
		"text": "分かるまでになっていた"
	},
	{
		"id": 151,
		"start": 582.2149999999998,
		"end": 583.8839999999998,
		"text": "（カズマ）んっ…"
	},
	{
		"id": 152,
		"start": 587.5959999999999,
		"end": 589.8899999999999,
		"text": "エクスプロージョン！"
	},
	{
		"id": 153,
		"start": 596.4379999999999,
		"end": 597.4799999999998,
		"text": "あっ…"
	},
	{
		"id": 154,
		"start": 597.6059999999999,
		"end": 600.1909999999998,
		"text": "おっ 今日のは いい感じだな"
	},
	{
		"id": 155,
		"start": 600.3169999999999,
		"end": 604.4869999999999,
		"text": "爆裂の衝撃波が ズンと骨身に<br />浸透するかのごとく響き―"
	},
	{
		"id": 156,
		"start": 604.6129999999998,
		"end": 609.2419999999998,
		"text": "それでいて肌をなでるかのように<br />空気の振動が遅れて来る"
	},
	{
		"id": 157,
		"start": 610.2429999999998,
		"end": 611.4939999999998,
		"text": "ナイス爆裂"
	},
	{
		"id": 158,
		"start": 612.1199999999999,
		"end": 613.9969999999998,
		"text": "（めぐみん）ナイス爆裂"
	},
	{
		"id": 159,
		"start": 614.1219999999998,
		"end": 617.4589999999998,
		"text": "カズマも爆裂道が<br />分かってきましたね"
	},
	{
		"id": 160,
		"start": 617.5839999999998,
		"end": 621.8799999999999,
		"text": "どうです？ いっそ本当に<br />爆裂魔法を覚えてみては"
	},
	{
		"id": 161,
		"start": 622.0049999999999,
		"end": 623.4649999999998,
		"text": "（カズマ）うーん"
	},
	{
		"id": 162,
		"start": 623.5899999999998,
		"end": 627.6359999999999,
		"text": "でも将来 余裕があったら<br />習得してみるのも面白そうだな"
	},
	{
		"id": 163,
		"start": 627.7609999999999,
		"end": 629.0949999999998,
		"text": "（めぐみん）いい心がけです"
	},
	{
		"id": 164,
		"start": 629.2199999999998,
		"end": 631.9319999999998,
		"text": "（２人の笑い声）"
	},
	{
		"id": 165,
		"start": 635.4349999999998,
		"end": 638.1039999999998,
		"text": "フウ… 将来か"
	},
	{
		"id": 166,
		"start": 638.6469999999998,
		"end": 640.3149999999998,
		"text": "（アクア）ただいま～"
	},
	{
		"id": 167,
		"start": 640.5229999999998,
		"end": 644.3609999999999,
		"text": "見て見て　今日 賄いが余ったから<br />持って帰っていいって"
	},
	{
		"id": 168,
		"start": 644.4859999999999,
		"end": 646.1539999999999,
		"text": "もうかっちゃったね"
	},
	{
		"id": 169,
		"start": 646.5289999999999,
		"end": 649.3659999999999,
		"text": "なあ アクア<br />言いたくないけどさ…"
	},
	{
		"id": 170,
		"start": 649.4909999999999,
		"end": 650.3249999999998,
		"text": "（アクア）何？"
	},
	{
		"id": 171,
		"start": 650.4499999999998,
		"end": 654.4959999999999,
		"text": "（カズマ）<br />ぶっちゃけ俺は 魔王討伐なんて<br />無理だと思うようになっている"
	},
	{
		"id": 172,
		"start": 655.9969999999998,
		"end": 659.4589999999998,
		"text": "いいか？ 俺には物語に出てくる<br />主人公みたいな―"
	},
	{
		"id": 173,
		"start": 659.5839999999998,
		"end": 661.7109999999998,
		"text": "すごい力なんて なかった"
	},
	{
		"id": 174,
		"start": 661.8359999999998,
		"end": 664.2969999999998,
		"text": "魔王軍と戦うのは<br />先に送り込まれた―"
	},
	{
		"id": 175,
		"start": 664.4219999999998,
		"end": 667.3419999999999,
		"text": "強力な能力だの装備だのを<br />もらった連中が―"
	},
	{
		"id": 176,
		"start": 667.4669999999999,
		"end": 668.9689999999998,
		"text": "やればいいんじゃないか？"
	},
	{
		"id": 177,
		"start": 669.0939999999998,
		"end": 672.1379999999998,
		"text": "そんな！ そんなんじゃ<br />私 帰れないじゃない"
	},
	{
		"id": 178,
		"start": 672.2629999999998,
		"end": 676.3089999999999,
		"text": "そんなセリフは もっとこう<br />女神として勇者を導いたり―"
	},
	{
		"id": 179,
		"start": 676.4339999999999,
		"end": 679.3539999999998,
		"text": "勇者が 一人前になるまで<br />魔王を封印して―"
	},
	{
		"id": 180,
		"start": 679.4789999999998,
		"end": 681.5229999999998,
		"text": "時間を稼いだりしてから言え"
	},
	{
		"id": 181,
		"start": 681.6479999999998,
		"end": 684.5259999999998,
		"text": "この 宴会芸しか<br />とりえのない穀つぶしが！"
	},
	{
		"id": 182,
		"start": 686.3609999999999,
		"end": 688.4879999999998,
		"text": "うわああ～っ！"
	},
	{
		"id": 183,
		"start": 688.6129999999998,
		"end": 689.8639999999998,
		"text": "…というわけだから―"
	},
	{
		"id": 184,
		"start": 689.9889999999998,
		"end": 693.1589999999999,
		"text": "何か手軽にできて<br />もうかる商売でも考えろ"
	},
	{
		"id": 185,
		"start": 693.2839999999999,
		"end": 697.9969999999998,
		"text": "あと お前の最後のとりえの<br />回復魔法をとっとと俺に教えろよ"
	},
	{
		"id": 186,
		"start": 698.1229999999998,
		"end": 702.0009999999999,
		"text": "（アクア）イヤーッ！<br />回復魔法だけはイヤよ"
	},
	{
		"id": 187,
		"start": 702.1269999999998,
		"end": 704.7539999999998,
		"text": "私の存在意義を奪わないでよ！"
	},
	{
		"id": 188,
		"start": 704.8789999999998,
		"end": 707.8819999999998,
		"text": "私がいるんだから いいじゃない<br />うわーん"
	},
	{
		"id": 189,
		"start": 708.0069999999998,
		"end": 709.3419999999999,
		"text": "（カズマのため息）"
	},
	{
		"id": 190,
		"start": 709.6339999999998,
		"end": 712.8869999999998,
		"text": "カズマは結構えげつない口撃力(こうげきりょく)が<br />ありますから―"
	},
	{
		"id": 191,
		"start": 713.0119999999998,
		"end": 716.6829999999999,
		"text": "遠慮なく本音をぶちまけると<br />大概の女性は泣きますよ"
	},
	{
		"id": 192,
		"start": 716.8079999999999,
		"end": 719.6859999999998,
		"text": "（ダクネス）うむ<br />ストレスがたまっているのなら―"
	},
	{
		"id": 193,
		"start": 719.8939999999999,
		"end": 723.8979999999998,
		"text": "代わりに私を口汚くののしって<br />くれても かまわないぞ"
	},
	{
		"id": 194,
		"start": 724.0229999999998,
		"end": 724.8579999999998,
		"text": "（カズマ）あっ"
	},
	{
		"id": 195,
		"start": 724.9829999999998,
		"end": 726.8179999999999,
		"text": "（アクア）うわーん ひどいよ！"
	},
	{
		"id": 196,
		"start": 727.2769999999998,
		"end": 728.8199999999998,
		"text": "（カズマ）こいつ…"
	},
	{
		"id": 197,
		"start": 729.3619999999999,
		"end": 730.8219999999999,
		"text": "緊急  緊急！"
	},
	{
		"id": 198,
		"start": 731.4059999999998,
		"end": 734.7009999999998,
		"text": "全冒険者の皆さんは<br />直ちに武装し―"
	},
	{
		"id": 199,
		"start": 734.8259999999998,
		"end": 737.8289999999998,
		"text": "戦闘態勢で<br />街の正門に集まってください！"
	},
	{
		"id": 200,
		"start": 741.3739999999998,
		"end": 744.0019999999998,
		"text": "（荒くれ者）<br />あれは… ただ事じゃねえ！"
	},
	{
		"id": 201,
		"start": 750.6329999999998,
		"end": 752.0929999999998,
		"text": "ん？ 何 何？"
	},
	{
		"id": 202,
		"start": 752.2179999999998,
		"end": 754.9709999999999,
		"text": "おい 何だ？<br />めちゃくちゃ強そうだぞ"
	},
	{
		"id": 203,
		"start": 756.0969999999999,
		"end": 762.6039999999998,
		"text": "（デュラハン）<br />俺は つい先日 この近くの城に<br />越してきた魔王軍の幹部の者だが"
	},
	{
		"id": 204,
		"start": 762.7289999999998,
		"end": 764.6469999999998,
		"text": "あれはデュラハンか"
	},
	{
		"id": 205,
		"start": 766.3569999999999,
		"end": 769.4019999999998,
		"text": "（デュラハン）うう…<br />毎日 毎日 毎日 毎日！"
	},
	{
		"id": 206,
		"start": 769.5269999999998,
		"end": 773.5729999999999,
		"text": "お… 俺の城に毎日 欠かさず<br />爆裂魔法 撃ち込んでく―"
	},
	{
		"id": 207,
		"start": 773.6979999999999,
		"end": 778.2439999999998,
		"text": "あ… 頭のおかしい大バカは<br />誰だあーっ！"
	},
	{
		"id": 208,
		"start": 781.7899999999998,
		"end": 786.9199999999998,
		"text": "（カズマ）魔王の幹部は<br />それはもう お怒りだった"
	},
	{
		"id": 209,
		"start": 787.7119999999999,
		"end": 789.9639999999998,
		"text": "（雷鳴）"
	},
	{
		"id": 210,
		"start": 790.0889999999998,
		"end": 792.9669999999999,
		"text": "（デュラハン）<br />毎日 毎日 毎日 毎日―"
	},
	{
		"id": 211,
		"start": 793.0919999999999,
		"end": 796.6379999999998,
		"text": "俺の城に爆裂魔法 撃ち込んでく<br />頭のおかしい大バカは―"
	},
	{
		"id": 212,
		"start": 796.7629999999998,
		"end": 799.1399999999999,
		"text": "誰だあーっ！"
	},
	{
		"id": 213,
		"start": 802.6439999999999,
		"end": 803.8119999999998,
		"text": "爆裂魔法？"
	},
	{
		"id": 214,
		"start": 803.9369999999998,
		"end": 806.5639999999999,
		"text": "爆裂魔法を使えるやつって<br />いったら…"
	},
	{
		"id": 215,
		"start": 806.6889999999999,
		"end": 808.8579999999998,
		"text": "（冒険者）<br />爆裂魔法っていったら…"
	},
	{
		"id": 216,
		"start": 808.9829999999998,
		"end": 810.0679999999999,
		"text": "ハッ"
	},
	{
		"id": 217,
		"start": 812.9459999999998,
		"end": 814.4889999999998,
		"text": "（魔法使い）え？ ええっ？"
	},
	{
		"id": 218,
		"start": 814.6139999999998,
		"end": 817.7419999999998,
		"text": "あ… 私？<br />なんで私が見られてんの？"
	},
	{
		"id": 219,
		"start": 817.8669999999998,
		"end": 819.5769999999999,
		"text": "爆裂魔法なんて使えないよ"
	},
	{
		"id": 220,
		"start": 820.0359999999998,
		"end": 822.0799999999998,
		"text": "あの… 私 まだ駆け出しで"
	},
	{
		"id": 221,
		"start": 822.2049999999998,
		"end": 824.7909999999998,
		"text": "あの ホント違うんです<br />信じてください"
	},
	{
		"id": 222,
		"start": 824.9159999999998,
		"end": 828.8369999999999,
		"text": "私 まだ死にたくない<br />小さい弟たちもいるのに…"
	},
	{
		"id": 223,
		"start": 831.5059999999999,
		"end": 832.7149999999998,
		"text": "めぐみん？"
	},
	{
		"id": 224,
		"start": 846.0619999999998,
		"end": 847.4799999999998,
		"text": "（デュラハン）お前が…"
	},
	{
		"id": 225,
		"start": 848.2729999999998,
		"end": 849.4819999999999,
		"text": "ぬう…"
	},
	{
		"id": 226,
		"start": 850.4829999999998,
		"end": 855.6969999999999,
		"text": "お前が毎日毎日 俺の城に<br />爆裂魔法ぶち込んでく大バカ者か！"
	},
	{
		"id": 227,
		"start": 855.8219999999999,
		"end": 859.1169999999998,
		"text": "俺が魔王軍の幹部だと知っていて<br />ケンカを売っているなら―"
	},
	{
		"id": 228,
		"start": 859.2419999999998,
		"end": 861.2439999999998,
		"text": "堂々と城に攻めてくるがいい"
	},
	{
		"id": 229,
		"start": 861.3689999999998,
		"end": 864.1219999999998,
		"text": "その気がないなら<br />街で震えているがいい"
	},
	{
		"id": 230,
		"start": 864.2469999999998,
		"end": 867.2079999999999,
		"text": "ねえ なんで こんな陰湿な<br />いやがらせするの？"
	},
	{
		"id": 231,
		"start": 867.3329999999999,
		"end": 869.7519999999998,
		"text": "どうせ雑魚しかいない街だと<br />放置しておれば―"
	},
	{
		"id": 232,
		"start": 869.8779999999998,
		"end": 873.3389999999998,
		"text": "調子に乗って毎日毎日 ポンポン<br />ポンポン ポンポン ポンポン―"
	},
	{
		"id": 233,
		"start": 873.4639999999998,
		"end": 874.7159999999999,
		"text": "撃ち込みに来おって！"
	},
	{
		"id": 234,
		"start": 874.8409999999999,
		"end": 877.7599999999999,
		"text": "頭 おかしいんじゃないのか<br />貴様！"
	},
	{
		"id": 235,
		"start": 879.8879999999998,
		"end": 881.8479999999998,
		"text": "我が名は めぐみん"
	},
	{
		"id": 236,
		"start": 881.9729999999998,
		"end": 885.7679999999998,
		"text": "アークウィザードにして<br />爆裂魔法を操る者！"
	},
	{
		"id": 237,
		"start": 887.4369999999998,
		"end": 890.3149999999998,
		"text": "（デュラハン）<br />“めぐみん”って何だ？<br />バカにしてんのか！"
	},
	{
		"id": 238,
		"start": 890.4399999999998,
		"end": 891.5239999999999,
		"text": "（めぐみん）ち… 違わい！"
	},
	{
		"id": 239,
		"start": 892.1499999999999,
		"end": 896.4459999999998,
		"text": "我は紅魔族(こうまぞく)の者にして<br />この街随一の魔法使い"
	},
	{
		"id": 240,
		"start": 896.8209999999998,
		"end": 899.5319999999998,
		"text": "我が爆裂魔法を<br />放ち続けていたのは―"
	},
	{
		"id": 241,
		"start": 899.6569999999998,
		"end": 902.8269999999999,
		"text": "魔王軍幹部のあなたを<br />おびき出すための作戦"
	},
	{
		"id": 242,
		"start": 903.9119999999998,
		"end": 907.8319999999999,
		"text": "こうして まんまと この街に<br />一人で出てきたのが運の尽きです"
	},
	{
		"id": 243,
		"start": 907.9569999999999,
		"end": 909.0829999999999,
		"text": "（一同）おおっ"
	},
	{
		"id": 244,
		"start": 910.0009999999999,
		"end": 912.0859999999998,
		"text": "いつの間に作戦になったんだ？"
	},
	{
		"id": 245,
		"start": 912.2119999999999,
		"end": 913.9629999999999,
		"text": "うむ　しかもサラッと―"
	},
	{
		"id": 246,
		"start": 914.0879999999999,
		"end": 916.3409999999999,
		"text": "“この街随一の魔法使い”とか<br />言い張ってるな"
	},
	{
		"id": 247,
		"start": 916.4659999999999,
		"end": 919.7609999999999,
		"text": "シーッ<br />黙っておいてあげなさいよ"
	},
	{
		"id": 248,
		"start": 919.8859999999999,
		"end": 922.1799999999998,
		"text": "今日は まだ爆裂魔法<br />使ってないし―"
	},
	{
		"id": 249,
		"start": 922.3049999999998,
		"end": 925.4749999999998,
		"text": "後ろに たくさんの冒険者が<br />控えてるから強気なのよ"
	},
	{
		"id": 250,
		"start": 925.8919999999998,
		"end": 929.4789999999998,
		"text": "今 いいところなんだから<br />このまま見守るのよ"
	},
	{
		"id": 251,
		"start": 930.3129999999999,
		"end": 932.3569999999999,
		"text": "（デュラハン）フンッ まあいい"
	},
	{
		"id": 252,
		"start": 932.4819999999999,
		"end": 936.2769999999998,
		"text": "俺はお前ら雑魚にちょっかいかけに<br />この地に来たわけではない"
	},
	{
		"id": 253,
		"start": 936.6109999999999,
		"end": 939.4889999999998,
		"text": "しばらくは あの城に<br />滞在することになるだろうが…"
	},
	{
		"id": 254,
		"start": 940.4059999999998,
		"end": 944.7439999999998,
		"text": "これからは爆裂魔法は使うな<br />いいな？"
	},
	{
		"id": 255,
		"start": 944.8689999999998,
		"end": 945.7869999999998,
		"text": "（めぐみん）無理です"
	},
	{
		"id": 256,
		"start": 945.9119999999998,
		"end": 949.3739999999998,
		"text": "紅魔族は日に１度<br />爆裂魔法を撃たないと死ぬんです"
	},
	{
		"id": 257,
		"start": 949.4989999999998,
		"end": 952.3769999999998,
		"text": "（デュラハン）お… おい<br />聞いたことがないぞ そんなこと"
	},
	{
		"id": 258,
		"start": 953.1269999999998,
		"end": 954.3789999999998,
		"text": "適当なウソをつくなよ！"
	},
	{
		"id": 259,
		"start": 955.3799999999999,
		"end": 958.8829999999998,
		"text": "どうあっても爆裂魔法を撃つのは<br />やめる気はないと？"
	},
	{
		"id": 260,
		"start": 959.5089999999998,
		"end": 960.6349999999999,
		"text": "うん"
	},
	{
		"id": 261,
		"start": 961.0939999999998,
		"end": 964.7639999999999,
		"text": "（デュラハン）<br />俺は魔に身を落とした身ではあるが<br />元は騎士だ"
	},
	{
		"id": 262,
		"start": 965.7229999999998,
		"end": 967.9759999999999,
		"text": "弱者を刈り取る趣味はない"
	},
	{
		"id": 263,
		"start": 968.1009999999999,
		"end": 969.1429999999998,
		"text": "だが…"
	},
	{
		"id": 264,
		"start": 969.4769999999999,
		"end": 970.5199999999999,
		"text": "フンッ"
	},
	{
		"id": 265,
		"start": 970.8119999999998,
		"end": 973.6059999999999,
		"text": "余裕ぶっていられるのも<br />今のうちです"
	},
	{
		"id": 266,
		"start": 973.7309999999999,
		"end": 976.1089999999998,
		"text": "先生 お願いします！"
	},
	{
		"id": 267,
		"start": 977.3179999999999,
		"end": 978.2359999999999,
		"text": "おい"
	},
	{
		"id": 268,
		"start": 978.3609999999999,
		"end": 979.6119999999999,
		"text": "しょうがないわね"
	},
	{
		"id": 269,
		"start": 979.7369999999999,
		"end": 980.5709999999998,
		"text": "はっ？"
	},
	{
		"id": 270,
		"start": 980.6969999999999,
		"end": 982.7819999999998,
		"text": "魔王の幹部だか知らないけど―"
	},
	{
		"id": 271,
		"start": 982.9069999999998,
		"end": 985.9939999999998,
		"text": "この私がいるときに来るとは<br />運が悪かったわね"
	},
	{
		"id": 272,
		"start": 986.1189999999998,
		"end": 989.2049999999998,
		"text": "あんたのせいで まともな<br />クエストが請けられないのよ"
	},
	{
		"id": 273,
		"start": 989.3299999999998,
		"end": 991.6239999999998,
		"text": "さあ　覚悟はいいかしら？"
	},
	{
		"id": 274,
		"start": 991.7489999999998,
		"end": 994.2519999999998,
		"text": "（デュラハン）<br />ほう  これは これは"
	},
	{
		"id": 275,
		"start": 994.7109999999998,
		"end": 996.5459999999998,
		"text": "アークプリーストか"
	},
	{
		"id": 276,
		"start": 996.6709999999998,
		"end": 999.7989999999999,
		"text": "俺は仮にも魔王軍の幹部の１人"
	},
	{
		"id": 277,
		"start": 999.9239999999999,
		"end": 1002.0929999999998,
		"text": "こんな街にいる低レベルの<br />アークプリーストに―"
	},
	{
		"id": 278,
		"start": 1002.2179999999998,
		"end": 1005.2209999999999,
		"text": "浄化されるほど<br />落ちぶれてはいない"
	},
	{
		"id": 279,
		"start": 1005.4709999999999,
		"end": 1007.8899999999999,
		"text": "そうだな　ここはひとつ―"
	},
	{
		"id": 280,
		"start": 1008.0159999999998,
		"end": 1010.4759999999999,
		"text": "紅魔の娘を苦しませてやろうか"
	},
	{
		"id": 281,
		"start": 1014.9389999999999,
		"end": 1016.9409999999998,
		"text": "私の祈りで浄化してやるわ"
	},
	{
		"id": 282,
		"start": 1017.4419999999999,
		"end": 1019.2349999999998,
		"text": "（デュラハン）間に合わんよ"
	},
	{
		"id": 283,
		"start": 1020.3609999999999,
		"end": 1022.9889999999997,
		"text": "なんじに死の宣告を"
	},
	{
		"id": 284,
		"start": 1023.1139999999997,
		"end": 1026.3260000000007,
		"text": "お前は１週間後に死ぬだろう"
	},
	{
		"id": 285,
		"start": 1027.2850000000005,
		"end": 1029.8290000000006,
		"text": "（ダクネス）ぐあ… ああーっ！"
	},
	{
		"id": 286,
		"start": 1030.4960000000005,
		"end": 1031.8310000000006,
		"text": "ダクネス！"
	},
	{
		"id": 287,
		"start": 1032.5820000000006,
		"end": 1034.9590000000007,
		"text": "（うめき声）"
	},
	{
		"id": 288,
		"start": 1035.0840000000007,
		"end": 1037.5870000000007,
		"text": "（カズマ）クソッ やられた<br />死の宣告か！"
	},
	{
		"id": 289,
		"start": 1037.7120000000007,
		"end": 1039.9640000000006,
		"text": "ダクネス　ダクネス！"
	},
	{
		"id": 290,
		"start": 1040.0890000000006,
		"end": 1041.3410000000006,
		"text": "（カズマ）大丈夫か？"
	},
	{
		"id": 291,
		"start": 1043.3840000000007,
		"end": 1045.1360000000006,
		"text": "何ともないようだが…"
	},
	{
		"id": 292,
		"start": 1045.5950000000007,
		"end": 1049.5990000000006,
		"text": "（デュラハン）<br />仲間どうしの結束が固い<br />貴様ら冒険者には―"
	},
	{
		"id": 293,
		"start": 1049.7240000000006,
		"end": 1052.3930000000007,
		"text": "むしろ こちらのほうが<br />こたえそうだな"
	},
	{
		"id": 294,
		"start": 1052.8940000000007,
		"end": 1057.6070000000007,
		"text": "紅魔族の娘よ<br />そのクルセイダーは１週間後に死ぬ"
	},
	{
		"id": 295,
		"start": 1057.7320000000007,
		"end": 1060.7770000000007,
		"text": "フフッ<br />お前の大切な仲間は―"
	},
	{
		"id": 296,
		"start": 1060.9020000000007,
		"end": 1064.3640000000007,
		"text": "それまで死の恐怖におびえ<br />苦しむことになるのだ"
	},
	{
		"id": 297,
		"start": 1064.4890000000007,
		"end": 1066.9910000000007,
		"text": "そう　貴様のせいでな"
	},
	{
		"id": 298,
		"start": 1067.4080000000006,
		"end": 1068.2420000000006,
		"text": "あっ"
	},
	{
		"id": 299,
		"start": 1068.3680000000006,
		"end": 1072.3300000000006,
		"text": "（デュラハン）これより１週間<br />仲間の苦しむ様を見て―"
	},
	{
		"id": 300,
		"start": 1072.4550000000006,
		"end": 1074.6650000000006,
		"text": "自らの行いを悔いるがいい"
	},
	{
		"id": 301,
		"start": 1074.7910000000006,
		"end": 1079.6290000000006,
		"text": "ハハハハッ　素直に俺の言うことを<br />聞いておけばよかったのだ"
	},
	{
		"id": 302,
		"start": 1083.3830000000007,
		"end": 1084.6340000000007,
		"text": "（ダクネス）なんてことだ"
	},
	{
		"id": 303,
		"start": 1084.7590000000007,
		"end": 1087.9700000000007,
		"text": "つまり貴様は この私に<br />死の呪いをかけ―"
	},
	{
		"id": 304,
		"start": 1088.0960000000007,
		"end": 1091.2650000000006,
		"text": "“呪いを解いてほしくば<br />俺の言うことを聞け”と―"
	},
	{
		"id": 305,
		"start": 1092.2250000000006,
		"end": 1093.9350000000006,
		"text": "つまりは そういうことなのか！"
	},
	{
		"id": 306,
		"start": 1094.4350000000006,
		"end": 1095.4360000000006,
		"text": "（デュラハン）ふぁっ？"
	},
	{
		"id": 307,
		"start": 1096.1040000000007,
		"end": 1098.9400000000007,
		"text": "（カズマ）<br />ダクネスが何を言ってるのか<br />理解したくない"
	},
	{
		"id": 308,
		"start": 1099.0650000000007,
		"end": 1103.1940000000006,
		"text": "くっ… 呪いぐらいでは<br />この私は屈しはしない"
	},
	{
		"id": 309,
		"start": 1103.3190000000006,
		"end": 1104.9460000000006,
		"text": "屈しはしないが…"
	},
	{
		"id": 310,
		"start": 1105.3630000000007,
		"end": 1107.6980000000005,
		"text": "ど… どうしよう カズマ！"
	},
	{
		"id": 311,
		"start": 1107.8240000000005,
		"end": 1108.9080000000006,
		"text": "見るがいい"
	},
	{
		"id": 312,
		"start": 1109.0330000000006,
		"end": 1111.4520000000007,
		"text": "あのデュラハンの兜(かぶと)の下の<br />いやらしい目を"
	},
	{
		"id": 313,
		"start": 1112.1200000000006,
		"end": 1114.7890000000007,
		"text": "あれは 私をこのまま<br />城へと連れ帰り―"
	},
	{
		"id": 314,
		"start": 1114.9140000000007,
		"end": 1117.8340000000007,
		"text": "“呪いを解いてほしくば<br />黙って言うことを聞け”と―"
	},
	{
		"id": 315,
		"start": 1117.9590000000007,
		"end": 1122.5880000000006,
		"text": "すさまじいハードコア変態プレーを<br />要求する変質者の目だ！"
	},
	{
		"id": 316,
		"start": 1123.7560000000005,
		"end": 1124.5900000000006,
		"text": "（デュラハン）えっ…"
	},
	{
		"id": 317,
		"start": 1124.7150000000006,
		"end": 1127.5100000000007,
		"text": "この私の体は好きにできても―"
	},
	{
		"id": 318,
		"start": 1127.6350000000007,
		"end": 1130.3880000000006,
		"text": "心まで自由にできるとは<br />思うなよ！"
	},
	{
		"id": 319,
		"start": 1130.5130000000006,
		"end": 1135.7680000000007,
		"text": "城にとらわれ 魔王の手先に<br />理不尽な要求をされる女騎士とか…"
	},
	{
		"id": 320,
		"start": 1135.8930000000007,
		"end": 1138.6460000000006,
		"text": "ああっ どうしよう<br />どうしよう カズマ！"
	},
	{
		"id": 321,
		"start": 1138.7710000000006,
		"end": 1141.2740000000006,
		"text": "予想外に燃える<br />シチュエーションだ！"
	},
	{
		"id": 322,
		"start": 1141.8580000000006,
		"end": 1145.3610000000006,
		"text": "行きたくはない<br />行きたくはないが しかたがない"
	},
	{
		"id": 323,
		"start": 1145.4860000000006,
		"end": 1148.6980000000005,
		"text": "ギリギリまで抵抗してみるから<br />邪魔はしないでくれ"
	},
	{
		"id": 324,
		"start": 1149.1150000000007,
		"end": 1151.2000000000007,
		"text": "では 行ってくりゅ！"
	},
	{
		"id": 325,
		"start": 1151.7420000000006,
		"end": 1153.2440000000006,
		"text": "（デュラハン）きちい…"
	},
	{
		"id": 326,
		"start": 1153.6190000000006,
		"end": 1156.7060000000006,
		"text": "（カズマ）やめろ 行くな！<br />デュラハンの人が困ってるだろ"
	},
	{
		"id": 327,
		"start": 1156.8310000000006,
		"end": 1159.2500000000007,
		"text": "（ダクネス）<br />離せ　止めてくれるな！"
	},
	{
		"id": 328,
		"start": 1159.3750000000007,
		"end": 1161.0020000000006,
		"text": "（カズマ）気の毒に<br />（ダクネス）やめろーっ！"
	},
	{
		"id": 329,
		"start": 1162.8790000000006,
		"end": 1167.8000000000006,
		"text": "（デュラハン）と… とにかく！<br />俺の城に爆裂魔法を放つのはやめろ"
	},
	{
		"id": 330,
		"start": 1167.9250000000006,
		"end": 1169.5520000000006,
		"text": "そして 紅魔族の娘よ"
	},
	{
		"id": 331,
		"start": 1169.6770000000006,
		"end": 1172.2210000000007,
		"text": "そこのクルセイダーの呪いを<br />解いてほしくば―"
	},
	{
		"id": 332,
		"start": 1172.3460000000007,
		"end": 1173.5980000000006,
		"text": "俺の城に来るがいい"
	},
	{
		"id": 333,
		"start": 1174.3900000000006,
		"end": 1178.3520000000005,
		"text": "俺の所まで来ることができたなら<br />その呪いを解いてやろう"
	},
	{
		"id": 334,
		"start": 1179.2280000000007,
		"end": 1184.1080000000006,
		"text": "だが お前たち 果たして<br />たどり着くことができるかな？"
	},
	{
		"id": 335,
		"start": 1184.2330000000006,
		"end": 1189.6140000000007,
		"text": "（デュラハンの笑い声）"
	},
	{
		"id": 336,
		"start": 1192.4500000000007,
		"end": 1193.9510000000007,
		"text": "（カズマたち）あ…"
	},
	{
		"id": 337,
		"start": 1199.2480000000007,
		"end": 1200.7080000000005,
		"text": "（めぐみん）う…"
	},
	{
		"id": 338,
		"start": 1203.7110000000007,
		"end": 1205.4210000000007,
		"text": "（カズマ）おい どこ行く気だ？"
	},
	{
		"id": 339,
		"start": 1206.7140000000006,
		"end": 1208.2570000000007,
		"text": "（めぐみん）<br />ちょっと 城まで行って―"
	},
	{
		"id": 340,
		"start": 1208.3820000000007,
		"end": 1211.4690000000007,
		"text": "あのデュラハンに<br />直接 爆裂魔法をぶち込んで―"
	},
	{
		"id": 341,
		"start": 1211.5940000000007,
		"end": 1213.9710000000007,
		"text": "ダクネスの呪いを解かせてきます"
	},
	{
		"id": 342,
		"start": 1214.8470000000007,
		"end": 1215.8900000000006,
		"text": "ハハッ"
	},
	{
		"id": 343,
		"start": 1216.7240000000006,
		"end": 1217.9340000000007,
		"text": "（ため息）"
	},
	{
		"id": 344,
		"start": 1219.9360000000006,
		"end": 1222.1460000000006,
		"text": "（カズマ）<br />俺も行くに決まってるだろうが"
	},
	{
		"id": 345,
		"start": 1222.4800000000007,
		"end": 1225.1070000000007,
		"text": "お前一人じゃ<br />雑魚相手に魔法を使って―"
	},
	{
		"id": 346,
		"start": 1225.2330000000006,
		"end": 1226.9840000000006,
		"text": "それで終わっちゃうだろ"
	},
	{
		"id": 347,
		"start": 1227.1090000000006,
		"end": 1229.8200000000006,
		"text": "そもそも<br />俺も毎回 一緒に行きながら―"
	},
	{
		"id": 348,
		"start": 1229.9460000000006,
		"end": 1233.3660000000007,
		"text": "幹部の城だって気付かなかった<br />マヌケだしな"
	},
	{
		"id": 349,
		"start": 1235.3680000000006,
		"end": 1237.2450000000006,
		"text": "じゃあ 一緒に行きますか"
	},
	{
		"id": 350,
		"start": 1237.4530000000007,
		"end": 1240.9980000000007,
		"text": "でも 城にはアンデッドが<br />ひしめいているらしいです"
	},
	{
		"id": 351,
		"start": 1241.1230000000007,
		"end": 1243.6680000000006,
		"text": "…となると<br />武器は効きにくいですね"
	},
	{
		"id": 352,
		"start": 1244.0430000000006,
		"end": 1246.5870000000007,
		"text": "私の魔法のほうが<br />効果的なはずです"
	},
	{
		"id": 353,
		"start": 1246.7120000000007,
		"end": 1248.0050000000006,
		"text": "なので…"
	},
	{
		"id": 354,
		"start": 1250.4660000000006,
		"end": 1253.5940000000007,
		"text": "こんなときこそ<br />私を頼りにしてくださいね"
	},
	{
		"id": 355,
		"start": 1254.1360000000006,
		"end": 1255.7630000000006,
		"text": "（ダクネス）よせ"
	},
	{
		"id": 356,
		"start": 1256.1800000000007,
		"end": 1259.2670000000007,
		"text": "やめるんだ ２人とも<br />私のために"
	},
	{
		"id": 357,
		"start": 1259.3920000000007,
		"end": 1263.4790000000007,
		"text": "おい ダクネス<br />呪いは絶対に何とかしてやるからな"
	},
	{
		"id": 358,
		"start": 1263.6040000000007,
		"end": 1264.6470000000006,
		"text": "だから安心して…"
	},
	{
		"id": 359,
		"start": 1264.7720000000006,
		"end": 1269.4850000000006,
		"text": "（アクア）<br />セイクリッド･ブレイクスペル！"
	},
	{
		"id": 360,
		"start": 1272.7800000000007,
		"end": 1276.3250000000007,
		"text": "（ダクネス）あああーっ！"
	},
	{
		"id": 361,
		"start": 1279.4530000000007,
		"end": 1282.8320000000006,
		"text": "ああ～っ"
	},
	{
		"id": 362,
		"start": 1284.4170000000006,
		"end": 1285.4180000000006,
		"text": "（２人）え？"
	},
	{
		"id": 363,
		"start": 1285.5430000000006,
		"end": 1286.7110000000007,
		"text": "あ…"
	},
	{
		"id": 364,
		"start": 1287.5030000000006,
		"end": 1288.6710000000007,
		"text": "え… あっ"
	},
	{
		"id": 365,
		"start": 1288.7960000000007,
		"end": 1292.5920000000006,
		"text": "（アクア）この私にかかれば<br />デュラハンの呪い解除なんて楽勝よ"
	},
	{
		"id": 366,
		"start": 1293.0090000000007,
		"end": 1296.3450000000007,
		"text": "どう どう？ 私だって たまには<br />プリーストっぽいでしょう"
	},
	{
		"id": 367,
		"start": 1296.4700000000007,
		"end": 1302.3510000000006,
		"text": "（喝采）"
	},
	{
		"id": 368,
		"start": 1303.0190000000007,
		"end": 1304.1030000000007,
		"text": "（アクア）ウフフッ"
	},
	{
		"id": 369,
		"start": 1304.2280000000007,
		"end": 1307.1060000000007,
		"text": "いや～<br />それほどでも ありますけど"
	},
	{
		"id": 370,
		"start": 1307.6900000000007,
		"end": 1311.6110000000006,
		"text": "（カズマ）勝手に盛り上がっていた<br />俺と めぐみんのやる気を返せ"
	},
	{
		"id": 371,
		"start": 1312.7370000000005,
		"end": 1314.1130000000007,
		"text": "（アクア）いよっ"
	},
	{
		"id": 372,
		"start": 1314.2380000000007,
		"end": 1316.0730000000005,
		"text": "勝利の花鳥風月！"
	},
	{
		"id": 373,
		"start": 1316.1980000000005,
		"end": 1317.6170000000006,
		"text": "（喝采）"
	},
	{
		"id": 374,
		"start": 1320.7030000000007,
		"end": 1327.3760000000007,
		"text": "♪～"
	},
	{
		"id": 375,
		"start": 1402.9100000000005,
		"end": 1409.9170000000006,
		"text": "～♪"
	}
]