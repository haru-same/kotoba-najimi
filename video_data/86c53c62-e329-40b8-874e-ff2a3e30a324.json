[
	{
		"id": 1,
		"start": 30.73,
		"end": 31.56,
		"text": "よし　"
	},
	{
		"id": 2,
		"start": 30.73,
		"end": 31.56,
		"text": "よし　"
	},
	{
		"id": 3,
		"start": 30.73,
		"end": 31.56,
		"text": "よし　"
	},
	{
		"id": 4,
		"start": 30.73,
		"end": 31.56,
		"text": "よし　"
	},
	{
		"id": 5,
		"start": 32.08,
		"end": 34.08,
		"text": "佐倉千代　行きます"
	},
	{
		"id": 6,
		"start": 48.42,
		"end": 51.85,
		"text": "の…のののの野崎くん"
	},
	{
		"id": 7,
		"start": 52.58,
		"end": 56.1,
		"text": "ず…ずっと　ファンでした"
	},
	{
		"id": 8,
		"start": 66.22,
		"end": 68.36,
		"text": "付き合ってくださいって言わなきゃ"
	},
	{
		"id": 9,
		"start": 69.19,
		"end": 73.04,
		"text": "あっ　あの　あの"
	},
	{
		"id": 10,
		"start": 81.37,
		"end": 82.21,
		"text": "どうぞ"
	},
	{
		"id": 11,
		"start": 97.76,
		"end": 99.57,
		"text": "佐倉千代　１６歳"
	},
	{
		"id": 12,
		"start": 99.93,
		"end": 103.6,
		"text": "好きな人に告白したら　サインを貰いました"
	},
	{
		"id": 13,
		"start": 105.69,
		"end": 109.77,
		"text": "ちょっと待って　いや　ファンだとは言ったけど"
	},
	{
		"id": 14,
		"start": 110.61,
		"end": 117.27,
		"text": "そうじゃなくて　そのっ　野崎くんとずっと一緒にいたいっていうか"
	},
	{
		"id": 15,
		"start": 117.37,
		"end": 118.62,
		"text": "ずっと一緒に？"
	},
	{
		"id": 16,
		"start": 118.97,
		"end": 119.75,
		"text": "って　"
	},
	{
		"id": 17,
		"start": 120.95,
		"end": 123.3,
		"text": "私　何言ってるの　"
	},
	{
		"id": 18,
		"start": 123.54,
		"end": 125.63,
		"text": "普通に好きって言えばいいのに"
	},
	{
		"id": 19,
		"start": 130.58,
		"end": 132.63,
		"text": "重い子って思われたらどうしよう"
	},
	{
		"id": 20,
		"start": 133.31,
		"end": 136.04,
		"text": "じゃあ　これから　俺の家来る？"
	},
	{
		"id": 21,
		"start": 136.69,
		"end": 137.47,
		"text": "軽っ"
	},
	{
		"id": 22,
		"start": 239.76,
		"end": 243.03,
		"text": "野崎くん　こんな所に住んでるんだ"
	},
	{
		"id": 23,
		"start": 249.71,
		"end": 253.26,
		"text": "いやー　助かった　今　誰も捕まらなくてさ"
	},
	{
		"id": 24,
		"start": 253.47,
		"end": 256.02,
		"text": "いつもは女の子連れ込み放題なの"
	},
	{
		"id": 25,
		"start": 262.78,
		"end": 265.35,
		"text": "これが野崎くんの部屋か"
	},
	{
		"id": 26,
		"start": 267.27,
		"end": 272.22,
		"text": "そう言えば　男の子の部屋に来たの　初めてだなぁ"
	},
	{
		"id": 27,
		"start": 272.82,
		"end": 274.6,
		"text": "男の子の　部屋！"
	},
	{
		"id": 28,
		"start": 276.22,
		"end": 278.28,
		"text": "思わず付いて来ちゃったけど　"
	},
	{
		"id": 29,
		"start": 279.69,
		"end": 281.8,
		"text": "やっぱり帰ろうかな"
	},
	{
		"id": 30,
		"start": 282.12,
		"end": 285.34,
		"text": "でも　そしたら他の子が来るんだよね"
	},
	{
		"id": 31,
		"start": 285.99,
		"end": 288.53,
		"text": "そんなの…イヤ"
	},
	{
		"id": 32,
		"start": 289.02,
		"end": 289.83,
		"text": "どうぞ"
	},
	{
		"id": 33,
		"start": 290.51,
		"end": 294.57,
		"text": "のっ　野崎くん　私　頑張ります！"
	},
	{
		"id": 34,
		"start": 296.81,
		"end": 299.49,
		"text": "じゃあ　そこ　ベタよろしく"
	},
	{
		"id": 35,
		"start": 301.63,
		"end": 302.87,
		"text": "黒く塗ってね"
	},
	{
		"id": 36,
		"start": 306.85,
		"end": 307.96,
		"text": "ここもよろしく"
	},
	{
		"id": 37,
		"start": 312.66,
		"end": 313.75,
		"text": "上手いな　佐倉"
	},
	{
		"id": 38,
		"start": 317.21,
		"end": 319.16,
		"text": "そろそろ休憩にしないか"
	},
	{
		"id": 39,
		"start": 323.32,
		"end": 325.16,
		"text": "野崎くんってマンガ家なの！？"
	},
	{
		"id": 40,
		"start": 327.43,
		"end": 330.03,
		"text": "知らないで４時間ベタ塗ってたのか"
	},
	{
		"id": 41,
		"start": 338.34,
		"end": 340.91,
		"text": "野崎くんって　すごい人だったんだなぁ"
	},
	{
		"id": 42,
		"start": 341.77,
		"end": 344.4,
		"text": "月刊誌で一応連載している"
	},
	{
		"id": 43,
		"start": 345.42,
		"end": 347.26,
		"text": "サインまでちゃんとあるんだ"
	},
	{
		"id": 44,
		"start": 347.8,
		"end": 352.02,
		"text": "えーと　ゆ…め…の　"
	},
	{
		"id": 45,
		"start": 352.65,
		"end": 354.68,
		"text": "あぁ　夢野咲子か"
	},
	{
		"id": 46,
		"start": 356.16,
		"end": 359.44,
		"text": "この名前　何処かで見たような"
	},
	{
		"id": 47,
		"start": 359.71,
		"end": 364.98,
		"text": "夢野咲子　夢野咲子　夢野…"
	},
	{
		"id": 48,
		"start": 365.12,
		"end": 366.55,
		"text": "夢野咲子？"
	},
	{
		"id": 49,
		"start": 386.65,
		"end": 388.71,
		"text": "夢野咲子！？"
	},
	{
		"id": 50,
		"start": 394.12,
		"end": 400.48,
		"text": "夢野咲子　繊細な心理描写と華やかな画面で大人気の作家"
	},
	{
		"id": 51,
		"start": 400.88,
		"end": 403.5,
		"text": "好き…っ　鈴木くん…っ"
	},
	{
		"id": 52,
		"start": 403.99,
		"end": 404.99,
		"text": "マミコ…"
	},
	{
		"id": 53,
		"start": 405.16,
		"end": 408.02,
		"text": "野崎ー　出た　出やがれた"
	},
	{
		"id": 54,
		"start": 408.16,
		"end": 412.3,
		"text": "ふざけんな　あんなのに近づけるなんて　よっぽど鈍い奴くらいしか"
	},
	{
		"id": 55,
		"start": 416.35,
		"end": 421.95,
		"text": "乙女心を大切に描き　女の子の心の代弁者とも言われている"
	},
	{
		"id": 56,
		"start": 421.98,
		"end": 424.5,
		"text": "ずっと大切にするね"
	},
	{
		"id": 57,
		"start": 425.36,
		"end": 428.5,
		"text": "おい　下着透けてるから　上着着た方がいいぞ"
	},
	{
		"id": 58,
		"start": 429.42,
		"end": 432.67,
		"text": "なんでそうハッキリ言うの　ほんとデリカシーないわね"
	},
	{
		"id": 59,
		"start": 436.7,
		"end": 438.43,
		"text": "それ　今月号か"
	},
	{
		"id": 60,
		"start": 442.73,
		"end": 446.68,
		"text": "このインタビュー受けた時　飲んだコーヒーが苦くてな"
	},
	{
		"id": 61,
		"start": 447.14,
		"end": 449.22,
		"text": "なんだっけ　エスプレッソ"
	},
	{
		"id": 62,
		"start": 450.03,
		"end": 451.85,
		"text": "高校生にエスプレッソって　"
	},
	{
		"id": 63,
		"start": 452.33,
		"end": 454.69,
		"text": "そもそも初対面から失礼な人でな"
	},
	{
		"id": 64,
		"start": 455.13,
		"end": 456.45,
		"text": "俺を見て"
	},
	{
		"id": 65,
		"start": 455.58,
		"end": 459.18,
		"text": "本当に　野崎くんが夢野先生なんだ"
	},
	{
		"id": 66,
		"start": 456.47,
		"end": 460.54,
		"text": "えっ　高校生って言うから美少女の女かだと思っていましたって"
	},
	{
		"id": 67,
		"start": 461.1,
		"end": 463.02,
		"text": "若いから　美少女の訳ないだろうが"
	},
	{
		"id": 68,
		"start": 463.34,
		"end": 465.08,
		"text": "夢野先生"
	},
	{
		"id": 69,
		"start": 465.56,
		"end": 466.18,
		"text": "はい"
	},
	{
		"id": 70,
		"start": 466.78,
		"end": 468,
		"text": "夢野先生"
	},
	{
		"id": 71,
		"start": 468.27,
		"end": 468.94,
		"text": "はい"
	},
	{
		"id": 72,
		"start": 469.16,
		"end": 471.41,
		"text": "夢野先生　夢野先生！"
	},
	{
		"id": 73,
		"start": 471.57,
		"end": 472.24,
		"text": "はい"
	},
	{
		"id": 74,
		"start": 474.22,
		"end": 476.74,
		"text": "学校で言われると変な気分だな"
	},
	{
		"id": 75,
		"start": 477.9,
		"end": 479.85,
		"text": "もしかして　みんな知らないの"
	},
	{
		"id": 76,
		"start": 480.06,
		"end": 484.09,
		"text": "そうだな　クラスの奴もほとんど知らないし"
	},
	{
		"id": 77,
		"start": 485.66,
		"end": 486.83,
		"text": "それって…"
	},
	{
		"id": 78,
		"start": 487.04,
		"end": 489.99,
		"text": "もしかして　私が特別"
	},
	{
		"id": 79,
		"start": 490.59,
		"end": 493.78,
		"text": "言ってもみんな信じてくれなかったからな"
	},
	{
		"id": 80,
		"start": 494.24,
		"end": 495.43,
		"text": "野崎くん"
	},
	{
		"id": 81,
		"start": 495.75,
		"end": 497.89,
		"text": "そのうちしつこいって怒られた"
	},
	{
		"id": 82,
		"start": 498.51,
		"end": 499.57,
		"text": "だよね"
	},
	{
		"id": 83,
		"start": 500.11,
		"end": 502.68,
		"text": "そうだ　昨日はありがとう"
	},
	{
		"id": 84,
		"start": 503.41,
		"end": 505.03,
		"text": "美術部は平気だったのか"
	},
	{
		"id": 85,
		"start": 506.63,
		"end": 508.82,
		"text": "私の部活知ってたの"
	},
	{
		"id": 86,
		"start": 509.14,
		"end": 511.47,
		"text": "ずっと目を付けていたからな"
	},
	{
		"id": 87,
		"start": 512.17,
		"end": 513.42,
		"text": "目っ　目って"
	},
	{
		"id": 88,
		"start": 515.74,
		"end": 518.75,
		"text": "あの　はみ出しの少ないポスター描ける奴は"
	},
	{
		"id": 89,
		"start": 519.37,
		"end": 521.75,
		"text": "いい仕事するだろうなぁーと"
	},
	{
		"id": 90,
		"start": 522.8,
		"end": 524.18,
		"text": "技術目当て"
	},
	{
		"id": 91,
		"start": 531.27,
		"end": 533.06,
		"text": "やっぱり上手いな　佐倉"
	},
	{
		"id": 92,
		"start": 534,
		"end": 537.58,
		"text": "おかげで　今度の締め切りは早めにあげられそうだ"
	},
	{
		"id": 93,
		"start": 538.01,
		"end": 539.44,
		"text": "そう言えば　野崎くん"
	},
	{
		"id": 94,
		"start": 539.63,
		"end": 542.44,
		"text": "たまにボロボロで学校に来ることがあったけど…"
	},
	{
		"id": 95,
		"start": 544.16,
		"end": 546.12,
		"text": "おい　野崎　大丈夫か"
	},
	{
		"id": 96,
		"start": 546.16,
		"end": 547.29,
		"text": "何があったんだ"
	},
	{
		"id": 97,
		"start": 546.66,
		"end": 547.91,
		"text": "ボロボロじゃねぇか"
	},
	{
		"id": 98,
		"start": 547.46,
		"end": 548.46,
		"text": "機嫌悪いな"
	},
	{
		"id": 99,
		"start": 551.34,
		"end": 553.81,
		"text": "あれって締め切りだったんだね"
	},
	{
		"id": 100,
		"start": 554.16,
		"end": 558.87,
		"text": "もしかして　喧嘩　裏で不良やってるのかも　なんて思ったよ"
	},
	{
		"id": 101,
		"start": 559.14,
		"end": 563.33,
		"text": "首ばっかり狙われる不良か　怖いな"
	},
	{
		"id": 102,
		"start": 564.52,
		"end": 569.12,
		"text": "だが　表は一般人　裏ではヤンキーというのはセオリーだな"
	},
	{
		"id": 103,
		"start": 569.5,
		"end": 571.42,
		"text": "うん　ネタになるな"
	},
	{
		"id": 104,
		"start": 572.15,
		"end": 574.75,
		"text": "隠れてタバコを吸ったり　お酒飲んだりするの"
	},
	{
		"id": 105,
		"start": 574.8,
		"end": 576.99,
		"text": "いや　少女漫画だからな"
	},
	{
		"id": 106,
		"start": 577.59,
		"end": 582.24,
		"text": "健全に　タバコはチョコレート  お酒はジュースにしておこう"
	},
	{
		"id": 107,
		"start": 582.56,
		"end": 583.94,
		"text": "いや　無理あるよ"
	},
	{
		"id": 108,
		"start": 584.4,
		"end": 586.97,
		"text": "漫画でも法律違反は怒られるんだ"
	},
	{
		"id": 109,
		"start": 587.51,
		"end": 589.92,
		"text": "そっか　大変なんだね"
	},
	{
		"id": 110,
		"start": 592.92,
		"end": 596.87,
		"text": "これはあの伝統的ラブイベントの前振りですか　夢野先生"
	},
	{
		"id": 111,
		"start": 597.44,
		"end": 600.63,
		"text": "い…一緒に帰ってもいい"
	},
	{
		"id": 112,
		"start": 600.99,
		"end": 602.64,
		"text": "俺　自転車だけど"
	},
	{
		"id": 113,
		"start": 603.96,
		"end": 604.8,
		"text": "何が"
	},
	{
		"id": 114,
		"start": 605.1,
		"end": 607.13,
		"text": "何って　自転車だよ"
	},
	{
		"id": 115,
		"start": 607.67,
		"end": 610.08,
		"text": "後ろで感じる彼の体温"
	},
	{
		"id": 116,
		"start": 610.43,
		"end": 614.38,
		"text": "どこを持ってばいいの　ドキドキして手が震えちゃう"
	},
	{
		"id": 117,
		"start": 614.81,
		"end": 618.03,
		"text": "それ　今は道路交通法違反だから"
	},
	{
		"id": 118,
		"start": 618.57,
		"end": 619.71,
		"text": "ごめんなさい"
	},
	{
		"id": 119,
		"start": 620.44,
		"end": 621.98,
		"text": "二人乗りができない今"
	},
	{
		"id": 120,
		"start": 622.36,
		"end": 625.71,
		"text": "チャリ通の奴と一緒に帰る図ってのは"
	},
	{
		"id": 121,
		"start": 626.12,
		"end": 627.17,
		"text": "こうだな"
	},
	{
		"id": 122,
		"start": 628.25,
		"end": 631.61,
		"text": "なんだか華やかさが足りないけど　しょうがないね"
	},
	{
		"id": 123,
		"start": 632.29,
		"end": 633.29,
		"text": "じゃあ　こうするか"
	},
	{
		"id": 124,
		"start": 633.37,
		"end": 635.48,
		"text": "最低　最低だよ　この男"
	},
	{
		"id": 125,
		"start": 635.61,
		"end": 636.59,
		"text": "もしくはこう"
	},
	{
		"id": 126,
		"start": 637.29,
		"end": 638.59,
		"text": "なんか馬っぽい"
	},
	{
		"id": 127,
		"start": 642.13,
		"end": 646.62,
		"text": "少女漫画のお話考えるのって色々大変なんだね"
	},
	{
		"id": 128,
		"start": 652.25,
		"end": 654.39,
		"text": "佐倉　一緒に帰らないか"
	},
	{
		"id": 129,
		"start": 654.63,
		"end": 656.01,
		"text": "帰る　帰る帰る"
	},
	{
		"id": 130,
		"start": 656.42,
		"end": 659.93,
		"text": "今日はチャリ通だから　後ろに乗っけてやる"
	},
	{
		"id": 131,
		"start": 665.72,
		"end": 667.75,
		"text": "月刊少女野崎くん"
	},
	{
		"id": 132,
		"start": 671.13,
		"end": 673.43,
		"text": "月刊少女野崎くん"
	},
	{
		"id": 133,
		"start": 692.29,
		"end": 694.45,
		"text": "どっ　どういうこと"
	},
	{
		"id": 134,
		"start": 694.99,
		"end": 701.08,
		"text": "野崎くん　もしかして　昨日私が羨ましがってたから　無理して"
	},
	{
		"id": 135,
		"start": 701.59,
		"end": 704.51,
		"text": "どうしよう　緊張してきた"
	},
	{
		"id": 136,
		"start": 705.3,
		"end": 708.06,
		"text": "そもそも　自転車の後ろってどうなるんだっけ？"
	},
	{
		"id": 137,
		"start": 708.82,
		"end": 709.76,
		"text": "こう"
	},
	{
		"id": 138,
		"start": 710.01,
		"end": 710.93,
		"text": "こう"
	},
	{
		"id": 139,
		"start": 712.74,
		"end": 714.58,
		"text": "荷台がない場合もあるよね"
	},
	{
		"id": 140,
		"start": 714.93,
		"end": 717.34,
		"text": "その時はこれでいいんだっけ"
	},
	{
		"id": 141,
		"start": 720.04,
		"end": 723.18,
		"text": "こんなことになるなら　もっと前もって練習しておけば"
	},
	{
		"id": 142,
		"start": 723.69,
		"end": 724.4,
		"text": "佐倉"
	},
	{
		"id": 143,
		"start": 724.7,
		"end": 725.89,
		"text": "はい"
	},
	{
		"id": 144,
		"start": 726.29,
		"end": 727.43,
		"text": "これなんだが　"
	},
	{
		"id": 145,
		"start": 727.83,
		"end": 729.86,
		"text": "一緒に乗って感想聞かせてくれ"
	},
	{
		"id": 146,
		"start": 732.22,
		"end": 733.46,
		"text": "四輪チャリです"
	},
	{
		"id": 147,
		"start": 733.65,
		"end": 734.65,
		"text": "二人乗り"
	},
	{
		"id": 148,
		"start": 749.91,
		"end": 751.07,
		"text": "どんな気分だ"
	},
	{
		"id": 149,
		"start": 751.61,
		"end": 753.51,
		"text": "ちょっとしたレジャー気分です"
	},
	{
		"id": 150,
		"start": 753.94,
		"end": 756.27,
		"text": "よし　次　右行くぞ"
	},
	{
		"id": 151,
		"start": 756.43,
		"end": 757.46,
		"text": "はい"
	},
	{
		"id": 152,
		"start": 760.62,
		"end": 762.22,
		"text": "次の角は曲がるぞ"
	},
	{
		"id": 153,
		"start": 762.35,
		"end": 763.03,
		"text": "はい"
	},
	{
		"id": 154,
		"start": 781.59,
		"end": 783.37,
		"text": "恋とか生まれると思うか"
	},
	{
		"id": 155,
		"start": 784.13,
		"end": 787.43,
		"text": "なんかこう…信頼感なら生まれそうです"
	},
	{
		"id": 156,
		"start": 790.3,
		"end": 793.52,
		"text": "そうか  後ろが漕いじゃいけないんじゃないか"
	},
	{
		"id": 157,
		"start": 794.19,
		"end": 796.95,
		"text": "なるほど　じゃ何もしないで座ってみるよ"
	},
	{
		"id": 158,
		"start": 805.74,
		"end": 808.67,
		"text": "自分だけ何もしてない罪悪感ハンパないよ"
	},
	{
		"id": 159,
		"start": 808.8,
		"end": 810.37,
		"text": "私にも漕がせて"
	},
	{
		"id": 160,
		"start": 812.07,
		"end": 813.24,
		"text": "じゃ  せめて…　"
	},
	{
		"id": 161,
		"start": 813.53,
		"end": 817.19,
		"text": "腰に手を回してドキドキ　だけでもやってみるか"
	},
	{
		"id": 162,
		"start": 832.74,
		"end": 836.64,
		"text": "とりあえず　今日の経験を踏まえて描いてみたんだが"
	},
	{
		"id": 163,
		"start": 837.23,
		"end": 838.04,
		"text": "どう思う"
	},
	{
		"id": 164,
		"start": 839.21,
		"end": 841.26,
		"text": "よければ　乗っていくか"
	},
	{
		"id": 165,
		"start": 842.05,
		"end": 843.08,
		"text": "ダセー"
	},
	{
		"id": 166,
		"start": 844,
		"end": 845.37,
		"text": "どうした　佐倉"
	},
	{
		"id": 167,
		"start": 845.89,
		"end": 851.11,
		"text": "いや　なんかこう…ヒーローにモテ要素が感じられないというか"
	},
	{
		"id": 168,
		"start": 851.54,
		"end": 854.38,
		"text": "なるほど　モテ要素だな"
	},
	{
		"id": 169,
		"start": 856.55,
		"end": 860.01,
		"text": "先着3人だけ　俺の後ろに　乗せてやるぜ"
	},
	{
		"id": 170,
		"start": 860.09,
		"end": 861.04,
		"text": "4人乗り"
	},
	{
		"id": 171,
		"start": 861.55,
		"end": 864.23,
		"text": "どいて　今日こそここに座るわ"
	},
	{
		"id": 172,
		"start": 864.31,
		"end": 867.05,
		"text": "ふざけないで　このサドルは私のものよ"
	},
	{
		"id": 173,
		"start": 868.13,
		"end": 873,
		"text": "君のために2番目のサドルはキープしておいたよ　特別さ"
	},
	{
		"id": 174,
		"start": 873.62,
		"end": 874.73,
		"text": "私…"
	},
	{
		"id": 175,
		"start": 876.21,
		"end": 879.24,
		"text": "私の他にも何人も座ったんでしょう"
	},
	{
		"id": 176,
		"start": 879.33,
		"end": 880.76,
		"text": "そんなのイヤッ"
	},
	{
		"id": 177,
		"start": 881.71,
		"end": 885.55,
		"text": "だめ　こんなワガママ言ったら　嫌われちゃうのに"
	},
	{
		"id": 178,
		"start": 886.52,
		"end": 889.06,
		"text": "これ　最終的にどうなるの"
	},
	{
		"id": 179,
		"start": 890.09,
		"end": 892.01,
		"text": "サドルが全部盗まれる"
	},
	{
		"id": 180,
		"start": 897.34,
		"end": 901.72,
		"text": "でも結局　雑誌の担当編集さんにふざけんなと却下されて"
	},
	{
		"id": 181,
		"start": 902.48,
		"end": 903.67,
		"text": "こうなりました"
	},
	{
		"id": 182,
		"start": 904.08,
		"end": 906.76,
		"text": "さ…先に帰っていいから…"
	},
	{
		"id": 183,
		"start": 906.84,
		"end": 909.35,
		"text": "いや　暗いし　送っていくよ"
	},
	{
		"id": 184,
		"start": 910,
		"end": 911.57,
		"text": "それに　こうして歩けば"
	},
	{
		"id": 185,
		"start": 911.99,
		"end": 914.09,
		"text": "少しでも長く君といられるしね"
	},
	{
		"id": 186,
		"start": 915.44,
		"end": 919.15,
		"text": "このまま時が　止まってしまえば　いいのに"
	},
	{
		"id": 187,
		"start": 920.5,
		"end": 925.12,
		"text": "今回も面白かったなぁ  夢野先生の『恋しよっ』"
	},
	{
		"id": 188,
		"start": 925.34,
		"end": 927.94,
		"text": "チャリのシーンとか　かっこよかったよね"
	},
	{
		"id": 189,
		"start": 928.13,
		"end": 928.99,
		"text": "ねー　佐倉"
	},
	{
		"id": 190,
		"start": 929.94,
		"end": 931.16,
		"text": "そうね"
	},
	{
		"id": 191,
		"start": 940.27,
		"end": 944.09,
		"text": "野崎くん　乗ってかない　君の特等席だぜ"
	},
	{
		"id": 192,
		"start": 945.49,
		"end": 946.86,
		"text": "野崎くん"
	},
	{
		"id": 193,
		"start": 946.87,
		"end": 948.25,
		"text": "気にいったのか"
	},
	{
		"id": 194,
		"start": 958.32,
		"end": 963.05,
		"text": "でもなんだかんだで　最終的にはロマンチックな漫画になっちゃうんだから"
	},
	{
		"id": 195,
		"start": 963.86,
		"end": 965.43,
		"text": "やっぱすごいなぁ"
	},
	{
		"id": 196,
		"start": 966.51,
		"end": 970.82,
		"text": "本当はこのままずっと　ぎゅっとしていて欲しいのに"
	},
	{
		"id": 197,
		"start": 971.44,
		"end": 974.31,
		"text": "これも野崎くんが考えてるんだよね"
	},
	{
		"id": 198,
		"start": 975.04,
		"end": 979.12,
		"text": "だめ　どうして鈴木君の事ばっかり考えちゃうの"
	},
	{
		"id": 199,
		"start": 979.42,
		"end": 981.61,
		"text": "もう　キライ　キライ　キライ"
	},
	{
		"id": 200,
		"start": 981.72,
		"end": 985.42,
		"text": "私の心の中　全部占領しちゃう鈴木君なんて"
	},
	{
		"id": 201,
		"start": 986.88,
		"end": 988.05,
		"text": "キライだもん"
	},
	{
		"id": 202,
		"start": 989.08,
		"end": 991.27,
		"text": "こ…これも野崎くんが…"
	},
	{
		"id": 203,
		"start": 991.92,
		"end": 993.49,
		"text": "佐倉　どうした"
	},
	{
		"id": 204,
		"start": 996.03,
		"end": 998.71,
		"text": "野崎くんは何で少女漫画描いてるの"
	},
	{
		"id": 205,
		"start": 1000.57,
		"end": 1003.01,
		"text": "まぁ　趣味だったから　そのままノリで"
	},
	{
		"id": 206,
		"start": 1003.52,
		"end": 1008.09,
		"text": "そんなこと言ってるけど　この内容だもん　私だったら絶対描けない"
	},
	{
		"id": 207,
		"start": 1008.66,
		"end": 1011.12,
		"text": "やっぱり恋愛経験の差かな"
	},
	{
		"id": 208,
		"start": 1011.45,
		"end": 1013.64,
		"text": "まぁ　あせることないんじゃないか"
	},
	{
		"id": 209,
		"start": 1014.78,
		"end": 1017.29,
		"text": "俺なんて　初恋もまだだしなぁ"
	},
	{
		"id": 210,
		"start": 1017.62,
		"end": 1019.92,
		"text": "本当　なんで少女漫画描いてるの"
	},
	{
		"id": 211,
		"start": 1020.4,
		"end": 1023.76,
		"text": "だが　恋愛相談にはけっこう乗ってきたほうだぞ"
	},
	{
		"id": 212,
		"start": 1024.78,
		"end": 1026.73,
		"text": "佐倉は好きなやつとかいるのか"
	},
	{
		"id": 213,
		"start": 1027.41,
		"end": 1029.03,
		"text": "わ…わ…わたし"
	},
	{
		"id": 214,
		"start": 1030.52,
		"end": 1031.66,
		"text": "えっと　"
	},
	{
		"id": 215,
		"start": 1032.74,
		"end": 1041.42,
		"text": "わ…私は　最初は背の高さと　お…男らしい性格に引かれて"
	},
	{
		"id": 216,
		"start": 1042.53,
		"end": 1046.07,
		"text": "告白したんだけど　気づいてもらえなくて"
	},
	{
		"id": 217,
		"start": 1046.86,
		"end": 1051.49,
		"text": "でも　そのおかげで　話す回数は増えたんだけど"
	},
	{
		"id": 218,
		"start": 1051.81,
		"end": 1055.84,
		"text": "便利屋くらいにしか思われてない…ような"
	},
	{
		"id": 219,
		"start": 1056.46,
		"end": 1058.71,
		"text": "そんなやつのどこがいいんだ"
	},
	{
		"id": 220,
		"start": 1090.03,
		"end": 1094.01,
		"text": "そうよ　私　このままでいいの"
	},
	{
		"id": 221,
		"start": 1113.62,
		"end": 1115.33,
		"text": "やべー　青木に怒られる"
	},
	{
		"id": 222,
		"start": 1115.63,
		"end": 1116.6,
		"text": "ちょっ　やばっ"
	},
	{
		"id": 223,
		"start": 1139.73,
		"end": 1141.65,
		"text": "野崎くんー"
	},
	{
		"id": 224,
		"start": 1181.34,
		"end": 1183.55,
		"text": "そうだ　私…"
	},
	{
		"id": 225,
		"start": 1186.42,
		"end": 1189.45,
		"text": "まだ自分の気持ち　伝えてない"
	},
	{
		"id": 226,
		"start": 1192.72,
		"end": 1195.32,
		"text": "もう一回　ちゃんと告白しよう"
	},
	{
		"id": 227,
		"start": 1197.7,
		"end": 1198.81,
		"text": "野崎くん"
	},
	{
		"id": 228,
		"start": 1201.03,
		"end": 1201.84,
		"text": "佐倉"
	},
	{
		"id": 229,
		"start": 1202.54,
		"end": 1204.22,
		"text": "千代　頑張るのよ"
	},
	{
		"id": 230,
		"start": 1204.71,
		"end": 1208.5,
		"text": "二度目なんだから　ちゃんと　きちんと自分の思いを伝えないと"
	},
	{
		"id": 231,
		"start": 1212.82,
		"end": 1213.64,
		"text": "どうした"
	},
	{
		"id": 232,
		"start": 1214.58,
		"end": 1217.15,
		"text": "わ…わたし…"
	},
	{
		"id": 233,
		"start": 1221.4,
		"end": 1225.86,
		"text": "その　あ…あの…あの"
	},
	{
		"id": 234,
		"start": 1230.19,
		"end": 1231.33,
		"text": "あの…"
	},
	{
		"id": 235,
		"start": 1238.17,
		"end": 1239.39,
		"text": "あーのー"
	},
	{
		"id": 236,
		"start": 1249.86,
		"end": 1250.94,
		"text": "ファンです"
	},
	{
		"id": 237,
		"start": 1251.59,
		"end": 1253,
		"text": "分かった　分かった"
	},
	{
		"id": 238,
		"start": 1255.21,
		"end": 1256.08,
		"text": "どうぞ"
	},
	{
		"id": 239,
		"start": 1259.06,
		"end": 1266.9,
		"text": "ワーイ　うれしいなぁー  二枚目のサインだー"
	},
	{
		"id": 240,
		"start": 1268.2,
		"end": 1272.31,
		"text": "ワーイ　ワーイ"
	},
	{
		"id": 241,
		"start": 1273.23,
		"end": 1277.64,
		"text": "私の…バカー"
	},
	{
		"id": 242,
		"start": 1375.76,
		"end": 1376.6,
		"text": "佐倉"
	},
	{
		"id": 243,
		"start": 1378.28,
		"end": 1379.28,
		"text": "野崎くん"
	},
	{
		"id": 244,
		"start": 1379.71,
		"end": 1383.69,
		"text": "今日は御子柴も手伝いにくるから　男だけど　大丈夫か"
	},
	{
		"id": 245,
		"start": 1384.17,
		"end": 1386.82,
		"text": "御子柴　どんな人"
	},
	{
		"id": 246,
		"start": 1387.15,
		"end": 1390.31,
		"text": "健気で傷つきやすい　デリケートなやつで"
	},
	{
		"id": 247,
		"start": 1391.1,
		"end": 1394.13,
		"text": "俺は心の中で「みこりん」と呼んでいる"
	},
	{
		"id": 248,
		"start": 1394.14,
		"end": 1395.53,
		"text": "{\\c&HFFDF7F&\\3c&HF1ECEB&}みこりん…"
	},
	{
		"id": 249,
		"start": 1396.26,
		"end": 1397.81,
		"text": "癒し系なのかな"
	},
	{
		"id": 250,
		"start": 1398.43,
		"end": 1399.75,
		"text": "はよーっス　野崎"
	},
	{
		"id": 251,
		"start": 1412.98,
		"end": 1414.5,
		"text": "なんだ　そのちっこいの"
	},
	{
		"id": 252,
		"start": 1415.61,
		"end": 1416.93,
		"text": "御子柴君です"
	},
	{
		"id": 253,
		"start": 1417.5,
		"end": 1418.5,
		"text": "嘘つき！"
	},
	{
		"id": 254,
		"start": 1422.13,
		"end": 1423.94,
		"text": "夢野先生のコメントが載ってる"
	},
	{
		"id": 255,
		"start": 1423.99,
		"end": 1427.4,
		"text": "先日　学校のお友達にサインをお願いされちゃいました"
	},
	{
		"id": 256,
		"start": 1427.41,
		"end": 1430.51,
		"text": "緊張して手が震えちゃったけど  喜んでくれるといいなぁ"
	},
	{
		"id": 257,
		"start": 1430.62,
		"end": 1432.7,
		"text": "これからもハートにキュンキュンで頑張るよ"
	},
	{
		"id": 258,
		"start": 1433.19,
		"end": 1435.19,
		"text": "次回　ニューヒロインをよろしくね"
	},
	{
		"id": 259,
		"start": 1435.27,
		"end": 1435.92,
		"text": "見てね"
	},
	{
		"id": 260,
		"start": 0,
		"end": 0,
		"text": ""
	},
	{
		"id": 261,
		"start": 0,
		"end": 0,
		"text": ""
	},
	{
		"id": 262,
		"start": 153.5,
