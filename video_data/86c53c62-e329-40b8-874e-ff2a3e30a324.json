	{
		"id": 309,
		"start": 1282.21,
		"end": 1288.11,
		"text": "反対の反対の反対の反対の反対の　フォーチュン"
	},
	{
		"id": 310,
		"start": 1290.46,
		"end": 1296.69,
		"text": "本当の本当の本当の本当の本当の　ハート"
	},
	{
		"id": 311,
		"start": 1298.69,
		"end": 1306.48,
		"text": "いつも君はウラオモテ　何を考えてるんだろ"
	},
	{
		"id": 312,
		"start": 1306.61,
		"end": 1312.7,
		"text": "今日も横顔ずっと見ていた"
	},
	{
		"id": 313,
		"start": 1312.97,
		"end": 1319.27,
		"text": "一人で気にして　切ないだけ"
	},
	{
		"id": 314,
		"start": 1319.38,
		"end": 1327.12,
		"text": "キライの　反対の反対の反対の反対の　そのさらに反対の"
	},
	{
		"id": 315,
		"start": 1327.25,
		"end": 1333.8,
		"text": "気持ちを伝えるの　ってなんだか難しい"
	},
	{
		"id": 316,
		"start": 1333.91,
		"end": 1341.78,
		"text": "君の本当の本当の本当の本当の包み隠せない本音を"
	},
	{
		"id": 317,
		"start": 1341.84,
		"end": 1345.76,
		"text": "聞きたいだけなのに"
	},
	{
		"id": 318,
		"start": 1345.79,
		"end": 1350.55,
		"text": "今日も　また揺らいでる"
	},
	{
		"id": 319,
		"start": 1357.47,
		"end": 1365.05,
		"text": "キライの反対の反対の反対の反対の反対の反対の反対は~"
