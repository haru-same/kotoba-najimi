[
	{
		"id": 1,
		"start": 15.14,
		"end": 17.475,
		"text": "（阿良々木(あららぎ)）<br />戦場ヶ原(せんじょうがはら)の家は豪邸だって"
	},
	{
		"id": 2,
		"start": 17.559,
		"end": 18.768,
		"text": "羽川(はねかわ)は言ってたけど"
	},
	{
		"id": 3,
		"start": 22.439,
		"end": 25.525,
		"text": "（戦場ヶ原）<br />母親が怪しい宗教に<br />はまってしまってね"
	},
	{
		"id": 4,
		"start": 26.192,
		"end": 28.737,
		"text": "財産を全て<br />貢いだどころじゃ済まなくて"
	},
	{
		"id": 5,
		"start": 28.862,
		"end": 31.197,
		"text": "多額の借金まで背負ってしまって"
	},
	{
		"id": 6,
		"start": 32.782,
		"end": 36.119,
		"text": "結局 去年の暮れに<br />協議離婚が成立して"
	},
	{
		"id": 7,
		"start": 36.536,
		"end": 39.039,
		"text": "私は お父さんに引き取られ"
	},
	{
		"id": 8,
		"start": 39.205,
		"end": 40.957,
		"text": "ここで２人で暮らしているわ"
	},
	{
		"id": 9,
		"start": 48.298,
		"end": 54.304,
		"text": "♪〜"
	},
	{
		"id": 10,
		"start": 130.213,
		"end": 136.219,
		"text": "〜♪"
	},
	{
		"id": 11,
		"start": 157.115,
		"end": 158.7,
		"text": "（戦場ヶ原）助けてくれるんですか"
	},
	{
		"id": 12,
		"start": 161.119,
		"end": 163.496,
		"text": "（忍野(おしの)）助けない　力は貸すけど"
	},
	{
		"id": 13,
		"start": 165.498,
		"end": 168.209,
		"text": "そうだね　一旦 家に帰りなさい"
	},
	{
		"id": 14,
		"start": 168.459,
		"end": 170.336,
		"text": "それで 身体を冷水で清めて"
	},
	{
		"id": 15,
		"start": 170.42,
		"end": 172.714,
		"text": "清潔な服に着替えて来てくれる？"
	},
	{
		"id": 16,
		"start": 173.339,
		"end": 177.635,
		"text": "夜中の零時ごろ もういっぺん<br />ここに集合ってことで いいかな？"
	},
	{
		"id": 17,
		"start": 178.052,
		"end": 179.053,
		"text": "（戦場ヶ原）いいですけれど"
	},
	{
		"id": 18,
		"start": 183.808,
		"end": 185.226,
		"text": "（戦場ヶ原）お礼は…<br />（忍野）うん？"
	},
	{
		"id": 19,
		"start": 186.769,
		"end": 187.604,
		"text": "（忍野）は？"
	},
	{
		"id": 20,
		"start": 188.146,
		"end": 189.689,
		"text": "（戦場ヶ原）とぼけないでください"
	},
	{
		"id": 21,
		"start": 189.939,
		"end": 192.734,
		"text": "ボランティアで助けてくれる<br />というわけでは ないんでしょう？"
	},
	{
		"id": 22,
		"start": 193.735,
		"end": 195.111,
		"text": "（忍野）んん… ん？"
	},
	{
		"id": 23,
		"start": 195.236,
		"end": 196.112,
		"text": "あ？"
	},
	{
		"id": 24,
		"start": 196.529,
		"end": 200.033,
		"text": "まっ そのほうで<br />お嬢ちゃんの気が楽だっていうなら"
	},
	{
		"id": 25,
		"start": 200.116,
		"end": 201.2,
		"text": "もらっておくことにしよっか"
	},
	{
		"id": 26,
		"start": 202.66,
		"end": 205.872,
		"text": "じゃ そうだね 10万円で"
	},
	{
		"id": 27,
		"start": 206.623,
		"end": 207.498,
		"text": "10万円…"
	},
	{
		"id": 28,
		"start": 207.999,
		"end": 210.335,
		"text": "僕のときとは 随分 対応が違うな"
	},
	{
		"id": 29,
		"start": 210.835,
		"end": 211.836,
		"text": "そうだっけ？"
	},
	{
		"id": 30,
		"start": 211.92,
		"end": 214.881,
		"text": "委員長ちゃんのときも<br />確か10万円だったと思うけれど"
	},
	{
		"id": 31,
		"start": 215.924,
		"end": 218.009,
		"text": "僕のときは<br />500万だったって言ってんだよ！"
	},
	{
		"id": 32,
		"start": 218.551,
		"end": 220.845,
		"text": "（忍野）<br />吸血鬼だもん しかたないよ"
	},
	{
		"id": 33,
		"start": 220.929,
		"end": 223.514,
		"text": "（阿良々木）<br />何でもかんでも安易に<br />吸血鬼のせいにするな！"
	},
	{
		"id": 34,
		"start": 224.182,
		"end": 225.016,
		"text": "払える？"
	},
	{
		"id": 35,
		"start": 226.184,
		"end": 227.018,
		"text": "もちろん"
	},
	{
		"id": 36,
		"start": 228.478,
		"end": 229.854,
		"text": "どんなことをしてでも"
	},
	{
		"id": 37,
		"start": 232.523,
		"end": 233.524,
		"text": "もちろん"
	},
	{
		"id": 38,
		"start": 238.863,
		"end": 241.783,
		"text": "（阿良々木）<br />う〜ん　10万円か…"
	},
	{
		"id": 39,
		"start": 244.035,
		"end": 245.87,
		"text": "（戦場ヶ原）<br />シャワー 済ませたわよ"
	},
	{
		"id": 40,
		"start": 248.289,
		"end": 249.123,
		"text": "うわっ"
	},
	{
		"id": 41,
		"start": 249.415,
		"end": 252.502,
		"text": "そこを どいてちょうだい<br />服が取り出せないわ"
	},
	{
		"id": 42,
		"start": 252.877,
		"end": 254.42,
		"text": "服を着ろ 服を！"
	},
	{
		"id": 43,
		"start": 254.921,
		"end": 256.923,
		"text": "だから 今から着るのよ"
	},
	{
		"id": 44,
		"start": 257.173,
		"end": 259.801,
		"text": "（阿良々木）<br />最初から着てろって言ってんだ！"
	},
	{
		"id": 45,
		"start": 260.176,
		"end": 262.22,
		"text": "（戦場ヶ原）<br />持って入るのを忘れていたのよ"
	},
	{
		"id": 46,
		"start": 262.303,
		"end": 264.472,
		"text": "（阿良々木）<br />だったら<br />タオルで隠すとかしやがれ！"
	},
	{
		"id": 47,
		"start": 264.555,
		"end": 266.557,
		"text": "（戦場ヶ原）<br />嫌よ そんな貧乏くさいマネ"
	},
	{
		"id": 48,
		"start": 272.188,
		"end": 273.022,
		"text": "（ゴクン）"
	},
	{
		"id": 49,
		"start": 275.191,
		"end": 277.777,
		"text": "（阿良々木）<br />女性の全裸を初めて見てしまった"
	},
	{
		"id": 50,
		"start": 278.319,
		"end": 279.404,
		"text": "（戦場ヶ原）清潔な服ね"
	},
	{
		"id": 51,
		"start": 280.238,
		"end": 281.99,
		"text": "白い服のほうが いいと思う？"
	},
	{
		"id": 52,
		"start": 282.073,
		"end": 282.907,
		"text": "（阿良々木）知らねえよ…"
	},
	{
		"id": 53,
		"start": 285.743,
		"end": 286.828,
		"text": "（戦場ヶ原）ショーツとブラは"
	},
	{
		"id": 54,
		"start": 289.163,
		"end": 290.832,
		"text": "柄物しか持っていないの"
	},
	{
		"id": 55,
		"start": 291.249,
		"end": 292.208,
		"text": "知らねえよ！"
	},
	{
		"id": 56,
		"start": 294.335,
		"end": 296.129,
		"text": "（戦場ヶ原）<br />阿良々木くん まさか あなた"
	},
	{
		"id": 57,
		"start": 296.212,
		"end": 299.215,
		"text": "私のヌードを見て<br />欲情したのではないでしょうね"
	},
	{
		"id": 58,
		"start": 299.799,
		"end": 303.011,
		"text": "（阿良々木）<br />仮に そうだったとしても<br />僕の責任じゃない！"
	},
	{
		"id": 59,
		"start": 303.094,
		"end": 305.471,
		"text": "（戦場ヶ原）<br />もういいわよ　こっちを向いても"
	},
	{
		"id": 60,
		"start": 305.555,
		"end": 307.223,
		"text": "（阿良々木）そうかよ ったく…"
	},
	{
		"id": 61,
		"start": 307.348,
		"end": 308.182,
		"text": "いっ"
	},
	{
		"id": 62,
		"start": 311.311,
		"end": 313.187,
		"text": "（阿良々木）<br />何が目的なんだ お前は！"
	},
	{
		"id": 63,
		"start": 313.521,
		"end": 314.397,
		"text": "（戦場ヶ原）何よ"
	},
	{
		"id": 64,
		"start": 314.48,
		"end": 317.317,
		"text": "今日のお礼のつもりで<br />大サービスしてあげてるんだから"
	},
	{
		"id": 65,
		"start": 317.4,
		"end": 318.693,
		"text": "ちょっとは喜びなさいよ"
	},
	{
		"id": 66,
		"start": 319.11,
		"end": 319.944,
		"text": "うっ"
	},
	{
		"id": 67,
		"start": 320.695,
		"end": 322.28,
		"text": "ちょっとは喜びなさいよ！"
	},
	{
		"id": 68,
		"start": 322.739,
		"end": 323.906,
		"text": "（阿良々木）<br />逆切れされた！"
	},
	{
		"id": 69,
		"start": 324.282,
		"end": 326.784,
		"text": "（戦場ヶ原）<br />感想くらい言うのが<br />礼儀でしょう"
	},
	{
		"id": 70,
		"start": 326.868,
		"end": 328.786,
		"text": "（阿良々木）か… 感想って"
	},
	{
		"id": 71,
		"start": 331.122,
		"end": 334,
		"text": "い… いい体してるね とか？"
	},
	{
		"id": 72,
		"start": 335.835,
		"end": 336.919,
		"text": "（戦場ヶ原）最低"
	},
	{
		"id": 73,
		"start": 337.003,
		"end": 340.089,
		"text": "そんなことだから<br />あなたは 一生 童貞なのよ"
	},
	{
		"id": 74,
		"start": 340.173,
		"end": 343.301,
		"text": "（阿良々木）<br />一生？<br />お前は未来から来た人なのか？"
	},
	{
		"id": 75,
		"start": 343.384,
		"end": 346.387,
		"text": "（戦場ヶ原）<br />唾を飛ばさないでくれる？<br />童貞がうつるわ"
	},
	{
		"id": 76,
		"start": 347.055,
		"end": 349.015,
		"text": "女に童貞がうつるか！"
	},
	{
		"id": 77,
		"start": 350.391,
		"end": 352.143,
		"text": "ていうか ちょっと待て"
	},
	{
		"id": 78,
		"start": 352.351,
		"end": 355.146,
		"text": "僕が童貞であることを前提に<br />話が進んでいるぞ"
	},
	{
		"id": 79,
		"start": 355.563,
		"end": 358.024,
		"text": "うん 確かに<br />偏見でものを言ったわね"
	},
	{
		"id": 80,
		"start": 358.357,
		"end": 360.359,
		"text": "ハア…<br />分かってくれればいいんだ"
	},
	{
		"id": 81,
		"start": 360.735,
		"end": 363.613,
		"text": "唾を飛ばさないで<br />素人童貞がうつるわ"
	},
	{
		"id": 82,
		"start": 363.988,
		"end": 366.866,
		"text": "認めましょう　僕は童貞野郎です！"
	},
	{
		"id": 83,
		"start": 369.077,
		"end": 370.745,
		"text": "（戦場ヶ原）心配しなくとも"
	},
	{
		"id": 84,
		"start": 370.828,
		"end": 373.289,
		"text": "羽川さんには<br />内緒にしておいてあげるのに"
	},
	{
		"id": 85,
		"start": 373.79,
		"end": 374.791,
		"text": "羽川って？"
	},
	{
		"id": 86,
		"start": 375.333,
		"end": 378.419,
		"text": "彼女 阿良々木くんの<br />片恋(かたこい)相手じゃないの？"
	},
	{
		"id": 87,
		"start": 378.503,
		"end": 379.67,
		"text": "よく話しているでしょう？"
	},
	{
		"id": 88,
		"start": 380.004,
		"end": 381.339,
		"text": "（阿良々木）それは違う"
	},
	{
		"id": 89,
		"start": 381.422,
		"end": 384.425,
		"text": "あれは単純に<br />羽川の面倒見がいいだけだ"
	},
	{
		"id": 90,
		"start": 384.509,
		"end": 386.928,
		"text": "あいつは いちばんダメなヤツが<br />いちばん かわいそうで"
	},
	{
		"id": 91,
		"start": 387.011,
		"end": 388.971,
		"text": "不当に損をしているって"
	},
	{
		"id": 92,
		"start": 389.055,
		"end": 390.932,
		"text": "そんな愉快な勘違いを<br />しているんだ"
	},
	{
		"id": 93,
		"start": 391.39,
		"end": 393.976,
		"text": "それは本当に愉快な勘違いね"
	},
	{
		"id": 94,
		"start": 394.102,
		"end": 396.687,
		"text": "いちばんダメなヤツは<br />いちばん愚かなだけなのに"
	},
	{
		"id": 95,
		"start": 397.146,
		"end": 399.065,
		"text": "（阿良々木）<br />いや 僕は<br />そこまでは言ってません"
	},
	{
		"id": 96,
		"start": 399.315,
		"end": 400.316,
		"text": "（戦場ヶ原）顔に書いてあるわ"
	},
	{
		"id": 97,
		"start": 400.441,
		"end": 401.4,
		"text": "書いてねえよ"
	},
	{
		"id": 98,
		"start": 401.692,
		"end": 403.986,
		"text": "そう言うと思って<br />さっき書いておいたわ"
	},
	{
		"id": 99,
		"start": 404.32,
		"end": 405.988,
		"text": "（阿良々木）<br />そんな手回しがあり得るか！"
	},
	{
		"id": 100,
		"start": 409.951,
		"end": 411.869,
		"text": "（戦場ヶ原）<br />忍野さんのお世話になったのね？"
	},
	{
		"id": 101,
		"start": 413.079,
		"end": 414.455,
		"text": "羽川さんも"
	},
	{
		"id": 102,
		"start": 414.622,
		"end": 416.541,
		"text": "（阿良々木）うん まあな"
	},
	{
		"id": 103,
		"start": 416.958,
		"end": 417.792,
		"text": "（戦場ヶ原）ふうん"
	},
	{
		"id": 104,
		"start": 417.875,
		"end": 420.461,
		"text": "（阿良々木）<br />だから 一応<br />信頼していいとは思うぜ"
	},
	{
		"id": 105,
		"start": 420.545,
		"end": 423.506,
		"text": "僕一人の証言じゃなく<br />羽川もそうだっていうんだから"
	},
	{
		"id": 106,
		"start": 424.006,
		"end": 424.841,
		"text": "（戦場ヶ原）そう…"
	},
	{
		"id": 107,
		"start": 425.508,
		"end": 427.426,
		"text": "でもね 阿良々木くん"
	},
	{
		"id": 108,
		"start": 428.469,
		"end": 430.471,
		"text": "彼のことを おいそれと信じるには"
	},
	{
		"id": 109,
		"start": 431.139,
		"end": 435.726,
		"text": "私は 今まで何度も何度も<br />だまされ続けているわ"
	},
	{
		"id": 110,
		"start": 436.31,
		"end": 438.563,
		"text": "だから そんな楽天的なふうには"
	},
	{
		"id": 111,
		"start": 438.646,
		"end": 440.731,
		"text": "どうしたって ちっとも思えないの"
	},
	{
		"id": 112,
		"start": 440.898,
		"end": 441.732,
		"text": "あっ！"
	},
	{
		"id": 113,
		"start": 441.858,
		"end": 442.692,
		"text": "ん？"
	},
	{
		"id": 114,
		"start": 446.571,
		"end": 448.322,
		"text": "（阿良々木）<br />せっかく着たのに<br />なんで脱ぐんだよ"
	},
	{
		"id": 115,
		"start": 449.031,
		"end": 450.867,
		"text": "（戦場ヶ原）<br />髪を乾かすのを忘れていたわ"
	},
	{
		"id": 116,
		"start": 451.451,
		"end": 454.12,
		"text": "（阿良々木）<br />お前 ひょっとして<br />ただのバカなんじゃないか？"
	},
	{
		"id": 117,
		"start": 454.203,
		"end": 456.456,
		"text": "（戦場ヶ原）<br />失礼なことを<br />言わないでくれるかしら？"
	},
	{
		"id": 118,
		"start": 456.539,
		"end": 458.499,
		"text": "私が傷ついたら大変じゃないの"
	},
	{
		"id": 119,
		"start": 463.838,
		"end": 466.257,
		"text": "（阿良々木）<br />さっき 楽天的って言ってたな"
	},
	{
		"id": 120,
		"start": 466.34,
		"end": 467.717,
		"text": "（戦場ヶ原）そうじゃなくて？"
	},
	{
		"id": 121,
		"start": 467.8,
		"end": 468.968,
		"text": "（阿良々木）かもしんね"
	},
	{
		"id": 122,
		"start": 469.051,
		"end": 472.263,
		"text": "でも いいんじゃねえの？<br />別に楽天的でも"
	},
	{
		"id": 123,
		"start": 472.722,
		"end": 474.432,
		"text": "悪いことをしてるわけじゃないし"
	},
	{
		"id": 124,
		"start": 474.515,
		"end": 475.975,
		"text": "ズルしているわけでも<br />ないんだから"
	},
	{
		"id": 125,
		"start": 476.809,
		"end": 479.729,
		"text": "悪いことを<br />しているわけじゃない… か"
	},
	{
		"id": 126,
		"start": 480.229,
		"end": 482.356,
		"text": "（阿良々木）だろ？<br />（戦場ヶ原）まあ そうね"
	},
	{
		"id": 127,
		"start": 483.399,
		"end": 484.233,
		"text": "でも…"
	},
	{
		"id": 128,
		"start": 485.109,
		"end": 486.235,
		"text": "ズルは しているかも"
	},
	{
		"id": 129,
		"start": 486.611,
		"end": 487.987,
		"text": "（阿良々木）え？<br />（戦場ヶ原）何でもないわ"
	},
	{
		"id": 130,
		"start": 492.283,
		"end": 494.869,
		"text": "（戦場ヶ原）<br />ねえ 阿良々木くん<br />１つ聞いていい？"
	},
	{
		"id": 131,
		"start": 495.578,
		"end": 498.08,
		"text": "月の模様みたいって<br />どういうこと？"
	},
	{
		"id": 132,
		"start": 498.164,
		"end": 499.749,
		"text": "（阿良々木）え？ 何の話だ？"
	},
	{
		"id": 133,
		"start": 499.832,
		"end": 502.126,
		"text": "（戦場ヶ原）<br />言っていたじゃないの 忍野さんに"
	},
	{
		"id": 134,
		"start": 502.502,
		"end": 504.754,
		"text": "ほら 月の模様って 日本からだと"
	},
	{
		"id": 135,
		"start": 505.338,
		"end": 507.256,
		"text": "うさぎが 餅をついているように<br />見えるけれど"
	},
	{
		"id": 136,
		"start": 507.882,
		"end": 509.634,
		"text": "海外からだと 蟹(かに)だったり"
	},
	{
		"id": 137,
		"start": 509.842,
		"end": 512.011,
		"text": "美人の横顔だったりする<br />っていうから"
	},
	{
		"id": 138,
		"start": 512.428,
		"end": 514.096,
		"text": "（戦場ヶ原）へえ そうなんだ"
	},
	{
		"id": 139,
		"start": 514.18,
		"end": 516.766,
		"text": "そんなくだらないことを<br />よく知っているわね"
	},
	{
		"id": 140,
		"start": 516.891,
		"end": 519.977,
		"text": "生まれて初めて<br />あなたに感心したわ"
	},
	{
		"id": 141,
		"start": 520.061,
		"end": 520.978,
		"text": "（阿良々木）なあに"
	},
	{
		"id": 142,
		"start": 521.062,
		"end": 524.232,
		"text": "僕は 天文学や宇宙科学には<br />詳しいんだよ"
	},
	{
		"id": 143,
		"start": 524.44,
		"end": 527.652,
		"text": "（戦場ヶ原）<br />いいのよ<br />私の前では格好つけなくとも"
	},
	{
		"id": 144,
		"start": 527.735,
		"end": 530.404,
		"text": "どうせ それ以外は<br />何も知らないんでしょう？"
	},
	{
		"id": 145,
		"start": 530.53,
		"end": 532.323,
		"text": "言葉の暴力って知ってるか"
	},
	{
		"id": 146,
		"start": 532.698,
		"end": 535.368,
		"text": "（戦場ヶ原）<br />なら 言葉の警察を呼びなさいよ"
	},
	{
		"id": 147,
		"start": 536.077,
		"end": 537.37,
		"text": "お前…"
	},
	{
		"id": 148,
		"start": 537.453,
		"end": 540.915,
		"text": "実は 自慢の肉体を<br />僕に見せびらかしたいだけなのか？"
	},
	{
		"id": 149,
		"start": 540.998,
		"end": 543.918,
		"text": "（戦場ヶ原）<br />自慢の肉体だなんて<br />そんなに うぬぼれていないわ"
	},
	{
		"id": 150,
		"start": 544.46,
		"end": 548.047,
		"text": "服を着るのは得意じゃないの<br />重たいのよ"
	},
	{
		"id": 151,
		"start": 548.339,
		"end": 549.173,
		"text": "あ…"
	},
	{
		"id": 152,
		"start": 549.757,
		"end": 552.76,
		"text": "（戦場ヶ原）<br />けれど 意外と学があるのね<br />阿良々木くん"
	},
	{
		"id": 153,
		"start": 552.843,
		"end": 554.095,
		"text": "びっくりしたわ"
	},
	{
		"id": 154,
		"start": 554.262,
		"end": 557.974,
		"text": "ひょっとしたら 頭の中に脳みそが<br />入っているのかもしれないわね"
	},
	{
		"id": 155,
		"start": 558.057,
		"end": 558.891,
		"text": "（阿良々木）当たり前だろ"
	},
	{
		"id": 156,
		"start": 559.267,
		"end": 560.226,
		"text": "（戦場ヶ原）当たり前って…"
	},
	{
		"id": 157,
		"start": 560.81,
		"end": 562.645,
		"text": "あなたのような生物(せいぶつ)の頭蓋骨に"
	},
	{
		"id": 158,
		"start": 563.229,
		"end": 565.231,
		"text": "脳みそが入っているというのは"
	},
	{
		"id": 159,
		"start": 565.314,
		"end": 568.359,
		"text": "それはそれは<br />もう奇跡のような出来事なのよ？"
	},
	{
		"id": 160,
		"start": 568.734,
		"end": 571.737,
		"text": "お前 僕の頭がすごく悪い<br />と思っているだろう"
	},
	{
		"id": 161,
		"start": 572.071,
		"end": 573.906,
		"text": "あっ！ なぜ気付いたの？"
	},
	{
		"id": 162,
		"start": 574.865,
		"end": 576.158,
		"text": "真顔で驚かれた！"
	},
	{
		"id": 163,
		"start": 578.327,
		"end": 580.329,
		"text": "私のせいで 阿良々木くんが"
	},
	{
		"id": 164,
		"start": 580.413,
		"end": 583.541,
		"text": "自分の頭のお粗末さ加減に<br />気付いてしまった"
	},
	{
		"id": 165,
		"start": 583.624,
		"end": 584.667,
		"text": "責任を感じるわ"
	},
	{
		"id": 166,
		"start": 585.001,
		"end": 586.586,
		"text": "おい ちょっと待て"
	},
	{
		"id": 167,
		"start": 586.669,
		"end": 589.63,
		"text": "僕は そんな深刻なレベルの<br />頭の悪さなのか？"
	},
	{
		"id": 168,
		"start": 589.964,
		"end": 592.383,
		"text": "偏差値チェック 私 74"
	},
	{
		"id": 169,
		"start": 592.8,
		"end": 594.635,
		"text": "くっ… 僕 46"
	},
	{
		"id": 170,
		"start": 594.885,
		"end": 596.345,
		"text": "四捨五入すればゼロね"
	},
	{
		"id": 171,
		"start": 596.887,
		"end": 598.764,
		"text": "はあ？ ウソつけ ６だから…"
	},
	{
		"id": 172,
		"start": 599.807,
		"end": 602.977,
		"text": "（阿良々木）<br />あっ！ お前 さては十の位を！"
	},
	{
		"id": 173,
		"start": 603.06,
		"end": 605.438,
		"text": "僕の偏差値に何てことするんだ！"
	},
	{
		"id": 174,
		"start": 605.521,
		"end": 608.566,
		"text": "（戦場ヶ原）<br />100… 差をつけないと<br />勝った気がしないのよ"
	},
	{
		"id": 175,
		"start": 608.649,
		"end": 610.443,
		"text": "（阿良々木）<br />自分の数値も十の位を…"
	},
	{
		"id": 176,
		"start": 611.736,
		"end": 614.03,
		"text": "うん　決めたわ"
	},
	{
		"id": 177,
		"start": 615.781,
		"end": 617.7,
		"text": "（戦場ヶ原）<br />もしも 全てがうまくいったら"
	},
	{
		"id": 178,
		"start": 617.783,
		"end": 619.869,
		"text": "北海道へ蟹を食べにいきましょう"
	},
	{
		"id": 179,
		"start": 619.952,
		"end": 622.121,
		"text": "（阿良々木）<br />まあ いいんじゃないのか？"
	},
	{
		"id": 180,
		"start": 622.371,
		"end": 623.456,
		"text": "（戦場ヶ原）あなたも行くのよ"
	},
	{
		"id": 181,
		"start": 623.623,
		"end": 624.457,
		"text": "（阿良々木）なんで？"
	},
	{
		"id": 182,
		"start": 624.957,
		"end": 626.709,
		"text": "（戦場ヶ原）<br />あら 知らなかったの？"
	},
	{
		"id": 183,
		"start": 627.877,
		"end": 630.671,
		"text": "蟹って とってもおししいのよ"
	},
	{
		"id": 184,
		"start": 653.194,
		"end": 654.028,
		"text": "あっ"
	},
	{
		"id": 185,
		"start": 657.448,
		"end": 658.366,
		"text": "（戦場ヶ原）え…"
	},
	{
		"id": 186,
		"start": 661.035,
		"end": 664.08,
		"text": "（戦場ヶ原）<br />忍野さんって<br />神職の方だったんですか？"
	},
	{
		"id": 187,
		"start": 664.163,
		"end": 665.665,
		"text": "（忍野）いや 違うよ"
	},
	{
		"id": 188,
		"start": 666.082,
		"end": 669.585,
		"text": "大学の学科は そうなんだけれど<br />神社に就職はしていない"
	},
	{
		"id": 189,
		"start": 669.919,
		"end": 671.837,
		"text": "いろいろ思うところがあってね"
	},
	{
		"id": 190,
		"start": 672.38,
		"end": 676.258,
		"text": "うん　お嬢ちゃん<br />いい感じに清廉になっているよ"
	},
	{
		"id": 191,
		"start": 676.342,
		"end": 677.468,
		"text": "見事なもんだね"
	},
	{
		"id": 192,
		"start": 678.219,
		"end": 681.305,
		"text": "一応 確認しておくけれど<br />お化粧は していない？"
	},
	{
		"id": 193,
		"start": 681.389,
		"end": 684.1,
		"text": "（戦場ヶ原）<br />しないほうがいいかと思って<br />していません"
	},
	{
		"id": 194,
		"start": 684.183,
		"end": 685.017,
		"text": "（忍野）そう"
	},
	{
		"id": 195,
		"start": 685.101,
		"end": 687.144,
		"text": "まっ とりあえず正しい判断だ"
	},
	{
		"id": 196,
		"start": 687.478,
		"end": 689.48,
		"text": "阿良々木くんも<br />シャワー 浴びてきたかい？"
	},
	{
		"id": 197,
		"start": 689.688,
		"end": 692.441,
		"text": "（阿良々木）<br />ああ 戦場ヶ原に借りた<br />問題ないよ"
	},
	{
		"id": 198,
		"start": 693.484,
		"end": 696.195,
		"text": "ふうん<br />君は代わり映えしないねえ"
	},
	{
		"id": 199,
		"start": 696.278,
		"end": 697.905,
		"text": "余計なこと言うな"
	},
	{
		"id": 200,
		"start": 701.867,
		"end": 703.035,
		"text": "（阿良々木）しかし 忍野"
	},
	{
		"id": 201,
		"start": 703.119,
		"end": 706.163,
		"text": "蟹だか何だかって そんな簡単に<br />退治できるもんなのか？"
	},
	{
		"id": 202,
		"start": 707.581,
		"end": 709.792,
		"text": "（忍野）<br />考え方が乱暴だなあ<br />阿良々木くんは"
	},
	{
		"id": 203,
		"start": 710.459,
		"end": 712.545,
		"text": "何か いいことでも あったのかい？"
	},
	{
		"id": 204,
		"start": 713.003,
		"end": 715.756,
		"text": "忍(しのぶ)ちゃんや 色ボケ猫は<br />悪意と敵意を持って"
	},
	{
		"id": 205,
		"start": 715.84,
		"end": 718.801,
		"text": "阿良々木くんと委員長ちゃんを<br />襲ったわけだけれど"
	},
	{
		"id": 206,
		"start": 718.968,
		"end": 721.262,
		"text": "今回の蟹は そうじゃないんだから"
	},
	{
		"id": 207,
		"start": 721.345,
		"end": 722.179,
		"text": "（阿良々木）そうじゃないって？"
	},
	{
		"id": 208,
		"start": 723.264,
		"end": 726.142,
		"text": "（忍野）<br />言ったろ？ 相手は神様なんだよ"
	},
	{
		"id": 209,
		"start": 726.225,
		"end": 729.645,
		"text": "だから 退治するのではなく<br />今から僕たちはね"
	},
	{
		"id": 210,
		"start": 729.937,
		"end": 731.439,
		"text": "神様にお願いするんだよ"
	},
	{
		"id": 211,
		"start": 731.939,
		"end": 733.441,
		"text": "下手(したて)に出てね"
	},
	{
		"id": 212,
		"start": 733.524,
		"end": 736.777,
		"text": "お願いしたら それで<br />返してもらえるもんなのか？"
	},
	{
		"id": 213,
		"start": 736.861,
		"end": 737.695,
		"text": "戦場ヶ原の"
	},
	{
		"id": 214,
		"start": 738.737,
		"end": 740.406,
		"text": "重み 体重は"
	},
	{
		"id": 215,
		"start": 741.031,
		"end": 741.907,
		"text": "（忍野）多分ね"
	},
	{
		"id": 216,
		"start": 742.45,
		"end": 746.912,
		"text": "切実な人間の頼みを断るほど<br />彼らは頑(かたく)なじゃないさ"
	},
	{
		"id": 217,
		"start": 746.996,
		"end": 750.708,
		"text": "それに神様っていうのは<br />結構 大ざっぱな連中なんだ"
	},
	{
		"id": 218,
		"start": 751.083,
		"end": 752.376,
		"text": "（阿良々木）大ざっぱって？"
	},
	{
		"id": 219,
		"start": 752.918,
		"end": 755.921,
		"text": "（忍野）<br />人間 個々人のことなんて<br />どうでもいいんだよ"
	},
	{
		"id": 220,
		"start": 756.005,
		"end": 757.298,
		"text": "実際 神様の前じゃ"
	},
	{
		"id": 221,
		"start": 757.381,
		"end": 761.01,
		"text": "僕ら３人 年齢も性別も重みも<br />区別なんかつかないよ"
	},
	{
		"id": 222,
		"start": 761.51,
		"end": 764.305,
		"text": "（戦場ヶ原）ねえ あの蟹は"
	},
	{
		"id": 223,
		"start": 764.388,
		"end": 766.14,
		"text": "今も 私のそばにいますか？"
	},
	{
		"id": 224,
		"start": 767.6,
		"end": 771.228,
		"text": "そう そこにいるし どこにでもいる"
	},
	{
		"id": 225,
		"start": 771.896,
		"end": 774.523,
		"text": "ただし ここに<br />降りてきてもらうためには"
	},
	{
		"id": 226,
		"start": 774.69,
		"end": 776.567,
		"text": "（忍野）手順が必要だけどね<br />（戦場ヶ原）あ…"
	},
	{
		"id": 227,
		"start": 786.327,
		"end": 788.245,
		"text": "（忍野）２人とも 目を伏せて"
	},
	{
		"id": 228,
		"start": 788.329,
		"end": 790.039,
		"text": "（忍野）頭を低くしてくれる？<br />（２人）え？"
	},
	{
		"id": 229,
		"start": 790.581,
		"end": 792.541,
		"text": "神前だよ　ここはもう"
	},
	{
		"id": 230,
		"start": 806.639,
		"end": 808.432,
		"text": "（戦場ヶ原）え… 何ですか？"
	},
	{
		"id": 231,
		"start": 808.557,
		"end": 809.892,
		"text": "（忍野）お酒を飲むと"
	},
	{
		"id": 232,
		"start": 809.975,
		"end": 813.771,
		"text": "神様との距離を<br />縮めることができるそうだよ"
	},
	{
		"id": 233,
		"start": 813.979,
		"end": 815.272,
		"text": "（戦場ヶ原）未成年です"
	},
	{
		"id": 234,
		"start": 815.356,
		"end": 818.567,
		"text": "（忍野）<br />酔うほどの量は飲まなくていいさ<br />ちっとだけ"
	},
	{
		"id": 235,
		"start": 828.327,
		"end": 829.245,
		"text": "さて…"
	},
	{
		"id": 236,
		"start": 829.328,
		"end": 831.705,
		"text": "じゃあ リラックスするところから<br />始めよう"
	},
	{
		"id": 237,
		"start": 833.415,
		"end": 837.711,
		"text": "ここは自分の場所だ<br />君がいて当たり前の場所"
	},
	{
		"id": 238,
		"start": 838.879,
		"end": 839.713,
		"text": "はい…"
	},
	{
		"id": 239,
		"start": 841.048,
		"end": 845.135,
		"text": "頭を下げたまま<br />目を閉じて 数を数えよう"
	},
	{
		"id": 240,
		"start": 850.307,
		"end": 853.185,
		"text": "（忍野）一つ 二つ"
	},
	{
		"id": 241,
		"start": 853.727,
		"end": 856.438,
		"text": "三つ 四つ"
	},
	{
		"id": 242,
		"start": 856.981,
		"end": 859.692,
		"text": "五つ 六つ"
	},
	{
		"id": 243,
		"start": 860.109,
		"end": 861.36,
		"text": "七つ"
	},
	{
		"id": 244,
		"start": 861.944,
		"end": 862.987,
		"text": "八つ"
	},
	{
		"id": 245,
		"start": 863.654,
		"end": 864.488,
		"text": "九つ"
	},
	{
		"id": 246,
		"start": 867.992,
		"end": 868.951,
		"text": "落ち着いた？"
	},
	{
		"id": 247,
		"start": 869.994,
		"end": 870.828,
		"text": "（戦場ヶ原）はい"
	},
	{
		"id": 248,
		"start": 870.911,
		"end": 873.622,
		"text": "（忍野）<br />そう じゃあ 質問に答えてみよう"
	},
	{
		"id": 249,
		"start": 875.708,
		"end": 877.626,
		"text": "お嬢ちゃん<br />君の名前は？"
	},
	{
		"id": 250,
		"start": 878.294,
		"end": 879.753,
		"text": "（戦場ヶ原）<br />戦場ヶ原ひたぎ"
	},
	{
		"id": 251,
		"start": 880.379,
		"end": 881.63,
		"text": "（忍野）<br />通っている学校は？"
	},
	{
		"id": 252,
		"start": 882.464,
		"end": 884.133,
		"text": "（戦場ヶ原）<br />私立直江津(なおえつ)高校"
	},
	{
		"id": 253,
		"start": 884.55,
		"end": 885.384,
		"text": "（忍野）誕生日は？"
	},
	{
		"id": 254,
		"start": 886.176,
		"end": 887.261,
		"text": "（戦場ヶ原）７月７日"
	},
	{
		"id": 255,
		"start": 888.012,
		"end": 889.513,
		"text": "（忍野）<br />いちばん好きな<br />小説家は？"
	},
	{
		"id": 256,
		"start": 890.389,
		"end": 891.39,
		"text": "（戦場ヶ原）夢野久作(ゆめのきゅうさく)"
	},
	{
		"id": 257,
		"start": 892.057,
		"end": 894.393,
		"text": "（忍野）<br />子供のころの失敗談を<br />聞かせてくれる？"
	},
	{
		"id": 258,
		"start": 894.977,
		"end": 896.02,
		"text": "言いたくありません"
	},
	{
		"id": 259,
		"start": 896.729,
		"end": 897.897,
		"text": "（忍野）<br />好きな古典音楽は？"
	},
	{
		"id": 260,
		"start": 898.647,
		"end": 900.399,
		"text": "（戦場ヶ原）<br />音楽は<br />あまり たしなみません"
	},
	{
		"id": 261,
		"start": 900.774,
		"end": 903.277,
		"text": "（忍野）<br />小学校を卒業するとき<br />どう思った？"
	},
	{
		"id": 262,
		"start": 903.903,
		"end": 907.239,
		"text": "（戦場ヶ原）<br />単純に中学校に<br />移るだけだと思いました"
	},
	{
		"id": 263,
		"start": 907.448,
		"end": 909.533,
		"text": "公立から公立へ<br />行くだけだったから"
	},
	{
		"id": 264,
		"start": 909.95,
		"end": 911.827,
		"text": "（忍野）<br />初恋の男の子は<br />どんな子だった？"
	},
	{
		"id": 265,
		"start": 912.453,
		"end": 913.329,
		"text": "（戦場ヶ原）<br />言いたくありません"
	},
	{
		"id": 266,
		"start": 914.038,
		"end": 915.331,
		"text": "（忍野）<br />今までの人生で"
	},
	{
		"id": 267,
		"start": 915.414,
		"end": 916.832,
		"text": "いちばん<br />つらかった思い出は？"
	},
	{
		"id": 268,
		"start": 919.752,
		"end": 920.961,
		"text": "（忍野）どうしたの？"
	},
	{
		"id": 269,
		"start": 921.337,
		"end": 924.715,
		"text": "いちばん つらかった思い出<br />記憶について聞いているんだ"
	},
	{
		"id": 270,
		"start": 926.717,
		"end": 929.845,
		"text": "（戦場ヶ原）<br />お… お… お母さんが…"
	},
	{
		"id": 271,
		"start": 930.846,
		"end": 931.722,
		"text": "（忍野）お母さんが"
	},
	{
		"id": 272,
		"start": 932.89,
		"end": 935.225,
		"text": "悪い宗教に はまったこと"
	},
	{
		"id": 273,
		"start": 936.31,
		"end": 937.227,
		"text": "（忍野）それだけかい？"
	},
	{
		"id": 274,
		"start": 938.187,
		"end": 939.355,
		"text": "それだけって…"
	},
	{
		"id": 275,
		"start": 940.397,
		"end": 942.983,
		"text": "（忍野）<br />言ってごらん<br />何があった？"
	},
	{
		"id": 276,
		"start": 944.693,
		"end": 949.239,
		"text": "（戦場ヶ原）<br />う… うちに<br />その宗教団体の幹部の人が"
	},
	{
		"id": 277,
		"start": 949.99,
		"end": 952.159,
		"text": "お母さんに連れられて<br />やって来て"
	},
	{
		"id": 278,
		"start": 952.993,
		"end": 954.62,
		"text": "（忍野）やって来て どうした？"
	},
	{
		"id": 279,
		"start": 955.913,
		"end": 958.123,
		"text": "（戦場ヶ原）<br />儀式だと言って 私を…"
	},
	{
		"id": 280,
		"start": 959.625,
		"end": 961.335,
		"text": "わ… 私に"
	},
	{
		"id": 281,
		"start": 962.336,
		"end": 963.17,
		"text": "乱暴を"
	},
	{
		"id": 282,
		"start": 963.337,
		"end": 964.171,
		"text": "あっ"
	},
	{
		"id": 283,
		"start": 967.716,
		"end": 970.636,
		"text": "私を… 犯そうとしたわ"
	},
	{
		"id": 284,
		"start": 971.387,
		"end": 972.471,
		"text": "（忍野）そうかい"
	},
	{
		"id": 285,
		"start": 973.18,
		"end": 977.893,
		"text": "でも“犯そうと”ということは<br />未遂だったんだろう？"
	},
	{
		"id": 286,
		"start": 979.061,
		"end": 981.647,
		"text": "近くにあったスパイクで<br />殴ってやったわ"
	},
	{
		"id": 287,
		"start": 982.773,
		"end": 987.236,
		"text": "でも お母さんは<br />私を助けてくれなかった"
	},
	{
		"id": 288,
		"start": 988.153,
		"end": 988.988,
		"text": "（忍野）そして？"
	},
	{
		"id": 289,
		"start": 990.239,
		"end": 995.536,
		"text": "（戦場ヶ原）<br />私が その幹部に<br />ケガをさせたせいで お母さんは…"
	},
	{
		"id": 290,
		"start": 996.704,
		"end": 999.665,
		"text": "（忍野）<br />お母さんはペナルティを負った？"
	},
	{
		"id": 291,
		"start": 1000.541,
		"end": 1004.086,
		"text": "はい　でも 考えてしまうんです"
	},
	{
		"id": 292,
		"start": 1004.294,
		"end": 1006.922,
		"text": "もしも 私が あのとき<br />抵抗しなかったら"
	},
	{
		"id": 293,
		"start": 1007.881,
		"end": 1011.719,
		"text": "少なくとも こんなことには<br />ならなかったんじゃないかって"
	},
	{
		"id": 294,
		"start": 1012.052,
		"end": 1013.971,
		"text": "壊れなかったんじゃないかって"
	},
	{
		"id": 295,
		"start": 1014.43,
		"end": 1018.684,
		"text": "（忍野）<br />だったら それは<br />お嬢ちゃん 君の思いだ"
	},
	{
		"id": 296,
		"start": 1018.892,
		"end": 1020.394,
		"text": "どんなに重かろうと"
	},
	{
		"id": 297,
		"start": 1020.519,
		"end": 1023.272,
		"text": "それは君が<br />背負わなくてはならないものだ"
	},
	{
		"id": 298,
		"start": 1023.439,
		"end": 1025.441,
		"text": "人任せにしちゃあ いけないね"
	},
	{
		"id": 299,
		"start": 1026.9,
		"end": 1028.694,
		"text": "（戦場ヶ原）<br />人任せに… し…"
	},
	{
		"id": 300,
		"start": 1029.403,
		"end": 1032.698,
		"text": "（忍野）<br />目を背けずに<br />目を開けて見てみよう"
	},
	{
		"id": 301,
		"start": 1041.498,
		"end": 1046.003,
		"text": "（戦場ヶ原）ハッ… あっ あ…"
	},
	{
		"id": 302,
		"start": 1046.587,
		"end": 1048.589,
		"text": "（忍野）何か見えるかい？"
	},
	{
		"id": 303,
		"start": 1048.797,
		"end": 1052.009,
		"text": "み… 見えます　あのときと同じ…"
	},
	{
		"id": 304,
		"start": 1052.676,
		"end": 1058.015,
		"text": "あのときと同じ大きな蟹が<br />蟹が見える"
	},
	{
		"id": 305,
		"start": 1059.475,
		"end": 1060.809,
		"text": "（忍野）そうかい"
	},
	{
		"id": 306,
		"start": 1060.893,
		"end": 1064.396,
		"text": "僕には 全く見えないがね<br />阿良々木くんは？"
	},
	{
		"id": 307,
		"start": 1064.897,
		"end": 1066.231,
		"text": "（阿良々木）何も見えない"
	},
	{
		"id": 308,
		"start": 1066.648,
		"end": 1070.486,
		"text": "で… でも<br />はっきりと見えます 私には"
	},
	{
		"id": 309,
		"start": 1071.153,
		"end": 1072.112,
		"text": "（忍野）そう　だったら…"
	},
	{
		"id": 310,
		"start": 1073.28,
		"end": 1075.616,
		"text": "だったら 言うべきことが<br />あるんじゃないか？"
	},
	{
		"id": 311,
		"start": 1077.284,
		"end": 1079.995,
		"text": "言うべき… こと あっ"
	},
	{
		"id": 312,
		"start": 1081.663,
		"end": 1082.498,
		"text": "あ…"
	},
	{
		"id": 313,
		"start": 1086.376,
		"end": 1087.669,
		"text": "あ…"
	},
	{
		"id": 314,
		"start": 1089.004,
		"end": 1091.173,
		"text": "あっ 戦場ヶ原！"
	},
	{
		"id": 315,
		"start": 1091.256,
		"end": 1093.008,
		"text": "あ… あ…"
	},
	{
		"id": 316,
		"start": 1093.383,
		"end": 1096.678,
		"text": "あっ う… うう…"
	},
	{
		"id": 317,
		"start": 1099.64,
		"end": 1101.35,
		"text": "（忍野）しかたがないな"
	},
	{
		"id": 318,
		"start": 1101.517,
		"end": 1103.936,
		"text": "やれやれ せっかちな神さんだ"
	},
	{
		"id": 319,
		"start": 1104.019,
		"end": 1106.355,
		"text": "気のいいヤツだよ 本当に"
	},
	{
		"id": 320,
		"start": 1106.438,
		"end": 1107.94,
		"text": "何か いいことでも あったのかな"
	},
	{
		"id": 321,
		"start": 1108.315,
		"end": 1110.275,
		"text": "（阿良々木）お… おい　忍野"
	},
	{
		"id": 322,
		"start": 1110.442,
		"end": 1113.57,
		"text": "（忍野）<br />分かっているよ 方針変更だ<br />やむをえん"
	},
	{
		"id": 323,
		"start": 1114.696,
		"end": 1117.324,
		"text": "よっ こらっ せっと"
	},
	{
		"id": 324,
		"start": 1120.994,
		"end": 1122.496,
		"text": "（戦場ヶ原）あっ ああ…"
	},
	{
		"id": 325,
		"start": 1126.291,
		"end": 1130.254,
		"text": "（忍野）<br />結局 こういうのって<br />心の持ちようの問題だからさ"
	},
	{
		"id": 326,
		"start": 1130.504,
		"end": 1133.507,
		"text": "お願いできないなら<br />言葉が通じないなら"
	},
	{
		"id": 327,
		"start": 1133.59,
		"end": 1135.509,
		"text": "戦争しかないのさ"
	},
	{
		"id": 328,
		"start": 1135.592,
		"end": 1136.426,
		"text": "（阿良々木）でも…"
	},
	{
		"id": 329,
		"start": 1136.51,
		"end": 1138.72,
		"text": "（忍野）<br />まあ このまま<br />潰しちゃったところで"
	},
	{
		"id": 330,
		"start": 1138.804,
		"end": 1141.014,
		"text": "形の上では解決するからさ"
	},
	{
		"id": 331,
		"start": 1141.473,
		"end": 1143.6,
		"text": "この際 それもありかなって"
	},
	{
		"id": 332,
		"start": 1143.684,
		"end": 1145.352,
		"text": "（阿良々木）あ… ありかなって"
	},
	{
		"id": 333,
		"start": 1145.435,
		"end": 1147.855,
		"text": "（忍野）<br />僕は 蟹が<br />とてつもなく嫌いなんだよ"
	},
	{
		"id": 334,
		"start": 1148.397,
		"end": 1149.481,
		"text": "食べにくいからね"
	},
	{
		"id": 335,
		"start": 1150.899,
		"end": 1151.733,
		"text": "（戦場ヶ原）待って"
	},
	{
		"id": 336,
		"start": 1152.568,
		"end": 1154.319,
		"text": "待ってください 忍野さん"
	},
	{
		"id": 337,
		"start": 1154.403,
		"end": 1155.237,
		"text": "ん？"
	},
	{
		"id": 338,
		"start": 1156.655,
		"end": 1159.783,
		"text": "（忍野）<br />待つって<br />待つって何をさ？ お嬢ちゃん"
	},
	{
		"id": 339,
		"start": 1160.159,
		"end": 1163.162,
		"text": "さっきは驚いただけだから"
	},
	{
		"id": 340,
		"start": 1163.954,
		"end": 1166.707,
		"text": "ちゃんと できますから"
	},
	{
		"id": 341,
		"start": 1166.79,
		"end": 1168.792,
		"text": "自分で できるから"
	},
	{
		"id": 342,
		"start": 1168.959,
		"end": 1169.793,
		"text": "ふうん"
	},
	{
		"id": 343,
		"start": 1170.377,
		"end": 1172.671,
		"text": "（忍野）<br />じゃあ どうぞ　やってごらん"
	},
	{
		"id": 344,
		"start": 1172.921,
		"end": 1173.755,
		"text": "あっ"
	},
	{
		"id": 345,
		"start": 1177.259,
		"end": 1178.26,
		"text": "（戦場ヶ原）ごめんなさい"
	},
	{
		"id": 346,
		"start": 1179.469,
		"end": 1182.014,
		"text": "それから ありがとうございました"
	},
	{
		"id": 347,
		"start": 1183.348,
		"end": 1185.184,
		"text": "でも もういいんです"
	},
	{
		"id": 348,
		"start": 1185.309,
		"end": 1190.147,
		"text": "それは私の気持ちで<br />私の思いで 私の記憶ですから"
	},
	{
		"id": 349,
		"start": 1190.355,
		"end": 1191.607,
		"text": "私が背負います"
	},
	{
		"id": 350,
		"start": 1192.566,
		"end": 1194.401,
		"text": "なくしちゃ いけないものでした"
	},
	{
		"id": 351,
		"start": 1197.029,
		"end": 1198.363,
		"text": "お願いします"
	},
	{
		"id": 352,
		"start": 1198.572,
		"end": 1202.117,
		"text": "どうか 私に<br />私の重みを返してください"
	},
	{
		"id": 353,
		"start": 1202.951,
		"end": 1206.747,
		"text": "どうか お母さんを<br />私に返してください"
	},
	{
		"id": 354,
		"start": 1212.711,
		"end": 1216.256,
		"text": "（戦場ヶ原の泣き声）"
	},
	{
		"id": 355,
		"start": 1217.633,
		"end": 1221.511,
		"text": "（阿良々木）<br />戦場ヶ原の母親が<br />悪徳宗教に はまったのは"
	},
	{
		"id": 356,
		"start": 1221.887,
		"end": 1225.098,
		"text": "戦場ヶ原が<br />小学５年生のときだったらしい"
	},
	{
		"id": 357,
		"start": 1225.515,
		"end": 1228.894,
		"text": "そのころ 戦場ヶ原は<br />ひどい大病を患った"
	},
	{
		"id": 358,
		"start": 1230.103,
		"end": 1231.605,
		"text": "娘の病気にあたり"
	},
	{
		"id": 359,
		"start": 1231.688,
		"end": 1235.15,
		"text": "戦場ヶ原の母親は<br />心のよりどころを求めた"
	},
	{
		"id": 360,
		"start": 1235.442,
		"end": 1236.276,
		"text": "そして…"
	},
	{
		"id": 361,
		"start": 1238.195,
		"end": 1242.241,
		"text": "戦場ヶ原は大手術の結果<br />九死に一生を得たそうだ"
	},
	{
		"id": 362,
		"start": 1243.325,
		"end": 1245.619,
		"text": "だが それによって母親は"
	},
	{
		"id": 363,
		"start": 1245.702,
		"end": 1248.205,
		"text": "ますます<br />宗教に のめり込んでしまった"
	},
	{
		"id": 364,
		"start": 1248.789,
		"end": 1252.125,
		"text": "戦場ヶ原は<br />母親と不仲になったそうだ"
	},
	{
		"id": 365,
		"start": 1252.376,
		"end": 1255.462,
		"text": "中学生になってからは<br />ほとんど 口を利かなかったらしい"
	},
	{
		"id": 366,
		"start": 1255.796,
		"end": 1256.63,
		"text": "そして…"
	},
	{
		"id": 367,
		"start": 1257.589,
		"end": 1260.592,
		"text": "中学卒業を間近にして事は起きた"
	},
	{
		"id": 368,
		"start": 1269.518,
		"end": 1270.936,
		"text": "その結果—"
	},
	{
		"id": 369,
		"start": 1274.356,
		"end": 1276.275,
		"text": "家庭は崩壊してしまった"
	},
	{
		"id": 370,
		"start": 1277.442,
		"end": 1278.277,
		"text": "破局した"
	},
	{
		"id": 371,
		"start": 1279.403,
		"end": 1284.157,
		"text": "そして 戦場ヶ原は行き遭った<br />１匹の蟹に"
	},
	{
		"id": 372,
		"start": 1285.033,
		"end": 1286.66,
		"text": "うう うう…"
	},
	{
		"id": 373,
		"start": 1286.743,
		"end": 1289.413,
		"text": "（忍野）<br />おもし蟹(がに)ってのはね 阿良々木くん"
	},
	{
		"id": 374,
		"start": 1289.538,
		"end": 1292.666,
		"text": "だからつまり “おもいし神(かみ)”<br />ってことなんだよね"
	},
	{
		"id": 375,
		"start": 1293.041,
		"end": 1295.127,
		"text": "また“思(おも)い出(と)しがみ”"
	},
	{
		"id": 376,
		"start": 1295.21,
		"end": 1297.17,
		"text": "つまり しがらみってことでも<br />あるんだ"
	},
	{
		"id": 377,
		"start": 1298.13,
		"end": 1299.673,
		"text": "（阿良々木）<br />つまり 蟹に行き遭ったとき"
	},
	{
		"id": 378,
		"start": 1299.756,
		"end": 1302.676,
		"text": "戦場ヶ原は<br />母親へのしがらみを切ったのだ"
	},
	{
		"id": 379,
		"start": 1303.76,
		"end": 1306.596,
		"text": "母親のことで<br />思い悩むことをやめた"
	},
	{
		"id": 380,
		"start": 1306.888,
		"end": 1308.056,
		"text": "思うのをやめた"
	},
	{
		"id": 381,
		"start": 1308.14,
		"end": 1309.683,
		"text": "重みをなくしたのだ"
	},
	{
		"id": 382,
		"start": 1310.392,
		"end": 1313.437,
		"text": "自ら進んでズルをした"
	},
	{
		"id": 383,
		"start": 1313.979,
		"end": 1316.898,
		"text": "別に<br />悪いことじゃないんだけどねえ"
	},
	{
		"id": 384,
		"start": 1316.982,
		"end": 1320.777,
		"text": "特に今回の場合<br />今更 思いを取り戻したところで"
	},
	{
		"id": 385,
		"start": 1320.861,
		"end": 1322.696,
		"text": "母親が帰ってくるわけでも"
	},
	{
		"id": 386,
		"start": 1322.779,
		"end": 1325.198,
		"text": "崩壊した家庭が<br />再生するわけでもない"
	},
	{
		"id": 387,
		"start": 1325.907,
		"end": 1327.451,
		"text": "（阿良々木）<br />それなのに それでも"
	},
	{
		"id": 388,
		"start": 1328.035,
		"end": 1331.705,
		"text": "それゆえに 戦場ヶ原ひたぎは<br />返してほしかった"
	},
	{
		"id": 389,
		"start": 1331.788,
		"end": 1333.29,
		"text": "返してほしがった"
	},
	{
		"id": 390,
		"start": 1333.373,
		"end": 1337.419,
		"text": "もう どうしようもない<br />母親の思い出を 記憶と悩みを"
	},
	{
		"id": 391,
		"start": 1337.836,
		"end": 1341.34,
		"text": "忍野の言うとおり<br />何も変わらないのだろうけれど"
	},
	{
		"id": 392,
		"start": 1342.758,
		"end": 1344.968,
		"text": "（戦場ヶ原）<br />何も変わらない<br />なんてことはないわ"
	},
	{
		"id": 393,
		"start": 1345.552,
		"end": 1348.43,
		"text": "それに 決して<br />ムダでもなかったのよ"
	},
	{
		"id": 394,
		"start": 1348.555,
		"end": 1351.85,
		"text": "少なくとも<br />大切な友達が１人 出来たのだから"
	},
	{
		"id": 395,
		"start": 1352.476,
		"end": 1353.351,
		"text": "誰のことだ？"
	},
	{
		"id": 396,
		"start": 1355.02,
		"end": 1356.605,
		"text": "（戦場ヶ原）あなたのことよ<br />（阿良々木）えっ！"
	},
	{
		"id": 397,
		"start": 1357.355,
		"end": 1359.399,
		"text": "（戦場ヶ原）<br />ありがとう 阿良々木くん"
	},
	{
		"id": 398,
		"start": 1359.566,
		"end": 1362.027,
		"text": "私は あなたに<br />とても感謝しているわ"
	},
	{
		"id": 399,
		"start": 1362.36,
		"end": 1364.571,
		"text": "今までのこと 全部 謝ります"
	},
	{
		"id": 400,
		"start": 1366.073,
		"end": 1368.158,
		"text": "図々(ずうずう)しいかもしれないけれど"
	},
	{
		"id": 401,
		"start": 1368.241,
		"end": 1371.828,
		"text": "これからも仲良くしてくれたら<br />私 とてもうれしいわ"
	},
	{
		"id": 402,
		"start": 1372.287,
		"end": 1373.121,
		"text": "あっ"
	},
	{
		"id": 403,
		"start": 1375.499,
		"end": 1376.333,
		"text": "フフッ"
	},
	{
		"id": 404,
		"start": 1377.417,
		"end": 1378.877,
		"text": "（火憐(かれん)）兄ちゃん！ 朝だぞ コラ！"
	},
	{
		"id": 405,
		"start": 1379.211,
		"end": 1381.338,
		"text": "（月火(つきひ)）<br />いいかげんに<br />起きないとダメだよ"
	},
	{
		"id": 406,
		"start": 1381.421,
		"end": 1383.381,
		"text": "さっき寝たばっかりなのに"
	},
	{
		"id": 407,
		"start": 1383.715,
		"end": 1386.384,
		"text": "あ〜 さては<br />また夜更かししてやがったな！"
	},
	{
		"id": 408,
		"start": 1386.51,
		"end": 1387.344,
		"text": "（２人）もう！"
	},
	{
		"id": 409,
		"start": 1387.677,
		"end": 1390.18,
		"text": "（月火）<br />朝ご飯 食べてないのは<br />お兄ちゃんだけだよ！"
	},
	{
		"id": 410,
		"start": 1392.557,
		"end": 1395.936,
		"text": "（阿良々木）<br />しかし 体がだるい<br />熱でもあるみたいだ"
	},
	{
		"id": 411,
		"start": 1397.687,
		"end": 1401.316,
		"text": "ちなみに僕の体重は55キロ でも…"
	},
	{
		"id": 412,
		"start": 1401.733,
		"end": 1402.567,
		"text": "おいおい"
	},
	{
		"id": 413,
		"start": 1403.318,
		"end": 1407.906,
		"text": "なるほど　神様ってのは<br />確かに大ざっぱな連中らしい"
	},
	{
		"id": 414,
		"start": 1410.617,
		"end": 1416.623,
		"text": "♪〜"
	},
	{
		"id": 415,
		"start": 1492.866,
		"end": 1498.872,
		"text": "〜♪"
	},
	{
		"id": 416,
		"start": 1500.999,
		"end": 1502.542,
		"text": "（火憐）火憐だぜ！<br />（月火）月火だよ！"
	},
	{
		"id": 417,
		"start": 1502.626,
		"end": 1503.46,
		"text": "（月火）<br />ジャングルジムって言うじゃん"
	},
	{
		"id": 418,
		"start": 1503.543,
		"end": 1504.377,
		"text": "（火憐）<br />私も言ったことある！"
	},
	{
		"id": 419,
		"start": 1504.461,
		"end": 1505.295,
		"text": "（月火）<br />言い過ぎじゃないかな"
	},
	{
		"id": 420,
		"start": 1505.378,
		"end": 1506.922,
		"text": "（火憐）言い過ぎって？<br />（月火）ジャングルて"
	},
	{
		"id": 421,
		"start": 1506.963,
		"end": 1508.048,
		"text": "（火憐）<br />ジャングル… じゃないな"
	},
	{
		"id": 422,
		"start": 1508.131,
		"end": 1509.424,
		"text": "（月火）ジムて<br />（火憐）ジムでも ねえや"
	},
	{
		"id": 423,
		"start": 1509.507,
		"end": 1510.383,
		"text": "（月火）<br />イメージ操作だよ"
	},
	{
		"id": 424,
		"start": 1510.425,
		"end": 1511.718,
		"text": "（火憐）<br />じゃ どんな名前が正しいんだ？"
	},
	{
		"id": 425,
		"start": 1511.801,
		"end": 1513.386,
		"text": "（月火）<br />“鉄棒スペシャル”とかが<br />いいんじゃないかな"
	},
	{
		"id": 426,
		"start": 1513.47,
		"end": 1514.304,
		"text": "（火憐）<br />すさまじそう！"
	},
	{
		"id": 427,
		"start": 1514.387,
		"end": 1515.43,
		"text": "（月火）<br />では ここで予告編クイズ！"
	},
	{
		"id": 428,
		"start": 1515.513,
		"end": 1516.348,
		"text": "（火憐）<br />クイズ！"
	},
	{
		"id": 429,
		"start": 1516.473,
		"end": 1517.515,
		"text": "（月火）<br />あなたはバスの運転手です"
	},
	{
		"id": 430,
		"start": 1517.599,
		"end": 1519.392,
		"text": "（火憐）<br />まさか 私が<br />バスの運転手だったとは！"
	},
	{
		"id": 431,
		"start": 1519.476,
		"end": 1520.477,
		"text": "（月火）<br />最初の駅で３人 乗りました！"
	},
	{
		"id": 432,
		"start": 1520.56,
		"end": 1521.603,
		"text": "（火憐）<br />ノリノリだぜ！"
	},
	{
		"id": 433,
		"start": 1521.645,
		"end": 1523.563,
		"text": "（月火）<br />さて バスの運転手の名前は<br />何でしょう！"
	},
	{
		"id": 434,
		"start": 1523.647,
		"end": 1524.94,
		"text": "（火憐）<br />はしょり過ぎだあ！"
	},
	{
		"id": 435,
		"start": 1525.065,
		"end": 1526.983,
		"text": "（火憐･月火）<br />次回「まよいマイマイ<br />其ノ壹(そのいち)」"
	},
	{
		"id": 436,
		"start": 1527.067,
		"end": 1528.276,
		"text": "（月火）<br />はい　それが あなたの名前です"
	},
	{
		"id": 437,
		"start": 1528.36,
		"end": 1530.111,
		"text": "（火憐）<br />手品っぽく締めた！"
	}
]