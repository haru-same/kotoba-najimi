[
	{
		"id": 1,
		"start": 122.91,
		"end": 132.91,
		"text": "♪♪～"
	},
	{
		"id": 2,
		"start": 344.298,
		"end": 348.302,
		"text": "＜ワンピースを狙う<br />全ての海賊たちが目指す→"
	},
	{
		"id": 3,
		"start": 348.302,
		"end": 354.302,
		"text": "グランドライン後半の海<br />人呼んで　新世界＞"
	},
	{
		"id": 4,
		"start": 356.31,
		"end": 359.313,
		"text": "＜しかし<br />かつて　その海を制したのは→"
	},
	{
		"id": 5,
		"start": 359.313,
		"end": 363.317,
		"text": "海賊王　ゴールド・ロジャー<br />ただ　一人＞"
	},
	{
		"id": 6,
		"start": 363.317,
		"end": 370.324,
		"text": "＜新世界は　幾多の海賊たちの<br />夢と野望を阻んできた＞"
	},
	{
		"id": 7,
		"start": 370.324,
		"end": 375.329,
		"text": "＜その海に今　ゴムゴムの実を食べゴム人間となった少年→"
	},
	{
		"id": 8,
		"start": 375.329,
		"end": 379.333,
		"text": "モンキー・Ｄ・ルフィと<br />その仲間たちが挑む！＞"
	},
	{
		"id": 9,
		"start": 379.333,
		"end": 383.333,
		"text": "（ルフィ）海賊王に　俺は　なる！"
	},
	{
		"id": 10,
		"start": 386.34,
		"end": 389.343,
		"text": "＜新世界を支配する<br />四皇の一角を崩すため→"
	},
	{
		"id": 11,
		"start": 389.343,
		"end": 392.346,
		"text": "トラファルガー・ローと<br />海賊同盟を組んだ　ルフィたちは→"
	},
	{
		"id": 12,
		"start": 392.346,
		"end": 395.349,
		"text": "ドレスローザに乗り込むも→"
	},
	{
		"id": 13,
		"start": 395.349,
		"end": 399.353,
		"text": "現国王　ドフラミンゴの<br />計略によって→"
	},
	{
		"id": 14,
		"start": 399.353,
		"end": 402.289,
		"text": "島ごと閉じ込められてしまう＞"
	},
	{
		"id": 15,
		"start": 402.289,
		"end": 405.292,
		"text": "＜一味が　ドンキホーテファミリーと<br />激しい戦いを繰り広げる中→"
	},
	{
		"id": 16,
		"start": 405.292,
		"end": 412.299,
		"text": "ルフィとローは　王宮で<br />ついに　ドフラミンゴと激突した＞"
	},
	{
		"id": 17,
		"start": 412.299,
		"end": 415.302,
		"text": "（キュロス）<br />ドレスローザに降り注いだ悲劇。→"
	},
	{
		"id": 18,
		"start": 415.302,
		"end": 421.308,
		"text": "その数に比べれば　こんなもの<br />痛みですらない！→"
	},
	{
		"id": 19,
		"start": 421.308,
		"end": 428.315,
		"text": "私は<br />この国の怒りの塊となって→"
	},
	{
		"id": 20,
		"start": 428.315,
		"end": 431.318,
		"text": "貴様を討つ！"
	},
	{
		"id": 21,
		"start": 431.318,
		"end": 436.318,
		"text": "（ディアマンテ）半月グレイブ！"
	},
	{
		"id": 22,
		"start": 439.326,
		"end": 444.326,
		"text": "（キュロス）雷の破壊剣！"
	},
	{
		"id": 23,
		"start": 447.334,
		"end": 450.334,
		"text": "あ～！！"
	},
	{
		"id": 24,
		"start": 455.342,
		"end": 457.344,
		"text": "（レベッカ）キャー！<br />（キュロス）うおっ！"
	},
	{
		"id": 25,
		"start": 457.344,
		"end": 461.344,
		"text": "（ロビン）<br />ゾロが止めていたはずのピーカ！"
	},
	{
		"id": 26,
		"start": 466.287,
		"end": 473.294,
		"text": "（ピーカ）ディアマンテ　ファミリーの最高幹部が情けない姿だ。"
	},
	{
		"id": 27,
		"start": 473.294,
		"end": 487.294,
		"text": "♪♪～"
	},
	{
		"id": 28,
		"start": 496.317,
		"end": 502.317,
		"text": "（壁にぶつかる音）"
	},
	{
		"id": 29,
		"start": 509.33,
		"end": 513.334,
		"text": "やめろ　ベラミー！<br />本当に　ぶっ壊れちまうぞ！"
	},
	{
		"id": 30,
		"start": 513.334,
		"end": 517.338,
		"text": "（ベラミー）くっ。　麦わら<br />てめえ　まだ言うか！"
	},
	{
		"id": 31,
		"start": 517.338,
		"end": 521.358,
		"text": "何度でも言うさ。<br />もう　お前の体は限界だ。"
	},
	{
		"id": 32,
		"start": 521.358,
		"end": 524.358,
		"text": "戦うのは…。<br />とりゃあ！"
	},
	{
		"id": 33,
		"start": 526.28,
		"end": 528.28,
		"text": "ぐあっ…。"
	},
	{
		"id": 34,
		"start": 532.286,
		"end": 535.286,
		"text": "ベラミー！　いいかげんにしろ！"
	},
	{
		"id": 35,
		"start": 537.291,
		"end": 542.296,
		"text": "黙ってろ！　てめえの死に場所は<br />てめえで決める！"
	},
	{
		"id": 36,
		"start": 542.296,
		"end": 546.3,
		"text": "お前も海賊王になるんだろうが！"
	},
	{
		"id": 37,
		"start": 546.3,
		"end": 551.3,
		"text": "こんなもんに<br />足止めを食らってる場合かよ！"
	},
	{
		"id": 38,
		"start": 553.307,
		"end": 558.307,
		"text": "ぐはっ。<br />麦わらぁ！"
	},
	{
		"id": 39,
		"start": 570.324,
		"end": 572.326,
		"text": "（キュロス）うっ。<br />（レベッカ）お父さま！"
	},
	{
		"id": 40,
		"start": 572.326,
		"end": 575.329,
		"text": "（キュロス）当然だな。→"
	},
	{
		"id": 41,
		"start": 575.329,
		"end": 579.333,
		"text": "何も　差しの試合を<br />しにきたわけじゃない。→"
	},
	{
		"id": 42,
		"start": 579.333,
		"end": 583.27,
		"text": "敵がいる限り　戦いは終わらない。"
	},
	{
		"id": 43,
		"start": 583.27,
		"end": 585.272,
		"text": "うっ。"
	},
	{
		"id": 44,
		"start": 585.272,
		"end": 587.274,
		"text": "（レベッカ）無茶よ　お父さま。<br />もう動けない。"
	},
	{
		"id": 45,
		"start": 587.274,
		"end": 589.276,
		"text": "（キュロス）下がれ　レベッカ！"
	},
	{
		"id": 46,
		"start": 589.276,
		"end": 594.281,
		"text": "命に代えても　必ず<br />こいつらを討ち倒し→"
	},
	{
		"id": 47,
		"start": 594.281,
		"end": 601.288,
		"text": "リク王様の元に<br />この国を取り戻すんだ。"
	},
	{
		"id": 48,
		"start": 601.288,
		"end": 607.294,
		"text": "（ピーカ）聞くが　お前たちにとってリク王とは何だ？"
	},
	{
		"id": 49,
		"start": 607.294,
		"end": 614.301,
		"text": "レベッカ　お前は<br />やつのせいで罵声を浴び続けた。"
	},
	{
		"id": 50,
		"start": 614.301,
		"end": 618.305,
		"text": "キュロス　お前は<br />やつに仕えたせいで→"
	},
	{
		"id": 51,
		"start": 618.305,
		"end": 623.31,
		"text": "片足を失い　オモチャとして<br />生きることになった。"
	},
	{
		"id": 52,
		"start": 623.31,
		"end": 625.312,
		"text": "やつさえ　いなければ。"
	},
	{
		"id": 53,
		"start": 625.312,
		"end": 628.315,
		"text": "（レベッカ）ふざけないで！<br />あなたたちが陥れたくせに！"
	},
	{
		"id": 54,
		"start": 628.315,
		"end": 632.319,
		"text": "リク王様は<br />私にとって義父であり→"
	},
	{
		"id": 55,
		"start": 632.319,
		"end": 636.323,
		"text": "悪童のころより世話をかけた<br />大恩人。→"
	},
	{
		"id": 56,
		"start": 636.323,
		"end": 643.263,
		"text": "世界にあっても　リク王家は<br />平和の真意を知る尊い一族。→"
	},
	{
		"id": 57,
		"start": 643.263,
		"end": 648.268,
		"text": "今ほどに富もなく<br />栄えてはいなかった。→"
	},
	{
		"id": 58,
		"start": 648.268,
		"end": 651.271,
		"text": "だからといって<br />不幸であったわけではない！"
	},
	{
		"id": 59,
		"start": 651.271,
		"end": 655.271,
		"text": "う～ん…　あ～。"
	},
	{
		"id": 60,
		"start": 657.277,
		"end": 661.281,
		"text": "（キュロス）空は高く　花が咲き誇り→"
	},
	{
		"id": 61,
		"start": 661.281,
		"end": 664.284,
		"text": "心あふれる　素晴らしき国。→"
	},
	{
		"id": 62,
		"start": 664.284,
		"end": 667.287,
		"text": "それが真のドレスローザだ。→"
	},
	{
		"id": 63,
		"start": 667.287,
		"end": 672.292,
		"text": "金や宝石なぞ<br />石ころに心は宿らない。→"
	},
	{
		"id": 64,
		"start": 672.292,
		"end": 678.298,
		"text": "命の営みこそが<br />本当の幸せだと教えてくれる。"
	},
	{
		"id": 65,
		"start": 678.298,
		"end": 681.301,
		"text": "リク王様は　そういうお方だ。→"
	},
	{
		"id": 66,
		"start": 681.301,
		"end": 683.303,
		"text": "貴様らを追い出し→"
	},
	{
		"id": 67,
		"start": 683.303,
		"end": 688.303,
		"text": "彼が再び王座に就くことを<br />今　国中が望んでいる。"
	},
	{
		"id": 68,
		"start": 694.314,
		"end": 696.314,
		"text": "（ピーカ）了解した。"
	},
	{
		"id": 69,
		"start": 698.318,
		"end": 702.256,
		"text": "（レベッカ）あっ　消えた？"
	},
	{
		"id": 70,
		"start": 702.256,
		"end": 705.256,
		"text": "（ロビン）どういうこと？"
	},
	{
		"id": 71,
		"start": 709.263,
		"end": 712.266,
		"text": "（レベッカ・キュロス）あっ。"
	},
	{
		"id": 72,
		"start": 712.266,
		"end": 715.269,
		"text": "（ピーカ）舞踏石！"
	},
	{
		"id": 73,
		"start": 715.269,
		"end": 720.274,
		"text": "（バルトロメオ）うわ～！<br />なっ　何だベ！？　ビビった！"
	},
	{
		"id": 74,
		"start": 720.274,
		"end": 722.274,
		"text": "今度は　３段目に。"
	},
	{
		"id": 75,
		"start": 724.278,
		"end": 726.28,
		"text": "（ゾロ）くっそ！　あの野郎。"
	},
	{
		"id": 76,
		"start": 726.28,
		"end": 730.28,
		"text": "（ゾロ）相手は俺だってのに<br />戦う気あんのか！"
	},
	{
		"id": 77,
		"start": 736.29,
		"end": 739.293,
		"text": "いずれにせよ　台地の中を<br />うろうろ移動されたんじゃ→"
	},
	{
		"id": 78,
		"start": 739.293,
		"end": 741.295,
		"text": "仕留められねえ。"
	},
	{
		"id": 79,
		"start": 741.295,
		"end": 744.298,
		"text": "次　射程距離内に入ったら<br />覚悟しろ！"
	},
	{
		"id": 80,
		"start": 744.298,
		"end": 747.301,
		"text": "≪（ピーカ）舞踏石！"
	},
	{
		"id": 81,
		"start": 747.301,
		"end": 749.303,
		"text": "ピーカさま　おやめください！<br />うおっ！"
	},
	{
		"id": 82,
		"start": 749.303,
		"end": 752.303,
		"text": "ピーカさま！<br />（部下）助けて！"
	},
	{
		"id": 83,
		"start": 758.312,
		"end": 761.331,
		"text": "あの野郎　負傷者を！"
	},
	{
		"id": 84,
		"start": 761.331,
		"end": 764.251,
		"text": "（サイ）来るぞ　気を付けろ！<br />（ベビー５）はい　あなた。"
	},
	{
		"id": 85,
		"start": 764.251,
		"end": 766.253,
		"text": "くっつくな！<br />（ベビー５）あっ。　もう→"
	},
	{
		"id": 86,
		"start": 766.253,
		"end": 770.257,
		"text": "夫婦になるんだから<br />そんなに照れなくったって。"
	},
	{
		"id": 87,
		"start": 770.257,
		"end": 773.26,
		"text": "あっ！<br />（ピーカ）裏切ったな　ベビー５。→"
	},
	{
		"id": 88,
		"start": 773.26,
		"end": 776.26,
		"text": "石押！<br />（サイ）危ねえ！"
	},
	{
		"id": 89,
		"start": 779.266,
		"end": 781.268,
		"text": "（ベビー５）ああっ。<br />（チンジャオ）サイ！"
	},
	{
		"id": 90,
		"start": 781.268,
		"end": 783.27,
		"text": "あなた！"
	},
	{
		"id": 91,
		"start": 783.27,
		"end": 785.27,
		"text": "（サイ）ああっ…。"
	},
	{
		"id": 92,
		"start": 787.274,
		"end": 789.276,
		"text": "（サイ）おおおお…！"
	},
	{
		"id": 93,
		"start": 789.276,
		"end": 792.279,
		"text": "おお　無事か。　今　助けるぞ。"
	},
	{
		"id": 94,
		"start": 792.279,
		"end": 797.284,
		"text": "バカ野郎！　じじい　後ろだ！<br />（チンジャオ）うん？"
	},
	{
		"id": 95,
		"start": 797.284,
		"end": 799.953,
		"text": "ごみどもが！　舞踏石！"
	},
	{
		"id": 96,
		"start": 799.953,
		"end": 802.253,
		"text": "（ベビー５）ああっ！　あっ。"
	},
	{
		"id": 97,
		"start": 805.192,
		"end": 807.194,
		"text": "（サイ）でりゃっ！→"
	},
	{
		"id": 98,
		"start": 807.194,
		"end": 812.194,
		"text": "ハァ　ハァ…　くそったれが。"
	},
	{
		"id": 99,
		"start": 814.201,
		"end": 817.204,
		"text": "石の野郎<br />いいかげんにしろ　てめえ！"
	},
	{
		"id": 100,
		"start": 817.204,
		"end": 820.207,
		"text": "弱ったやつらに<br />手ぇ出しやがって！"
	},
	{
		"id": 101,
		"start": 820.207,
		"end": 823.143,
		"text": "ロロノア。　噛石！"
	},
	{
		"id": 102,
		"start": 823.143,
		"end": 826.146,
		"text": "一刀流　馬鬼！"
	},
	{
		"id": 103,
		"start": 826.146,
		"end": 828.148,
		"text": "なあ　ロロノア・ゾロ→"
	},
	{
		"id": 104,
		"start": 828.148,
		"end": 833.153,
		"text": "旧　王の台地に<br />誰がいるか知ってるか？"
	},
	{
		"id": 105,
		"start": 833.153,
		"end": 836.156,
		"text": "この国の王だろ？<br />（ピーカ）「元」だ。"
	},
	{
		"id": 106,
		"start": 836.156,
		"end": 842.162,
		"text": "何が言いてえんだ　てめえ。<br />おかしいと思わないか？"
	},
	{
		"id": 107,
		"start": 842.162,
		"end": 846.166,
		"text": "何が！<br />侵略は世の常だ。→"
	},
	{
		"id": 108,
		"start": 846.166,
		"end": 851.171,
		"text": "なぜ　ドフィーだけが責められる。"
	},
	{
		"id": 109,
		"start": 851.171,
		"end": 854.171,
		"text": "ごちゃごちゃ　うるせえんだよ。"
	},
	{
		"id": 110,
		"start": 857.177,
		"end": 863.183,
		"text": "（ピーカ）旧　王の台地には<br />昔のこの国の要人たちに加え→"
	},
	{
		"id": 111,
		"start": 863.183,
		"end": 867.187,
		"text": "侍たちに<br />ゴッド・ウソップもいる。→"
	},
	{
		"id": 112,
		"start": 867.187,
		"end": 873.193,
		"text": "国中の人間たちが　リク王の復古を望んでいるそうだ。"
	},
	{
		"id": 113,
		"start": 873.193,
		"end": 876.196,
		"text": "それが　どうした！<br />（ピーカ）目障りじゃないか。→"
	},
	{
		"id": 114,
		"start": 876.196,
		"end": 879.196,
		"text": "王は　ドフィー１人だ。"
	},
	{
		"id": 115,
		"start": 888.141,
		"end": 890.141,
		"text": "うん？"
	},
	{
		"id": 116,
		"start": 901.154,
		"end": 903.154,
		"text": "あっ。"
	},
	{
		"id": 117,
		"start": 907.16,
		"end": 909.162,
		"text": "何やい！？"
	},
	{
		"id": 118,
		"start": 909.162,
		"end": 911.164,
		"text": "（ロビン）何が起こっているの？"
	},
	{
		"id": 119,
		"start": 911.164,
		"end": 913.166,
		"text": "（キュロス）あれを見ろ！"
	},
	{
		"id": 120,
		"start": 913.166,
		"end": 933.186,
		"text": "♪♪～"
	},
	{
		"id": 121,
		"start": 933.186,
		"end": 948.201,
		"text": "♪♪～"
	},
	{
		"id": 122,
		"start": 948.201,
		"end": 950.203,
		"text": "やめろ。"
	},
	{
		"id": 123,
		"start": 950.203,
		"end": 970.223,
		"text": "♪♪～"
	},
	{
		"id": 124,
		"start": 970.223,
		"end": 980.233,
		"text": "♪♪～"
	},
	{
		"id": 125,
		"start": 980.233,
		"end": 983.236,
		"text": "また動き始めたぞ。"
	},
	{
		"id": 126,
		"start": 983.236,
		"end": 985.236,
		"text": "ど…　どうするつもりだ。"
	},
	{
		"id": 127,
		"start": 992.245,
		"end": 996.245,
		"text": "ここは　ドンキホーテの国だ。"
	},
	{
		"id": 128,
		"start": 1020.207,
		"end": 1023.21,
		"text": "（海兵）歩くぞ　逃げろ！"
	},
	{
		"id": 129,
		"start": 1023.21,
		"end": 1027.214,
		"text": "石像が動きだした　逃げろ！<br />逃げろ！"
	},
	{
		"id": 130,
		"start": 1027.214,
		"end": 1040.227,
		"text": "♪♪～"
	},
	{
		"id": 131,
		"start": 1040.227,
		"end": 1042.227,
		"text": "おいおい　まさか…。"
	},
	{
		"id": 132,
		"start": 1047.234,
		"end": 1050.237,
		"text": "（ウソップ）あ～。"
	},
	{
		"id": 133,
		"start": 1050.237,
		"end": 1054.241,
		"text": "な…　何なんだ！？<br />何しようってんだ。"
	},
	{
		"id": 134,
		"start": 1054.241,
		"end": 1068.288,
		"text": "♪♪～"
	},
	{
		"id": 135,
		"start": 1068.288,
		"end": 1070.29,
		"text": "あの野郎！"
	},
	{
		"id": 136,
		"start": 1070.29,
		"end": 1073.29,
		"text": "王を消す気か！？"
	},
	{
		"id": 137,
		"start": 1310.263,
		"end": 1320.273,
		"text": "♪♪～"
	},
	{
		"id": 138,
		"start": 1320.273,
		"end": 1335.288,
		"text": "♪♪～"
	},
	{
		"id": 139,
		"start": 1335.288,
		"end": 1338.291,
		"text": "（男性）逃げろ！<br />（男性）つぶされるぞ！"
	},
	{
		"id": 140,
		"start": 1338.291,
		"end": 1358.311,
		"text": "♪♪～"
	},
	{
		"id": 141,
		"start": 1358.311,
		"end": 1378.264,
		"text": "♪♪～"
	},
	{
		"id": 142,
		"start": 1378.264,
		"end": 1381.267,
		"text": "♪♪～"
	},
	{
		"id": 143,
		"start": 1381.267,
		"end": 1385.271,
		"text": "（将校）砲撃用意！→"
	},
	{
		"id": 144,
		"start": 1385.271,
		"end": 1387.271,
		"text": "撃て！"
	},
	{
		"id": 145,
		"start": 1390.276,
		"end": 1392.276,
		"text": "撃て　撃て！"
	},
	{
		"id": 146,
		"start": 1405.291,
		"end": 1410.296,
		"text": "野郎！　それで　俺を<br />こんな端まで追い込んだのか。"
	},
	{
		"id": 147,
		"start": 1410.296,
		"end": 1413.296,
		"text": "くっ　追い付けねえ！"
	},
	{
		"id": 148,
		"start": 1416.302,
		"end": 1418.304,
		"text": "まんまと　わなにはまった。"
	},
	{
		"id": 149,
		"start": 1418.304,
		"end": 1421.307,
		"text": "まずいな　このままじゃ。"
	},
	{
		"id": 150,
		"start": 1421.307,
		"end": 1426.246,
		"text": "あいつのパンチなら<br />王の台地は一発で粉々だ。"
	},
	{
		"id": 151,
		"start": 1426.246,
		"end": 1431.251,
		"text": "ウソップも王も　つぶされて即死。<br />何か　いい手はねえか。"
	},
	{
		"id": 152,
		"start": 1431.251,
		"end": 1437.257,
		"text": "（部下）海賊狩り！　これ以上<br />ピーカさまの邪魔はさせないぞ！"
	},
	{
		"id": 153,
		"start": 1437.257,
		"end": 1439.259,
		"text": "（部下たち）あ～！"
	},
	{
		"id": 154,
		"start": 1439.259,
		"end": 1441.259,
		"text": "うっとうしいんだよ。"
	},
	{
		"id": 155,
		"start": 1443.263,
		"end": 1446.266,
		"text": "（ロビン）ゾロ。<br />（キュロス）まずいぞ！→"
	},
	{
		"id": 156,
		"start": 1446.266,
		"end": 1449.266,
		"text": "あいつを行かせては。"
	},
	{
		"id": 157,
		"start": 1451.271,
		"end": 1455.271,
		"text": "だが　あんなもの<br />止める手段がない。"
	},
	{
		"id": 158,
		"start": 1459.279,
		"end": 1461.279,
		"text": "おじいさま。<br />（ロビン）ウソップ。"
	},
	{
		"id": 159,
		"start": 1463.283,
		"end": 1468.283,
		"text": "あー！　あんの野郎！"
	},
	{
		"id": 160,
		"start": 1483.236,
		"end": 1488.236,
		"text": "いや　遠過ぎて威力不足。<br />仕留めきれねえ。"
	},
	{
		"id": 161,
		"start": 1498.251,
		"end": 1502.251,
		"text": "どりゃあ～！"
	},
	{
		"id": 162,
		"start": 1509.262,
		"end": 1511.262,
		"text": "《無理だな》"
	},
	{
		"id": 163,
		"start": 1523.276,
		"end": 1528.276,
		"text": "ウソップ～！"
	},
	{
		"id": 164,
		"start": 1533.286,
		"end": 1537.286,
		"text": "聞こえるわけねえか　この距離。"
	},
	{
		"id": 165,
		"start": 1555.241,
		"end": 1558.244,
		"text": "ああ　持ってねえな。"
	},
	{
		"id": 166,
		"start": 1558.244,
		"end": 1564.244,
		"text": "ギャ～！　こっち来んな～！"
	},
	{
		"id": 167,
		"start": 1570.256,
		"end": 1576.262,
		"text": "どのみち　それを知らせても<br />全員で逃げ出す時間がねえ。"
	},
	{
		"id": 168,
		"start": 1576.262,
		"end": 1578.262,
		"text": "（海兵たち）うわ～！"
	},
	{
		"id": 169,
		"start": 1584.27,
		"end": 1588.274,
		"text": "やっちゃえ！<br />（部下）ピーカさま！"
	},
	{
		"id": 170,
		"start": 1588.274,
		"end": 1591.274,
		"text": "リク王を討つ気だわ。"
	},
	{
		"id": 171,
		"start": 1593.279,
		"end": 1596.282,
		"text": "（サイ）俺らにゃ関係ねえが<br />解せねえ。"
	},
	{
		"id": 172,
		"start": 1596.282,
		"end": 1599.285,
		"text": "（オオロンブス）<br />いったい　どうしたものか。"
	},
	{
		"id": 173,
		"start": 1599.285,
		"end": 1603.222,
		"text": "やめれ～　石の怪物！"
	},
	{
		"id": 174,
		"start": 1603.222,
		"end": 1606.225,
		"text": "そっちの台地にゃ<br />そげキング先輩が！"
	},
	{
		"id": 175,
		"start": 1606.225,
		"end": 1612.225,
		"text": "石！　戻ってこい！<br />俺が相手だ～！"
	},
	{
		"id": 176,
		"start": 1616.235,
		"end": 1620.239,
		"text": "（ウソップ）い～や～！"
	},
	{
		"id": 177,
		"start": 1620.239,
		"end": 1623.239,
		"text": "（３人）うわ～！"
	},
	{
		"id": 178,
		"start": 1627.246,
		"end": 1631.25,
		"text": "（エリザベロー）<br />なるほど　海賊狩りの言うとおり→"
	},
	{
		"id": 179,
		"start": 1631.25,
		"end": 1635.25,
		"text": "このキング・パンチ<br />取って置いてよかった。"
	},
	{
		"id": 180,
		"start": 1640.259,
		"end": 1643.262,
		"text": "今　救うぞ<br />わが友　リク王。"
	},
	{
		"id": 181,
		"start": 1643.262,
		"end": 1647.266,
		"text": "≪ちょっと待て！　どっかの王。<br />「どっかの」！？"
	},
	{
		"id": 182,
		"start": 1647.266,
		"end": 1651.27,
		"text": "私は　プロデンスの王<br />エリザベローだ　海賊狩り。"
	},
	{
		"id": 183,
		"start": 1651.27,
		"end": 1654.273,
		"text": "ただ破壊しても無駄と言ったろ。"
	},
	{
		"id": 184,
		"start": 1654.273,
		"end": 1658.277,
		"text": "本体を倒さなきゃ<br />何度でも復活する。"
	},
	{
		"id": 185,
		"start": 1658.277,
		"end": 1660.279,
		"text": "ならば　どうする？"
	},
	{
		"id": 186,
		"start": 1660.279,
		"end": 1680.233,
		"text": "♪♪～"
	},
	{
		"id": 187,
		"start": 1680.233,
		"end": 1688.241,
		"text": "♪♪～"
	},
	{
		"id": 188,
		"start": 1688.241,
		"end": 1691.244,
		"text": "もう時間はなさそうだ。"
	},
	{
		"id": 189,
		"start": 1691.244,
		"end": 1701.254,
		"text": "♪♪～"
	},
	{
		"id": 190,
		"start": 1701.254,
		"end": 1704.254,
		"text": "助けて～！"
	},
	{
		"id": 191,
		"start": 1706.259,
		"end": 1708.261,
		"text": "（エリザベロー）海賊狩り！"
	},
	{
		"id": 192,
		"start": 1708.261,
		"end": 1720.273,
		"text": "♪♪～"
	},
	{
		"id": 193,
		"start": 1720.273,
		"end": 1722.291,
		"text": "うん？"
	},
	{
		"id": 194,
		"start": 1722.291,
		"end": 1724.291,
		"text": "フッ。"
	},
	{
		"id": 195,
		"start": 1734.22,
		"end": 1737.223,
		"text": "（エリザベロー）おい。<br />２分　待て。"
	},
	{
		"id": 196,
		"start": 1737.223,
		"end": 1757.243,
		"text": "♪♪～"
	},
	{
		"id": 197,
		"start": 1757.243,
		"end": 1762.248,
		"text": "♪♪～"
	},
	{
		"id": 198,
		"start": 1762.248,
		"end": 1764.25,
		"text": "（ピーカ）リク王はいるか？"
	},
	{
		"id": 199,
		"start": 1764.25,
		"end": 1766.252,
		"text": "（ウソップ）あ～！"
	},
	{
		"id": 200,
		"start": 1766.252,
		"end": 1769.252,
		"text": "あー　あー　あー　あー　あ～！"
	},
	{
		"id": 201,
		"start": 1789.208,
		"end": 1792.208,
		"text": "これでいこう。"
	},
	{
		"id": 202,
		"start": 1800.219,
		"end": 1803.222,
		"text": "＜天を覆うほど巨大な<br />ピーカの拳が→"
	},
	{
		"id": 203,
		"start": 1803.222,
		"end": 1805.224,
		"text": "リク王　目がけ　落ちてくる＞"
	},
	{
		"id": 204,
		"start": 1805.224,
		"end": 1808.227,
		"text": "＜ドレスローザにともった<br />希望の光が押しつぶされ→"
	},
	{
		"id": 205,
		"start": 1808.227,
		"end": 1812.231,
		"text": "再び　力の支配の時代が<br />到来しようとした　その時→"
	},
	{
		"id": 206,
		"start": 1812.231,
		"end": 1815.234,
		"text": "ゾロとピーカとの<br />最後の戦いが始まる！＞"
	},
	{
		"id": 207,
		"start": 1815.234,
		"end": 1817.234,
		"text": "次回　『ワンピース』"
	},
	{
		"id": 208,
		"start": 1820.239,
		"end": 1823.239,
		"text": "海賊王に　俺は　なる！"
	},
	{
		"id": 209,
		"start": 1834.687,
		"end": 1836.172,
		"text": "（山﨑・東野）おはようございます。<br />（東野）『ワイドナショー』のお時間です。"
	},
	{
		"id": 210,
		"start": 1836.172,
		"end": 1838.791,
		"text": "さあ　松本さん　ちょっと<br />お疲れな様子でございますけど。"
	},
	{
		"id": 211,
		"start": 1838.791,
		"end": 1840.793,
		"text": "（松本）そんなことは<br />全然ないですよ。"
	},
	{
		"id": 212,
		"start": 1840.793,
		"end": 1843.796,
		"text": "そんなことは　全然ないんですが<br />今日もね→"
	},
	{
		"id": 213,
		"start": 1843.796,
		"end": 1847.066,
		"text": "頭のいい人と　バカな人と<br />色々　いますよね。"
	},
	{
		"id": 214,
		"start": 1847.066,
		"end": 1849.852,
		"text": "エキセントリックな人とかね<br />いますから。"
	},
	{
		"id": 215,
		"start": 1849.852,
		"end": 1851.804,
		"text": "よろしくお願いします。<br />さあ　堀　潤さん。"
	},
	{
		"id": 216,
		"start": 1851.804,
		"end": 1853.806,
		"text": "２回目でございます。<br />よろしくお願いします。"
	},
	{
		"id": 217,
		"start": 1853.806,
		"end": 1856.375,
		"text": "（堀）前回も<br />ありがとうございました。"
	},
	{
		"id": 218,
		"start": 1856.375,
		"end": 1858.928,
		"text": "色々　反響も大きくて。<br />ちょっと　何かありましたね。"
	}
]