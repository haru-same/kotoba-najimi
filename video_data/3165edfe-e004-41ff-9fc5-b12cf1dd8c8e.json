[
	{
		"id": 1,
		"start": 2.419,
		"end": 4.17,
		"text": "（くしゃみ）"
	},
	{
		"id": 2,
		"start": 4.504,
		"end": 7.757,
		"text": "（アクア）ううう… 寒う"
	},
	{
		"id": 3,
		"start": 7.882,
		"end": 11.636,
		"text": "コートも質屋に入れちゃったし<br />寒いよお"
	},
	{
		"id": 4,
		"start": 12.846,
		"end": 14.514,
		"text": "ああっ ひ… 火が"
	},
	{
		"id": 5,
		"start": 15.807,
		"end": 16.808,
		"text": "よっ"
	},
	{
		"id": 6,
		"start": 19.269,
		"end": 20.812,
		"text": "（佐藤和真(さとうカズマ)）<br />…って 何やってんだ お前！"
	},
	{
		"id": 7,
		"start": 20.937,
		"end": 22.48,
		"text": "ふざけんな"
	},
	{
		"id": 8,
		"start": 22.731,
		"end": 25.775,
		"text": "俺の唯一の日本の思い出の品に<br />なんてことすんだ！"
	},
	{
		"id": 9,
		"start": 25.9,
		"end": 28.862,
		"text": "しかたないでしょ<br />ほかに燃やす物 ないんだから"
	},
	{
		"id": 10,
		"start": 28.987,
		"end": 30.822,
		"text": "（カズマ）<br />だったら その羽衣よこせ"
	},
	{
		"id": 11,
		"start": 30.947,
		"end": 33.158,
		"text": "ジャージより<br />よく燃えそうじゃねえか"
	},
	{
		"id": 12,
		"start": 33.408,
		"end": 35.201,
		"text": "（アクア）<br />放しなさいよ ヒキニート"
	},
	{
		"id": 13,
		"start": 35.702,
		"end": 38.329,
		"text": "暖(あった)めてよ もっと私を暖めてよ！"
	},
	{
		"id": 14,
		"start": 38.455,
		"end": 40.749,
		"text": "（風の音）"
	},
	{
		"id": 15,
		"start": 43.126,
		"end": 44.961,
		"text": "ああーっ！"
	},
	{
		"id": 16,
		"start": 45.086,
		"end": 46.087,
		"text": "ちょ… タンマ！"
	},
	{
		"id": 17,
		"start": 46.755,
		"end": 50.133,
		"text": "（吹く音）"
	},
	{
		"id": 18,
		"start": 50.258,
		"end": 51.259,
		"text": "あっ"
	},
	{
		"id": 19,
		"start": 52.135,
		"end": 53.845,
		"text": "あっ あ…"
	},
	{
		"id": 20,
		"start": 53.97,
		"end": 55.722,
		"text": "（泣き声）"
	},
	{
		"id": 21,
		"start": 56.139,
		"end": 60.06,
		"text": "（カズマ）このままじゃダメだ<br />早く何とかしないと"
	},
	{
		"id": 22,
		"start": 62.395,
		"end": 69.402,
		"text": "♪～"
	},
	{
		"id": 23,
		"start": 144.644,
		"end": 151.651,
		"text": "～♪"
	},
	{
		"id": 24,
		"start": 154.362,
		"end": 156.656,
		"text": "（カズマ）俺たちのパーティーは<br />バランスが悪い"
	},
	{
		"id": 25,
		"start": 157.198,
		"end": 161.202,
		"text": "もう少しこう メインの武器になる<br />スキルが欲しいところだ"
	},
	{
		"id": 26,
		"start": 164.122,
		"end": 165.331,
		"text": "（カズマ）おし 着いたぞ"
	},
	{
		"id": 27,
		"start": 165.456,
		"end": 166.291,
		"text": "いいか アクア"
	},
	{
		"id": 28,
		"start": 166.416,
		"end": 169.419,
		"text": "今のうちに言っとくが<br />絶対に暴れるなよ　分かったか？"
	},
	{
		"id": 29,
		"start": 169.669,
		"end": 173.673,
		"text": "ちょっと 何言ってんの？<br />カズマって私を何だと思ってんの？"
	},
	{
		"id": 30,
		"start": 173.798,
		"end": 178.011,
		"text": "私 チンピラや無法者じゃないのよ<br />女神よ 神様なのよ"
	},
	{
		"id": 31,
		"start": 178.136,
		"end": 180.263,
		"text": "…って ちょっと<br />話 聞いてんの？"
	},
	{
		"id": 32,
		"start": 180.722,
		"end": 181.973,
		"text": "待ってよ！ 女神の話は…"
	},
	{
		"id": 33,
		"start": 182.098,
		"end": 183.474,
		"text": "（ウィズ）いらっしゃ…"
	},
	{
		"id": 34,
		"start": 183.766,
		"end": 184.726,
		"text": "ああっ！"
	},
	{
		"id": 35,
		"start": 185.476,
		"end": 187.228,
		"text": "ん…　ああっ！"
	},
	{
		"id": 36,
		"start": 187.353,
		"end": 191.357,
		"text": "出たわね このクソアンデッド<br />こんな所で店なんて出してたの？"
	},
	{
		"id": 37,
		"start": 192.108,
		"end": 194.193,
		"text": "私が馬小屋で<br />寝泊まりしてるってのに―"
	},
	{
		"id": 38,
		"start": 194.319,
		"end": 196.946,
		"text": "あんたは<br />お店の経営者ってわけ？"
	},
	{
		"id": 39,
		"start": 197.071,
		"end": 199.24,
		"text": "リッチーのくせに生意気よ！"
	},
	{
		"id": 40,
		"start": 199.365,
		"end": 201.284,
		"text": "こんな店 神の名のもとに<br />燃やして…"
	},
	{
		"id": 41,
		"start": 201.409,
		"end": 202.493,
		"text": "（なぐる音）<br />（アクア）痛い！"
	},
	{
		"id": 42,
		"start": 202.619,
		"end": 203.494,
		"text": "あっ"
	},
	{
		"id": 43,
		"start": 203.995,
		"end": 205.246,
		"text": "カズマさん"
	},
	{
		"id": 44,
		"start": 206.08,
		"end": 207.749,
		"text": "よう ウィズ　久しぶり"
	},
	{
		"id": 45,
		"start": 212.587,
		"end": 214.923,
		"text": "お茶も出ないのかしら？<br />このお店は"
	},
	{
		"id": 46,
		"start": 215.048,
		"end": 218.509,
		"text": "あっ す… すみません<br />今すぐ持ってきます"
	},
	{
		"id": 47,
		"start": 221.596,
		"end": 222.931,
		"text": "（割れる音）<br />（ウィズ）ああっ！"
	},
	{
		"id": 48,
		"start": 223.056,
		"end": 224.807,
		"text": "相変わらずだな"
	},
	{
		"id": 49,
		"start": 228.186,
		"end": 229.771,
		"text": "（カズマ）彼女の名前はウィズ"
	},
	{
		"id": 50,
		"start": 230.23,
		"end": 233.691,
		"text": "こう見えて<br />普通の人間ではなくリッチー"
	},
	{
		"id": 51,
		"start": 233.816,
		"end": 236.527,
		"text": "ノーライフキングと呼ばれる<br />アンデッドの王だ"
	},
	{
		"id": 52,
		"start": 237.654,
		"end": 240.031,
		"text": "ウィズはリッチーなのに<br />優しい子で―"
	},
	{
		"id": 53,
		"start": 240.156,
		"end": 243.785,
		"text": "毎夜 墓地をさまよう魂を<br />天に返してあげていた"
	},
	{
		"id": 54,
		"start": 244.202,
		"end": 248.623,
		"text": "いろいろあって 俺たちは<br />その仕事を代わりに引き受けて―"
	},
	{
		"id": 55,
		"start": 248.748,
		"end": 251.793,
		"text": "彼女を見逃してあげることに<br />したんだが…"
	},
	{
		"id": 56,
		"start": 253.419,
		"end": 255.213,
		"text": "ど… どうぞ"
	},
	{
		"id": 57,
		"start": 259.05,
		"end": 260.927,
		"text": "（アクア）う… おいしい"
	},
	{
		"id": 58,
		"start": 261.386,
		"end": 263.972,
		"text": "ふん　アンデッドのくせに<br />店なんて構えて"
	},
	{
		"id": 59,
		"start": 264.097,
		"end": 266.724,
		"text": "アンデッドのくせに暖かいお茶とか<br />作っちゃったりして"
	},
	{
		"id": 60,
		"start": 266.849,
		"end": 268.184,
		"text": "すいません すいません"
	},
	{
		"id": 61,
		"start": 268.309,
		"end": 270.478,
		"text": "私なんかが<br />こんな ぜいたくしちゃって"
	},
	{
		"id": 62,
		"start": 270.603,
		"end": 272.563,
		"text": "小姑(こじゅうと)みたいな嫌みは やめろ"
	},
	{
		"id": 63,
		"start": 273.189,
		"end": 274.315,
		"text": "フウ"
	},
	{
		"id": 64,
		"start": 275.817,
		"end": 278.319,
		"text": "ウィズ<br />ポイントに余裕が出来たから―"
	},
	{
		"id": 65,
		"start": 278.444,
		"end": 279.821,
		"text": "スキルを何か<br />教えてくれないか？"
	},
	{
		"id": 66,
		"start": 281.572,
		"end": 283.866,
		"text": "ちょっと！<br />何 考えてんのよ カズマ"
	},
	{
		"id": 67,
		"start": 283.992,
		"end": 286.077,
		"text": "女神の従者がリッチーのスキルを<br />覚えるなんて―"
	},
	{
		"id": 68,
		"start": 286.202,
		"end": 287.829,
		"text": "見過ごすわけには<br />いかないんですけど！"
	},
	{
		"id": 69,
		"start": 287.954,
		"end": 289.247,
		"text": "誰が従者だ 誰が！"
	},
	{
		"id": 70,
		"start": 289.956,
		"end": 292.125,
		"text": "いい？ リッチーってのはね―"
	},
	{
		"id": 71,
		"start": 292.25,
		"end": 294.502,
		"text": "薄暗くてジメジメした所が<br />大好きな―"
	},
	{
		"id": 72,
		"start": 294.627,
		"end": 297.588,
		"text": "言ってみればナメクジの親戚<br />みたいな連中なの"
	},
	{
		"id": 73,
		"start": 297.714,
		"end": 298.756,
		"text": "ひ… ひどい"
	},
	{
		"id": 74,
		"start": 298.881,
		"end": 302.76,
		"text": "いや リッチーのスキルなんて<br />普通は覚えられないだろ"
	},
	{
		"id": 75,
		"start": 302.885,
		"end": 304.637,
		"text": "そんなスキルを覚えられたら―"
	},
	{
		"id": 76,
		"start": 304.762,
		"end": 307.14,
		"text": "結構な戦力に<br />なるんじゃないかと思ってな"
	},
	{
		"id": 77,
		"start": 307.598,
		"end": 309.058,
		"text": "（ウィズ）あの…<br />（カズマ）ん？"
	},
	{
		"id": 78,
		"start": 309.183,
		"end": 311.144,
		"text": "さっき“女神の従者”って"
	},
	{
		"id": 79,
		"start": 311.269,
		"end": 312.77,
		"text": "（カズマ）あっ<br />（アクア）まあね"
	},
	{
		"id": 80,
		"start": 312.895,
		"end": 314.314,
		"text": "私はアクア"
	},
	{
		"id": 81,
		"start": 314.439,
		"end": 318.109,
		"text": "そう アクシズ教団で<br />あがめられている女神アクアよ"
	},
	{
		"id": 82,
		"start": 318.484,
		"end": 320.028,
		"text": "ひかえなさい リッチー！"
	},
	{
		"id": 83,
		"start": 320.153,
		"end": 321.863,
		"text": "ヒイイッ"
	},
	{
		"id": 84,
		"start": 321.988,
		"end": 324.782,
		"text": "おい ウィズ<br />そんなに おびえなくても"
	},
	{
		"id": 85,
		"start": 324.907,
		"end": 326.034,
		"text": "いえ その…"
	},
	{
		"id": 86,
		"start": 326.159,
		"end": 329.495,
		"text": "アクシズ教団の人は<br />頭のおかしい人が多く―"
	},
	{
		"id": 87,
		"start": 329.62,
		"end": 333.291,
		"text": "関わり合いにならないほうがいい<br />というのが世間の常識なので"
	},
	{
		"id": 88,
		"start": 333.416,
		"end": 336.878,
		"text": "アクシズ教団の元締めの<br />女神様と聞いて…"
	},
	{
		"id": 89,
		"start": 337.211,
		"end": 339.047,
		"text": "うっ うう…"
	},
	{
		"id": 90,
		"start": 339.172,
		"end": 340.84,
		"text": "何ですってえ？"
	},
	{
		"id": 91,
		"start": 340.965,
		"end": 343.217,
		"text": "この この！<br />こんな物 こうして…"
	},
	{
		"id": 92,
		"start": 343.342,
		"end": 345.595,
		"text": "（カズマ）話が進まねえ"
	},
	{
		"id": 93,
		"start": 350.85,
		"end": 352.101,
		"text": "う～ん"
	},
	{
		"id": 94,
		"start": 352.935,
		"end": 356.314,
		"text": "そういえば あのベルディアさんを<br />倒されたそうで"
	},
	{
		"id": 95,
		"start": 357.44,
		"end": 359.776,
		"text": "あの方は幹部の中でも―"
	},
	{
		"id": 96,
		"start": 359.901,
		"end": 363.529,
		"text": "剣の腕に関しては相当なものだった<br />はずなのですが"
	},
	{
		"id": 97,
		"start": 363.654,
		"end": 365.156,
		"text": "すごいですね"
	},
	{
		"id": 98,
		"start": 365.656,
		"end": 368.785,
		"text": "なんか ベルディアを<br />知ってるみたいな口ぶりだな"
	},
	{
		"id": 99,
		"start": 369.202,
		"end": 370.078,
		"text": "ああ"
	},
	{
		"id": 100,
		"start": 370.495,
		"end": 373.623,
		"text": "私 魔王軍の８人の幹部の<br />１人ですから"
	},
	{
		"id": 101,
		"start": 373.748,
		"end": 375.208,
		"text": "（アクア）確保！"
	},
	{
		"id": 102,
		"start": 376.292,
		"end": 379.003,
		"text": "（ウィズ）待って！<br />待ってください アクア様"
	},
	{
		"id": 103,
		"start": 379.128,
		"end": 382.215,
		"text": "お願いします 話を聞いてください"
	},
	{
		"id": 104,
		"start": 382.882,
		"end": 384.008,
		"text": "フウーッ"
	},
	{
		"id": 105,
		"start": 384.133,
		"end": 387.011,
		"text": "やったわね カズマ<br />これで借金なんてチャラよ チャラ"
	},
	{
		"id": 106,
		"start": 387.47,
		"end": 389.222,
		"text": "それどころか お釣りが来るわ"
	},
	{
		"id": 107,
		"start": 389.347,
		"end": 392.266,
		"text": "おい アクア<br />一応 事情は聞いてやれよ"
	},
	{
		"id": 108,
		"start": 393.726,
		"end": 399.44,
		"text": "えっと さすがに魔王軍の幹部だと<br />冒険者な手前 見逃すわけにも…"
	},
	{
		"id": 109,
		"start": 399.565,
		"end": 403.986,
		"text": "違うんです　魔王城を守る結界の<br />維持のために頼まれたんです"
	},
	{
		"id": 110,
		"start": 404.112,
		"end": 407.198,
		"text": "もちろん 今まで人に危害を<br />加えたことなんて ないですし―"
	},
	{
		"id": 111,
		"start": 407.323,
		"end": 411.536,
		"text": "私を倒したところで そもそも<br />賞金も懸かっていませんから"
	},
	{
		"id": 112,
		"start": 412.286,
		"end": 413.287,
		"text": "（２人）あ…"
	},
	{
		"id": 113,
		"start": 413.704,
		"end": 414.914,
		"text": "んん？"
	},
	{
		"id": 114,
		"start": 415.039,
		"end": 418.292,
		"text": "よく分かんないけど<br />念のために退治しておくわね"
	},
	{
		"id": 115,
		"start": 418.417,
		"end": 420.545,
		"text": "待ってください アクア様！"
	},
	{
		"id": 116,
		"start": 423.214,
		"end": 424.882,
		"text": "えっと つまり―"
	},
	{
		"id": 117,
		"start": 425.007,
		"end": 429.428,
		"text": "幹部を全部 倒すと魔王の城への<br />道が開けるとか そんな感じか？"
	},
	{
		"id": 118,
		"start": 430.763,
		"end": 434.1,
		"text": "そういうことです<br />魔王さんに頼まれたんです"
	},
	{
		"id": 119,
		"start": 434.225,
		"end": 436.31,
		"text": "“人里で<br />お店を経営しながら―”"
	},
	{
		"id": 120,
		"start": 436.436,
		"end": 438.312,
		"text": "“のんびり暮らすのは<br />止めないから―”"
	},
	{
		"id": 121,
		"start": 438.438,
		"end": 441.816,
		"text": "“幹部として結界の維持だけ<br />頼めないか”って"
	},
	{
		"id": 122,
		"start": 441.941,
		"end": 443.651,
		"text": "つまり あんたがいるかぎり―"
	},
	{
		"id": 123,
		"start": 443.776,
		"end": 446.779,
		"text": "人類は魔王城には<br />攻め込めないってことね"
	},
	{
		"id": 124,
		"start": 447.905,
		"end": 449.824,
		"text": "カズマ 退治しときましょう"
	},
	{
		"id": 125,
		"start": 449.949,
		"end": 451.784,
		"text": "待って！ 待ってください"
	},
	{
		"id": 126,
		"start": 451.909,
		"end": 454.328,
		"text": "せめて もう少しだけ<br />生かしておいてください"
	},
	{
		"id": 127,
		"start": 455.538,
		"end": 458.624,
		"text": "私には まだ<br />やるべきことがあるんです"
	},
	{
		"id": 128,
		"start": 459.5,
		"end": 462.837,
		"text": "ふん　私だったら<br />結界くらい破れるけどね"
	},
	{
		"id": 129,
		"start": 463.588,
		"end": 465.256,
		"text": "リッチーだし退治しましょう"
	},
	{
		"id": 130,
		"start": 465.381,
		"end": 466.716,
		"text": "（ウィズ）待ってください！"
	},
	{
		"id": 131,
		"start": 466.841,
		"end": 470.303,
		"text": "待て待て　どうせ今すぐ<br />結界を解いたところで―"
	},
	{
		"id": 132,
		"start": 470.428,
		"end": 472.847,
		"text": "今の俺たちのレベルじゃ<br />魔王は倒せないんだし"
	},
	{
		"id": 133,
		"start": 473.723,
		"end": 475.516,
		"text": "首ちょんぱされるのがオチだ"
	},
	{
		"id": 134,
		"start": 475.641,
		"end": 477.393,
		"text": "ふん 分かったわよ"
	},
	{
		"id": 135,
		"start": 477.518,
		"end": 479.312,
		"text": "あ… ありがとうございます"
	},
	{
		"id": 136,
		"start": 479.437,
		"end": 483.274,
		"text": "（カズマ）<br />でもいいのか？ ベルディアを<br />倒した俺たちに恨みとか…"
	},
	{
		"id": 137,
		"start": 484.025,
		"end": 486.903,
		"text": "ベルディアさんとは<br />特に仲がよかったとか―"
	},
	{
		"id": 138,
		"start": 487.028,
		"end": 489.03,
		"text": "そんなことも<br />なかったですからね"
	},
	{
		"id": 139,
		"start": 492.116,
		"end": 493.618,
		"text": "私が歩いていると…"
	},
	{
		"id": 140,
		"start": 493.743,
		"end": 495.536,
		"text": "（デュラハン）おおっと～！"
	},
	{
		"id": 141,
		"start": 496.204,
		"end": 499.916,
		"text": "ハア ハア<br />フハハッ ハハッ…"
	},
	{
		"id": 142,
		"start": 500.541,
		"end": 502.752,
		"text": "手がすべった～！"
	},
	{
		"id": 143,
		"start": 503.294,
		"end": 504.212,
		"text": "ハハハハッ"
	},
	{
		"id": 144,
		"start": 504.337,
		"end": 507.59,
		"text": "（ウィズ）よく 足元に<br />自分の首を転がしてきて"
	},
	{
		"id": 145,
		"start": 507.715,
		"end": 510.635,
		"text": "スカートの中を<br />のぞこうとする人でした"
	},
	{
		"id": 146,
		"start": 510.801,
		"end": 513.721,
		"text": "（デュラハン）ストラーイク！<br />いろんな意味で ドストラーイク！"
	},
	{
		"id": 147,
		"start": 514.764,
		"end": 516.098,
		"text": "それに―"
	},
	{
		"id": 148,
		"start": 516.39,
		"end": 520.353,
		"text": "今でも心だけは<br />人間のつもりですしね"
	},
	{
		"id": 149,
		"start": 520.811,
		"end": 522.939,
		"text": "いくぞ　オリャッ"
	},
	{
		"id": 150,
		"start": 523.439,
		"end": 526.192,
		"text": "それでは私のスキルを<br />お教えしますね"
	},
	{
		"id": 151,
		"start": 526.984,
		"end": 531.447,
		"text": "以前 私を見逃してくれたことへの<br />せめてもの恩返し…"
	},
	{
		"id": 152,
		"start": 531.572,
		"end": 532.698,
		"text": "どうした？"
	},
	{
		"id": 153,
		"start": 532.823,
		"end": 533.95,
		"text": "あの…"
	},
	{
		"id": 154,
		"start": 534.075,
		"end": 538.079,
		"text": "私のスキルは相手がいないと<br />使えないものばかりでして"
	},
	{
		"id": 155,
		"start": 538.204,
		"end": 540.706,
		"text": "アクア 悪いけど頼めないか？"
	},
	{
		"id": 156,
		"start": 541.249,
		"end": 546.045,
		"text": "ほう？ アンデッドが この私に<br />何のスキルを使おうっての？"
	},
	{
		"id": 157,
		"start": 546.17,
		"end": 547.922,
		"text": "そ… その"
	},
	{
		"id": 158,
		"start": 548.047,
		"end": 550.132,
		"text": "ドレインタッチなんて<br />どうでしょう？"
	},
	{
		"id": 159,
		"start": 550.258,
		"end": 552.51,
		"text": "相手の体力や魔力を<br />吸い取ったり―"
	},
	{
		"id": 160,
		"start": 552.635,
		"end": 555.555,
		"text": "逆に相手に分け与えたりできる<br />スキルです"
	},
	{
		"id": 161,
		"start": 555.68,
		"end": 557.89,
		"text": "なるほど<br />使い方によっては―"
	},
	{
		"id": 162,
		"start": 558.015,
		"end": 561.561,
		"text": "俺たちのパーティーの火力不足を<br />補うことができるかもな"
	},
	{
		"id": 163,
		"start": 561.686,
		"end": 565.398,
		"text": "も… もちろん ほんの<br />ちょっぴりしか吸いませんので"
	},
	{
		"id": 164,
		"start": 565.523,
		"end": 566.899,
		"text": "（アクア）ふーん"
	},
	{
		"id": 165,
		"start": 568.985,
		"end": 571.362,
		"text": "いいわよ<br />いくらでも吸ってちょうだい"
	},
	{
		"id": 166,
		"start": 571.779,
		"end": 573.239,
		"text": "さあ どうぞ"
	},
	{
		"id": 167,
		"start": 574.24,
		"end": 578.369,
		"text": "（カズマ）どちらがリッチーで<br />どちらが女神だか分からなくなるな"
	},
	{
		"id": 168,
		"start": 578.494,
		"end": 581.789,
		"text": "で… では 失礼します"
	},
	{
		"id": 169,
		"start": 585.668,
		"end": 587.044,
		"text": "あれ？　あ… あれ？"
	},
	{
		"id": 170,
		"start": 587.169,
		"end": 588.838,
		"text": "ほらほら どうしたの？"
	},
	{
		"id": 171,
		"start": 588.963,
		"end": 591.173,
		"text": "私から魔力や体力を<br />吸うんじゃないの？"
	},
	{
		"id": 172,
		"start": 591.299,
		"end": 593.467,
		"text": "あ… あれ～？"
	},
	{
		"id": 173,
		"start": 593.843,
		"end": 594.677,
		"text": "（アクア）痛い！"
	},
	{
		"id": 174,
		"start": 594.802,
		"end": 599.181,
		"text": "ちょっとカズマ 邪魔しないでよ<br />これはリッチーと女神の戦いなのよ"
	},
	{
		"id": 175,
		"start": 599.307,
		"end": 601.392,
		"text": "そうやすやすと吸われて<br />たまるもんですか！"
	},
	{
		"id": 176,
		"start": 601.517,
		"end": 604.437,
		"text": "いいや 話進まんから<br />吸わせてやれよ"
	},
	{
		"id": 177,
		"start": 607.732,
		"end": 608.858,
		"text": "（カズマ）…っし"
	},
	{
		"id": 178,
		"start": 626.208,
		"end": 627.418,
		"text": "習得完了"
	},
	{
		"id": 179,
		"start": 627.543,
		"end": 629.754,
		"text": "（ウィズ）あ… あの アクア様？"
	},
	{
		"id": 180,
		"start": 630.254,
		"end": 633.257,
		"text": "もう大丈夫ですよ<br />手を離していただいても"
	},
	{
		"id": 181,
		"start": 633.382,
		"end": 637.219,
		"text": "…というかアクア様に触れていると<br />なんだか手がピリピリするので―"
	},
	{
		"id": 182,
		"start": 637.345,
		"end": 639.68,
		"text": "そろそろ離してほしいのですが"
	},
	{
		"id": 183,
		"start": 639.889,
		"end": 640.806,
		"text": "（アクア）ウフッ"
	},
	{
		"id": 184,
		"start": 641.39,
		"end": 642.975,
		"text": "（ウィズ）ア… アクア様？"
	},
	{
		"id": 185,
		"start": 643.643,
		"end": 646.145,
		"text": "あの 手が熱くなってきたんですが"
	},
	{
		"id": 186,
		"start": 646.27,
		"end": 649.982,
		"text": "アクア様 消えちゃう消えちゃう<br />私 消えちゃいます！"
	},
	{
		"id": 187,
		"start": 650.358,
		"end": 651.192,
		"text": "（アクア）痛い！"
	},
	{
		"id": 188,
		"start": 651.317,
		"end": 653.527,
		"text": "どさくさに紛れて何してんだ"
	},
	{
		"id": 189,
		"start": 653.653,
		"end": 656.28,
		"text": "うう… ひどいです"
	},
	{
		"id": 190,
		"start": 656.405,
		"end": 660.493,
		"text": "（カズマ）ウィズが なんだか<br />薄くなってるのは気のせいだろうか"
	},
	{
		"id": 191,
		"start": 660.826,
		"end": 662.286,
		"text": "（不動産屋）ごめんください"
	},
	{
		"id": 192,
		"start": 663.037,
		"end": 665.122,
		"text": "ウィズさんは<br />いらっしゃいますか？"
	},
	{
		"id": 193,
		"start": 673.547,
		"end": 674.548,
		"text": "ここか"
	},
	{
		"id": 194,
		"start": 674.673,
		"end": 677.676,
		"text": "悪くないわね<br />ええ 悪くないわ"
	},
	{
		"id": 195,
		"start": 677.968,
		"end": 680.137,
		"text": "この私が住むのに<br />ふさわしいんじゃないかしら"
	},
	{
		"id": 196,
		"start": 680.763,
		"end": 684.35,
		"text": "（めぐみん）元は とある貴族の<br />別荘だったらしいですね"
	},
	{
		"id": 197,
		"start": 684.475,
		"end": 687.853,
		"text": "（ダクネス）<br />しかし除霊の報酬として<br />ここに住んでいいとは―"
	},
	{
		"id": 198,
		"start": 687.978,
		"end": 689.438,
		"text": "太っ腹な大家さんだな"
	},
	{
		"id": 199,
		"start": 689.939,
		"end": 693.442,
		"text": "ウィズは街では高名な魔法使いで<br />通ってるらしくてな"
	},
	{
		"id": 200,
		"start": 694.402,
		"end": 697.446,
		"text": "それで よく この手の案件が<br />持ち込まれるんだそうだ"
	},
	{
		"id": 201,
		"start": 698.072,
		"end": 701.2,
		"text": "これで安心して冬を越すことが<br />できるってもんよ"
	},
	{
		"id": 202,
		"start": 701.325,
		"end": 705.246,
		"text": "災い転じて福と成すとは<br />よく言ったもんね　さっすが私！"
	},
	{
		"id": 203,
		"start": 705.371,
		"end": 706.747,
		"text": "でも大家さんが言うには―"
	},
	{
		"id": 204,
		"start": 706.872,
		"end": 710.543,
		"text": "はらっても はらっても<br />すぐにまた霊が現れるらしいぞ"
	},
	{
		"id": 205,
		"start": 710.668,
		"end": 711.836,
		"text": "任せなさいよ"
	},
	{
		"id": 206,
		"start": 711.961,
		"end": 714.63,
		"text": "何せ私は 女神にして<br />アークプリースト"
	},
	{
		"id": 207,
		"start": 714.755,
		"end": 717.341,
		"text": "言ってみれば 対アンデッドの<br />エキスパートよ"
	},
	{
		"id": 208,
		"start": 719.427,
		"end": 720.344,
		"text": "んっ"
	},
	{
		"id": 209,
		"start": 720.469,
		"end": 722.263,
		"text": "（３人）おお～"
	},
	{
		"id": 210,
		"start": 722.388,
		"end": 724.89,
		"text": "（アクア）見える　見えるわ"
	},
	{
		"id": 211,
		"start": 725.015,
		"end": 726.058,
		"text": "この屋敷には―"
	},
	{
		"id": 212,
		"start": 726.183,
		"end": 729.854,
		"text": "貴族が遊び半分で手を出した<br />メイドとのあいだに出来た子ども"
	},
	{
		"id": 213,
		"start": 729.979,
		"end": 732.565,
		"text": "その隠し子が<br />幽閉されていたようね"
	},
	{
		"id": 214,
		"start": 732.69,
		"end": 735.151,
		"text": "もともと体の弱かった<br />貴族の男は病死"
	},
	{
		"id": 215,
		"start": 735.818,
		"end": 738.32,
		"text": "隠し子の母親のメイドも<br />行方知れず"
	},
	{
		"id": 216,
		"start": 738.446,
		"end": 740.364,
		"text": "この屋敷に<br />一人残された少女は―"
	},
	{
		"id": 217,
		"start": 740.489,
		"end": 742.908,
		"text": "やがて 若くして<br />父親と同じ病に伏して…"
	},
	{
		"id": 218,
		"start": 743.033,
		"end": 746.912,
		"text": "なんで そんな よけいな設定まで<br />分かるんだってツッコみたいんだが"
	},
	{
		"id": 219,
		"start": 747.037,
		"end": 750.166,
		"text": "（フクロウの鳴き声）"
	},
	{
		"id": 220,
		"start": 751.459,
		"end": 752.668,
		"text": "フウ"
	},
	{
		"id": 221,
		"start": 753.586,
		"end": 755.504,
		"text": "これで ひととおり<br />掃除も終わったな"
	},
	{
		"id": 222,
		"start": 755.629,
		"end": 758.674,
		"text": "部屋割りも決めたし<br />荷物も運び込んだし―"
	},
	{
		"id": 223,
		"start": 758.799,
		"end": 760.426,
		"text": "あとは夜を待つだけだ"
	},
	{
		"id": 224,
		"start": 760.551,
		"end": 762.178,
		"text": "（カズマ）さすがに ちょっと<br />ホコリっぽいな"
	},
	{
		"id": 225,
		"start": 762.303,
		"end": 764.513,
		"text": "（アクア）少女の名は<br />アンナ･フィランテ･エステロイド"
	},
	{
		"id": 226,
		"start": 764.638,
		"end": 769.185,
		"text": "好きな物は ぬいぐるみ人形<br />そして冒険者たちの冒険話"
	},
	{
		"id": 227,
		"start": 769.768,
		"end": 772.48,
		"text": "でも安心して<br />この霊は悪い子じゃないわ"
	},
	{
		"id": 228,
		"start": 772.605,
		"end": 774.273,
		"text": "おっと<br />でも 子どもながらに―"
	},
	{
		"id": 229,
		"start": 774.398,
		"end": 776.817,
		"text": "ちょっぴり大人ぶったことが<br />好きなようね"
	},
	{
		"id": 230,
		"start": 776.942,
		"end": 778.194,
		"text": "甘いお酒を飲んだりしてた…"
	},
	{
		"id": 231,
		"start": 779.528,
		"end": 782.198,
		"text": "んじゃ これから<br />自由行動ってことで"
	},
	{
		"id": 232,
		"start": 782.323,
		"end": 784.492,
		"text": "悪霊が出たら<br />すぐに連絡すること"
	},
	{
		"id": 233,
		"start": 785.493,
		"end": 786.619,
		"text": "解散"
	},
	{
		"id": 234,
		"start": 789.33,
		"end": 790.998,
		"text": "ハア～"
	},
	{
		"id": 235,
		"start": 791.874,
		"end": 795.628,
		"text": "いいなあ 自分の部屋があるって<br />すばらしいなあ"
	},
	{
		"id": 236,
		"start": 795.753,
		"end": 798.714,
		"text": "（カズマ）<br />朝までには悪霊のたぐいも<br />出ていってくれるだろう"
	},
	{
		"id": 237,
		"start": 798.839,
		"end": 800.049,
		"text": "アクアのことだ"
	},
	{
		"id": 238,
		"start": 800.174,
		"end": 802.843,
		"text": "自分の家が悪霊なんぞに<br />好き勝手にやられるのを―"
	},
	{
		"id": 239,
		"start": 802.968,
		"end": 804.72,
		"text": "放置しておくような<br />やつではない"
	},
	{
		"id": 240,
		"start": 805.221,
		"end": 808.182,
		"text": "一応 アークプリーストにして<br />女神だしな"
	},
	{
		"id": 241,
		"start": 808.849,
		"end": 810.518,
		"text": "（アクアの叫び声）"
	},
	{
		"id": 242,
		"start": 810.643,
		"end": 811.477,
		"text": "何だ？"
	},
	{
		"id": 243,
		"start": 812.895,
		"end": 815.481,
		"text": "どうした？ おい アクア<br />何があった 大丈夫か？"
	},
	{
		"id": 244,
		"start": 815.606,
		"end": 817.525,
		"text": "（アクア）うっ うう…"
	},
	{
		"id": 245,
		"start": 817.775,
		"end": 819.485,
		"text": "カ… カズマ"
	},
	{
		"id": 246,
		"start": 820.027,
		"end": 820.861,
		"text": "おい"
	},
	{
		"id": 247,
		"start": 821.862,
		"end": 825.241,
		"text": "これは大事に取っておいた<br />すごく高いお酒なのよ"
	},
	{
		"id": 248,
		"start": 825.366,
		"end": 828.077,
		"text": "お風呂から上がったら ゆっくり<br />チビチビ大事に飲もうと―"
	},
	{
		"id": 249,
		"start": 828.202,
		"end": 829.828,
		"text": "楽しみにしてたのに"
	},
	{
		"id": 250,
		"start": 829.954,
		"end": 834.458,
		"text": "それが 私が部屋に帰ってきたら<br />見てのとおり空(から)だったのよ～！"
	},
	{
		"id": 251,
		"start": 834.583,
		"end": 836.627,
		"text": "そうか　じゃあおやすみ<br />またあしたな"
	},
	{
		"id": 252,
		"start": 836.752,
		"end": 838.128,
		"text": "（アクア）これは悪霊の仕業よ"
	},
	{
		"id": 253,
		"start": 838.254,
		"end": 843.133,
		"text": "ちょっと私 屋敷の中を探索して<br />目につく霊をしばき回してくるわ！"
	},
	{
		"id": 254,
		"start": 843.259,
		"end": 845.636,
		"text": "オリャアアーッ！"
	},
	{
		"id": 255,
		"start": 845.761,
		"end": 849.431,
		"text": "出てこいやあーっ！"
	},
	{
		"id": 256,
		"start": 849.557,
		"end": 850.641,
		"text": "（あくび）"
	},
	{
		"id": 257,
		"start": 850.766,
		"end": 852.81,
		"text": "（アクア）ターンアンデッド！<br />ターンアンデッド！"
	},
	{
		"id": 258,
		"start": 852.935,
		"end": 854.687,
		"text": "ターンアンデッド！<br />ターンアンデッド！"
	},
	{
		"id": 259,
		"start": 854.812,
		"end": 855.729,
		"text": "ターンアンデッド！"
	},
	{
		"id": 260,
		"start": 855.854,
		"end": 856.897,
		"text": "花鳥風月"
	},
	{
		"id": 261,
		"start": 857.022,
		"end": 858.649,
		"text": "ターンアンデッド！<br />ターンアンデッド！"
	},
	{
		"id": 262,
		"start": 858.774,
		"end": 860.651,
		"text": "ターンアンデッド！"
	},
	{
		"id": 263,
		"start": 860.776,
		"end": 863.904,
		"text": "（鳴き声）"
	},
	{
		"id": 264,
		"start": 867.157,
		"end": 868.409,
		"text": "（カズマ）ん…"
	},
	{
		"id": 265,
		"start": 870.494,
		"end": 872.121,
		"text": "（カズマ）トイレ 行きたい"
	},
	{
		"id": 266,
		"start": 872.663,
		"end": 873.914,
		"text": "（物音）<br />（カズマ）ん？"
	},
	{
		"id": 267,
		"start": 877.126,
		"end": 878.043,
		"text": "（カズマ）怖っ！"
	},
	{
		"id": 268,
		"start": 878.544,
		"end": 881.213,
		"text": "は？ あんな人形<br />この部屋にあったっけ？"
	},
	{
		"id": 269,
		"start": 881.672,
		"end": 883.299,
		"text": "ないわ～！"
	},
	{
		"id": 270,
		"start": 883.424,
		"end": 887.261,
		"text": "こんなベタな展開 ないわ～！"
	},
	{
		"id": 271,
		"start": 896.27,
		"end": 899.481,
		"text": "ヒッ！　う… くうっ"
	},
	{
		"id": 272,
		"start": 900.816,
		"end": 901.692,
		"text": "うっ"
	},
	{
		"id": 273,
		"start": 901.817,
		"end": 903.444,
		"text": "はああっ！"
	},
	{
		"id": 274,
		"start": 903.861,
		"end": 906.447,
		"text": "（叫び声）"
	},
	{
		"id": 275,
		"start": 906.906,
		"end": 909.45,
		"text": "（カズマ）アクア！<br />アクア様あーっ！"
	},
	{
		"id": 276,
		"start": 909.95,
		"end": 911.952,
		"text": "怖い　怖い　超怖い！<br />何 これ？"
	},
	{
		"id": 277,
		"start": 912.077,
		"end": 913.704,
		"text": "（人形の笑い声）"
	},
	{
		"id": 278,
		"start": 914.163,
		"end": 915.623,
		"text": "（カズマ）アクアーッ！"
	},
	{
		"id": 279,
		"start": 915.748,
		"end": 916.582,
		"text": "助け…"
	},
	{
		"id": 280,
		"start": 918.083,
		"end": 919.335,
		"text": "ギイヤーッ！"
	},
	{
		"id": 281,
		"start": 919.46,
		"end": 921.921,
		"text": "ワアアーッ！"
	},
	{
		"id": 282,
		"start": 923.088,
		"end": 926.759,
		"text": "お… 脅かすなよ めぐみん<br />危うく漏らすところだったぞ"
	},
	{
		"id": 283,
		"start": 926.884,
		"end": 928.594,
		"text": "それは こっちのセリフです！"
	},
	{
		"id": 284,
		"start": 928.719,
		"end": 931.889,
		"text": "なぜカズマが この部屋に<br />飛び込んでくるんですか？"
	},
	{
		"id": 285,
		"start": 932.014,
		"end": 934.224,
		"text": "アクアが戻ってきたかと<br />思ったのに"
	},
	{
		"id": 286,
		"start": 934.35,
		"end": 936.518,
		"text": "めぐみんこそ<br />なんでアクアの部屋に？"
	},
	{
		"id": 287,
		"start": 936.644,
		"end": 937.478,
		"text": "（めぐみん）うっ"
	},
	{
		"id": 288,
		"start": 937.895,
		"end": 941.774,
		"text": "いや… 人形があちこちで<br />動いておりまして"
	},
	{
		"id": 289,
		"start": 941.899,
		"end": 946.236,
		"text": "その アクアに身の安全を<br />守ってもらうのと…"
	},
	{
		"id": 290,
		"start": 947.363,
		"end": 948.489,
		"text": "一緒に―"
	},
	{
		"id": 291,
		"start": 948.614,
		"end": 951.492,
		"text": "トイレにと… 思いまして"
	},
	{
		"id": 292,
		"start": 951.617,
		"end": 952.952,
		"text": "（カズマ）お前もか"
	},
	{
		"id": 293,
		"start": 953.452,
		"end": 957.081,
		"text": "（カズマ）<br />多分アクアは まだ屋敷内の除霊に<br />行ってるんじゃないか"
	},
	{
		"id": 294,
		"start": 957.206,
		"end": 958.374,
		"text": "そうですか"
	},
	{
		"id": 295,
		"start": 958.499,
		"end": 960.584,
		"text": "では きっと<br />ダクネスも一緒ですね"
	},
	{
		"id": 296,
		"start": 960.709,
		"end": 961.961,
		"text": "ハア…"
	},
	{
		"id": 297,
		"start": 962.086,
		"end": 964.797,
		"text": "ああ あいつ あれで<br />クルセイダーだったな"
	},
	{
		"id": 298,
		"start": 964.922,
		"end": 965.965,
		"text": "ううっ"
	},
	{
		"id": 299,
		"start": 966.507,
		"end": 967.883,
		"text": "う… うう"
	},
	{
		"id": 300,
		"start": 968.008,
		"end": 969.468,
		"text": "なあ めぐみん"
	},
	{
		"id": 301,
		"start": 969.802,
		"end": 972.221,
		"text": "しばらく あっち向いて<br />耳塞いでてくれ"
	},
	{
		"id": 302,
		"start": 972.346,
		"end": 974.223,
		"text": "ちょっと失礼してベランダから…"
	},
	{
		"id": 303,
		"start": 974.932,
		"end": 977.601,
		"text": "おい 何してんだよ？<br />離してくれ"
	},
	{
		"id": 304,
		"start": 977.851,
		"end": 979.77,
		"text": "さもないと 俺のズボンと<br />この部屋のじゅうたんが―"
	},
	{
		"id": 305,
		"start": 979.895,
		"end": 981.23,
		"text": "大変なことになる"
	},
	{
		"id": 306,
		"start": 981.772,
		"end": 983.107,
		"text": "行かせませんよ"
	},
	{
		"id": 307,
		"start": 983.232,
		"end": 985.734,
		"text": "何 一人でスッキリしようと<br />してるんですか"
	},
	{
		"id": 308,
		"start": 986.235,
		"end": 988.862,
		"text": "私たちは仲間じゃないですか"
	},
	{
		"id": 309,
		"start": 988.988,
		"end": 990.99,
		"text": "トイレだろうと どこだろうと―"
	},
	{
		"id": 310,
		"start": 991.907,
		"end": 993.117,
		"text": "逝くときは 一緒です"
	},
	{
		"id": 311,
		"start": 993.242,
		"end": 997.454,
		"text": "ええい 離せ！ こんなときだけ<br />仲間の絆を主張するな"
	},
	{
		"id": 312,
		"start": 997.579,
		"end": 1000.582,
		"text": "お前“紅魔族(こうまぞく)はトイレ行かない”<br />とか言ってたじゃねえか"
	},
	{
		"id": 313,
		"start": 1000.708,
		"end": 1003.377,
		"text": "なんなら そこに空いた酒瓶が<br />転がってるから！"
	},
	{
		"id": 314,
		"start": 1003.919,
		"end": 1006.255,
		"text": "今 とんでもないことを<br />口走りましたね！"
	},
	{
		"id": 315,
		"start": 1006.38,
		"end": 1008.882,
		"text": "その空いた酒瓶で<br />私に何をしろと？"
	},
	{
		"id": 316,
		"start": 1009.008,
		"end": 1012.136,
		"text": "させませんよ  私もカズマが<br />用を足そうとしてるところを―"
	},
	{
		"id": 317,
		"start": 1012.553,
		"end": 1014.43,
		"text": "後ろから<br />揺らしてやるぐらいは―"
	},
	{
		"id": 318,
		"start": 1014.555,
		"end": 1016.432,
		"text": "できますか… ら…"
	},
	{
		"id": 319,
		"start": 1018.851,
		"end": 1019.685,
		"text": "ん？"
	},
	{
		"id": 320,
		"start": 1025.024,
		"end": 1027.526,
		"text": "（２人の叫び声）"
	},
	{
		"id": 321,
		"start": 1029.695,
		"end": 1033.782,
		"text": "（荒い息）"
	},
	{
		"id": 322,
		"start": 1033.907,
		"end": 1036.16,
		"text": "（めぐみん）うう…<br />カズマ いますか？"
	},
	{
		"id": 323,
		"start": 1036.618,
		"end": 1037.661,
		"text": "いるよ"
	},
	{
		"id": 324,
		"start": 1038.912,
		"end": 1040.497,
		"text": "（めぐみん）本当にいますか？"
	},
	{
		"id": 325,
		"start": 1040.622,
		"end": 1041.457,
		"text": "いるって"
	},
	{
		"id": 326,
		"start": 1041.582,
		"end": 1044.251,
		"text": "（めぐみん）本当に本当に<br />ちゃんと いてくれますか？"
	},
	{
		"id": 327,
		"start": 1044.376,
		"end": 1045.794,
		"text": "いるって！"
	},
	{
		"id": 328,
		"start": 1045.919,
		"end": 1048.63,
		"text": "ちゃんといるし 置いてったり<br />しないから早くしてくれ"
	},
	{
		"id": 329,
		"start": 1049.381,
		"end": 1050.549,
		"text": "（めぐみん）あの…"
	},
	{
		"id": 330,
		"start": 1050.674,
		"end": 1052.843,
		"text": "さすがに ちょっと<br />恥ずかしいので―"
	},
	{
		"id": 331,
		"start": 1052.968,
		"end": 1055.387,
		"text": "大きめの声で歌でも<br />歌ってくれません？"
	},
	{
		"id": 332,
		"start": 1055.512,
		"end": 1056.805,
		"text": "何が悲しくて―"
	},
	{
		"id": 333,
		"start": 1056.93,
		"end": 1060.1,
		"text": "こんな夜中にトイレの前で<br />歌わなきゃならないんだよ"
	},
	{
		"id": 334,
		"start": 1060.225,
		"end": 1061.769,
		"text": "（物音）<br />（カズマ）うっ"
	},
	{
		"id": 335,
		"start": 1064.271,
		"end": 1065.606,
		"text": "（カズマ）ヒイイッ！"
	},
	{
		"id": 336,
		"start": 1065.731,
		"end": 1066.94,
		"text": "（ノック）<br />（カズマ）おい めぐみん　急げ！"
	},
	{
		"id": 337,
		"start": 1067.066,
		"end": 1069.151,
		"text": "（めぐみん）そんなに<br />急がせないでくださいよ！"
	},
	{
		"id": 338,
		"start": 1069.276,
		"end": 1071.361,
		"text": "カズマもピンチなのは<br />分かってますから"
	},
	{
		"id": 339,
		"start": 1071.487,
		"end": 1073.614,
		"text": "（カズマ）確かに俺とお前の<br />膀胱(ぼうこう)もピンチだが―"
	},
	{
		"id": 340,
		"start": 1073.739,
		"end": 1075.783,
		"text": "もっとヤバいものが<br />近づいてきてんだよ！"
	},
	{
		"id": 341,
		"start": 1075.908,
		"end": 1079.244,
		"text": "そんなに せかされたら<br />出るものも出ませんよ"
	},
	{
		"id": 342,
		"start": 1079.369,
		"end": 1080.788,
		"text": "あああーっ ダメだ"
	},
	{
		"id": 343,
		"start": 1081.246,
		"end": 1083.457,
		"text": "もう待てねえ！"
	},
	{
		"id": 344,
		"start": 1084.833,
		"end": 1087.586,
		"text": "アアアーッ！"
	},
	{
		"id": 345,
		"start": 1087.711,
		"end": 1088.837,
		"text": "急げ！"
	},
	{
		"id": 346,
		"start": 1091.465,
		"end": 1093.592,
		"text": "（めぐみん）<br />ここまで非常識な人とは！"
	},
	{
		"id": 347,
		"start": 1093.717,
		"end": 1096.512,
		"text": "お前は半分でも出せたんだから<br />いいだろ！"
	},
	{
		"id": 348,
		"start": 1096.637,
		"end": 1099.848,
		"text": "何の話ですか？ 紅魔族は<br />トイレになんか行きませんよ"
	},
	{
		"id": 349,
		"start": 1100.265,
		"end": 1104.228,
		"text": "今さら それ言うか？<br />俺のほうは もう限界だってのに"
	},
	{
		"id": 350,
		"start": 1104.353,
		"end": 1106.897,
		"text": "そんなこともあろうかと<br />持ってきました"
	},
	{
		"id": 351,
		"start": 1107.189,
		"end": 1108.357,
		"text": "使ってください"
	},
	{
		"id": 352,
		"start": 1108.482,
		"end": 1109.858,
		"text": "要らんわ！"
	},
	{
		"id": 353,
		"start": 1109.983,
		"end": 1113.862,
		"text": "（鳴き声）"
	},
	{
		"id": 354,
		"start": 1115.739,
		"end": 1118.117,
		"text": "（人形たちの声）"
	},
	{
		"id": 355,
		"start": 1118.659,
		"end": 1120.369,
		"text": "もう限界だ"
	},
	{
		"id": 356,
		"start": 1120.661,
		"end": 1122.329,
		"text": "このまま いっそ花瓶に…"
	},
	{
		"id": 357,
		"start": 1122.454,
		"end": 1126.333,
		"text": "（めぐみん）<br />黒より黒く 闇より暗き漆黒に―"
	},
	{
		"id": 358,
		"start": 1126.458,
		"end": 1128.919,
		"text": "我が真紅(しんく)の混交を望みたもう"
	},
	{
		"id": 359,
		"start": 1129.044,
		"end": 1131.588,
		"text": "何 唱えてんだ<br />この屋敷ごと吹き飛ばす気か！"
	},
	{
		"id": 360,
		"start": 1131.713,
		"end": 1132.923,
		"text": "（物音）"
	},
	{
		"id": 361,
		"start": 1133.549,
		"end": 1136.927,
		"text": "（ドアに ぶつかる音）"
	},
	{
		"id": 362,
		"start": 1137.052,
		"end": 1138.929,
		"text": "クソッ やるしかないか"
	},
	{
		"id": 363,
		"start": 1139.471,
		"end": 1141.89,
		"text": "めぐみん<br />ドアを開けたら お前は走れ"
	},
	{
		"id": 364,
		"start": 1142.015,
		"end": 1143.559,
		"text": "俺は覚えたての<br />ドレインタッチで―"
	},
	{
		"id": 365,
		"start": 1143.684,
		"end": 1146.562,
		"text": "人形から魔力の１つでも<br />吸い取ってやる！"
	},
	{
		"id": 366,
		"start": 1147.688,
		"end": 1150.274,
		"text": "オラア かかってこいやーっ！"
	},
	{
		"id": 367,
		"start": 1150.399,
		"end": 1151.441,
		"text": "この悪霊が"
	},
	{
		"id": 368,
		"start": 1151.567,
		"end": 1154.069,
		"text": "あとで うちの狂犬女神<br />けしかけてやんぞ コラーッ！"
	},
	{
		"id": 369,
		"start": 1154.57,
		"end": 1155.821,
		"text": "（ぶつかる音）"
	},
	{
		"id": 370,
		"start": 1157.197,
		"end": 1158.073,
		"text": "（２人）あ？"
	},
	{
		"id": 371,
		"start": 1158.615,
		"end": 1160.325,
		"text": "（アクア）あ… ああ…"
	},
	{
		"id": 372,
		"start": 1160.45,
		"end": 1163.287,
		"text": "お… おい アクア<br />大丈夫か？"
	},
	{
		"id": 373,
		"start": 1166.707,
		"end": 1170.711,
		"text": "（ルナ）<br />悪霊を退治したということで<br />臨時報酬が出てますよ"
	},
	{
		"id": 374,
		"start": 1170.836,
		"end": 1173.881,
		"text": "ギルドでも討伐クエストを<br />設定したんですが―"
	},
	{
		"id": 375,
		"start": 1174.006,
		"end": 1177.342,
		"text": "いくら退治しても すぐに<br />新しい悪霊が住み着いて―"
	},
	{
		"id": 376,
		"start": 1177.467,
		"end": 1178.802,
		"text": "困っていたんです"
	},
	{
		"id": 377,
		"start": 1178.927,
		"end": 1182.598,
		"text": "らしいですね　なんでそんなに<br />悪霊が集まってきてたんだろ？"
	},
	{
		"id": 378,
		"start": 1182.723,
		"end": 1184.516,
		"text": "それなんですが―"
	},
	{
		"id": 379,
		"start": 1184.641,
		"end": 1188.187,
		"text": "あの屋敷の近くに<br />共同墓地があるじゃないですか"
	},
	{
		"id": 380,
		"start": 1188.437,
		"end": 1190.272,
		"text": "共同墓地って…"
	},
	{
		"id": 381,
		"start": 1190.397,
		"end": 1192.482,
		"text": "ウィズと初めて会ったとこだな"
	},
	{
		"id": 382,
		"start": 1192.608,
		"end": 1197.237,
		"text": "（ルナ）誰かが いたずらか何かで<br />巨大な結界を張ったようなんですよ"
	},
	{
		"id": 383,
		"start": 1197.905,
		"end": 1202.242,
		"text": "それで行(ゆ)き場(ば)をなくした霊が<br />あの空き家に住み着いてたみたいで"
	},
	{
		"id": 384,
		"start": 1202.367,
		"end": 1203.66,
		"text": "（カズマ）うっ…"
	},
	{
		"id": 385,
		"start": 1203.952,
		"end": 1205.162,
		"text": "ちょっと失礼"
	},
	{
		"id": 386,
		"start": 1207.581,
		"end": 1208.498,
		"text": "（ルナ）うん？"
	},
	{
		"id": 387,
		"start": 1208.874,
		"end": 1211.501,
		"text": "（カズマ）<br />おい 心当たりがあるな？<br />言え"
	},
	{
		"id": 388,
		"start": 1212.169,
		"end": 1213.003,
		"text": "はい"
	},
	{
		"id": 389,
		"start": 1213.128,
		"end": 1217.174,
		"text": "以前ウィズの代わりに墓地の除霊を<br />引き受けたじゃないですか"
	},
	{
		"id": 390,
		"start": 1217.299,
		"end": 1218.967,
		"text": "でも しょっちゅう<br />墓地まで行くのって―"
	},
	{
		"id": 391,
		"start": 1219.092,
		"end": 1220.594,
		"text": "面倒くさいじゃないですか"
	},
	{
		"id": 392,
		"start": 1220.719,
		"end": 1223.263,
		"text": "それで いっそ墓地に<br />霊の住み場所をなくしてやれば―"
	},
	{
		"id": 393,
		"start": 1223.764,
		"end": 1227.184,
		"text": "そのうち適当に散って<br />いなくなるかなって思って"
	},
	{
		"id": 394,
		"start": 1227.476,
		"end": 1229.77,
		"text": "やり… ま… した"
	},
	{
		"id": 395,
		"start": 1230.771,
		"end": 1235.025,
		"text": "（カズマ）なんというマッチポンプ<br />これは どう考えてもダメだろ"
	},
	{
		"id": 396,
		"start": 1235.734,
		"end": 1239.571,
		"text": "（カズマ）<br />ギルドからの臨時報酬は<br />受け取らない　いいな？"
	},
	{
		"id": 397,
		"start": 1239.696,
		"end": 1240.53,
		"text": "（アクア）はい…"
	},
	{
		"id": 398,
		"start": 1244.326,
		"end": 1245.41,
		"text": "んっ"
	},
	{
		"id": 399,
		"start": 1245.911,
		"end": 1250.832,
		"text": "てっきりアクアの妄想かと思ったら<br />まさか本当の話だったとはな"
	},
	{
		"id": 400,
		"start": 1251.083,
		"end": 1252.084,
		"text": "（ウィズ）カズマさん"
	},
	{
		"id": 401,
		"start": 1252.501,
		"end": 1255.212,
		"text": "こんにちは お墓の掃除ですか？"
	},
	{
		"id": 402,
		"start": 1255.337,
		"end": 1258.966,
		"text": "よかったですね<br />ここに住んでいいことになって"
	},
	{
		"id": 403,
		"start": 1259.508,
		"end": 1262.594,
		"text": "ホント　大家さんがいい人で<br />助かったよ"
	},
	{
		"id": 404,
		"start": 1262.719,
		"end": 1264.429,
		"text": "ウィズにも迷惑かけちまったな"
	},
	{
		"id": 405,
		"start": 1264.554,
		"end": 1268.475,
		"text": "（ウィズ）<br />いえいえ　むしろ私としては<br />これでよかったと思ってますから"
	},
	{
		"id": 406,
		"start": 1269.434,
		"end": 1272.187,
		"text": "これなら きっと<br />寂しくないでしょうし"
	},
	{
		"id": 407,
		"start": 1272.312,
		"end": 1273.272,
		"text": "フフッ"
	},
	{
		"id": 408,
		"start": 1274.314,
		"end": 1275.44,
		"text": "…だな"
	},
	{
		"id": 409,
		"start": 1275.857,
		"end": 1278.735,
		"text": "頼むから もう<br />いたずらしないでくれよ"
	},
	{
		"id": 410,
		"start": 1278.86,
		"end": 1282.281,
		"text": "（ウィズ）<br />では店番があるので<br />今日は帰りますね"
	},
	{
		"id": 411,
		"start": 1282.406,
		"end": 1284.116,
		"text": "皆さんにも よろしく"
	},
	{
		"id": 412,
		"start": 1284.241,
		"end": 1286.368,
		"text": "（カズマ）ああ<br />来てくれてありがとうな"
	},
	{
		"id": 413,
		"start": 1289.913,
		"end": 1290.998,
		"text": "フウ…"
	},
	{
		"id": 414,
		"start": 1291.456,
		"end": 1295.002,
		"text": "（アクア）カズマ！<br />ごはん出来たから早く来て"
	},
	{
		"id": 415,
		"start": 1295.419,
		"end": 1299.256,
		"text": "早く来ないと せっかくのお昼が<br />冷めちゃうんですけど！"
	},
	{
		"id": 416,
		"start": 1299.381,
		"end": 1301.633,
		"text": "（カズマ）<br />これで ようやく念願の"
	},
	{
		"id": 417,
		"start": 1301.758,
		"end": 1304.845,
		"text": "地に足の着いた生活が<br />手に入ったというわけだ"
	},
	{
		"id": 418,
		"start": 1305.387,
		"end": 1307.764,
		"text": "分かったよ  今 行く！"
	},
	{
		"id": 419,
		"start": 1307.973,
		"end": 1311.143,
		"text": "（ダクネス）アクア<br />暖炉のまきが もうないんだが"
	},
	{
		"id": 420,
		"start": 1311.268,
		"end": 1312.644,
		"text": "（アクア）<br />ああ じゃあ そこの―"
	},
	{
		"id": 421,
		"start": 1312.769,
		"end": 1314.896,
		"text": "カズマのジャージでも<br />くべておいて"
	},
	{
		"id": 422,
		"start": 1315.022,
		"end": 1316.481,
		"text": "（カズマ）やめろお！"
	},
	{
		"id": 423,
		"start": 1316.606,
		"end": 1320.902,
		"text": "だから俺の大事な思い出を<br />燃やそうとすんなーっ！"
	},
	{
		"id": 424,
		"start": 1321.445,
		"end": 1328.16,
		"text": "♪～"
	},
	{
		"id": 425,
		"start": 1403.693,
		"end": 1410.7,
		"text": "～♪"
	}
]