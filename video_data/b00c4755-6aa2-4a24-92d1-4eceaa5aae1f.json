[
	{
		"id": 1,
		"start": 4.587,
		"end": 5.38,
		"text": "（あおい）<br />タイトルは"
	},
	{
		"id": 2,
		"start": 5.505,
		"end": 8.133,
		"text": "「神仏混淆(こんこう)七福陣」で<br />いきたいと思います"
	},
	{
		"id": 3,
		"start": 8.508,
		"end": 9.551,
		"text": "（拍手）"
	},
	{
		"id": 4,
		"start": 9.676,
		"end": 12.595,
		"text": "（あおい）<br />ではでは皆さん<br />準備をお願いします"
	},
	{
		"id": 5,
		"start": 13.054,
		"end": 15.432,
		"text": "（あおい）<br />この ひょうたん屋の<br />ドーナツにかけまして"
	},
	{
		"id": 6,
		"start": 15.932,
		"end": 19.102,
		"text": "（しずか）<br />我らが<br />上山(かみのやま)高校 アニメーション同好会は"
	},
	{
		"id": 7,
		"start": 19.686,
		"end": 22.23,
		"text": "（絵麻(えま)）<br />自主作品制作に努力することを"
	},
	{
		"id": 8,
		"start": 22.772,
		"end": 23.44,
		"text": "（美沙(みさ)）<br />誓いま…"
	},
	{
		"id": 9,
		"start": 24.149,
		"end": 24.816,
		"text": "（みどり）<br />す！"
	},
	{
		"id": 10,
		"start": 25.108,
		"end": 25.775,
		"text": "ドンドン"
	},
	{
		"id": 11,
		"start": 25.9,
		"end": 28.028,
		"text": "（一同）<br />ドーナツ ど～んといこう！"
	},
	{
		"id": 12,
		"start": 36.327,
		"end": 38.705,
		"text": "そんなの さくっと<br />家出ちゃえばいいじゃん"
	},
	{
		"id": 13,
		"start": 38.83,
		"end": 40.04,
		"text": "えっ…"
	},
	{
		"id": 14,
		"start": 40.373,
		"end": 42.876,
		"text": "ずか先輩 簡単に言いますね"
	},
	{
		"id": 15,
		"start": 43.001,
		"end": 45.378,
		"text": "絵麻なら どうにでもなるっしょ"
	},
	{
		"id": 16,
		"start": 45.503,
		"end": 47.005,
		"text": "絵の才能あるんだし"
	},
	{
		"id": 17,
		"start": 47.13,
		"end": 48.506,
		"text": "でも受かるかな？　会社"
	},
	{
		"id": 18,
		"start": 49.674,
		"end": 50.884,
		"text": "（絵麻）あっ…<br />（しずか）そんなこと言ったら―"
	},
	{
		"id": 19,
		"start": 51.009,
		"end": 53.803,
		"text": "私なんて<br />養成所に入れるかどうかだよ"
	},
	{
		"id": 20,
		"start": 53.928,
		"end": 57.599,
		"text": "声優志望なんて<br />ものすごい倍率だしさ"
	},
	{
		"id": 21,
		"start": 64.606,
		"end": 66.024,
		"text": "（マウスのクリック音）<br />（美沙）ああ…"
	},
	{
		"id": 22,
		"start": 69.152,
		"end": 72.489,
		"text": "ずびばせん…<br />暑くて ちょいギブです"
	},
	{
		"id": 23,
		"start": 72.614,
		"end": 75.241,
		"text": "（みどり）みー先輩 私が<br />（美沙）うう…"
	},
	{
		"id": 24,
		"start": 75.366,
		"end": 78.244,
		"text": "へえ～ 東京 今日36℃だって"
	},
	{
		"id": 25,
		"start": 78.369,
		"end": 79.871,
		"text": "こっちはまだマシっすか～"
	},
	{
		"id": 26,
		"start": 79.996,
		"end": 81.289,
		"text": "絶対 向こう行ったら―"
	},
	{
		"id": 27,
		"start": 81.498,
		"end": 83.666,
		"text": "エアコンのある部屋に<br />住もうね 絵麻"
	},
	{
		"id": 28,
		"start": 83.792,
		"end": 84.667,
		"text": "あっ…"
	},
	{
		"id": 29,
		"start": 85.293,
		"end": 86.628,
		"text": "ああ… うん"
	},
	{
		"id": 30,
		"start": 87.003,
		"end": 89.38,
		"text": "何？　まだもめてんの？"
	},
	{
		"id": 31,
		"start": 92.425,
		"end": 96.262,
		"text": "おいちゃんさあ ドーナツって<br />夏向きじゃなくなくない？"
	},
	{
		"id": 32,
		"start": 96.638,
		"end": 99.599,
		"text": "えっ？　ドーナツはオールシーズン<br />オールマイティーの"
	},
	{
		"id": 33,
		"start": 99.724,
		"end": 101.267,
		"text": "オール オーケーだよ"
	},
	{
		"id": 34,
		"start": 101.518,
		"end": 103.978,
		"text": "まさに私にとっては完全食！"
	},
	{
		"id": 35,
		"start": 109.442,
		"end": 111.361,
		"text": "（みどり）動いた！<br />（あおい）動いてるね～"
	},
	{
		"id": 36,
		"start": 111.486,
		"end": 113.488,
		"text": "うん！　今の私が描いたとこ！"
	},
	{
		"id": 37,
		"start": 113.613,
		"end": 116.199,
		"text": "やっぱ動くの見ると<br />ガン上がりますね～！"
	},
	{
		"id": 38,
		"start": 116.324,
		"end": 117.951,
		"text": "でも まだ原画だから"
	},
	{
		"id": 39,
		"start": 118.076,
		"end": 120.787,
		"text": "作業的には<br />やっと半分って感じかな"
	},
	{
		"id": 40,
		"start": 120.912,
		"end": 122.914,
		"text": "いや３分の１… ２割…"
	},
	{
		"id": 41,
		"start": 123.039,
		"end": 126.209,
		"text": "もう 絵麻はなんで<br />そういうこと言うかな～"
	},
	{
		"id": 42,
		"start": 126.334,
		"end": 128.837,
		"text": "え～ だって<br />学園祭に間に合うかな？"
	},
	{
		"id": 43,
		"start": 128.962,
		"end": 130.588,
		"text": "（あおい）もっかい見ていい？<br />（一同）見る！"
	},
	{
		"id": 44,
		"start": 134.342,
		"end": 135.26,
		"text": "（しずか）<br />マジで？"
	},
	{
		"id": 45,
		"start": 135.385,
		"end": 137.512,
		"text": "（絵麻）<br />１年だけ仕送りしてくれるって"
	},
	{
		"id": 46,
		"start": 137.637,
		"end": 140.723,
		"text": "でも その間に<br />ちゃんと生活できるようになれって"
	},
	{
		"id": 47,
		"start": 140.849,
		"end": 144.853,
		"text": "（美沙）<br />ハンストとかやったんですか？<br />無視とか無言とか家庭内暴力とか"
	},
	{
		"id": 48,
		"start": 145.937,
		"end": 149.482,
		"text": "手紙 毎日<br />口だとダメだから"
	},
	{
		"id": 49,
		"start": 149.732,
		"end": 151.151,
		"text": "へえ～！　すごい！"
	},
	{
		"id": 50,
		"start": 151.401,
		"end": 152.235,
		"text": "粘り腰"
	},
	{
		"id": 51,
		"start": 152.36,
		"end": 154.404,
		"text": "私も絶対 東京行きますから"
	},
	{
		"id": 52,
		"start": 154.529,
		"end": 156.281,
		"text": "みーも りーも 待ってるよ"
	},
	{
		"id": 53,
		"start": 156.781,
		"end": 157.657,
		"text": "おいちゃんは？"
	},
	{
		"id": 54,
		"start": 157.782,
		"end": 160.201,
		"text": "こっちの短大出たら<br />東京 来るんだよね？"
	},
	{
		"id": 55,
		"start": 160.326,
		"end": 162.37,
		"text": "ああ… 頑張る"
	},
	{
		"id": 56,
		"start": 167.667,
		"end": 169.627,
		"text": "（美沙）<br />はい もっかい いきま～す"
	},
	{
		"id": 57,
		"start": 170.211,
		"end": 174.924,
		"text": "うう… しゃらくせえ！<br />10人まとめて引きちぎってやる！"
	},
	{
		"id": 58,
		"start": 175.049,
		"end": 177.927,
		"text": "何を～！　神様の分際で！"
	},
	{
		"id": 59,
		"start": 178.052,
		"end": 180.93,
		"text": "挑発に乗っちゃダメ！<br />これはワナよ！"
	},
	{
		"id": 60,
		"start": 181.264,
		"end": 183.975,
		"text": "そうだ ワナに違いなゃ… あっ！"
	},
	{
		"id": 61,
		"start": 184.934,
		"end": 186.811,
		"text": "（美沙）<br />はい もっかい～"
	},
	{
		"id": 62,
		"start": 186.936,
		"end": 188.146,
		"text": "ごめん"
	},
	{
		"id": 63,
		"start": 195.653,
		"end": 198.865,
		"text": "これより<br />アニメーション同好会制作"
	},
	{
		"id": 64,
		"start": 199.532,
		"end": 202.619,
		"text": "「神仏混淆七福陣」を<br />上映いたします"
	},
	{
		"id": 65,
		"start": 203.202,
		"end": 206.539,
		"text": "どうぞ最後まで<br />ごゆっくりご覧ください"
	},
	{
		"id": 66,
		"start": 214.088,
		"end": 216.633,
		"text": "（しずか）<br />時代は西暦３億年"
	},
	{
		"id": 67,
		"start": 217.091,
		"end": 218.551,
		"text": "心を忘れた大地に<br />７人の神様的な人物が現れた"
	},
	{
		"id": 68,
		"start": 218.551,
		"end": 219.344,
		"text": "心を忘れた大地に<br />７人の神様的な人物が現れた"
	},
	{
		"id": 69,
		"start": 218.551,
		"end": 219.344,
		"text": "わあ～"
	},
	{
		"id": 70,
		"start": 219.344,
		"end": 223.723,
		"text": "心を忘れた大地に<br />７人の神様的な人物が現れた"
	},
	{
		"id": 71,
		"start": 239.906,
		"end": 241.324,
		"text": "（あおい）<br />みんな ドーナツ取った？"
	},
	{
		"id": 72,
		"start": 241.491,
		"end": 242.533,
		"text": "（一同）<br />は～い！"
	},
	{
		"id": 73,
		"start": 242.659,
		"end": 246.287,
		"text": "いつか！　絶対！　必ず！<br />なんとしてでも！"
	},
	{
		"id": 74,
		"start": 246.621,
		"end": 247.747,
		"text": "この５人で"
	},
	{
		"id": 75,
		"start": 248.289,
		"end": 249.791,
		"text": "アニメーション作品を"
	},
	{
		"id": 76,
		"start": 250.291,
		"end": 250.959,
		"text": "作りま…"
	},
	{
		"id": 77,
		"start": 251.751,
		"end": 252.418,
		"text": "す！"
	},
	{
		"id": 78,
		"start": 252.919,
		"end": 255.755,
		"text": "（一同）<br />ドンドン ドーナツ<br />ど～んといこう！"
	},
	{
		"id": 79,
		"start": 258.049,
		"end": 261.886,
		"text": "（ラジオ:男）<br />はい というわけでラジオネーム<br />アニメ元帥さんからのお便りでした"
	},
	{
		"id": 80,
		"start": 262.011,
		"end": 264.013,
		"text": "（女）<br />アニメ元帥さん いつもメール<br />ありがとうございま～す"
	},
	{
		"id": 81,
		"start": 264.347,
		"end": 266.099,
		"text": "（男）<br />まあ確かにね<br />メールにもあったけど"
	},
	{
		"id": 82,
		"start": 266.224,
		"end": 268.184,
		"text": "秋アニメ多すぎて<br />そりゃまあ何見ていいか"
	},
	{
		"id": 83,
		"start": 268.309,
		"end": 269.018,
		"text": "迷うわな～"
	},
	{
		"id": 84,
		"start": 269.185,
		"end": 272.021,
		"text": "（女）<br />一応 私なんかも片っ端から<br />録画しちゃったりしてるんですけど"
	},
	{
		"id": 85,
		"start": 272.272,
		"end": 274.023,
		"text": "ハードディスクが<br />すぐにいっぱいになっちゃって"
	},
	{
		"id": 86,
		"start": 274.148,
		"end": 275.525,
		"text": "泣く泣く夏アニメ消したりとか"
	},
	{
		"id": 87,
		"start": 275.9,
		"end": 277.443,
		"text": "（男）<br />“アニメバブル”とか<br />言われてるもんね"
	},
	{
		"id": 88,
		"start": 277.568,
		"end": 278.82,
		"text": "つまり いつかはじけちゃう！"
	},
	{
		"id": 89,
		"start": 278.945,
		"end": 280.238,
		"text": "（女）<br />怖いこと言わないでくださいよ～"
	},
	{
		"id": 90,
		"start": 280.363,
		"end": 282.198,
		"text": "（男）<br />いい時なんて一瞬なんだよ"
	},
	{
		"id": 91,
		"start": 282.323,
		"end": 285.076,
		"text": "（女）<br />は～いはい<br />そんなネガティブなのは置いといて"
	},
	{
		"id": 92,
		"start": 285.201,
		"end": 286.828,
		"text": "（男）<br />数多くの秋アニメの中からですね"
	},
	{
		"id": 93,
		"start": 286.953,
		"end": 288.454,
		"text": "我々「アニラジポクポク」が<br />１番プッシュしてる作品は"
	},
	{
		"id": 94,
		"start": 288.454,
		"end": 289.497,
		"text": "我々「アニラジポクポク」が<br />１番プッシュしてる作品は"
	},
	{
		"id": 95,
		"start": 288.454,
		"end": 289.497,
		"text": "ああ… ハッ！"
	},
	{
		"id": 96,
		"start": 289.497,
		"end": 289.622,
		"text": "ああ… ハッ！"
	},
	{
		"id": 97,
		"start": 289.622,
		"end": 290.415,
		"text": "ああ… ハッ！"
	},
	{
		"id": 98,
		"start": 289.622,
		"end": 290.415,
		"text": "じゃ～ん！　「えくそだすっ！」"
	},
	{
		"id": 99,
		"start": 290.415,
		"end": 290.999,
		"text": "じゃ～ん！　「えくそだすっ！」"
	},
	{
		"id": 100,
		"start": 290.999,
		"end": 291.916,
		"text": "じゃ～ん！　「えくそだすっ！」"
	},
	{
		"id": 101,
		"start": 290.999,
		"end": 291.916,
		"text": "あっ！"
	},
	{
		"id": 102,
		"start": 292.083,
		"end": 294.711,
		"text": "（女）<br />は～い！　その一押しタイトル<br />「えくそだすっ！」のですね"
	},
	{
		"id": 103,
		"start": 294.836,
		"end": 295.795,
		"text": "（男）<br />中の人をお呼びしました～！"
	},
	{
		"id": 104,
		"start": 296.212,
		"end": 296.921,
		"text": "（女）<br />イェ～イ！"
	},
	{
		"id": 105,
		"start": 296.921,
		"end": 297.547,
		"text": "（女）<br />イェ～イ！"
	},
	{
		"id": 106,
		"start": 296.921,
		"end": 297.547,
		"text": "わあ～！"
	},
	{
		"id": 107,
		"start": 297.547,
		"end": 297.714,
		"text": "わあ～！"
	},
	{
		"id": 108,
		"start": 297.714,
		"end": 298.381,
		"text": "わあ～！"
	},
	{
		"id": 109,
		"start": 297.714,
		"end": 298.381,
		"text": "（声優たち）<br />こんばんは～！"
	},
	{
		"id": 110,
		"start": 298.506,
		"end": 300.383,
		"text": "（中春(なかはる)）<br />「えくそだすっ！」あかね役の<br />中春鳴(めい)です"
	},
	{
		"id": 111,
		"start": 300.55,
		"end": 302.051,
		"text": "（伊藤(いとう)）<br />「えくそだすっ！」あや役の<br />伊藤鈴鹿(すずか)です"
	},
	{
		"id": 112,
		"start": 302.343,
		"end": 304.637,
		"text": "（茅菜(かやな)）<br />「えくそだすっ！」あるぴん役の<br />茅菜夢衣(むい)です"
	},
	{
		"id": 113,
		"start": 304.762,
		"end": 306.097,
		"text": "夢の衣と書いて“夢衣”で～す"
	},
	{
		"id": 114,
		"start": 306.222,
		"end": 307.473,
		"text": "（男）<br />いや すいませんね"
	},
	{
		"id": 115,
		"start": 307.598,
		"end": 309.392,
		"text": "わざわざノーギャラなのに<br />お越しいただいちゃって"
	},
	{
		"id": 116,
		"start": 309.517,
		"end": 311.394,
		"text": "（ラジオからの笑い声）"
	},
	{
		"id": 117,
		"start": 311.519,
		"end": 314.022,
		"text": "（中春）<br />今日ね この後すぐオンエアなので<br />宣伝にやってきました"
	},
	{
		"id": 118,
		"start": 314.147,
		"end": 315.565,
		"text": "うん？"
	},
	{
		"id": 119,
		"start": 314.147,
		"end": 315.565,
		"text": "（伊藤）<br />は～い ライバル作品多いんで<br />負けられないんですよ～"
	},
	{
		"id": 120,
		"start": 315.565,
		"end": 315.982,
		"text": "（伊藤）<br />は～い ライバル作品多いんで<br />負けられないんですよ～"
	},
	{
		"id": 121,
		"start": 315.982,
		"end": 316.19,
		"text": "（伊藤）<br />は～い ライバル作品多いんで<br />負けられないんですよ～"
	},
	{
		"id": 122,
		"start": 315.982,
		"end": 316.19,
		"text": "（富ヶ谷(とみがや)）<br />フンッ！"
	},
	{
		"id": 123,
		"start": 316.19,
		"end": 316.316,
		"text": "（富ヶ谷(とみがや)）<br />フンッ！"
	},
	{
		"id": 124,
		"start": 316.316,
		"end": 316.899,
		"text": "（富ヶ谷(とみがや)）<br />フンッ！"
	},
	{
		"id": 125,
		"start": 316.316,
		"end": 316.899,
		"text": "（茅菜）<br />うん たたき潰さないとね！"
	},
	{
		"id": 126,
		"start": 316.899,
		"end": 317.066,
		"text": "（茅菜）<br />うん たたき潰さないとね！"
	},
	{
		"id": 127,
		"start": 317.066,
		"end": 317.442,
		"text": "（茅菜）<br />うん たたき潰さないとね！"
	},
	{
		"id": 128,
		"start": 317.066,
		"end": 317.442,
		"text": "（エンジン音）"
	},
	{
		"id": 129,
		"start": 317.442,
		"end": 317.567,
		"text": "（エンジン音）"
	},
	{
		"id": 130,
		"start": 317.567,
		"end": 319.402,
		"text": "（エンジン音）"
	},
	{
		"id": 131,
		"start": 317.567,
		"end": 319.402,
		"text": "（男）<br />はいはい 危険な話はね<br />ここら辺にしといてと"
	},
	{
		"id": 132,
		"start": 319.402,
		"end": 319.944,
		"text": "（男）<br />はいはい 危険な話はね<br />ここら辺にしといてと"
	},
	{
		"id": 133,
		"start": 320.069,
		"end": 322.405,
		"text": "（女）<br />はい「えくそだすっ！」って<br />どんなお話なんですか？"
	},
	{
		"id": 134,
		"start": 322.655,
		"end": 325.742,
		"text": "（中春）<br />はい アイドルユニット<br />トレイシーが逃げまくるお話で～す"
	},
	{
		"id": 135,
		"start": 325.867,
		"end": 326.909,
		"text": "（ため息）"
	},
	{
		"id": 136,
		"start": 325.867,
		"end": 326.909,
		"text": "（中春）以上で～す<br />（男）終わっちゃうの？"
	},
	{
		"id": 137,
		"start": 326.909,
		"end": 327.535,
		"text": "（中春）以上で～す<br />（男）終わっちゃうの？"
	},
	{
		"id": 138,
		"start": 327.66,
		"end": 329.746,
		"text": "（男）<br />なんという<br />まあ ざっくりなあらすじ"
	},
	{
		"id": 139,
		"start": 329.871,
		"end": 332.623,
		"text": "（中春）<br />はい あの内容は<br />この後じっくりお話しするとして"
	},
	{
		"id": 140,
		"start": 332.749,
		"end": 334.042,
		"text": "まずは聴いてください"
	},
	{
		"id": 141,
		"start": 334.167,
		"end": 337.462,
		"text": "私たちの演じるアイドルユニット<br />トレイシーが歌うオープニング曲"
	},
	{
		"id": 142,
		"start": 337.587,
		"end": 339.172,
		"text": "“あいむそーりーEXODUS(エクソダス)”"
	},
	{
		"id": 143,
		"start": 339.714,
		"end": 344.719,
		"text": "♪～"
	},
	{
		"id": 144,
		"start": 365.198,
		"end": 366.365,
		"text": "うっ うう…"
	},
	{
		"id": 145,
		"start": 396.229,
		"end": 399.232,
		"text": "（〝あいむそーりー<br />ＥＸＯＤＵＳ〞を歌う）"
	},
	{
		"id": 146,
		"start": 410.326,
		"end": 410.993,
		"text": "フッ フフッ…"
	},
	{
		"id": 147,
		"start": 414.997,
		"end": 416.207,
		"text": "（衝撃音）<br />うわ～！　うわっ うおっ！"
	},
	{
		"id": 148,
		"start": 422.338,
		"end": 427.343,
		"text": "～♪"
	},
	{
		"id": 149,
		"start": 429.846,
		"end": 432.306,
		"text": "クソッ！　またお前か<br />宮森(みやもり)あおい！"
	},
	{
		"id": 150,
		"start": 432.432,
		"end": 434.6,
		"text": "ヘヘッ 富ヶ谷さん お先です！"
	},
	{
		"id": 151,
		"start": 437.77,
		"end": 438.938,
		"text": "（チャイム）"
	},
	{
		"id": 152,
		"start": 439.063,
		"end": 441.441,
		"text": "武蔵野(むさしの)アニメーションの<br />宮森あおいです"
	},
	{
		"id": 153,
		"start": 441.566,
		"end": 444.861,
		"text": "「えくそだすっ！」４話の<br />作監上がりを頂きに参りました"
	},
	{
		"id": 154,
		"start": 445.236,
		"end": 446.279,
		"text": "（瀬川(せがわ)）<br />は～い"
	},
	{
		"id": 155,
		"start": 448.865,
		"end": 450.616,
		"text": "ちょっと今日 少ないんだけど"
	},
	{
		"id": 156,
		"start": 450.908,
		"end": 452.785,
		"text": "いえいえ ありがとうございます"
	},
	{
		"id": 157,
		"start": 452.91,
		"end": 455.538,
		"text": "それで これで作監入れ全部です"
	},
	{
		"id": 158,
		"start": 455.663,
		"end": 458.04,
		"text": "あと２日で作監アップだったよね？"
	},
	{
		"id": 159,
		"start": 458.166,
		"end": 460.626,
		"text": "今夜 もう１話<br />始まっちゃうんだもんね"
	},
	{
		"id": 160,
		"start": 460.751,
		"end": 462.628,
		"text": "始まっちゃいます<br />いよいよです"
	},
	{
		"id": 161,
		"start": 463.171,
		"end": 465.965,
		"text": "そっか 宮森さんは楽しみなんだ"
	},
	{
		"id": 162,
		"start": 466.132,
		"end": 468.342,
		"text": "（あおい）えっ？<br />（瀬川）私は１話の放映前が―"
	},
	{
		"id": 163,
		"start": 468.468,
		"end": 469.802,
		"text": "一番緊張する"
	},
	{
		"id": 164,
		"start": 470.636,
		"end": 474.724,
		"text": "待ったなしの１(ワン)クール13本が<br />始まっちゃうよって"
	},
	{
		"id": 165,
		"start": 475.349,
		"end": 479.02,
		"text": "瀬川さんに入っていただいて<br />すごいクオリティーになってますよ"
	},
	{
		"id": 166,
		"start": 479.145,
		"end": 480.771,
		"text": "監督も大喜びですもん"
	},
	{
		"id": 167,
		"start": 480.897,
		"end": 483.399,
		"text": "ホント？　だといいな～"
	},
	{
		"id": 168,
		"start": 483.983,
		"end": 486.694,
		"text": "もうね さすがに肩凝っちゃって"
	},
	{
		"id": 169,
		"start": 487.278,
		"end": 489.53,
		"text": "やっぱり 大変ですよね？"
	},
	{
		"id": 170,
		"start": 489.697,
		"end": 492.158,
		"text": "うん？　ああ まあね"
	},
	{
		"id": 171,
		"start": 492.283,
		"end": 494.327,
		"text": "あとちょっとだし 頑張りますよ！"
	},
	{
		"id": 172,
		"start": 494.744,
		"end": 496.537,
		"text": "はい よろしくお願いします！"
	},
	{
		"id": 173,
		"start": 496.662,
		"end": 497.33,
		"text": "うん！"
	},
	{
		"id": 174,
		"start": 505.63,
		"end": 507.173,
		"text": "（あおい）<br />戻りました～！"
	},
	{
		"id": 175,
		"start": 508.799,
		"end": 510.051,
		"text": "はあ～"
	},
	{
		"id": 176,
		"start": 510.551,
		"end": 513.012,
		"text": "あっ？　うん？"
	},
	{
		"id": 177,
		"start": 528.402,
		"end": 529.445,
		"text": "うん？"
	},
	{
		"id": 178,
		"start": 551.175,
		"end": 553.135,
		"text": "（本田(ほんだ)）<br />今期はやっぱり<br />「ファイト」かな～"
	},
	{
		"id": 179,
		"start": 553.344,
		"end": 555.805,
		"text": "（落合(おちあい)）<br />注目度だと<br />「Ｇコレ」じゃないですか？"
	},
	{
		"id": 180,
		"start": 556.055,
		"end": 557.765,
		"text": "（太郎(たろう)）<br />いや そこはうちの<br />「えくそだすっ！」っと"
	},
	{
		"id": 181,
		"start": 557.89,
		"end": 559.016,
		"text": "ズバっと言い切りましょ～！"
	},
	{
		"id": 182,
		"start": 559.475,
		"end": 562.019,
		"text": "（下柳(しもやなぎ)）<br />へえ あのスタジオ<br />出身だったんですね"
	},
	{
		"id": 183,
		"start": 562.52,
		"end": 564.981,
		"text": "（堂本(どうもと)）<br />そうそう<br />あそこ もうトイレが最悪で"
	},
	{
		"id": 184,
		"start": 565.231,
		"end": 566.399,
		"text": "（井口(いぐち)）<br />木佐(きさ)さん 自転車で"
	},
	{
		"id": 185,
		"start": 566.524,
		"end": 567.984,
		"text": "20キロも痩せたんですって"
	},
	{
		"id": 186,
		"start": 568.317,
		"end": 571.654,
		"text": "でもケガして入院して<br />23キロ 太ったらしいですよ"
	},
	{
		"id": 187,
		"start": 571.988,
		"end": 574.407,
		"text": "（内田(うちだ)）<br />なんか緊張するね　オンエアって"
	},
	{
		"id": 188,
		"start": 574.657,
		"end": 575.95,
		"text": "そうですね"
	},
	{
		"id": 189,
		"start": 576.075,
		"end": 578.035,
		"text": "私の絵で<br />いいのかなって感じです"
	},
	{
		"id": 190,
		"start": 578.411,
		"end": 580.413,
		"text": "（佐倉(さくら)）<br />あれ？　そういえばナベＰは？"
	},
	{
		"id": 191,
		"start": 580.705,
		"end": 582.665,
		"text": "（新川(しんかわ)）<br />“天牌(てんぱい)”でしょ　いつものこと"
	},
	{
		"id": 192,
		"start": 582.957,
		"end": 585.251,
		"text": "（矢野(やの)）<br />１話の放映なのに ありえない"
	},
	{
		"id": 193,
		"start": 585.376,
		"end": 586.919,
		"text": "（新川）<br />ナベＰだから"
	},
	{
		"id": 194,
		"start": 587.92,
		"end": 590.631,
		"text": "（円(まどか)）<br />監督 もう実況 始まってますよ"
	},
	{
		"id": 195,
		"start": 590.756,
		"end": 591.966,
		"text": "（木下(きのした)）<br />いちいち言わなくていい！"
	},
	{
		"id": 196,
		"start": 592.216,
		"end": 594.093,
		"text": "（山田(やまだ)）<br />円 お前ネット見すぎ"
	},
	{
		"id": 197,
		"start": 594.343,
		"end": 596.971,
		"text": "（丸川(まるかわ)）<br />おっ 宮森さん<br />カレーあるよ カレー"
	},
	{
		"id": 198,
		"start": 597.138,
		"end": 598.139,
		"text": "あっ はい！"
	},
	{
		"id": 199,
		"start": 598.306,
		"end": 599.724,
		"text": "みゃーもり こっちこっち"
	},
	{
		"id": 200,
		"start": 600.057,
		"end": 601.35,
		"text": "あと20秒です"
	},
	{
		"id": 201,
		"start": 601.475,
		"end": 603.019,
		"text": "ハッシュタグ付けて<br />ツイートでもしますか？"
	},
	{
		"id": 202,
		"start": 603.144,
		"end": 604.562,
		"text": "ネットはやらん！"
	},
	{
		"id": 203,
		"start": 604.687,
		"end": 606.188,
		"text": "貧乏ゆすり やめてもらえます？"
	},
	{
		"id": 204,
		"start": 606.314,
		"end": 606.981,
		"text": "ごめん"
	},
	{
		"id": 205,
		"start": 607.315,
		"end": 608.691,
		"text": "（円）<br />あと10秒！"
	},
	{
		"id": 206,
		"start": 609.358,
		"end": 612.111,
		"text": "（太郎）７･６･５…<br />（矢野）太郎 静かに！"
	},
	{
		"id": 207,
		"start": 612.278,
		"end": 614.614,
		"text": "（テレビＣＭ）<br />10月25日オンセール！"
	},
	{
		"id": 208,
		"start": 616.866,
		"end": 619.201,
		"text": "（歓声）<br />（本田）始まりましたね～"
	},
	{
		"id": 209,
		"start": 619.327,
		"end": 620.119,
		"text": "（木下）<br />分かってるよ！"
	},
	{
		"id": 210,
		"start": 623.998,
		"end": 626.125,
		"text": "（あるぴん）<br />どっ どっ どっ… どうしよう？"
	},
	{
		"id": 211,
		"start": 626.292,
		"end": 628.628,
		"text": "（あかね）どうって？<br />（あや）すぐに警備員も来る"
	},
	{
		"id": 212,
		"start": 628.794,
		"end": 631.631,
		"text": "（あかね）<br />待って こういう危険な時ほど<br />落ち着けって"
	},
	{
		"id": 213,
		"start": 631.797,
		"end": 632.715,
		"text": "おじじが言ってた"
	},
	{
		"id": 214,
		"start": 632.84,
		"end": 635.092,
		"text": "（あや）<br />さすが<br />現実主義者のカッパ"
	},
	{
		"id": 215,
		"start": 644.935,
		"end": 647.063,
		"text": "（あかね）<br />来週も明日に向かって"
	},
	{
		"id": 216,
		"start": 647.271,
		"end": 648.564,
		"text": "（あや･あるぴん･あかね）<br />「えくそだすっ！」"
	},
	{
		"id": 217,
		"start": 650.399,
		"end": 652.568,
		"text": "（歓声と拍手）"
	},
	{
		"id": 218,
		"start": 653.277,
		"end": 654.028,
		"text": "（ため息）"
	},
	{
		"id": 219,
		"start": 654.153,
		"end": 657.156,
		"text": "丁寧に作ってあって<br />いい出来じゃないの 木下君"
	},
	{
		"id": 220,
		"start": 657.281,
		"end": 659.575,
		"text": "（木下）<br />あっ ありがとうございます<br />頑張ります"
	},
	{
		"id": 221,
		"start": 659.742,
		"end": 661.535,
		"text": "キーワードランキング２位<br />きました！"
	},
	{
		"id": 222,
		"start": 661.661,
		"end": 662.745,
		"text": "（あおい）お～！<br />（木下）だからいいって"
	},
	{
		"id": 223,
		"start": 662.87,
		"end": 663.537,
		"text": "そういうの！"
	},
	{
		"id": 224,
		"start": 663.788,
		"end": 667.5,
		"text": "早いな～ もう感想か<br />そこそこ評判いいですよ"
	},
	{
		"id": 225,
		"start": 667.625,
		"end": 669.543,
		"text": "（木下）そこそこ…<br />（太郎）よかったっすね～"
	},
	{
		"id": 226,
		"start": 669.669,
		"end": 671.587,
		"text": "みそぎ済んだんじゃないっすか？"
	},
	{
		"id": 227,
		"start": 672.046,
		"end": 673.589,
		"text": "みそぎ…"
	},
	{
		"id": 228,
		"start": 673.798,
		"end": 675.049,
		"text": "ああ…"
	},
	{
		"id": 229,
		"start": 675.174,
		"end": 677.885,
		"text": "ああ みんな お代わりあるからね<br />どんどん食べて"
	},
	{
		"id": 230,
		"start": 678.01,
		"end": 679.303,
		"text": "（太郎）<br />はいはい！　いただきま～す！"
	},
	{
		"id": 231,
		"start": 679.428,
		"end": 680.846,
		"text": "俺も食べよっかな"
	},
	{
		"id": 232,
		"start": 681.305,
		"end": 684.225,
		"text": "（山田）<br />しかし来週は２話か 早(はえ)えよな～"
	},
	{
		"id": 233,
		"start": 684.392,
		"end": 687.061,
		"text": "（円）…てことは その次３話<br />（太郎）あっ！"
	},
	{
		"id": 234,
		"start": 687.186,
		"end": 688.479,
		"text": "どうしたの？　太郎君"
	},
	{
		"id": 235,
		"start": 688.604,
		"end": 690.731,
		"text": "いや いいです<br />おなかいっぱいです"
	},
	{
		"id": 236,
		"start": 690.856,
		"end": 692.191,
		"text": "（太郎）でした…<br />（丸川）えっ？"
	},
	{
		"id": 237,
		"start": 694.61,
		"end": 696.612,
		"text": "おいちゃん おいちゃん<br />ちゃんと見れた？"
	},
	{
		"id": 238,
		"start": 696.737,
		"end": 699.657,
		"text": "見れた 見れた！<br />動いてたね　すごいよ！"
	},
	{
		"id": 239,
		"start": 699.782,
		"end": 701.45,
		"text": "もう何べんも見たでしょ？"
	},
	{
		"id": 240,
		"start": 701.867,
		"end": 703.994,
		"text": "でも テレビは何十万"
	},
	{
		"id": 241,
		"start": 704.12,
		"end": 707.331,
		"text": "何百万人の人が<br />一緒に見てるんだよ　一緒に！"
	},
	{
		"id": 242,
		"start": 708.374,
		"end": 709.792,
		"text": "そう… だね"
	},
	{
		"id": 243,
		"start": 710.209,
		"end": 712.711,
		"text": "そう考えると すごいかな？"
	},
	{
		"id": 244,
		"start": 713.504,
		"end": 714.463,
		"text": "お先です"
	},
	{
		"id": 245,
		"start": 714.588,
		"end": 716.465,
		"text": "（あおい）お疲れさまで～す<br />（本田）お疲れ～"
	},
	{
		"id": 246,
		"start": 718.467,
		"end": 722.054,
		"text": "（あおい）<br />俺 この進行表が全部埋まって<br />４話が完成したら"
	},
	{
		"id": 247,
		"start": 722.179,
		"end": 724.515,
		"text": "吉祥寺(きちじょうじ)のペッカドーナツ<br />一気食いするんだ"
	},
	{
		"id": 248,
		"start": 724.64,
		"end": 726.684,
		"text": "（太郎）<br />ああ～"
	},
	{
		"id": 249,
		"start": 726.809,
		"end": 727.601,
		"text": "うん？"
	},
	{
		"id": 250,
		"start": 727.726,
		"end": 731.105,
		"text": "どうしたんですか？　タロさん<br />まだ帰らないんですか？"
	},
	{
		"id": 251,
		"start": 731.23,
		"end": 733.482,
		"text": "いやっ まあ ちょっとね うん…"
	},
	{
		"id": 252,
		"start": 737.987,
		"end": 739.28,
		"text": "（本田）太郎<br />（太郎）でひっ！"
	},
	{
		"id": 253,
		"start": 739.405,
		"end": 741.657,
		"text": "（本田）<br />３話のラッシュ<br />差し替え終わってるか？"
	},
	{
		"id": 254,
		"start": 741.907,
		"end": 743.951,
		"text": "宮森さん あした記録頼める？"
	},
	{
		"id": 255,
		"start": 744.076,
		"end": 746.12,
		"text": "（あおい）<br />はい　12時からですよね？"
	},
	{
		"id": 256,
		"start": 746.287,
		"end": 748.664,
		"text": "あっ うっ うん　ええ…"
	},
	{
		"id": 257,
		"start": 749.248,
		"end": 750.207,
		"text": "うん？"
	},
	{
		"id": 258,
		"start": 751.459,
		"end": 753.878,
		"text": "（興津(おきつ)）おはようございます<br />（一同）おはようございま～す"
	},
	{
		"id": 259,
		"start": 754.211,
		"end": 756.088,
		"text": "はい おはよう"
	},
	{
		"id": 260,
		"start": 756.213,
		"end": 759.717,
		"text": "ゆうべ 我が社が<br />７年ぶりに元請けになる作品"
	},
	{
		"id": 261,
		"start": 759.967,
		"end": 762.761,
		"text": "「えくそだすっ！」の<br />第１話が放映されました"
	},
	{
		"id": 262,
		"start": 763.387,
		"end": 765.681,
		"text": "たった１クールですが<br />あと12本"
	},
	{
		"id": 263,
		"start": 765.806,
		"end": 768.267,
		"text": "気を抜かずに<br />頑張っていきましょう"
	},
	{
		"id": 264,
		"start": 768.684,
		"end": 770.478,
		"text": "デスクから連絡で～す"
	},
	{
		"id": 265,
		"start": 770.603,
		"end": 773.898,
		"text": "本日12時から３話の<br />オールラッシュを行います"
	},
	{
		"id": 266,
		"start": 774.148,
		"end": 776.609,
		"text": "17時から７話<br />カッティングの予定です"
	},
	{
		"id": 267,
		"start": 776.942,
		"end": 779.737,
		"text": "16時から６話の<br />３Ｄチェックをやります"
	},
	{
		"id": 268,
		"start": 780.196,
		"end": 781.322,
		"text": "総務からですが"
	},
	{
		"id": 269,
		"start": 781.447,
		"end": 785.075,
		"text": "最後に帰る人は 必ずエアコンを<br />消すようにしてください"
	},
	{
		"id": 270,
		"start": 785.201,
		"end": 788.871,
		"text": "あと冷蔵庫に 賞味期限の<br />切れた物がいくつかあります"
	},
	{
		"id": 271,
		"start": 788.996,
		"end": 791.123,
		"text": "今週末までに片づけてください"
	},
	{
		"id": 272,
		"start": 791.457,
		"end": 792.875,
		"text": "（興津）以上です<br />（一同）はい"
	},
	{
		"id": 273,
		"start": 793.626,
		"end": 796.295,
		"text": "では本日も… じゃなかった"
	},
	{
		"id": 274,
		"start": 796.42,
		"end": 798.756,
		"text": "明日に向かって えくそだ～すっ！"
	},
	{
		"id": 275,
		"start": 800.716,
		"end": 801.759,
		"text": "（一同）<br />お～！"
	},
	{
		"id": 276,
		"start": 802.593,
		"end": 803.761,
		"text": "（円）<br />ラッシュ止めろ"
	},
	{
		"id": 277,
		"start": 804.512,
		"end": 806.388,
		"text": "戻して もっかい見せて"
	},
	{
		"id": 278,
		"start": 806.722,
		"end": 809.308,
		"text": "（モニターから聞こえる声）"
	},
	{
		"id": 279,
		"start": 809.433,
		"end": 811.519,
		"text": "（本田）<br />あれっ コンテ撮のまま？"
	},
	{
		"id": 280,
		"start": 811.894,
		"end": 813.187,
		"text": "なんで色ついてないの？"
	},
	{
		"id": 281,
		"start": 813.312,
		"end": 814.772,
		"text": "あっ！　そういや俺"
	},
	{
		"id": 282,
		"start": 814.897,
		"end": 817.441,
		"text": "まだ このカット<br />チェックしてなかった～"
	},
	{
		"id": 283,
		"start": 819.902,
		"end": 820.861,
		"text": "（円）<br />これさあ なんで？"
	},
	{
		"id": 284,
		"start": 821.07,
		"end": 821.737,
		"text": "（太郎）<br />はい…"
	},
	{
		"id": 285,
		"start": 821.862,
		"end": 823.614,
		"text": "（円）“はい”じゃないよね？<br />（太郎）はい！"
	},
	{
		"id": 286,
		"start": 824.031,
		"end": 826.825,
		"text": "（円）<br />ラッシュがコンテのままって<br />どういうことか分かる？"
	},
	{
		"id": 287,
		"start": 826.951,
		"end": 830.329,
		"text": "動きのタイミングも分からないし<br />効果音もつけられないよね？"
	},
	{
		"id": 288,
		"start": 830.496,
		"end": 831.163,
		"text": "（太郎）<br />はい！"
	},
	{
		"id": 289,
		"start": 831.288,
		"end": 832.581,
		"text": "つまりダビングできない"
	},
	{
		"id": 290,
		"start": 832.706,
		"end": 834.667,
		"text": "（円）完成しないってことだよね？<br />（太郎）はい！"
	},
	{
		"id": 291,
		"start": 834.959,
		"end": 836.502,
		"text": "俺の言うこと間違ってる？"
	},
	{
		"id": 292,
		"start": 836.627,
		"end": 837.336,
		"text": "（太郎）はい！<br />（あおい･本田）えっ！"
	},
	{
		"id": 293,
		"start": 837.628,
		"end": 840.172,
		"text": "（太郎）<br />じゃじゃじゃ… じゃなくて<br />間違ってまへん！"
	},
	{
		"id": 294,
		"start": 840.339,
		"end": 842.174,
		"text": "まっ… まあまあまあ…<br />あれだよね？"
	},
	{
		"id": 295,
		"start": 842.383,
		"end": 845.886,
		"text": "ただの差し替えミスだよね？<br />絵の素材はあるんだもんね？"
	},
	{
		"id": 296,
		"start": 846.804,
		"end": 847.638,
		"text": "えっ？"
	},
	{
		"id": 297,
		"start": 847.763,
		"end": 850.975,
		"text": "申し訳ありません！<br />実は原画は上がっておりません！"
	},
	{
		"id": 298,
		"start": 851.225,
		"end": 852.56,
		"text": "上がる予定もございません！"
	},
	{
		"id": 299,
		"start": 852.893,
		"end": 856.772,
		"text": "何べんも 何べんも な～んべんも<br />俺言ったよな？"
	},
	{
		"id": 300,
		"start": 856.897,
		"end": 859.525,
		"text": "あの人 絵はうまいけど<br />遅いから気をつけろって"
	},
	{
		"id": 301,
		"start": 859.859,
		"end": 860.526,
		"text": "はい！"
	},
	{
		"id": 302,
		"start": 860.651,
		"end": 863.696,
		"text": "そりゃあ俺も もっと早く<br />気付くべきだったけどさ"
	},
	{
		"id": 303,
		"start": 863.821,
		"end": 864.864,
		"text": "はい… あっ！"
	},
	{
		"id": 304,
		"start": 865.155,
		"end": 868.158,
		"text": "お前が“大丈夫っす”っつうから<br />任せたんじゃねえか！"
	},
	{
		"id": 305,
		"start": 868.284,
		"end": 869.535,
		"text": "ダイジョブくありませんでした"
	},
	{
		"id": 306,
		"start": 869.702,
		"end": 871.537,
		"text": "やっぱ無理って<br />おととい戻されました"
	},
	{
		"id": 307,
		"start": 871.662,
		"end": 874.999,
		"text": "（円）<br />“ジョブく”じゃねえよ！<br />代わりの原画マン探せよ！"
	},
	{
		"id": 308,
		"start": 875.332,
		"end": 878.544,
		"text": "いや 一応俺の持ってるカード<br />全部使ったんですけど"
	},
	{
		"id": 309,
		"start": 878.669,
		"end": 880.462,
		"text": "ことごとく<br />ノーでございました"
	},
	{
		"id": 310,
		"start": 880.963,
		"end": 882.673,
		"text": "（円）どうしましょう 監督？<br />（木下）うん？"
	},
	{
		"id": 311,
		"start": 882.798,
		"end": 884.466,
		"text": "視聴継続か切りか"
	},
	{
		"id": 312,
		"start": 884.884,
		"end": 889.263,
		"text": "最初に山場の第３話<br />更にそれの見せ場の超重要カット"
	},
	{
		"id": 313,
		"start": 889.513,
		"end": 892.725,
		"text": "それがただの止め絵で<br />十数秒なんてことになったら…"
	},
	{
		"id": 314,
		"start": 892.85,
		"end": 893.809,
		"text": "あっ あが…"
	},
	{
		"id": 315,
		"start": 894.101,
		"end": 896.186,
		"text": "社内でなんとかできないですかね？"
	},
	{
		"id": 316,
		"start": 896.312,
		"end": 898.439,
		"text": "橋本(はしもと)さんとか内田さんとか"
	},
	{
		"id": 317,
		"start": 898.564,
		"end": 900.816,
		"text": "う～ん まだ無理じゃね？"
	},
	{
		"id": 318,
		"start": 901.15,
		"end": 902.526,
		"text": "（本田）<br />杉江(すぎえ)さんは？"
	},
	{
		"id": 319,
		"start": 902.651,
		"end": 903.319,
		"text": "（円）<br />萌(も)えアニメなんて―"
	},
	{
		"id": 320,
		"start": 903.444,
		"end": 904.904,
		"text": "描けるわけねえじゃん"
	},
	{
		"id": 321,
		"start": 905.029,
		"end": 906.822,
		"text": "ありえねえって あの人　無理！"
	},
	{
		"id": 322,
		"start": 907.114,
		"end": 908.449,
		"text": "（本田）<br />安原(やすはら)さんとか？"
	},
	{
		"id": 323,
		"start": 908.574,
		"end": 912.077,
		"text": "（円）<br />新人原画には まだ無理だろ<br />勝負カットだぞ"
	},
	{
		"id": 324,
		"start": 912.828,
		"end": 913.829,
		"text": "万策尽きた～！"
	},
	{
		"id": 325,
		"start": 917.207,
		"end": 919.335,
		"text": "瀬川さんに頼めないかな？"
	},
	{
		"id": 326,
		"start": 919.543,
		"end": 920.961,
		"text": "（あおい）ハッ！<br />（本田）４話の作監の？"
	},
	{
		"id": 327,
		"start": 921.086,
		"end": 922.463,
		"text": "（円）なるほど！<br />（あおい）はあっ！"
	},
	{
		"id": 328,
		"start": 929.511,
		"end": 931.055,
		"text": "まままま… 待って！"
	},
	{
		"id": 329,
		"start": 933.098,
		"end": 935.517,
		"text": "重っ！　原画アップいつまで？"
	},
	{
		"id": 330,
		"start": 935.643,
		"end": 937.686,
		"text": "あさって午前中… みたいな"
	},
	{
		"id": 331,
		"start": 937.811,
		"end": 938.771,
		"text": "それは無理！"
	},
	{
		"id": 332,
		"start": 938.896,
		"end": 941.023,
		"text": "お願いします！<br />瀬川さんにしか頼めないんです！"
	},
	{
		"id": 333,
		"start": 941.148,
		"end": 941.899,
		"text": "しゃあっす！"
	},
	{
		"id": 334,
		"start": 942.024,
		"end": 944.443,
		"text": "二原も立てます<br />ラフ原でかまいませんから"
	},
	{
		"id": 335,
		"start": 944.568,
		"end": 947.863,
		"text": "それはやだ<br />やるなら ちゃんとやりたいし"
	},
	{
		"id": 336,
		"start": 947.988,
		"end": 949.239,
		"text": "う～ん…"
	},
	{
		"id": 337,
		"start": 949.907,
		"end": 953.077,
		"text": "もし私がこれをやったら<br />４話はどうするの？"
	},
	{
		"id": 338,
		"start": 953.202,
		"end": 954.453,
		"text": "あした作監アップなんでしょ？"
	},
	{
		"id": 339,
		"start": 954.578,
		"end": 956.455,
		"text": "それはいったん 止めていただいて"
	},
	{
		"id": 340,
		"start": 956.58,
		"end": 957.748,
		"text": "え～？"
	},
	{
		"id": 341,
		"start": 957.915,
		"end": 960.876,
		"text": "それと３話の作監 遠藤(えんどう)君でしょ？"
	},
	{
		"id": 342,
		"start": 961.335,
		"end": 964.38,
		"text": "遠藤君と演出の円さんは<br />なんて言ってるの？"
	},
	{
		"id": 343,
		"start": 965.756,
		"end": 967.132,
		"text": "（遠藤）<br />瀬川さんか～"
	},
	{
		"id": 344,
		"start": 967.424,
		"end": 970.344,
		"text": "おっ 遠藤って瀬川さん苦手？"
	},
	{
		"id": 345,
		"start": 970.469,
		"end": 975.224,
		"text": "いや 苦手ってことはないですけど<br />前もって言ってほしかったな"
	},
	{
		"id": 346,
		"start": 975.349,
		"end": 978.352,
		"text": "ライバルだもんな<br />借りは作りたくないと"
	},
	{
		"id": 347,
		"start": 978.477,
		"end": 983.107,
		"text": "貸しとか借りとかないですから！<br />ちょっと絵柄が合わないだけですよ"
	},
	{
		"id": 348,
		"start": 983.232,
		"end": 985.275,
		"text": "んなこと言ったって 実際問題"
	},
	{
		"id": 349,
		"start": 985.401,
		"end": 988.112,
		"text": "お前 作画リテーク対応で<br />作監 手いっぱいだろ？"
	},
	{
		"id": 350,
		"start": 988.237,
		"end": 990.906,
		"text": "それは円さんが<br />いっぱい出すからじゃないですか"
	},
	{
		"id": 351,
		"start": 991.031,
		"end": 992.908,
		"text": "俺じゃないよ　監督だよ<br />（携帯電話の着信音）"
	},
	{
		"id": 352,
		"start": 995.202,
		"end": 995.911,
		"text": "おっ！"
	},
	{
		"id": 353,
		"start": 996.036,
		"end": 998.706,
		"text": "ああ 本田？<br />お疲れさまです"
	},
	{
		"id": 354,
		"start": 999.248,
		"end": 1000.165,
		"text": "えっ ホント？"
	},
	{
		"id": 355,
		"start": 1000.29,
		"end": 1002.626,
		"text": "ああ～ でも瀬川さんじゃ<br />どうも不満っぽい…"
	},
	{
		"id": 356,
		"start": 1002.751,
		"end": 1003.961,
		"text": "ちょちょ… 待って！"
	},
	{
		"id": 357,
		"start": 1004.086,
		"end": 1006.005,
		"text": "不満なんて<br />全然言ってませんから！"
	},
	{
		"id": 358,
		"start": 1006.13,
		"end": 1008.424,
		"text": "むしろ やっていただけるなら<br />むっちゃ光栄です"
	},
	{
		"id": 359,
		"start": 1008.924,
		"end": 1009.675,
		"text": "はあ～"
	},
	{
		"id": 360,
		"start": 1009.8,
		"end": 1012.177,
		"text": "“頼めるなら ぜひお願いしたい”<br />って言ってました"
	},
	{
		"id": 361,
		"start": 1012.302,
		"end": 1014.054,
		"text": "（瀬川）ホントかな？<br />（太郎）しゃあっす！"
	},
	{
		"id": 362,
		"start": 1014.722,
		"end": 1016.557,
		"text": "４話 ホントに大丈夫？"
	},
	{
		"id": 363,
		"start": 1016.682,
		"end": 1019.727,
		"text": "多分… じゃなくて<br />はい 大丈夫です！"
	},
	{
		"id": 364,
		"start": 1019.852,
		"end": 1021.562,
		"text": "なんとかしたい　なんとかします！"
	},
	{
		"id": 365,
		"start": 1021.937,
		"end": 1024.148,
		"text": "（瀬川）う～ん…<br />（太郎）しゃあっす！"
	},
	{
		"id": 366,
		"start": 1024.273,
		"end": 1026.15,
		"text": "（瀬川）高梨(たかなし)君だっけ？<br />（太郎）しゃあっす！"
	},
	{
		"id": 367,
		"start": 1026.275,
		"end": 1029.57,
		"text": "（瀬川）<br />回収はきみじゃなくて<br />宮森さんにお願いできるかな？"
	},
	{
		"id": 368,
		"start": 1029.695,
		"end": 1031.864,
		"text": "（瀬川）ちょっとそれイラっとする<br />（太郎）えっ？"
	},
	{
		"id": 369,
		"start": 1032.239,
		"end": 1033.073,
		"text": "しゃあっす…"
	},
	{
		"id": 370,
		"start": 1033.407,
		"end": 1036.577,
		"text": "俺 瀬川さんと<br />あんま合わないんですよ"
	},
	{
		"id": 371,
		"start": 1036.91,
		"end": 1039.288,
		"text": "彼女って<br />理詰めで描くじゃないっすか"
	},
	{
		"id": 372,
		"start": 1039.413,
		"end": 1041.04,
		"text": "けれんみに欠けるっていうか"
	},
	{
		"id": 373,
		"start": 1041.331,
		"end": 1045.252,
		"text": "そういうお前は<br />感覚最優先の暴走列車じゃね？"
	},
	{
		"id": 374,
		"start": 1045.377,
		"end": 1047.755,
		"text": "（遠藤）<br />だって そういうカットですよね？"
	},
	{
		"id": 375,
		"start": 1047.88,
		"end": 1051.675,
		"text": "分かったから　とにかく<br />リテーク修正しっかり頼むぞ"
	},
	{
		"id": 376,
		"start": 1052.176,
		"end": 1055.179,
		"text": "いや～ 瀬川さん<br />マジすげえ"
	},
	{
		"id": 377,
		"start": 1055.304,
		"end": 1059.099,
		"text": "あの人 トミオカ監督とケンカして<br />勝ったらしいからね～"
	},
	{
		"id": 378,
		"start": 1059.224,
		"end": 1062.478,
		"text": "じゃなくて ライトボックスに<br />乗っかりますよね？"
	},
	{
		"id": 379,
		"start": 1062.603,
		"end": 1063.479,
		"text": "ああ～"
	},
	{
		"id": 380,
		"start": 1063.604,
		"end": 1064.813,
		"text": "大変なんですよ！"
	},
	{
		"id": 381,
		"start": 1064.938,
		"end": 1065.814,
		"text": "（太郎）あっ？<br />（本田）えっ？"
	},
	{
		"id": 382,
		"start": 1066.19,
		"end": 1069.234,
		"text": "ホントもう 大変なんですから！"
	},
	{
		"id": 383,
		"start": 1070.069,
		"end": 1071.862,
		"text": "おっ お疲れ みゃーもり"
	},
	{
		"id": 384,
		"start": 1071.987,
		"end": 1073.113,
		"text": "お疲れさまで～す"
	},
	{
		"id": 385,
		"start": 1073.238,
		"end": 1077.034,
		"text": "あっ そうだ 仕上げの新川さん<br />顔出せって言ってたよ"
	},
	{
		"id": 386,
		"start": 1077.159,
		"end": 1078.202,
		"text": "はい 分かりました"
	},
	{
		"id": 387,
		"start": 1078.327,
		"end": 1079.745,
		"text": "（杉江）<br />お先～"
	},
	{
		"id": 388,
		"start": 1079.87,
		"end": 1080.954,
		"text": "（矢野･あおい）<br />お疲れさまで～す"
	},
	{
		"id": 389,
		"start": 1081.497,
		"end": 1083.373,
		"text": "４話 止めたんだって？"
	},
	{
		"id": 390,
		"start": 1083.499,
		"end": 1084.166,
		"text": "（あおい）<br />はい"
	},
	{
		"id": 391,
		"start": 1084.291,
		"end": 1087.961,
		"text": "はあ～ そこは<br />断るところだったと思うけどね"
	},
	{
		"id": 392,
		"start": 1088.087,
		"end": 1090.631,
		"text": "はいはい言うだけが進行じゃないよ"
	},
	{
		"id": 393,
		"start": 1090.839,
		"end": 1091.757,
		"text": "（あおい）<br />すみません"
	},
	{
		"id": 394,
		"start": 1091.882,
		"end": 1094.843,
		"text": "（新川）<br />作画に時間取られるのは<br />ある程度しょうがない"
	},
	{
		"id": 395,
		"start": 1095.094,
		"end": 1097.429,
		"text": "でも ２日<br />作監作業が止まると"
	},
	{
		"id": 396,
		"start": 1097.554,
		"end": 1101.85,
		"text": "うちらは実質 それ以上<br />作業時間がなくなることになるよね"
	},
	{
		"id": 397,
		"start": 1102.101,
		"end": 1104.269,
		"text": "尻ぬぐいをするのは<br />こっちなんだよ"
	},
	{
		"id": 398,
		"start": 1104.394,
		"end": 1105.104,
		"text": "はい…"
	},
	{
		"id": 399,
		"start": 1105.437,
		"end": 1106.605,
		"text": "撮影の佐倉さんにも"
	},
	{
		"id": 400,
		"start": 1106.73,
		"end": 1109.024,
		"text": "ちゃんと説明したほうが<br />いいと思うよ"
	},
	{
		"id": 401,
		"start": 1109.149,
		"end": 1110.734,
		"text": "スケジュールのこと"
	},
	{
		"id": 402,
		"start": 1111.527,
		"end": 1115.739,
		"text": "いいよ 別に<br />撮影は待つのも仕事だからね"
	},
	{
		"id": 403,
		"start": 1115.864,
		"end": 1116.698,
		"text": "すみません"
	},
	{
		"id": 404,
		"start": 1117.366,
		"end": 1121.078,
		"text": "（佐倉）<br />テレビシリーズってのはさ<br />１話コケたら その後 全部"
	},
	{
		"id": 405,
		"start": 1121.662,
		"end": 1123.914,
		"text": "バタバタって<br />ドミノ倒しになるんだよ"
	},
	{
		"id": 406,
		"start": 1124.039,
		"end": 1126.041,
		"text": "それだけ覚えててくれれば"
	},
	{
		"id": 407,
		"start": 1126.166,
		"end": 1126.959,
		"text": "（あおい）<br />あっ はい！"
	},
	{
		"id": 408,
		"start": 1127.167,
		"end": 1128.961,
		"text": "（丸川）<br />イェイ イェイ イェイイェイ♪<br />…と"
	},
	{
		"id": 409,
		"start": 1129.378,
		"end": 1131.463,
		"text": "おお うまそうだ ハハッ"
	},
	{
		"id": 410,
		"start": 1131.713,
		"end": 1134.258,
		"text": "いい黒糖使うと違うな～"
	},
	{
		"id": 411,
		"start": 1134.383,
		"end": 1135.676,
		"text": "（ドアが開く音）<br />（興津）失礼します"
	},
	{
		"id": 412,
		"start": 1135.801,
		"end": 1140.681,
		"text": "ああ興津さん あしたはモツね<br />モツ煮込み とろっとろだよ"
	},
	{
		"id": 413,
		"start": 1140.806,
		"end": 1144.685,
		"text": "社長 下のフロアから<br />においの苦情が来ています"
	},
	{
		"id": 414,
		"start": 1144.893,
		"end": 1146.812,
		"text": "ああ… ごめん"
	},
	{
		"id": 415,
		"start": 1146.937,
		"end": 1149.398,
		"text": "それから ヴァーナ兄弟社の<br />プロデューサーから"
	},
	{
		"id": 416,
		"start": 1149.523,
		"end": 1150.816,
		"text": "メール来てました"
	},
	{
		"id": 417,
		"start": 1150.941,
		"end": 1152.234,
		"text": "“「えくそだすっ！」面白い”って"
	},
	{
		"id": 418,
		"start": 1152.651,
		"end": 1154.153,
		"text": "ホ… ホント？"
	},
	{
		"id": 419,
		"start": 1161.577,
		"end": 1162.369,
		"text": "（あおい）<br />ただいま～"
	},
	{
		"id": 420,
		"start": 1167.708,
		"end": 1169.71,
		"text": "は～！　ドーナツ食べたい！"
	},
	{
		"id": 421,
		"start": 1169.835,
		"end": 1172.796,
		"text": "ドーナツ ドーナツ<br />ど～んといきたいよ～！"
	},
	{
		"id": 422,
		"start": 1172.921,
		"end": 1173.839,
		"text": "あっ…"
	},
	{
		"id": 423,
		"start": 1179.803,
		"end": 1181.346,
		"text": "はあ…"
	},
	{
		"id": 424,
		"start": 1187.436,
		"end": 1188.228,
		"text": "（あおい）<br />デブるよ？"
	},
	{
		"id": 425,
		"start": 1188.52,
		"end": 1190.772,
		"text": "いいじゃん<br />疲れた時 ヘコんだ時には"
	},
	{
		"id": 426,
		"start": 1190.898,
		"end": 1192.065,
		"text": "甘い物だよ"
	},
	{
		"id": 427,
		"start": 1192.191,
		"end": 1194.026,
		"text": "はあ？　自業自得！"
	},
	{
		"id": 428,
		"start": 1194.151,
		"end": 1196.028,
		"text": "４話の作監作業が<br />ストップしちゃうのに"
	},
	{
		"id": 429,
		"start": 1196.153,
		"end": 1197.654,
		"text": "なんで断らないのさ！"
	},
	{
		"id": 430,
		"start": 1197.779,
		"end": 1200.324,
		"text": "監督とデスクに言われちゃ<br />断れないよ～"
	},
	{
		"id": 431,
		"start": 1200.699,
		"end": 1202.201,
		"text": "ミンチにしちゃえ！<br />えっ？"
	},
	{
		"id": 432,
		"start": 1202.326,
		"end": 1203.243,
		"text": "太郎"
	},
	{
		"id": 433,
		"start": 1203.368,
		"end": 1204.036,
		"text": "うわ～！"
	},
	{
		"id": 434,
		"start": 1204.494,
		"end": 1206.83,
		"text": "こねて こねて<br />こねくりまくって"
	},
	{
		"id": 435,
		"start": 1207.372,
		"end": 1208.207,
		"text": "ハンバーグ？"
	},
	{
		"id": 436,
		"start": 1208.665,
		"end": 1209.333,
		"text": "捨てる！"
	},
	{
		"id": 437,
		"start": 1209.458,
		"end": 1210.584,
		"text": "ひどい～！"
	},
	{
		"id": 438,
		"start": 1216.089,
		"end": 1220.302,
		"text": "本日16時よりＲ＆Ｂスタジオで<br />３話のダビングです！"
	},
	{
		"id": 439,
		"start": 1220.427,
		"end": 1222.262,
		"text": "３話の瀬川さんの原画は？"
	},
	{
		"id": 440,
		"start": 1222.387,
		"end": 1224.056,
		"text": "13時ごろになるそうです"
	},
	{
		"id": 441,
		"start": 1224.223,
		"end": 1227.142,
		"text": "（ラジオ:男）<br />残暑の暑さも陰りが見え…"
	},
	{
		"id": 442,
		"start": 1227.309,
		"end": 1229.061,
		"text": "綱渡りか…"
	},
	{
		"id": 443,
		"start": 1232.064,
		"end": 1233.44,
		"text": "（携帯電話のバイブ音）<br />あっ！"
	},
	{
		"id": 444,
		"start": 1233.774,
		"end": 1235.609,
		"text": "はい 宮森です！"
	},
	{
		"id": 445,
		"start": 1236.151,
		"end": 1237.152,
		"text": "はあ？"
	},
	{
		"id": 446,
		"start": 1237.986,
		"end": 1239.863,
		"text": "ああ うん 届いてるよ"
	},
	{
		"id": 447,
		"start": 1239.988,
		"end": 1242.199,
		"text": "ありがとう<br />うん ごめん　おいしかった"
	},
	{
		"id": 448,
		"start": 1244.826,
		"end": 1246.036,
		"text": "瀬川さん どう？"
	},
	{
		"id": 449,
		"start": 1246.161,
		"end": 1247.496,
		"text": "（あおい）<br />もう上がると思います"
	},
	{
		"id": 450,
		"start": 1247.621,
		"end": 1250.832,
		"text": "直撃してせっつけよ<br />ツメが甘いんだよな"
	},
	{
		"id": 451,
		"start": 1251.25,
		"end": 1252.292,
		"text": "（あおい）<br />ミンチ…"
	},
	{
		"id": 452,
		"start": 1254.127,
		"end": 1255.879,
		"text": "ダビングまで あと２時間半"
	},
	{
		"id": 453,
		"start": 1256.004,
		"end": 1259.299,
		"text": "瀬川さんとこ行って<br />直接待機したほうが…"
	},
	{
		"id": 454,
		"start": 1259.508,
		"end": 1262.135,
		"text": "よし… うわ～っ！　あっ！<br />（携帯電話のバイブ音）"
	},
	{
		"id": 455,
		"start": 1262.511,
		"end": 1264.054,
		"text": "はい ムサニの宮森です！"
	},
	{
		"id": 456,
		"start": 1264.638,
		"end": 1267.391,
		"text": "はい 分かりました！<br />すぐに伺います！"
	},
	{
		"id": 457,
		"start": 1267.516,
		"end": 1270.102,
		"text": "ありがとうございました！<br />失礼します！"
	},
	{
		"id": 458,
		"start": 1270.227,
		"end": 1271.395,
		"text": "（矢野）上がった？<br />（あおい）上がりました！"
	},
	{
		"id": 459,
		"start": 1271.52,
		"end": 1273.605,
		"text": "よしっ！<br />演出と撮影のスタンバイ！"
	},
	{
		"id": 460,
		"start": 1273.73,
		"end": 1274.481,
		"text": "イェッサー！"
	},
	{
		"id": 461,
		"start": 1277.734,
		"end": 1280.821,
		"text": "遅れちゃってごめん<br />ダビング大丈夫？"
	},
	{
		"id": 462,
		"start": 1281.655,
		"end": 1284.741,
		"text": "バリバリ大丈夫です！<br />ありがとうございます！"
	},
	{
		"id": 463,
		"start": 1289.037,
		"end": 1290.831,
		"text": "（円）<br />ここでぶつかる音と…"
	},
	{
		"id": 464,
		"start": 1290.956,
		"end": 1291.873,
		"text": "チェック"
	},
	{
		"id": 465,
		"start": 1292.249,
		"end": 1293.583,
		"text": "これでダビングいける！"
	},
	{
		"id": 466,
		"start": 1293.792,
		"end": 1296.086,
		"text": "（佐倉）<br />えっ ダビングまであと１時間？"
	},
	{
		"id": 467,
		"start": 1296.461,
		"end": 1299.339,
		"text": "何言ってんの？<br />20分あれば余裕だよ"
	},
	{
		"id": 468,
		"start": 1300.048,
		"end": 1302.259,
		"text": "（木下）う～ん…<br />（矢野）お疲れさまで～す"
	},
	{
		"id": 469,
		"start": 1302.384,
		"end": 1304.303,
		"text": "（円）ダビング行ってきま～す<br />（木下）はい 行ってらっしゃ～い"
	},
	{
		"id": 470,
		"start": 1304.428,
		"end": 1307.139,
		"text": "（落合）<br />あれ？<br />また中央線 人身で止まってる"
	},
	{
		"id": 471,
		"start": 1307.264,
		"end": 1310.976,
		"text": "マジで？　太郎は… そっか<br />動仕まき出たばっかか"
	},
	{
		"id": 472,
		"start": 1312.102,
		"end": 1314.646,
		"text": "ああ 宮森さん<br />円さんをダビングに連れてって"
	},
	{
		"id": 473,
		"start": 1314.771,
		"end": 1316.356,
		"text": "今出たところだから捕まえて！"
	},
	{
		"id": 474,
		"start": 1316.481,
		"end": 1317.316,
		"text": "あっ はい！"
	},
	{
		"id": 475,
		"start": 1318.817,
		"end": 1322.487,
		"text": "宮森ってダビング初めてだっけ？<br />見学していけばいいのに"
	},
	{
		"id": 476,
		"start": 1322.612,
		"end": 1325.324,
		"text": "したいんですけど<br />４話が控えてるんで"
	},
	{
		"id": 477,
		"start": 1325.449,
		"end": 1326.45,
		"text": "それもそっか"
	},
	{
		"id": 478,
		"start": 1326.658,
		"end": 1329.661,
		"text": "（車のスキール音）"
	},
	{
		"id": 479,
		"start": 1332.414,
		"end": 1333.498,
		"text": "（ドアが開く音）"
	},
	{
		"id": 480,
		"start": 1333.623,
		"end": 1334.791,
		"text": "（円）<br />お疲れさまです"
	},
	{
