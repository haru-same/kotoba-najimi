[
	{
		"id": 1,
		"start": 3.878,
		"end": 7.757,
		"text": "（いびき）"
	},
	{
		"id": 2,
		"start": 12.137,
		"end": 14.723,
		"text": "（佐藤和真(さとうカズマ)）むなしい金(かね)だ"
	},
	{
		"id": 3,
		"start": 15.265,
		"end": 18.893,
		"text": "小銭稼いで満足したり<br />駄女神(ダメがみ)の面倒見たり"
	},
	{
		"id": 4,
		"start": 19.31,
		"end": 23.273,
		"text": "違っただろ<br />俺が望んでた異世界暮らしは"
	},
	{
		"id": 5,
		"start": 23.773,
		"end": 27.318,
		"text": "魔剣や聖剣を持って<br />凶悪なドラゴンと戦ったり"
	},
	{
		"id": 6,
		"start": 27.444,
		"end": 30.28,
		"text": "“選ばれし勇者”とか<br />称賛されたり"
	},
	{
		"id": 7,
		"start": 31.031,
		"end": 34.367,
		"text": "パーティーメンバーと<br />恋が芽生えたり"
	},
	{
		"id": 8,
		"start": 34.576,
		"end": 37.12,
		"text": "そういうもののはずだろ…"
	},
	{
		"id": 9,
		"start": 39.539,
		"end": 41.666,
		"text": "花いちもんめ！"
	},
	{
		"id": 10,
		"start": 42.542,
		"end": 46.045,
		"text": "（アクア）もう限界<br />借金に追われる生活！"
	},
	{
		"id": 11,
		"start": 46.379,
		"end": 47.505,
		"text": "クエストよ"
	},
	{
		"id": 12,
		"start": 47.63,
		"end": 50.008,
		"text": "あのデュラハンのせいで<br />きついクエストしかないけど―"
	},
	{
		"id": 13,
		"start": 50.133,
		"end": 52.844,
		"text": "請けましょう<br />お金が欲しいの！"
	},
	{
		"id": 14,
		"start": 53.178,
		"end": 56.222,
		"text": "（ダクネス）<br />わ… 私は かまわないが"
	},
	{
		"id": 15,
		"start": 56.556,
		"end": 58.808,
		"text": "（アクア）お… お願いよ～！"
	},
	{
		"id": 16,
		"start": 58.933,
		"end": 63.396,
		"text": "もう商店街のバイトはイヤなのよ<br />コロッケが売れ残ると店長が怒るの"
	},
	{
		"id": 17,
		"start": 63.521,
		"end": 67.108,
		"text": "頑張るから！ 今回は私<br />全力で頑張るからあー！"
	},
	{
		"id": 18,
		"start": 67.609,
		"end": 70.612,
		"text": "（カズマ）<br />惨めだ　この自称女神"
	},
	{
		"id": 19,
		"start": 71.488,
		"end": 76.451,
		"text": "俺の金も いずれ なくなるだろうし<br />よさそうなクエスト見つけてこいよ"
	},
	{
		"id": 20,
		"start": 76.576,
		"end": 77.994,
		"text": "（アクア）分かったわ"
	},
	{
		"id": 21,
		"start": 80.663,
		"end": 82.749,
		"text": "（めぐみん）<br />アクア 大丈夫でしょうか"
	},
	{
		"id": 22,
		"start": 83.041,
		"end": 85.46,
		"text": "とんでもないものを<br />持ってきそうですが"
	},
	{
		"id": 23,
		"start": 85.585,
		"end": 87.587,
		"text": "私は無茶なクエストでもいいが"
	},
	{
		"id": 24,
		"start": 87.712,
		"end": 90.381,
		"text": "（アクア）ん～"
	},
	{
		"id": 25,
		"start": 91.591,
		"end": 92.884,
		"text": "よし"
	},
	{
		"id": 26,
		"start": 93.009,
		"end": 96.179,
		"text": "“よし”じゃねえ！<br />お前 何請けようとしてんだよ"
	},
	{
		"id": 27,
		"start": 97.722,
		"end": 99.349,
		"text": "“マンティコアと<br />グリフォンが―”"
	},
	{
		"id": 28,
		"start": 99.474,
		"end": 102.018,
		"text": "“縄張り争いをしている<br />場所があります”"
	},
	{
		"id": 29,
		"start": 102.143,
		"end": 106.272,
		"text": "“２匹まとめて討伐してください<br />報酬は50万エリス”"
	},
	{
		"id": 30,
		"start": 106.397,
		"end": 107.774,
		"text": "…って アホか！"
	},
	{
		"id": 31,
		"start": 107.899,
		"end": 109.651,
		"text": "（カズマ）見に来て正解だった"
	},
	{
		"id": 32,
		"start": 109.776,
		"end": 111.694,
		"text": "危うく とんでもない<br />高レベルのクエストを―"
	},
	{
		"id": 33,
		"start": 111.82,
		"end": 113.78,
		"text": "請けさせられるところだった"
	},
	{
		"id": 34,
		"start": 113.905,
		"end": 115.74,
		"text": "（アクア）ちょっと これこれ！"
	},
	{
		"id": 35,
		"start": 116.616,
		"end": 118.993,
		"text": "“街の湖の水質が悪くなり―”"
	},
	{
		"id": 36,
		"start": 119.119,
		"end": 121.454,
		"text": "“ブルータル･アリゲーターが<br />住み着き始めたので―”"
	},
	{
		"id": 37,
		"start": 121.579,
		"end": 123.456,
		"text": "“水の浄化を依頼したい”"
	},
	{
		"id": 38,
		"start": 123.581,
		"end": 127.252,
		"text": "“湖の浄化ができればモンスターは<br />生息地をほかに移すため―”"
	},
	{
		"id": 39,
		"start": 127.377,
		"end": 129.379,
		"text": "“討伐はしなくてもいい”"
	},
	{
		"id": 40,
		"start": 129.504,
		"end": 131.506,
		"text": "“報酬30万エリス”"
	},
	{
		"id": 41,
		"start": 132.132,
		"end": 134.676,
		"text": "私にピッタリの<br />クエストじゃない！"
	},
	{
		"id": 42,
		"start": 135.468,
		"end": 142.433,
		"text": "♪～"
	},
	{
		"id": 43,
		"start": 217.675,
		"end": 224.682,
		"text": "～♪"
	},
	{
		"id": 44,
		"start": 226.309,
		"end": 227.602,
		"text": "（ルナ）あ…"
	},
	{
		"id": 45,
		"start": 228.269,
		"end": 229.646,
		"text": "（カズマ）うん？"
	},
	{
		"id": 46,
		"start": 230.396,
		"end": 231.439,
		"text": "どうよ？"
	},
	{
		"id": 47,
		"start": 231.564,
		"end": 233.9,
		"text": "水の浄化なんて できるのか？"
	},
	{
		"id": 48,
		"start": 234.025,
		"end": 237.07,
		"text": "バカね<br />私を誰だと思ってんの？"
	},
	{
		"id": 49,
		"start": 237.612,
		"end": 239.197,
		"text": "名前や外見のイメージで―"
	},
	{
		"id": 50,
		"start": 239.322,
		"end": 242.325,
		"text": "私が何をつかさどる女神か<br />分かるでしょう？"
	},
	{
		"id": 51,
		"start": 242.784,
		"end": 244.118,
		"text": "宴会の神様だろ？"
	},
	{
		"id": 52,
		"start": 244.244,
		"end": 245.703,
		"text": "違(ちっが)うわよ ヒキニート！"
	},
	{
		"id": 53,
		"start": 245.828,
		"end": 249.707,
		"text": "水よ！ この美しい水色の瞳と<br />この髪が見えないの？"
	},
	{
		"id": 54,
		"start": 250.458,
		"end": 252.335,
		"text": "じゃあ それを請けろよ"
	},
	{
		"id": 55,
		"start": 252.46,
		"end": 255.463,
		"text": "お前一人で請ければ<br />報酬 独り占めできるだろ"
	},
	{
		"id": 56,
		"start": 255.588,
		"end": 257.674,
		"text": "え… ええ～？"
	},
	{
		"id": 57,
		"start": 258.383,
		"end": 262.428,
		"text": "湖を浄化してると<br />モンスターが襲ってきそうだし"
	},
	{
		"id": 58,
		"start": 262.679,
		"end": 265.265,
		"text": "守ってほしいんですけど…"
	},
	{
		"id": 59,
		"start": 265.598,
		"end": 267.433,
		"text": "（カズマ）そういうことか"
	},
	{
		"id": 60,
		"start": 268.059,
		"end": 270.728,
		"text": "どれくらい かかるんだ？<br />５分くらい？"
	},
	{
		"id": 61,
		"start": 271.104,
		"end": 272.939,
		"text": "（アクア）うーん"
	},
	{
		"id": 62,
		"start": 273.064,
		"end": 274.232,
		"text": "半日くらい"
	},
	{
		"id": 63,
		"start": 274.357,
		"end": 275.692,
		"text": "長えよ！"
	},
	{
		"id": 64,
		"start": 275.817,
		"end": 279.32,
		"text": "お願い お願いよ<br />協力してよ カズマさーん！"
	},
	{
		"id": 65,
		"start": 279.445,
		"end": 281.239,
		"text": "浄化って どうやって<br />やるんだよ？"
	},
	{
		"id": 66,
		"start": 281.864,
		"end": 282.824,
		"text": "へ？"
	},
	{
		"id": 67,
		"start": 283.324,
		"end": 285.827,
		"text": "まあ 私クラスの女神なら―"
	},
	{
		"id": 68,
		"start": 285.952,
		"end": 289.163,
		"text": "水に触れてるだけで<br />湖は浄化されていくけど"
	},
	{
		"id": 69,
		"start": 289.747,
		"end": 291.082,
		"text": "なるほど"
	},
	{
		"id": 70,
		"start": 292.083,
		"end": 293.167,
		"text": "おい アクア"
	},
	{
		"id": 71,
		"start": 293.293,
		"end": 296.254,
		"text": "多分 安全に<br />浄化ができる手があるんだが"
	},
	{
		"id": 72,
		"start": 296.754,
		"end": 297.714,
		"text": "え？"
	},
	{
		"id": 73,
		"start": 306.681,
		"end": 308.641,
		"text": "ねえ 本当にやるの？"
	},
	{
		"id": 74,
		"start": 308.766,
		"end": 313.229,
		"text": "俺の考えた隙のない作戦の<br />いったい何が不満なんだ？"
	},
	{
		"id": 75,
		"start": 314.397,
		"end": 316.816,
		"text": "私 今から売られていく―"
	},
	{
		"id": 76,
		"start": 316.941,
		"end": 320.486,
		"text": "捕まった希少モンスターの気分<br />なんですけど"
	},
	{
		"id": 77,
		"start": 333.624,
		"end": 338.004,
		"text": "（カズマ）<br />決して 使えない女神を湖に<br />投棄しにきたわけではない"
	},
	{
		"id": 78,
		"start": 338.755,
		"end": 342.675,
		"text": "これで安全な檻(おり)の中から<br />水に触れて浄化ができるのだ"
	},
	{
		"id": 79,
		"start": 343.009,
		"end": 347.513,
		"text": "アクア！ 何かあったら言えよ<br />檻ごと引き上げてやるから"
	},
	{
		"id": 80,
		"start": 348.222,
		"end": 353.519,
		"text": "私 ダシを取られてる紅茶の<br />ティーバッグの気分なんですけど"
	},
	{
		"id": 81,
		"start": 358.107,
		"end": 360.818,
		"text": "鉄格子が312本"
	},
	{
		"id": 82,
		"start": 360.943,
		"end": 363.279,
		"text": "鉄格子が313本"
	},
	{
		"id": 83,
		"start": 363.738,
		"end": 365.698,
		"text": "モンスターは出てこないな"
	},
	{
		"id": 84,
		"start": 365.99,
		"end": 367.617,
		"text": "そのようですね"
	},
	{
		"id": 85,
		"start": 367.909,
		"end": 370.411,
		"text": "…にしても 今日は<br />なんか おとなしいな お前"
	},
	{
		"id": 86,
		"start": 370.536,
		"end": 371.662,
		"text": "え？"
	},
	{
		"id": 87,
		"start": 372.163,
		"end": 374.165,
		"text": "いつもだったら<br />中二っぽいことを言って―"
	},
	{
		"id": 88,
		"start": 374.29,
		"end": 376.501,
		"text": "湖ごと吹っ飛ばそうとするだろ"
	},
	{
		"id": 89,
		"start": 376.626,
		"end": 378.044,
		"text": "うむ 確かに"
	},
	{
		"id": 90,
		"start": 378.169,
		"end": 380.797,
		"text": "（めぐみん）<br />２人は私にどんなイメージを<br />持っているのですか！"
	},
	{
		"id": 91,
		"start": 380.922,
		"end": 381.964,
		"text": "フッ"
	},
	{
		"id": 92,
		"start": 382.465,
		"end": 384.509,
		"text": "我が究極の爆裂魔法は―"
	},
	{
		"id": 93,
		"start": 384.634,
		"end": 387.053,
		"text": "ワニごときに使うものでは<br />ないのです"
	},
	{
		"id": 94,
		"start": 387.512,
		"end": 390.056,
		"text": "ふだんは ムダに<br />ポンポン撃つくせに"
	},
	{
		"id": 95,
		"start": 391.14,
		"end": 393.059,
		"text": "おーい アクア！"
	},
	{
		"id": 96,
		"start": 393.476,
		"end": 396.229,
		"text": "湖の浄化は どんなもんだ？"
	},
	{
		"id": 97,
		"start": 396.687,
		"end": 398.648,
		"text": "浄化は順調よ！"
	},
	{
		"id": 98,
		"start": 398.773,
		"end": 401.567,
		"text": "（カズマ）<br />水に つかりっぱなしだと<br />冷えるだろ"
	},
	{
		"id": 99,
		"start": 401.692,
		"end": 404.07,
		"text": "トイレ 行きたくなったら<br />言えよ！"
	},
	{
		"id": 100,
		"start": 404.362,
		"end": 406.697,
		"text": "アークプリーストは<br />トイレなんか行かないし！"
	},
	{
		"id": 101,
		"start": 407.49,
		"end": 409.534,
		"text": "なんだか大丈夫そうですね"
	},
	{
		"id": 102,
		"start": 409.659,
		"end": 412.745,
		"text": "ちなみに紅魔族(こうまぞく)も<br />トイレなんて行きませんから"
	},
	{
		"id": 103,
		"start": 412.87,
		"end": 414.747,
		"text": "お前らは昔のアイドルか"
	},
	{
		"id": 104,
		"start": 414.872,
		"end": 416.791,
		"text": "わ… 私も―"
	},
	{
		"id": 105,
		"start": 416.916,
		"end": 418.918,
		"text": "クルセイダーだから―"
	},
	{
		"id": 106,
		"start": 419.043,
		"end": 421.587,
		"text": "トレイは… トイレは…"
	},
	{
		"id": 107,
		"start": 422.964,
		"end": 426.05,
		"text": "ダクネス 無理して対抗するな"
	},
	{
		"id": 108,
		"start": 426.175,
		"end": 429.262,
		"text": "トイレに行かないって言い張る<br />めぐみんとアクアには 今度―"
	},
	{
		"id": 109,
		"start": 429.387,
		"end": 431.222,
		"text": "日帰りじゃ終わらない<br />クエストを請けて―"
	},
	{
		"id": 110,
		"start": 431.347,
		"end": 433.766,
		"text": "本当にトイレ 行かないか<br />確認してやる"
	},
	{
		"id": 111,
		"start": 434.434,
		"end": 435.56,
		"text": "やめてください！"
	},
	{
		"id": 112,
		"start": 435.685,
		"end": 438.271,
		"text": "紅魔族は<br />トイレなんて行きませんよ"
	},
	{
		"id": 113,
		"start": 438.396,
		"end": 440.44,
		"text": "でも 謝るので やめてください"
	},
	{
		"id": 114,
		"start": 440.565,
		"end": 443.067,
		"text": "さすがは私の見込んだ男だ"
	},
	{
		"id": 115,
		"start": 444.485,
		"end": 446.821,
		"text": "しかし ワニ 来ませんね"
	},
	{
		"id": 116,
		"start": 446.946,
		"end": 449.949,
		"text": "このまま何事もなく<br />終わってくれればいいのですが"
	},
	{
		"id": 117,
		"start": 450.074,
		"end": 452.618,
		"text": "おまっ…<br />フラグとしか思えないセリフを"
	},
	{
		"id": 118,
		"start": 452.743,
		"end": 454.704,
		"text": "（アクア）カ… カズマーッ！"
	},
	{
		"id": 119,
		"start": 454.829,
		"end": 457.707,
		"text": "何か来た！<br />ねえ 何か いっぱい来た！"
	},
	{
		"id": 120,
		"start": 457.832,
		"end": 459.459,
		"text": "ワアアーッ！"
	},
	{
		"id": 121,
		"start": 459.876,
		"end": 463.963,
		"text": "カズマ　カズマさーん！"
	},
	{
		"id": 122,
		"start": 464.088,
		"end": 465.089,
		"text": "キャーッ！"
	},
	{
		"id": 123,
		"start": 470.678,
		"end": 472.555,
		"text": "（カズマ）<br />ワニが出てきてからアクアは"
	},
	{
		"id": 124,
		"start": 472.68,
		"end": 474.599,
		"text": "女神としての浄化能力<br />だけでなく―"
	},
	{
		"id": 125,
		"start": 474.724,
		"end": 477.685,
		"text": "もう 一心不乱に<br />浄化魔法を使いまくっている"
	},
	{
		"id": 126,
		"start": 477.81,
		"end": 479.687,
		"text": "（アクア）ピュリフィケーション！<br />ピュリフィケーション！"
	},
	{
		"id": 127,
		"start": 479.812,
		"end": 481.606,
		"text": "ピュリフィケーション！<br />ピュリフィケーション！"
	},
	{
		"id": 128,
		"start": 481.731,
		"end": 484.484,
		"text": "ピュリフィケ… ヒイーッ！"
	},
	{
		"id": 129,
		"start": 485.276,
		"end": 487.195,
		"text": "ピュリフィケーション！<br />ピュリフィケーション！"
	},
	{
		"id": 130,
		"start": 487.32,
		"end": 490.615,
		"text": "ピュリフィケーション！<br />ピュリフィケーション…"
	},
	{
		"id": 131,
		"start": 491.782,
		"end": 494.494,
		"text": "檻が！ 檻が変な音<br />立ててるんですけど"
	},
	{
		"id": 132,
		"start": 494.619,
		"end": 497.246,
		"text": "アクア<br />ギブアップなら言えよ！"
	},
	{
		"id": 133,
		"start": 497.83,
		"end": 500.625,
		"text": "鎖 引っ張って<br />檻ごと引きずって逃げるから"
	},
	{
		"id": 134,
		"start": 500.75,
		"end": 504.128,
		"text": "イヤよ！ ここで あきらめたら<br />報酬がもらえないじゃない"
	},
	{
		"id": 135,
		"start": 505.254,
		"end": 507.298,
		"text": "ワアアーッ！"
	},
	{
		"id": 136,
		"start": 507.423,
		"end": 508.508,
		"text": "“メキッ”て言った"
	},
	{
		"id": 137,
		"start": 508.633,
		"end": 511.761,
		"text": "今 檻から<br />鳴っちゃいけない音が鳴った！"
	},
	{
		"id": 138,
		"start": 511.886,
		"end": 513.804,
		"text": "ピュリフィケーション！<br />ピュリフィケーション！"
	},
	{
		"id": 139,
		"start": 513.93,
		"end": 518.226,
		"text": "あの檻の中<br />ちょっとだけ楽しそうだな"
	},
	{
		"id": 140,
		"start": 518.351,
		"end": 519.185,
		"text": "行くなよ？"
	},
	{
		"id": 141,
		"start": 519.31,
		"end": 521.187,
		"text": "（アクア）<br />ピュリフィケーション！<br />ピュリフィケーション！"
	},
	{
		"id": 142,
		"start": 521.312,
		"end": 523.689,
		"text": "ピュリフィケーション！<br />ピュリフィケーション！"
	},
	{
		"id": 143,
		"start": 524.148,
		"end": 525.983,
		"text": "ピュリフィケーション！"
	},
	{
		"id": 144,
		"start": 535.785,
		"end": 538.329,
		"text": "（めぐみん）<br />浄化は完了したみたいですね"
	},
	{
		"id": 145,
		"start": 538.454,
		"end": 540.706,
		"text": "ワニたちも全部<br />どこかへ行ったようです"
	},
	{
		"id": 146,
		"start": 541.249,
		"end": 542.917,
		"text": "アクア 無事か？"
	},
	{
		"id": 147,
		"start": 543.709,
		"end": 544.71,
		"text": "アクア？"
	},
	{
		"id": 148,
		"start": 544.835,
		"end": 547.713,
		"text": "（カズマ）おーい アクア<br />アクアさーん？"
	},
	{
		"id": 149,
		"start": 548.589,
		"end": 549.674,
		"text": "おーい"
	},
	{
		"id": 150,
		"start": 550.091,
		"end": 552.885,
		"text": "“おい”っつってんだろ！<br />どうした？"
	},
	{
		"id": 151,
		"start": 554.095,
		"end": 556.222,
		"text": "（アクアの泣き声）<br />（カズマ）ハア"
	},
	{
		"id": 152,
		"start": 556.347,
		"end": 559.225,
		"text": "泣くくらいなら<br />とっととリタイアすればいいのに"
	},
	{
		"id": 153,
		"start": 560.309,
		"end": 561.978,
		"text": "ほら　もう帰るぞ"
	},
	{
		"id": 154,
		"start": 563.104,
		"end": 565.565,
		"text": "なあ アクア<br />話し合ったんだが―"
	},
	{
		"id": 155,
		"start": 565.69,
		"end": 568.234,
		"text": "今回の報酬は<br />俺たちは要らないから"
	},
	{
		"id": 156,
		"start": 568.359,
		"end": 572.405,
		"text": "そうだぞ アクア<br />30万エリスは全部アクアの物だ"
	},
	{
		"id": 157,
		"start": 572.53,
		"end": 575.866,
		"text": "そうですね  今回はすべて<br />アクアの働きですから"
	},
	{
		"id": 158,
		"start": 577.493,
		"end": 580.329,
		"text": "お… おい<br />いいかげん檻から出ろよ"
	},
	{
		"id": 159,
		"start": 580.454,
		"end": 582.29,
		"text": "もうワニは いないから"
	},
	{
		"id": 160,
		"start": 582.415,
		"end": 584.292,
		"text": "（アクア）このまま連れてって"
	},
	{
		"id": 161,
		"start": 584.417,
		"end": 585.376,
		"text": "何だって？"
	},
	{
		"id": 162,
		"start": 586.711,
		"end": 589.589,
		"text": "檻の外の世界 怖い"
	},
	{
		"id": 163,
		"start": 589.714,
		"end": 592.174,
		"text": "このまま街まで連れてって"
	},
	{
		"id": 164,
		"start": 592.508,
		"end": 594.176,
		"text": "（３人）あ…"
	},
	{
		"id": 165,
		"start": 594.969,
		"end": 596.929,
		"text": "（カズマ）<br />どうやら 今回のクエストは"
	},
	{
		"id": 166,
		"start": 597.054,
		"end": 599.974,
		"text": "またアクアに<br />トラウマを植え付けたようだ"
	},
	{
		"id": 167,
		"start": 614.655,
		"end": 617.45,
		"text": "（咆哮(ほうこう)）"
	},
	{
		"id": 168,
		"start": 618.284,
		"end": 620.62,
		"text": "（クレメア）あれが伝説の<br />エンシェント･ドラゴン"
	},
	{
		"id": 169,
		"start": 620.745,
		"end": 624.915,
		"text": "（フィオ）あんなやつに勝てるの？<br />逃げたほうがいいんじゃ…"
	},
	{
		"id": 170,
		"start": 626.626,
		"end": 628.711,
		"text": "僕の名は ミツルギ･キョウヤ"
	},
	{
		"id": 171,
		"start": 628.836,
		"end": 631.505,
		"text": "どこにでもいる<br />平凡な高校生だったが―"
	},
	{
		"id": 172,
		"start": 631.631,
		"end": 633.674,
		"text": "死のふちで<br />美しい女神に出会い―"
	},
	{
		"id": 173,
		"start": 633.799,
		"end": 636.802,
		"text": "選ばれし勇者として<br />この世界に転生した"
	},
	{
		"id": 174,
		"start": 637.678,
		"end": 643.1,
		"text": "（ミツルギ）<br />やつを野放しにすれば 多くの人が<br />傷つき命を落とすかもしれない"
	},
	{
		"id": 175,
		"start": 643.684,
		"end": 645.77,
		"text": "やつを倒すことができるのは―"
	},
	{
		"id": 176,
		"start": 646.354,
		"end": 647.563,
		"text": "僕たちだけなんだ！"
	},
	{
		"id": 177,
		"start": 648.147,
		"end": 649.315,
		"text": "（２人）キョウヤ…"
	},
	{
		"id": 178,
		"start": 649.982,
		"end": 653.944,
		"text": "（フィオ）神々が作ったといわれ<br />キョウヤにしか扱えない神器"
	},
	{
		"id": 179,
		"start": 654.07,
		"end": 655.321,
		"text": "（クレメア）魔剣グラム"
	},
	{
		"id": 180,
		"start": 655.446,
		"end": 658.157,
		"text": "うおおーっ！"
	},
	{
		"id": 181,
		"start": 667.291,
		"end": 669.168,
		"text": "（フィオ）<br />さっすが 私のキョウヤだね"
	},
	{
		"id": 182,
		"start": 669.71,
		"end": 672.213,
		"text": "エンシェント･ドラゴンを<br />一撃で倒しちゃうなんて"
	},
	{
		"id": 183,
		"start": 672.338,
		"end": 675.132,
		"text": "い… いつ あんたのに<br />なったのよ！"
	},
	{
		"id": 184,
		"start": 675.257,
		"end": 676.675,
		"text": "おいおい やめろって"
	},
	{
		"id": 185,
		"start": 677.385,
		"end": 679.011,
		"text": "（フィオ）<br />えー？ キョウヤ 私のだもん"
	},
	{
		"id": 186,
		"start": 679.136,
		"end": 681.597,
		"text": "（クレメア）何言ってんのよ！<br />キョウヤは私のものなんだからね"
	},
	{
		"id": 187,
		"start": 681.722,
		"end": 685.184,
		"text": "（ミツルギ）僕は必ず<br />この世界を救ってみせる"
	},
	{
		"id": 188,
		"start": 685.726,
		"end": 687.436,
		"text": "あの人との約束だから"
	},
	{
		"id": 189,
		"start": 689.438,
		"end": 694.652,
		"text": "（アクア）<br />さあ その魔剣グラムと共に<br />旅立ちなさい　選ばれし勇者よ"
	},
	{
		"id": 190,
		"start": 696.195,
		"end": 700.991,
		"text": "女神様　必ず僕が魔王を討ち倒し<br />世界を救ってみせます"
	},
	{
		"id": 191,
		"start": 701.951,
		"end": 704.578,
		"text": "はい　期待していますよ"
	},
	{
		"id": 192,
		"start": 706.288,
		"end": 709.083,
		"text": "（アクア）♪ るーるー るるる"
	},
	{
		"id": 193,
		"start": 709.583,
		"end": 713.17,
		"text": "（アクア）♪ 出がらし女神が<br />（ミツルギ）この声は？"
	},
	{
		"id": 194,
		"start": 713.295,
		"end": 715.089,
		"text": "（ミツルギ）聞き覚えが"
	},
	{
		"id": 195,
		"start": 715.214,
		"end": 717.591,
		"text": "キョウヤ？ どこ行くの？"
	},
	{
		"id": 196,
		"start": 717.716,
		"end": 720.344,
		"text": "（アクア）♪ 運ばれてくよ"
	},
	{
		"id": 197,
		"start": 720.886,
		"end": 726.183,
		"text": "♪ きっと このまま"
	},
	{
		"id": 198,
		"start": 726.6,
		"end": 731.397,
		"text": "♪ 売られていくよ～"
	},
	{
		"id": 199,
		"start": 732.565,
		"end": 734.608,
		"text": "女神様あーっ！"
	},
	{
		"id": 200,
		"start": 738.487,
		"end": 740.698,
		"text": "（アクア）♪ きっと このまま"
	},
	{
		"id": 201,
		"start": 740.823,
		"end": 742.199,
		"text": "（カズマ）おい アクア"
	},
	{
		"id": 202,
		"start": 742.324,
		"end": 744.869,
		"text": "もう 街なかなんだから<br />その歌は やめてくれ"
	},
	{
		"id": 203,
		"start": 745.494,
		"end": 748.998,
		"text": "ボロボロの檻に<br />ひざ抱えた女を運んでる時点で―"
	},
	{
		"id": 204,
		"start": 749.123,
		"end": 751.125,
		"text": "人の注目 集めてるんだからな"
	},
	{
		"id": 205,
		"start": 751.876,
		"end": 753.752,
		"text": "…というか<br />いいかげん出てこいよ！"
	},
	{
		"id": 206,
		"start": 754.253,
		"end": 755.629,
		"text": "（アクア）イヤ"
	},
	{
		"id": 207,
		"start": 755.754,
		"end": 758.299,
		"text": "この中こそが<br />私の聖域よ"
	},
	{
		"id": 208,
		"start": 758.424,
		"end": 761.385,
		"text": "外の世界は怖いから<br />しばらく出ないわ"
	},
	{
		"id": 209,
		"start": 762.136,
		"end": 764.638,
		"text": "（めぐみん）すっかり<br />引きこもってしまいましたね"
	},
	{
		"id": 210,
		"start": 764.972,
		"end": 766.39,
		"text": "そうだな"
	},
	{
		"id": 211,
		"start": 766.724,
		"end": 768.476,
		"text": "以前の俺みたいだな"
	},
	{
		"id": 212,
		"start": 768.809,
		"end": 770.478,
		"text": "いや 何でもない"
	},
	{
		"id": 213,
		"start": 770.603,
		"end": 773.189,
		"text": "（ミツルギ）女神様！<br />女神様じゃないですか！"
	},
	{
		"id": 214,
		"start": 773.522,
		"end": 774.94,
		"text": "むうっ！"
	},
	{
		"id": 215,
		"start": 775.9,
		"end": 776.817,
		"text": "ええっ？"
	},
	{
		"id": 216,
		"start": 776.942,
		"end": 778.068,
		"text": "マジですか"
	},
	{
		"id": 217,
		"start": 778.194,
		"end": 781.28,
		"text": "何をしているのですか 女神様<br />こんな所で…"
	},
	{
		"id": 218,
		"start": 781.405,
		"end": 782.698,
		"text": "（ダクネス）おい"
	},
	{
		"id": 219,
		"start": 782.823,
		"end": 785.451,
		"text": "私の仲間に<br />なれなれしく触れるな"
	},
	{
		"id": 220,
		"start": 785.951,
		"end": 787.953,
		"text": "貴様 何者だ？"
	},
	{
		"id": 221,
		"start": 788.829,
		"end": 791.79,
		"text": "（カズマ）<br />あれ お前の知り合いだろ？<br />“女神”とか言ってたし"
	},
	{
		"id": 222,
		"start": 791.916,
		"end": 793.459,
		"text": "何とかしろよ"
	},
	{
		"id": 223,
		"start": 793.876,
		"end": 794.752,
		"text": "女神？"
	},
	{
		"id": 224,
		"start": 794.877,
		"end": 795.836,
		"text": "そうだよ"
	},
	{
		"id": 225,
		"start": 798.422,
		"end": 799.34,
		"text": "うん？"
	},
	{
		"id": 226,
		"start": 801.133,
		"end": 802.843,
		"text": "そうよ　女神よ 私は！"
	},
	{
		"id": 227,
		"start": 803.302,
		"end": 807.598,
		"text": "（カズマ）<br />こいつ… 自分が女神だってこと<br />本気で忘れてたんじゃないだろうな"
	},
	{
		"id": 228,
		"start": 808.39,
		"end": 809.558,
		"text": "（アクア）よっ"
	},
	{
		"id": 229,
		"start": 810.893,
		"end": 812.019,
		"text": "うわっ"
	},
	{
		"id": 230,
		"start": 814.772,
		"end": 817.608,
		"text": "さあ 女神の私に何の用かしら？"
	},
	{
		"id": 231,
		"start": 819.276,
		"end": 820.486,
		"text": "あんた 誰？"
	},
	{
		"id": 232,
		"start": 820.611,
		"end": 823.197,
		"text": "僕です<br />ミツルギ･キョウヤですよ！"
	},
	{
		"id": 233,
		"start": 823.822,
		"end": 825.866,
		"text": "あなたに<br />この魔剣グラムを頂き―"
	},
	{
		"id": 234,
		"start": 825.991,
		"end": 828.452,
		"text": "この世界へ転生した<br />ミツルギ･キョウヤです"
	},
	{
		"id": 235,
		"start": 828.577,
		"end": 829.578,
		"text": "え？"
	},
	{
		"id": 236,
		"start": 829.703,
		"end": 830.704,
		"text": "え？"
	},
	{
		"id": 237,
		"start": 830.829,
		"end": 831.956,
		"text": "（カズマ）え？"
	},
	{
		"id": 238,
		"start": 832.081,
		"end": 836.252,
		"text": "（アクア）あっ… ああ～<br />いたわね そんな人も"
	},
	{
		"id": 239,
		"start": 836.377,
		"end": 838.671,
		"text": "ごめんね すっかり忘れてたわ"
	},
	{
		"id": 240,
		"start": 838.796,
		"end": 842.883,
		"text": "結構な数の人を送ったし<br />忘れてたって しょうがないわよね"
	},
	{
		"id": 241,
		"start": 843.008,
		"end": 844.677,
		"text": "え… ええ"
	},
	{
		"id": 242,
		"start": 846.637,
		"end": 848.806,
		"text": "お久しぶりです アクア様"
	},
	{
		"id": 243,
		"start": 848.931,
		"end": 852.518,
		"text": "あなたに選ばれし勇者として<br />日々 頑張っていますよ"
	},
	{
		"id": 244,
		"start": 853.06,
		"end": 854.228,
		"text": "ところで…"
	},
	{
		"id": 245,
		"start": 854.353,
		"end": 857.982,
		"text": "アクア様は なぜ檻の中に<br />閉じ込められていたのですか？"
	},
	{
		"id": 246,
		"start": 858.607,
		"end": 861.235,
		"text": "（カズマ）本人が檻から<br />出たがらなかったんだが"
	},
	{
		"id": 247,
		"start": 864.113,
		"end": 865.489,
		"text": "はああっ？"
	},
	{
		"id": 248,
		"start": 865.614,
		"end": 868.409,
		"text": "女神様をこの世界に<br />引きずり込んで？"
	},
	{
		"id": 249,
		"start": 868.534,
		"end": 870.911,
		"text": "しかも檻に閉じ込めて<br />湖に つけた？"
	},
	{
		"id": 250,
		"start": 871.036,
		"end": 873.497,
		"text": "君は いったい<br />何を考えてるんですか？"
	},
	{
		"id": 251,
		"start": 873.622,
		"end": 877.543,
		"text": "ちょ… ちょっと　私としては<br />結構 楽しい日々を送ってるし"
	},
	{
		"id": 252,
		"start": 877.668,
		"end": 880.421,
		"text": "ここに連れてこられたことも<br />もう気にしてないし"
	},
	{
		"id": 253,
		"start": 880.546,
		"end": 884.55,
		"text": "アクア様 こんな男に<br />どう丸め込まれたか知りませんが"
	},
	{
		"id": 254,
		"start": 884.925,
		"end": 887.678,
		"text": "あなたは女神ですよ<br />それが こんな…"
	},
	{
		"id": 255,
		"start": 888.22,
		"end": 890.431,
		"text": "（カズマ）<br />言いたい放題だな この野郎"
	},
	{
		"id": 256,
		"start": 890.556,
		"end": 893.017,
		"text": "アクアのこと ろくに<br />知りもしないくせに"
	},
	{
		"id": 257,
		"start": 893.684,
		"end": 896.02,
		"text": "（通行人Ａ）何だ？<br />（通行人Ｂ）女の取り合いか？"
	},
	{
		"id": 258,
		"start": 896.145,
		"end": 899.815,
		"text": "（ミツルギ）ちなみにアクア様は<br />今 どこに寝泊まりしてるんです？"
	},
	{
		"id": 259,
		"start": 899.94,
		"end": 902.651,
		"text": "えっと 馬小屋で…"
	},
	{
		"id": 260,
		"start": 902.776,
		"end": 903.944,
		"text": "は？"
	},
	{
		"id": 261,
		"start": 905.529,
		"end": 907.406,
		"text": "（ダクネス）<br />おい いいかげん その手を離せ"
	},
	{
		"id": 262,
		"start": 907.907,
		"end": 909.909,
		"text": "礼儀知らずにも程があるだろ"
	},
	{
		"id": 263,
		"start": 910.284,
		"end": 912.119,
		"text": "ちょっと撃ちたくなってきました"
	},
	{
		"id": 264,
		"start": 912.244,
		"end": 914.163,
		"text": "（カズマ）<br />…って それはやめろ<br />俺も死ぬ"
	},
	{
		"id": 265,
		"start": 914.496,
		"end": 915.956,
		"text": "君たちは…"
	},
	{
		"id": 266,
		"start": 916.248,
		"end": 918.667,
		"text": "クルセイダーに<br />アークウィザードか"
	},
	{
		"id": 267,
		"start": 918.792,
		"end": 921.921,
		"text": "なるほど パーティーメンバーには<br />恵まれているんだね"
	},
	{
		"id": 268,
		"start": 922.796,
		"end": 925.132,
		"text": "君は こんな優秀そうな人たちが<br />いるのに―"
	},
	{
		"id": 269,
		"start": 925.257,
		"end": 929.178,
		"text": "アクア様を馬小屋で寝泊まりさせて<br />恥ずかしいと思わないのか？"
	},
	{
		"id": 270,
		"start": 929.303,
		"end": 930.721,
		"text": "（カズマ）うーん"
	},
	{
		"id": 271,
		"start": 931.055,
		"end": 931.931,
		"text": "ん？"
	},
	{
		"id": 272,
		"start": 932.056,
		"end": 936.477,
		"text": "こいつは きっと転生の特典で<br />もらった魔剣グラムとやらで―"
	},
	{
		"id": 273,
		"start": 936.602,
		"end": 938.938,
		"text": "何の苦労もせずに<br />生きてきたんだろう"
	},
	{
		"id": 274,
		"start": 939.605,
		"end": 942.441,
		"text": "そんなやつに なぜ<br />一から頑張ってきた俺が―"
	},
	{
		"id": 275,
		"start": 942.566,
		"end": 944.944,
		"text": "上から目線で<br />説教されなきゃいけないんだ？"
	},
	{
		"id": 276,
		"start": 945.694,
		"end": 948.405,
		"text": "大体 こいつらが“優秀”？"
	},
	{
		"id": 277,
		"start": 948.53,
		"end": 953.118,
		"text": "そんな 片鱗(へんりん) 一度も―"
	},
	{
		"id": 278,
		"start": 953.243,
		"end": 954.662,
		"text": "見たことが―"
	},
	{
		"id": 279,
		"start": 954.787,
		"end": 956.33,
		"text": "ないんだが！"
	},
	{
		"id": 280,
		"start": 956.705,
		"end": 957.873,
		"text": "君たち―"
	},
	{
		"id": 281,
		"start": 957.998,
		"end": 960.876,
		"text": "これからはソードマスターの僕と<br />一緒に来るといい"
	},
	{
		"id": 282,
		"start": 961.001,
		"end": 963.337,
		"text": "高級な装備品も<br />買いそろえてあげよう"
	},
	{
		"id": 283,
		"start": 963.462,
		"end": 965.005,
		"text": "（２人）ああ…"
	},
	{
		"id": 284,
		"start": 965.339,
		"end": 967.049,
		"text": "ちょっとヤバいんですけど"
	},
	{
		"id": 285,
		"start": 967.174,
		"end": 969.635,
		"text": "あの人 本気で引くぐらい<br />ヤバいんですけど"
	},
	{
		"id": 286,
		"start": 969.76,
		"end": 972.304,
		"text": "ナルシストも入ってる系で<br />怖いんですけど"
	},
	{
		"id": 287,
		"start": 972.554,
		"end": 975.975,
		"text": "どうしよう あの男は<br />生理的に受け付けない"
	},
	{
		"id": 288,
		"start": 976.1,
		"end": 981.105,
		"text": "攻めるより受けるのが好きな私だが<br />あいつだけは無性に殴りたいのだが"
	},
	{
		"id": 289,
		"start": 981.23,
		"end": 983.357,
		"text": "撃っていいですか？<br />撃っていいですか？"
	},
	{
		"id": 290,
		"start": 983.941,
		"end": 986.944,
		"text": "えーと<br />俺の仲間は満場一致で―"
	},
	{
		"id": 291,
		"start": 987.069,
		"end": 989.446,
		"text": "あなたのパーティーには<br />行きたくないみたいです"
	},
	{
		"id": 292,
		"start": 989.863,
		"end": 990.948,
		"text": "じゃ これで"
	},
	{
		"id": 293,
		"start": 994.118,
		"end": 995.369,
		"text": "（ミツルギ）待て"
	},
	{
		"id": 294,
		"start": 997.454,
		"end": 998.998,
		"text": "どいてくれます？"
	},
	{
		"id": 295,
		"start": 999.123,
		"end": 1003.335,
		"text": "悪いが アクア様を<br />こんな境遇に置いてはおけない"
	},
	{
		"id": 296,
		"start": 1003.46,
		"end": 1006.005,
		"text": "（カズマ）どうしよう<br />人の話を聞かない系だ"
	},
	{
		"id": 297,
		"start": 1006.463,
		"end": 1008.757,
		"text": "このあとの展開は目に見える"
	},
	{
		"id": 298,
		"start": 1009.341,
		"end": 1010.634,
		"text": "勝負をしないか？"
	},
	{
		"id": 299,
		"start": 1011.093,
		"end": 1012.136,
		"text": "（カズマ）やっぱりか"
	},
	{
		"id": 300,
		"start": 1012.261,
		"end": 1015.222,
		"text": "僕が勝ったら<br />アクア様を譲ってくれ"
	},
	{
		"id": 301,
		"start": 1015.347,
		"end": 1018.308,
		"text": "君が勝ったら 何でも１つ<br />言うことを聞こうじゃないか"
	},
	{
		"id": 302,
		"start": 1018.434,
		"end": 1020.602,
		"text": "よし 乗った<br />いくぞ！ ええい"
	},
	{
		"id": 303,
		"start": 1020.728,
		"end": 1021.687,
		"text": "（ミツルギ）ちょ… 待っ！"
	},
	{
		"id": 304,
		"start": 1022.146,
		"end": 1024.523,
		"text": "スティール！"
	},
	{
		"id": 305,
		"start": 1028.485,
		"end": 1029.57,
		"text": "あ？"
	},
	{
		"id": 306,
		"start": 1029.945,
		"end": 1030.946,
		"text": "ほいっ"
	},
	{
		"id": 307,
		"start": 1035.2,
		"end": 1037.369,
		"text": "言いたい放題 言いやがって"
	},
	{
		"id": 308,
		"start": 1037.995,
		"end": 1040.497,
		"text": "（フィオ）ひ… ひきょう者"
	},
	{
		"id": 309,
		"start": 1040.622,
		"end": 1043.167,
		"text": "ひきょう者 ひきょう者<br />ひきょう者～！"
	},
	{
		"id": 310,
		"start": 1044.209,
		"end": 1046.462,
		"text": "（カズマ）<br />あんたたち こいつの仲間か？"
	},
	{
		"id": 311,
		"start": 1046.587,
		"end": 1049.089,
		"text": "（クレメア）そうよ<br />この最低男 ひきょう者！"
	},
	{
		"id": 312,
		"start": 1049.798,
		"end": 1051.592,
		"text": "（カズマ）<br />魔剣持ちのチーターが"
	},
	{
		"id": 313,
		"start": 1051.717,
		"end": 1054.553,
		"text": "貧弱装備の駆け出し冒険者に<br />勝負を挑むほうが―"
	},
	{
		"id": 314,
		"start": 1054.678,
		"end": 1055.846,
		"text": "ひきょうってもんだ"
	},
	{
		"id": 315,
		"start": 1056.43,
		"end": 1057.973,
		"text": "グラムを返しなさい！"
	},
	{
		"id": 316,
		"start": 1058.098,
		"end": 1060.517,
		"text": "その魔剣はキョウヤにしか<br />使えないんだから"
	},
	{
		"id": 317,
		"start": 1060.642,
		"end": 1062.77,
		"text": "えっ マジで？"
	},
	{
		"id": 318,
		"start": 1063.27,
		"end": 1066.023,
		"text": "魔剣グラムは<br />そのイタい人専用よ"
	},
	{
		"id": 319,
		"start": 1066.148,
		"end": 1068.025,
		"text": "（カズマ）うーん"
	},
	{
		"id": 320,
		"start": 1070.611,
		"end": 1072.529,
		"text": "まあ せっかくだし<br />もらっておくか"
	},
	{
		"id": 321,
		"start": 1072.654,
		"end": 1074.615,
		"text": "ちょ…<br />ちょっと待ちなさいよ！"
	},
	{
		"id": 322,
		"start": 1074.74,
		"end": 1076.784,
		"text": "こんな勝ち方<br />私たちは認めない！"
	},
	{
		"id": 323,
		"start": 1079.62,
		"end": 1082.873,
		"text": "（カズマ）<br />真の男女平等主義者な俺は―"
	},
	{
		"id": 324,
		"start": 1082.998,
		"end": 1087.294,
		"text": "女の子相手でもドロップキックを<br />くらわせられる男"
	},
	{
		"id": 325,
		"start": 1087.961,
		"end": 1090.464,
		"text": "手加減してもらえると思うなよ"
	},
	{
		"id": 326,
		"start": 1091.006,
		"end": 1094.802,
		"text": "公衆の面前で俺のスティールが<br />さく裂するぞ"
	},
	{
		"id": 327,
		"start": 1094.927,
		"end": 1097.054,
		"text": "（フィオ）え？<br />（クレメア）え… え？"
	},
	{
		"id": 328,
		"start": 1097.179,
		"end": 1100.057,
		"text": "（カズマ）ヌッフフフ"
	},
	{
		"id": 329,
		"start": 1100.182,
		"end": 1102.309,
		"text": "さあ どうする？"
	},
	{
		"id": 330,
		"start": 1102.434,
		"end": 1105.145,
		"text": "ヌワ～ッハッハッハ"
	},
	{
		"id": 331,
		"start": 1106.772,
		"end": 1109.525,
		"text": "ほほ ほーれ<br />ほほほほ ほれ"
	},
	{
		"id": 332,
		"start": 1109.65,
		"end": 1111.318,
		"text": "（２人）ああっ…"
	},
	{
		"id": 333,
		"start": 1111.86,
		"end": 1115.239,
		"text": "イヤああーっ！"
	},
	{
		"id": 334,
		"start": 1116.865,
		"end": 1117.95,
		"text": "（ダクネス）いいっ"
	},
	{
		"id": 335,
		"start": 1118.075,
		"end": 1119.118,
		"text": "（カズマ）あ… いや"
	},
	{
		"id": 336,
		"start": 1123.038,
		"end": 1125.916,
		"text": "な… なんでよお～！"
	},
	{
		"id": 337,
		"start": 1126.416,
		"end": 1128.377,
		"text": "アクアの声だったな"
	},
	{
		"id": 338,
		"start": 1128.502,
		"end": 1130.17,
		"text": "何があったのでしょう"
	},
	{
		"id": 339,
		"start": 1130.295,
		"end": 1133.423,
		"text": "あいつは とにかく騒ぎを<br />起こさないと気が済まないのか"
	},
	{
		"id": 340,
		"start": 1137.094,
		"end": 1139.054,
		"text": "（アクア）今回の報酬…"
	},
	{
		"id": 341,
		"start": 1139.429,
		"end": 1142.683,
		"text": "壊した檻のお金を引いて<br />10万エリスだって"
	},
	{
		"id": 342,
		"start": 1143.976,
		"end": 1146.395,
		"text": "檻の修理代が20万エリス"
	},
	{
		"id": 343,
		"start": 1146.52,
		"end": 1148.981,
		"text": "私が壊したんじゃないのに"
	},
	{
		"id": 344,
		"start": 1149.314,
		"end": 1151.817,
		"text": "あの男 今度会ったら絶対―"
	},
	{
		"id": 345,
		"start": 1151.942,
		"end": 1153.569,
		"text": "ゴッドブローを<br />くらわしてやるんだから！"
	},
	{
		"id": 346,
		"start": 1153.694,
		"end": 1154.736,
		"text": "（ため息）"
	},
	{
		"id": 347,
		"start": 1154.862,
		"end": 1157.322,
		"text": "（ミツルギ）<br />捜したぞ サトウ･カズマ！"
	},
	{
		"id": 348,
		"start": 1157.447,
		"end": 1159.575,
		"text": "（荒い息）"
	},
	{
		"id": 349,
		"start": 1159.7,
		"end": 1163.287,
		"text": "君のことは ある盗賊の少女に<br />教えてもらったよ"
	},
	{
		"id": 350,
		"start": 1163.412,
		"end": 1164.83,
		"text": "“パンツ脱がせ魔だ”ってね"
	},
	{
		"id": 351,
		"start": 1165.372,
		"end": 1168.333,
		"text": "ほかにも女の子を粘液まみれに<br />するのが趣味だとか―"
	},
	{
		"id": 352,
		"start": 1168.458,
		"end": 1170.085,
		"text": "ウワサになっている<br />そうじゃないか"
	},
	{
		"id": 353,
		"start": 1170.669,
		"end": 1171.962,
		"text": "“鬼畜のカズマだ”ってね"
	},
	{
		"id": 354,
		"start": 1172.087,
		"end": 1174.464,
		"text": "お… おい 待て！<br />誰がそれ広めたのか詳しく"
	},
	{
		"id": 355,
		"start": 1175.09,
		"end": 1178.969,
		"text": "アクア様<br />僕は必ず魔王を倒すと誓います"
	},
	{
		"id": 356,
		"start": 1179.094,
		"end": 1180.971,
		"text": "ですから この僕と同じ…"
	},
	{
		"id": 357,
		"start": 1181.471,
		"end": 1183.098,
		"text": "（アクア）ゴッドブロー！"
	},
	{
		"id": 358,
		"start": 1183.223,
		"end": 1184.516,
		"text": "（ミツルギ）はあ～んっ"
	},
	{
		"id": 359,
		"start": 1184.641,
		"end": 1186.268,
		"text": "（倒れた音）<br />（一同）うん？"
	},
	{
		"id": 360,
		"start": 1186.393,
		"end": 1188.27,
		"text": "（フィオたち）キャアーッ！"
	},
	{
		"id": 361,
		"start": 1188.854,
		"end": 1190.272,
		"text": "（アクア）ちょっと あんた！"
	},
	{
		"id": 362,
		"start": 1190.397,
		"end": 1194.61,
		"text": "壊した檻の修理代 払いなさいよ<br />30万よ 30万！"
	},
	{
		"id": 363,
		"start": 1194.735,
		"end": 1196.862,
		"text": "（ミツルギ）え… あっ はい"
	},
	{
		"id": 364,
		"start": 1196.987,
		"end": 1199.364,
		"text": "（カズマ）さっき“20万”って<br />言ってなかったか？"
	},
	{
		"id": 365,
		"start": 1200.073,
		"end": 1201.366,
		"text": "（アクア）すいませーん"
	},
	{
		"id": 366,
		"start": 1201.909,
		"end": 1205.12,
		"text": "シュワシュワとカエルのから揚げ<br />山盛り下さーい"
	},
	{
		"id": 367,
		"start": 1207.581,
		"end": 1211.543,
		"text": "こ… こんなことを頼むのは<br />虫がいいのも理解している"
	},
	{
		"id": 368,
		"start": 1212.336,
		"end": 1215.214,
		"text": "だが頼む！<br />魔剣を返してくれないか？"
	},
	{
		"id": 369,
		"start": 1215.464,
		"end": 1217.758,
		"text": "代わりに店で<br />一番いい剣を買ってあげ…"
	},
	{
		"id": 370,
		"start": 1220.636,
		"end": 1224.806,
		"text": "まず この男がすでに<br />魔剣を持っていない件について"
	},
	{
		"id": 371,
		"start": 1225.349,
		"end": 1226.6,
		"text": "え？"
	},
	{
		"id": 372,
		"start": 1229.603,
		"end": 1230.771,
		"text": "プハーッ"
	},
	{
		"id": 373,
		"start": 1230.896,
		"end": 1234.483,
		"text": "サ… サトウ･カズマ<br />魔剣は？"
	},
	{
		"id": 374,
		"start": 1234.608,
		"end": 1236.777,
		"text": "ぼぼ… 僕の魔剣はどこへ？"
	},
	{
		"id": 375,
		"start": 1237.903,
		"end": 1239.029,
		"text": "売った"
	},
	{
		"id": 376,
		"start": 1239.238,
		"end": 1242.491,
		"text": "（ミツルギ）チックショオー！"
	},
	{
		"id": 377,
		"start": 1242.616,
		"end": 1244.993,
		"text": "（２人）キョウヤ～！"
	},
	{
		"id": 378,
		"start": 1247.246,
		"end": 1249.665,
		"text": "むなしい金だ　フッ"
	},
	{
		"id": 379,
		"start": 1249.79,
		"end": 1251.917,
		"text": "（ダクネス）<br />いったい何だったのだ あいつは"
	},
	{
		"id": 380,
		"start": 1252.042,
		"end": 1253.001,
		"text": "ところで…"
	},
	{
		"id": 381,
		"start": 1253.418,
		"end": 1257.881,
		"text": "先ほどからアクアが“女神”だとか<br />呼ばれていたが何の話だ？"
	},
	{
		"id": 382,
		"start": 1258.006,
		"end": 1259.132,
		"text": "（カズマ）あ…"
	},
	{
		"id": 383,
		"start": 1260.05,
		"end": 1263.637,
		"text": "（カズマ）<br />この際だ　めぐみんとダクネスには<br />話していいかもな"
	},
	{
		"id": 384,
		"start": 1265.389,
		"end": 1269.685,
		"text": "今まで黙っていたけれど<br />あなたたちには言っておくわ"
	},
	{
		"id": 385,
		"start": 1270.185,
		"end": 1271.52,
		"text": "私はアクア"
	},
	{
		"id": 386,
		"start": 1271.645,
		"end": 1275.065,
		"text": "アクシズ教団が崇拝する<br />水をつかさどる女神"
	},
	{
		"id": 387,
		"start": 1275.19,
		"end": 1278.694,
		"text": "そう　私こそが<br />あの女神アクアなのよ！"
	},
	{
		"id": 388,
		"start": 1278.819,
		"end": 1280.529,
		"text": "（ダクネス･めぐみん）<br />…ていう夢を見たのか"
	},
	{
		"id": 389,
		"start": 1280.654,
		"end": 1281.613,
		"text": "（アクア）違うわよ！"
	},
	{
		"id": 390,
		"start": 1281.738,
		"end": 1283.448,
		"text": "（ルナ）緊急　緊急！"
	},
	{
		"id": 391,
		"start": 1284.032,
		"end": 1288.578,
		"text": "全冒険者の皆さんは直ちに武装し<br />街の正門に集まってください"
	},
	{
		"id": 392,
		"start": 1288.704,
		"end": 1293.542,
		"text": "特に冒険者サトウ･カズマさんと<br />その一行は大至急でお願いします！"
	},
	{
		"id": 393,
		"start": 1294.209,
		"end": 1295.085,
		"text": "え？"
	},
	{
		"id": 394,
		"start": 1295.544,
		"end": 1298.088,
		"text": "（冒険者たちのどよめき）"
	},
	{
		"id": 395,
		"start": 1298.505,
		"end": 1300.632,
		"text": "（冒険者）<br />今度こそ ヤベえんじゃねえか"
	},
	{
		"id": 396,
		"start": 1305.512,
		"end": 1306.847,
		"text": "また あいつか"
	},
	{
		"id": 397,
		"start": 1308.056,
		"end": 1310.017,
		"text": "（デュラハン）貴様ら…"
	},
	{
		"id": 398,
		"start": 1311.476,
		"end": 1313.729,
		"text": "なぜ城に来ないのだ！"
	},
	{
		"id": 399,
		"start": 1313.854,
		"end": 1316.69,
		"text": "この 人でなしどもがーっ！"
	},
	{
		"id": 400,
		"start": 1319.484,
		"end": 1320.861,
		"text": "はい？"
	},
	{
		"id": 401,
		"start": 1321.445,
		"end": 1328.118,
		"text": "♪～"
	},
	{
		"id": 402,
