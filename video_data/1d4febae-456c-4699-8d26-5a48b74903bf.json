[
	{
		"id": 1,
		"start": 123.642,
		"end": 133.642,
		"text": "♪♪～"
	},
	{
		"id": 2,
		"start": 168.6763519999999,
		"end": 172.6803519999999,
		"text": "＜ワンピースを狙う<br />全ての海賊たちが目指す→"
	},
	{
		"id": 3,
		"start": 172.6803519999999,
		"end": 177.6803519999999,
		"text": "グランドライン後半の海<br />人呼んで　新世界＞"
	},
	{
		"id": 4,
		"start": 179.68735199999992,
		"end": 182.6903519999999,
		"text": "＜しかし<br />かつて　その海を制したのは→"
	},
	{
		"id": 5,
		"start": 182.6903519999999,
		"end": 187.6953519999999,
		"text": "海賊王　ゴールド・ロジャー<br />ただ　一人＞"
	},
	{
		"id": 6,
		"start": 187.6953519999999,
		"end": 194.7023519999999,
		"text": "＜新世界は　幾多の海賊たちの<br />夢と野望を阻んできた＞"
	},
	{
		"id": 7,
		"start": 194.7023519999999,
		"end": 198.70635199999987,
		"text": "＜その海に今　ゴムゴムの実を食べゴム人間となった少年→"
	},
	{
		"id": 8,
		"start": 198.70635199999987,
		"end": 202.7103519999999,
		"text": "モンキー・Ｄ・ルフィと<br />その仲間たちが挑む！＞"
	},
	{
		"id": 9,
		"start": 202.7103519999999,
		"end": 206.7103519999999,
		"text": "（ルフィ）海賊王に　俺は　なる！"
	},
	{
		"id": 10,
		"start": 209.7173519999999,
		"end": 213.7213519999999,
		"text": "＜新世界を支配する<br />四皇の一角を崩すため→"
	},
	{
		"id": 11,
		"start": 213.7213519999999,
		"end": 216.7243519999999,
		"text": "トラファルガー・ローと<br />海賊同盟を組んだ　ルフィたちは→"
	},
	{
		"id": 12,
		"start": 216.7243519999999,
		"end": 218.7263519999999,
		"text": "ドレスローザに乗り込むも→"
	},
	{
		"id": 13,
		"start": 218.7263519999999,
		"end": 222.73035199999987,
		"text": "現国王　ドフラミンゴの<br />計略によって→"
	},
	{
		"id": 14,
		"start": 222.73035199999987,
		"end": 224.73235199999988,
		"text": "島ごと閉じ込められてしまう＞"
	},
	{
		"id": 15,
		"start": 224.73235199999988,
		"end": 228.6693519999999,
		"text": "＜一味が　ドンキホーテファミリーと<br />激しい戦いを繰り広げる中→"
	},
	{
		"id": 16,
		"start": 228.6693519999999,
		"end": 233.67435199999989,
		"text": "ルフィとローは　王宮で<br />ついに　ドフラミンゴと激突した＞"
	},
	{
		"id": 17,
		"start": 233.67435199999989,
		"end": 236.67735199999987,
		"text": "（ベラミー）いくぞ！　麦わら！"
	},
	{
		"id": 18,
		"start": 236.67735199999987,
		"end": 242.6833519999999,
		"text": "スプリング跳人！"
	},
	{
		"id": 19,
		"start": 242.6833519999999,
		"end": 248.68935199999987,
		"text": "お前も強くなったが<br />俺も強くなった！"
	},
	{
		"id": 20,
		"start": 248.68935199999987,
		"end": 253.68935199999987,
		"text": "この技の破壊力は<br />昔の比じゃねえぞ！"
	},
	{
		"id": 21,
		"start": 265.7063520000004,
		"end": 268.7093520000004,
		"text": "くうー！！"
	},
	{
		"id": 22,
		"start": 268.7093520000004,
		"end": 271.7093520000004,
		"text": "ベラミー！　やめろ！"
	},
	{
		"id": 23,
		"start": 278.7193520000004,
		"end": 281.72235200000046,
		"text": "おい　お前！<br />そんなに動いたら死んじまうぞ！"
	},
	{
		"id": 24,
		"start": 281.72235200000046,
		"end": 285.7433520000004,
		"text": "黙れ！<br />敵の心配してるんじゃねえ！"
	},
	{
		"id": 25,
		"start": 285.7433520000004,
		"end": 289.66435200000046,
		"text": "ハァ　ハァ…。"
	},
	{
		"id": 26,
		"start": 289.66435200000046,
		"end": 292.6673520000004,
		"text": "うおおー！！"
	},
	{
		"id": 27,
		"start": 292.6673520000004,
		"end": 295.6673520000004,
		"text": "うわあー！！"
	},
	{
		"id": 28,
		"start": 297.6723520000004,
		"end": 299.6723520000004,
		"text": "（ピーカ）舞踏石！"
	},
	{
		"id": 29,
		"start": 303.6783520000005,
		"end": 305.6803520000004,
		"text": "（部下たち）ピーカさまー！"
	},
	{
		"id": 30,
		"start": 305.6803520000004,
		"end": 310.6853520000004,
		"text": "（ゾロ）くっ…　何なんだ<br />この無差別攻撃は！"
	},
	{
		"id": 31,
		"start": 310.6853520000004,
		"end": 313.68835200000046,
		"text": "あの野郎…<br />敵　味方　関係なしかよ！"
	},
	{
		"id": 32,
		"start": 313.68835200000046,
		"end": 318.69335200000046,
		"text": "ピッキャ　ピッキャ<br />ピッキャララ。"
	},
	{
		"id": 33,
		"start": 318.69335200000046,
		"end": 321.6963520000004,
		"text": "上等だ…　石ころ。"
	},
	{
		"id": 34,
		"start": 321.6963520000004,
		"end": 323.69835200000045,
		"text": "（サイ）さあ　こっちだ！<br />（ベビー５）はい！"
	},
	{
		"id": 35,
		"start": 323.69835200000045,
		"end": 328.70335200000045,
		"text": "（ベビー５）《私…　私…<br />必要とされてる！》"
	},
	{
		"id": 36,
		"start": 328.70335200000045,
		"end": 330.7053520000004,
		"text": "（ベビー５）<br />もっと　はっきり言って！"
	},
	{
		"id": 37,
		"start": 330.7053520000004,
		"end": 332.70735200000047,
		"text": "逃がさないわよ！<br />（サイ）それは→"
	},
	{
		"id": 38,
		"start": 332.70735200000047,
		"end": 335.7103520000004,
		"text": "こっちのせりふやい！<br />決着がつくまで→"
	},
	{
		"id": 39,
		"start": 335.7103520000004,
		"end": 337.71235200000046,
		"text": "決して　お前を逃がさねえ！"
	},
	{
		"id": 40,
		"start": 337.71235200000046,
		"end": 342.71735200000046,
		"text": "（ベビー５）えっ！<br />決して　お前を逃がさない…。"
	},
	{
		"id": 41,
		"start": 342.71735200000046,
		"end": 344.7193520000004,
		"text": "今のプロポーズね！"
	},
	{
		"id": 42,
		"start": 344.7193520000004,
		"end": 348.6563520000004,
		"text": "どう聞いたら　そうなるんやい！"
	},
	{
		"id": 43,
		"start": 348.6563520000004,
		"end": 350.6583520000005,
		"text": "（ラオ・Ｇ）ハダー！→"
	},
	{
		"id": 44,
		"start": 350.6583520000005,
		"end": 352.66035200000044,
		"text": "ハッ…　ハーッ！→"
	},
	{
		"id": 45,
		"start": 352.66035200000044,
		"end": 355.6633520000005,
		"text": "ホダー！　てやっ！"
	},
	{
		"id": 46,
		"start": 355.6633520000005,
		"end": 357.66535200000044,
		"text": "（部下たち）うわ！"
	},
	{
		"id": 47,
		"start": 357.66535200000044,
		"end": 359.6673520000005,
		"text": "（ラオ・Ｇ）ウダー！→"
	},
	{
		"id": 48,
		"start": 359.6673520000005,
		"end": 361.66935200000046,
		"text": "ホダー！<br />（部下たち）うわー！"
	},
	{
		"id": 49,
		"start": 361.66935200000046,
		"end": 363.6713520000004,
		"text": "（ラオ・Ｇ）スチャ！　アタッ！"
	},
	{
		"id": 50,
		"start": 363.6713520000004,
		"end": 366.67435200000045,
		"text": "あっ！　腰いったか？<br />（ラオ・Ｇ）アタタタ…。"
	},
	{
		"id": 51,
		"start": 366.67435200000045,
		"end": 369.6773520000005,
		"text": "アッタタタ…。→"
	},
	{
		"id": 52,
		"start": 369.6773520000005,
		"end": 374.6823520000005,
		"text": "アタタタ…。<br />タタタタターッ！"
	},
	{
		"id": 53,
		"start": 374.6823520000005,
		"end": 376.68435200000044,
		"text": "ハダ～！！"
	},
	{
		"id": 54,
		"start": 376.68435200000044,
		"end": 380.68835200000046,
		"text": "出た！　ラオ・Ｇさまの…。<br />（部下たち）腰痛の構え！"
	},
	{
		"id": 55,
		"start": 380.68835200000046,
		"end": 382.6903520000004,
		"text": "（チンジャオ）あやつめ…。"
	},
	{
		"id": 56,
		"start": 382.6903520000004,
		"end": 384.6923520000005,
		"text": "（ラオ・Ｇ）ハーッ！"
	},
	{
		"id": 57,
		"start": 384.6923520000005,
		"end": 388.6923520000005,
		"text": "受けてみよ！　地翁拳！"
	},
	{
		"id": 58,
		"start": 391.69935200000043,
		"end": 393.7013520000005,
		"text": "おお！？<br />（ラオ・Ｇ）ホワチャ…！"
	},
	{
		"id": 59,
		"start": 393.7013520000005,
		"end": 395.70335200000045,
		"text": "（チンジャオ）うっ…　うわ！"
	},
	{
		"id": 60,
		"start": 395.70335200000045,
		"end": 397.70335200000045,
		"text": "（ラオ・Ｇ）ハーッ！"
	},
	{
		"id": 61,
		"start": 399.70735200000047,
		"end": 401.7093520000004,
		"text": "（チンジャオ）うわああー！"
	},
	{
		"id": 62,
		"start": 401.7093520000004,
		"end": 403.7093520000004,
		"text": "（ラオ・Ｇ）ハーッ！"
	},
	{
		"id": 63,
		"start": 407.6483520000005,
		"end": 409.6483520000005,
		"text": "（ラオ・Ｇ）ハーッ！"
	},
	{
		"id": 64,
		"start": 411.6523520000005,
		"end": 414.6523520000005,
		"text": "（チンジャオ）うわあー！<br />（ラオ・Ｇ）ハーッ！"
	},
	{
		"id": 65,
		"start": 417.6583520000005,
		"end": 420.6613520000004,
		"text": "ハダダダ…！"
	},
	{
		"id": 66,
		"start": 420.6613520000004,
		"end": 424.66535200000044,
		"text": "（チンジャオ）うぎっ　ぐぎ…<br />いぎ…　ぎゃ！"
	},
	{
		"id": 67,
		"start": 424.66535200000044,
		"end": 426.6673520000005,
		"text": "ハダーッ！"
	},
	{
		"id": 68,
		"start": 426.6673520000005,
		"end": 428.66935200000046,
		"text": "（チンジャオ）もぬっ…。"
	},
	{
		"id": 69,
		"start": 428.66935200000046,
		"end": 430.6713520000004,
		"text": "さすがは　ラオ・Ｇさま！"
	},
	{
		"id": 70,
		"start": 430.6713520000004,
		"end": 434.67535200000043,
		"text": "懸賞金５億の　首領・チンジャオを<br />圧倒してる！"
	},
	{
		"id": 71,
		"start": 434.67535200000043,
		"end": 439.67535200000043,
		"text": "（ラオ・Ｇ）老うほどに痛みを知り<br />切れ味を増す！"
	},
	{
		"id": 72,
		"start": 442.68335200000047,
		"end": 446.6873520000005,
		"text": "Ｇ！"
	},
	{
		"id": 73,
		"start": 446.6873520000005,
		"end": 451.6923520000005,
		"text": "あっ…　あの　ラオ・Ｇさま→"
	},
	{
		"id": 74,
		"start": 451.6923520000005,
		"end": 454.6923520000005,
		"text": "こっちじゃなくて　後ろ…。"
	},
	{
		"id": 75,
		"start": 456.6973520000005,
		"end": 458.69935200000043,
		"text": "Ｇ！<br />（チンジャオ）あっ…。"
	},
	{
		"id": 76,
		"start": 458.69935200000043,
		"end": 460.69935200000043,
		"text": "（部下たち）あっ！"
	},
	{
		"id": 77,
		"start": 464.7053520000005,
		"end": 466.64035200000046,
		"text": "（チンジャオ）反対だぞ！<br />（部下たち）あっ！"
	},
	{
		"id": 78,
		"start": 466.64035200000046,
		"end": 469.6433520000005,
		"text": "その結婚！<br />（部下たち）あっちの話かよ！"
	},
	{
		"id": 79,
		"start": 469.6433520000005,
		"end": 472.64635200000043,
		"text": "（サイ）聞いてたのか　ジジイ！<br />しねえよ！"
	},
	{
		"id": 80,
		"start": 472.64635200000043,
		"end": 475.6493520000005,
		"text": "何ですって！？<br />（サイ）どんだけ意外なんだ！"
	},
	{
		"id": 81,
		"start": 475.6493520000005,
		"end": 477.65135200000043,
		"text": "するか！！<br />（チンジャオ）サイ！"
	},
	{
		"id": 82,
		"start": 477.65135200000043,
		"end": 482.6563520000004,
		"text": "おのれには　二宝水軍　棟梁の娘<br />ウホリシアという→"
	},
	{
		"id": 83,
		"start": 482.6563520000004,
		"end": 484.6583520000005,
		"text": "いいなずけがおろう！"
	},
	{
		"id": 84,
		"start": 484.6583520000005,
		"end": 487.6613520000004,
		"text": "（ウホリシア）サイ。　チュッ。"
	},
	{
		"id": 85,
		"start": 487.6613520000004,
		"end": 491.66535200000044,
		"text": "分かってる。　俺ぁ　嫁なんざ<br />誰だっていいんやい！→"
	},
	{
		"id": 86,
		"start": 491.66535200000044,
		"end": 495.66935200000046,
		"text": "二宝　八宝　足して<br />十宝水軍の政略結婚でも→"
	},
	{
		"id": 87,
		"start": 495.66935200000046,
		"end": 498.6723520000005,
		"text": "何でも好きにしやがれ！"
	},
	{
		"id": 88,
		"start": 498.6723520000005,
		"end": 501.67535200000043,
		"text": "何てこった…。<br />ラオ・Ｇさまがシカトされてる！"
	},
	{
		"id": 89,
		"start": 501.67535200000043,
		"end": 503.67535200000043,
		"text": "ブチギレさせちまう！"
	},
	{
		"id": 90,
		"start": 509.68335200000047,
		"end": 511.6853520000004,
		"text": "え？"
	},
	{
		"id": 91,
		"start": 511.6853520000004,
		"end": 513.6873520000005,
		"text": "（部下たち）<br />何も聞こえてなかった！"
	},
	{
		"id": 92,
		"start": 513.6873520000005,
		"end": 515.6893520000004,
		"text": "だが　待て！<br />やっぱり怒ってる！→"
	},
	{
		"id": 93,
		"start": 515.6893520000004,
		"end": 519.6933520000005,
		"text": "見ろ！　頭の先から<br />怒りのオーラが…。"
	},
	{
		"id": 94,
		"start": 519.6933520000005,
		"end": 522.6963520000005,
		"text": "（部下たち）って…　寿命ー！<br />あーっ！"
	},
	{
		"id": 95,
		"start": 522.6963520000005,
		"end": 525.7163520000005,
		"text": "（部下）よりによって　今　寿命！？"
	},
	{
		"id": 96,
		"start": 525.7163520000005,
		"end": 528.6363520000004,
		"text": "（部下）こんな勝負のつき方が<br />あるのか！？"
	},
	{
		"id": 97,
		"start": 528.6363520000004,
		"end": 531.6393520000005,
		"text": "えっ！　嘘でしょ！<br />ラオ・Ｇ　死なないで！"
	},
	{
		"id": 98,
		"start": 531.6393520000005,
		"end": 533.6413520000004,
		"text": "（部下たち）ラオ・Ｇさま～！！"
	},
	{
		"id": 99,
		"start": 533.6413520000004,
		"end": 535.6433520000005,
		"text": "帰ってきてー！！→"
	},
	{
		"id": 100,
		"start": 535.6433520000005,
		"end": 539.6473520000004,
		"text": "きてー　きてー　きてー…。"
	},
	{
		"id": 101,
		"start": 539.6473520000004,
		"end": 542.6473520000004,
		"text": "あ？"
	},
	{
		"id": 102,
		"start": 544.6523520000005,
		"end": 546.6543520000005,
		"text": "（ラオ・Ｇ）えーっ？"
	},
	{
		"id": 103,
		"start": 546.6543520000005,
		"end": 548.6563520000004,
		"text": "よく聞こえてなかったけど<br />帰ってきたー！"
	},
	{
		"id": 104,
		"start": 548.6563520000004,
		"end": 551.6593520000005,
		"text": "よかった。<br />（ラオ・Ｇ）ハハハ…。"
	},
	{
		"id": 105,
		"start": 551.6593520000005,
		"end": 553.6613520000004,
		"text": "うっ！"
	},
	{
		"id": 106,
		"start": 553.6613520000004,
		"end": 556.6643520000005,
		"text": "かの首領・チンジャオも<br />衰えたな！"
	},
	{
		"id": 107,
		"start": 556.6643520000005,
		"end": 559.6673520000005,
		"text": "（部下）あんたが言うの！？<br />（チンジャオ）ふぬっ！"
	},
	{
		"id": 108,
		"start": 559.6673520000005,
		"end": 561.6693520000005,
		"text": "（部下）ヤバい！<br />首領・チンジャオが怒ってる！"
	},
	{
		"id": 109,
		"start": 561.6693520000005,
		"end": 563.6713520000004,
		"text": "何してる　サイー！"
	},
	{
		"id": 110,
		"start": 563.6713520000004,
		"end": 565.6733520000005,
		"text": "（部下）話　かみ合ってねえ！"
	},
	{
		"id": 111,
		"start": 565.6733520000005,
		"end": 568.6763520000004,
		"text": "（ラオ・Ｇ）え？<br />（部下たち）やっぱ　聞こえてねえ！"
	},
	{
		"id": 112,
		"start": 568.6763520000004,
		"end": 571.6793520000004,
		"text": "何をしてるって？<br />（ベビー５）曲がっていますわ。"
	},
	{
		"id": 113,
		"start": 571.6793520000004,
		"end": 573.6813520000005,
		"text": "うおい！　襟を正すな！"
	},
	{
		"id": 114,
		"start": 573.6813520000005,
		"end": 577.6853520000004,
		"text": "妻か！　てめえ！<br />（ベビー５）エヘ。"
	},
	{
		"id": 115,
		"start": 577.6853520000004,
		"end": 579.6873520000005,
		"text": "（サイ）離れろ！<br />ドンキホーテファミリー！"
	},
	{
		"id": 116,
		"start": 579.6873520000005,
		"end": 581.6893520000004,
		"text": "（ベビー５）ハッ…　どうしたの？"
	},
	{
		"id": 117,
		"start": 581.6893520000004,
		"end": 583.6913520000005,
		"text": "なっ　何だ　お前は！→"
	},
	{
		"id": 118,
		"start": 583.6913520000005,
		"end": 587.6283520000005,
		"text": "怪しいやつめ！<br />お前は　敵だろう！→"
	},
	{
		"id": 119,
		"start": 587.6283520000005,
		"end": 590.6313520000004,
		"text": "それを急に結婚などと！"
	},
	{
		"id": 120,
		"start": 590.6313520000004,
		"end": 592.6333520000005,
		"text": "（ベビー５）そんな！<br />私のこと必要なんでしょ？→"
	},
	{
		"id": 121,
		"start": 592.6333520000005,
		"end": 595.6363520000004,
		"text": "何してほしい？<br />何でも言って！"
	},
	{
		"id": 122,
		"start": 595.6363520000004,
		"end": 600.6413520000004,
		"text": "カッカッカッカ。<br />ばかげてる。　じゃあ頼もうか。"
	},
	{
		"id": 123,
		"start": 600.6413520000004,
		"end": 605.6463520000004,
		"text": "死んでくれ！<br />（ベビー５）ハッ…。"
	},
	{
		"id": 124,
		"start": 605.6463520000004,
		"end": 610.6513520000004,
		"text": "お前らを野放しにして<br />麦わらたちの邪魔をされちゃあ→"
	},
	{
		"id": 125,
		"start": 610.6513520000004,
		"end": 612.6533520000005,
		"text": "恩返しも形無しだ！→"
	},
	{
		"id": 126,
		"start": 612.6533520000005,
		"end": 618.6593520000005,
		"text": "せめて　幹部どもの息の根は<br />俺たちが止めるやい！→"
	},
	{
		"id": 127,
		"start": 618.6593520000005,
		"end": 621.6623520000005,
		"text": "覚悟しろ！！"
	},
	{
		"id": 128,
		"start": 621.6623520000005,
		"end": 624.6653520000004,
		"text": "（ベビー５）それだけ？"
	},
	{
		"id": 129,
		"start": 624.6653520000004,
		"end": 626.6653520000004,
		"text": "あ？"
	},
	{
		"id": 130,
		"start": 631.6723520000005,
		"end": 635.6763520000004,
		"text": "分かった。　喜んで死ぬわ！"
	},
	{
		"id": 131,
		"start": 635.6763520000004,
		"end": 638.6793520000004,
		"text": "それで役に立てるなら。"
	},
	{
		"id": 132,
		"start": 638.6793520000004,
		"end": 640.6813520000005,
		"text": "えっ…　いや…。"
	},
	{
		"id": 133,
		"start": 640.6813520000005,
		"end": 645.7033520000005,
		"text": "私は　人の役に立てることが<br />何より　うれしい！"
	},
	{
		"id": 134,
		"start": 645.7033520000005,
		"end": 647.6213520000005,
		"text": "バカ！　よせ！　何してる！"
	},
	{
		"id": 135,
		"start": 647.6213520000005,
		"end": 649.6233520000004,
		"text": "売り言葉に買い言葉だ！"
	},
	{
		"id": 136,
		"start": 649.6233520000004,
		"end": 653.6273520000004,
		"text": "そんな自滅で勝利をもらっても<br />後味が悪ぃだけやい！"
	},
	{
		"id": 137,
		"start": 653.6273520000004,
		"end": 655.6293520000005,
		"text": "ほっとけ！　サイ！"
	},
	{
		"id": 138,
		"start": 655.6293520000005,
		"end": 661.6353520000005,
		"text": "敵の死を止めようなど<br />おのれ　頭でもいかれたか？"
	},
	{
		"id": 139,
		"start": 661.6353520000005,
		"end": 663.6373520000004,
		"text": "あっ…。"
	},
	{
		"id": 140,
		"start": 663.6373520000004,
		"end": 666.6403520000005,
		"text": "一団の棟梁ならば<br />もっと冷酷であれ！"
	},
	{
		"id": 141,
		"start": 666.6403520000005,
		"end": 668.6423520000004,
		"text": "黙ってろ　ジジイ！"
	},
	{
		"id": 142,
		"start": 668.6423520000004,
		"end": 671.6453520000005,
		"text": "いつまでも<br />師匠面してんじゃねえ！"
	},
	{
		"id": 143,
		"start": 671.6453520000005,
		"end": 675.6453520000005,
		"text": "なっ！<br />（サイ）おい　女！"
	},
	{
		"id": 144,
		"start": 678.6523520000005,
		"end": 680.6543520000005,
		"text": "（ベビー５）《何て　幸せ…》→"
	},
	{
		"id": 145,
		"start": 680.6543520000005,
		"end": 686.6603520000004,
		"text": "《死ぬことでさえ　役に立てる》"
	},
	{
		"id": 146,
		"start": 686.6603520000004,
		"end": 688.6623520000005,
		"text": "（男）《なぜ　子を生んだ》"
	},
	{
		"id": 147,
		"start": 688.6623520000005,
		"end": 692.6663520000004,
		"text": "（女）《役に立ちもしない上に<br />いっぱしの飯は食らう》"
	},
	{
		"id": 148,
		"start": 692.6663520000004,
		"end": 694.6683520000005,
		"text": "（男）《口減らしだ》→"
	},
	{
		"id": 149,
		"start": 694.6683520000005,
		"end": 698.6723520000005,
		"text": "《その子を山へ捨ててこい》"
	},
	{
		"id": 150,
		"start": 698.6723520000005,
		"end": 702.6763520000004,
		"text": "（ベビー５）<br />《ママ　どこ行くの？》→"
	},
	{
		"id": 151,
		"start": 702.6763520000004,
		"end": 705.6463520000004,
		"text": "《ねえ　ママ》"
	},
	{
		"id": 152,
		"start": 705.6463520000004,
		"end": 709.5163520000004,
		"text": "（ベビー５）《ママ？　あっ…》"
	},
	{
		"id": 153,
		"start": 709.5163520000004,
		"end": 713.5163520000004,
		"text": "《あっ　ママ！<br />ママ　待って！　お願い！》"
	},
	{
		"id": 154,
		"start": 718.5253520000005,
		"end": 720.5273520000005,
		"text": "（母親）<br />《付いてくるんじゃないよ》"
	},
	{
		"id": 155,
		"start": 720.5273520000005,
		"end": 725.5323520000005,
		"text": "《やだ！　お願い！<br />ママと一緒に行く！》"
	},
	{
		"id": 156,
		"start": 725.5323520000005,
		"end": 729.5363520000004,
		"text": "《お前は　役に立たない》"
	},
	{
		"id": 157,
		"start": 729.5363520000004,
		"end": 736.5363520000004,
		"text": "《役に立たない　お前は<br />必要のない人間なんだから》"
	},
	{
		"id": 158,
		"start": 738.5453520000004,
		"end": 745.5453520000004,
		"text": "（ベビー５）《ママにも？<br />ママにも…　必要ない？》"
	},
	{
		"id": 159,
		"start": 747.5543520000004,
		"end": 754.5613520000005,
		"text": "（ベビー５の泣き声）"
	},
	{
		"id": 160,
		"start": 754.5613520000005,
		"end": 757.5643520000004,
		"text": "《えっ？<br />新聞５０社とってほしい？》"
	},
	{
		"id": 161,
		"start": 757.5643520000004,
		"end": 760.5673520000005,
		"text": "《私　必要とされてる！》"
	},
	{
		"id": 162,
		"start": 760.5673520000005,
		"end": 763.5703520000004,
		"text": "（ベビー５）<br />《えっ！　結婚してほしい？》"
	},
	{
		"id": 163,
		"start": 763.5703520000004,
		"end": 766.5733520000005,
		"text": "《私　必要とされてる！》"
	},
	{
		"id": 164,
		"start": 766.5733520000005,
		"end": 773.5803520000005,
		"text": "（母親）《役に立たない　お前は<br />必要のない人間なんだから》"
	},
	{
		"id": 165,
		"start": 773.5803520000005,
		"end": 778.5853520000005,
		"text": "《私　今　必要とされてる》"
	},
	{
		"id": 166,
		"start": 778.5853520000005,
		"end": 781.5883520000004,
		"text": "よせってんだ！　バカ女！"
	},
	{
		"id": 167,
		"start": 781.5883520000004,
		"end": 783.5903520000005,
		"text": "バカは　おのれじゃー！"
	},
	{
		"id": 168,
		"start": 783.5903520000005,
		"end": 785.5923520000005,
		"text": "ジジイ！？"
	},
	{
		"id": 169,
		"start": 785.5923520000005,
		"end": 788.5953520000005,
		"text": "（部下）ひい！　何て威力だ！"
	},
	{
		"id": 170,
		"start": 788.5953520000005,
		"end": 790.5953520000005,
		"text": "（部下）あれは！"
	},
	{
		"id": 171,
		"start": 795.6023520000005,
		"end": 801.6083520000004,
		"text": "（チンジャオ）サイ！<br />この八宝水軍の恥さらしめが！→"
	},
	{
		"id": 172,
		"start": 801.6083520000004,
		"end": 806.6133520000004,
		"text": "おのれのような者は<br />八宝水軍にはいらぬ！"
	},
	{
		"id": 173,
		"start": 806.6133520000004,
		"end": 808.6133520000004,
		"text": "くっ！"
	},
	{
		"id": 174,
		"start": 811.6183520000004,
		"end": 815.6223520000004,
		"text": "サイ！　聞いておるのか！"
	},
	{
		"id": 175,
		"start": 815.6223520000004,
		"end": 820.6273520000004,
		"text": "これは　俺の問題だ！<br />立ち入るな～！"
	},
	{
		"id": 176,
		"start": 820.6273520000004,
		"end": 825.6323520000004,
		"text": "たわけが～！　があ～！！"
	},
	{
		"id": 177,
		"start": 825.6323520000004,
		"end": 827.6683520000005,
		"text": "八衝拳　奥義！"
	},
	{
		"id": 178,
		"start": 827.6683520000005,
		"end": 829.6703520000004,
		"text": "あれは　まさか！"
	},
	{
		"id": 179,
		"start": 829.6703520000004,
		"end": 832.6733520000005,
		"text": "氷の大陸を割る頭突き！？"
	},
	{
		"id": 180,
		"start": 832.6733520000005,
		"end": 837.6783520000005,
		"text": "（チンジャオ）２人まとめて<br />わが錐頭で　かち割ってくれる！"
	},
	{
		"id": 181,
		"start": 837.6783520000005,
		"end": 842.6833520000005,
		"text": "錐龍錐釘！"
	},
	{
		"id": 182,
		"start": 842.6833520000005,
		"end": 845.6863520000005,
		"text": "ジジイ～！！"
	},
	{
		"id": 183,
		"start": 845.6863520000005,
		"end": 849.6903520000005,
		"text": "どーっ！！<br />（チンジャオ）うおおー！"
	},
	{
		"id": 184,
		"start": 849.6903520000005,
		"end": 852.6903520000005,
		"text": "武脚跟！"
	},
	{
		"id": 185,
		"start": 859.7003520000005,
		"end": 862.7033520000005,
		"text": "ぐっ…！<br />（チンジャオ）ぬおー！！"
	},
	{
		"id": 186,
		"start": 862.7033520000005,
		"end": 866.7073520000004,
		"text": "ぐわー！！<br />（チンジャオ）うぬっ…！"
	},
	{
		"id": 187,
		"start": 866.7073520000004,
		"end": 869.7073520000004,
		"text": "（サイ）ぬおおー！！"
	},
	{
		"id": 188,
		"start": 873.7143520000004,
		"end": 875.7143520000004,
		"text": "（部下たち）ああ！"
	},
	{
		"id": 189,
		"start": 893.6244759999997,
		"end": 899.6244759999997,
		"text": "（母親）《お前は<br />必要のない人間なんだから》"
	},
	{
		"id": 190,
		"start": 901.6324759999995,
		"end": 905.6364759999997,
		"text": "（ベビー５）<br />《私　今　必要とされてる》"
	},
	{
		"id": 191,
		"start": 905.6364759999997,
		"end": 909.6404759999996,
		"text": "うっ…　ぐわ！"
	},
	{
		"id": 192,
		"start": 909.6404759999996,
		"end": 913.6404759999996,
		"text": "つまらねえまね　するんじゃねえ！"
	},
	{
		"id": 193,
		"start": 923.6544759999997,
		"end": 926.6574759999996,
		"text": "ハァ　ハァ…。"
	},
	{
		"id": 194,
		"start": 926.6574759999996,
		"end": 928.6594759999996,
		"text": "（チンジャオ）うっ…。"
	},
	{
		"id": 195,
		"start": 928.6594759999996,
		"end": 930.6594759999996,
		"text": "（サイ・ベビー５）あっ！"
	},
	{
		"id": 196,
		"start": 941.6724759999997,
		"end": 946.6934759999997,
		"text": "（ベビー５）どうして…<br />どうして　止めたの？"
	},
	{
		"id": 197,
		"start": 946.6934759999997,
		"end": 950.6144759999995,
		"text": "あなたの役に立とうとしたのに！"
	},
	{
		"id": 198,
		"start": 950.6144759999995,
		"end": 956.6204759999996,
		"text": "死んで…<br />必要な人間になりたかったのに！"
	},
	{
		"id": 199,
		"start": 956.6204759999996,
		"end": 963.6204759999996,
		"text": "それを否定されねえ場所で<br />生きてきたのか。"
	},
	{
		"id": 200,
		"start": 969.6334759999995,
		"end": 971.6334759999995,
		"text": "ん？"
	},
	{
		"id": 201,
		"start": 976.6404759999996,
		"end": 980.6444759999997,
		"text": "あの　首領・チンジャオの錐の頭が<br />やっぱり折れてる！"
	},
	{
		"id": 202,
		"start": 980.6444759999997,
		"end": 982.6464759999997,
		"text": "信じられねえ…。"
	},
	{
		"id": 203,
		"start": 982.6464759999997,
		"end": 986.6504759999996,
		"text": "これを俺がやったのか？"
	},
	{
		"id": 204,
		"start": 986.6504759999996,
		"end": 992.6564759999997,
		"text": "（チンジャオ）サイ…　おのれ　この錐頭<br />よくぞ　へし折ったな。"
	},
	{
		"id": 205,
		"start": 992.6564759999997,
		"end": 994.6584759999996,
		"text": "仲間割れが始まるのか？"
	},
	{
		"id": 206,
		"start": 994.6584759999996,
		"end": 1002.6664759999996,
		"text": "ついに…<br />ついに　力が覚醒したのだ！→"
	},
	{
		"id": 207,
		"start": 1002.6664759999996,
		"end": 1007.6044759999995,
		"text": "血と汗と涙の修業が<br />今　実を結んだのだ！"
	},
	{
		"id": 208,
		"start": 1007.6044759999995,
		"end": 1010.6074759999997,
		"text": "ジジイ？<br />（チンジャオ）よくやった。"
	},
	{
		"id": 209,
		"start": 1010.6074759999997,
		"end": 1015.6124759999996,
		"text": "八衝拳の奥義は<br />伝承されたのじゃ。"
	},
	{
		"id": 210,
		"start": 1015.6124759999996,
		"end": 1019.6164759999997,
		"text": "サイ。<br />おのれの　その武脚に→"
	},
	{
		"id": 211,
		"start": 1019.6164759999997,
		"end": 1023.6204760000003,
		"text": "錐龍錐釘の名を授けよう。"
	},
	{
		"id": 212,
		"start": 1023.6204760000003,
		"end": 1025.6224760000007,
		"text": "えっ…。"
	},
	{
		"id": 213,
		"start": 1025.6224760000007,
		"end": 1027.6244760000009,
		"text": "（チンジャオ）もはや<br />おのれの　その脚は→"
	},
	{
		"id": 214,
		"start": 1027.6244760000009,
		"end": 1032.6294760000007,
		"text": "氷の大陸をも打ち砕くだろう。"
	},
	{
		"id": 215,
		"start": 1032.6294760000007,
		"end": 1037.6344760000009,
		"text": "八宝水軍は　これより<br />完全に　おのれのもの！"
	},
	{
		"id": 216,
		"start": 1037.6344760000009,
		"end": 1040.6374760000008,
		"text": "いいなずけも破棄する。→"
	},
	{
		"id": 217,
		"start": 1040.6374760000008,
		"end": 1043.6404760000007,
		"text": "全て　おのれの自由だ！"
	},
	{
		"id": 218,
		"start": 1043.6404760000007,
		"end": 1045.6424760000007,
		"text": "（サイ）ジジイ…。"
	},
	{
		"id": 219,
		"start": 1045.6424760000007,
		"end": 1048.6454760000008,
		"text": "よくぞ…　よくぞ…。"
	},
	{
		"id": 220,
		"start": 1048.6454760000008,
		"end": 1050.6474760000008,
		"text": "サイよ！<br />（サイ）ジジイ…。→"
	},
	{
		"id": 221,
		"start": 1050.6474760000008,
		"end": 1052.6494760000007,
		"text": "恥ずかしいじゃねえか。"
	},
	{
		"id": 222,
		"start": 1052.6494760000007,
		"end": 1055.6524760000007,
		"text": "いい話だな～。"
	},
	{
		"id": 223,
		"start": 1055.6524760000007,
		"end": 1059.6564760000008,
		"text": "（チンジャオ）実は<br />麦わらとの対戦以降→"
	},
	{
		"id": 224,
		"start": 1059.6564760000008,
		"end": 1064.6614760000007,
		"text": "持病がうずき<br />戦いが　ままならんかったのじゃ。"
	},
	{
		"id": 225,
		"start": 1064.6614760000007,
		"end": 1067.5974760000008,
		"text": "わしも年だ。　うれしいぞ。"
	},
	{
		"id": 226,
		"start": 1067.5974760000008,
		"end": 1071.6014760000007,
		"text": "おのれが　こうして…。<br />（ラオ・Ｇ）ホダー！"
	},
	{
		"id": 227,
		"start": 1071.6014760000007,
		"end": 1075.6054760000009,
		"text": "ぐわあー！！<br />（ラオ・Ｇ）Ｇの刻印！→"
	},
	{
		"id": 228,
		"start": 1075.6054760000009,
		"end": 1077.6054760000009,
		"text": "ハダー！"
	},
	{
		"id": 229,
		"start": 1079.6094760000008,
		"end": 1081.6114760000007,
		"text": "ジジイ！！"
	},
	{
		"id": 230,
		"start": 1081.6114760000007,
		"end": 1083.6114760000007,
		"text": "ラオ・Ｇさま！"
	},
	{
		"id": 231,
		"start": 1088.6184760000008,
		"end": 1091.6214760000007,
		"text": "（チンジャオ）うあ…。"
	},
	{
		"id": 232,
		"start": 1091.6214760000007,
		"end": 1096.6264760000008,
		"text": "（部下）ぬわー！　ラオ・Ｇさまが<br />首領・チンジャオを倒した！"
	},
	{
		"id": 233,
		"start": 1096.6264760000008,
		"end": 1098.6284760000008,
		"text": "（部下）しかも　何だ　あの体は！"
	},
	{
		"id": 234,
		"start": 1098.6284760000008,
		"end": 1100.6304760000007,
		"text": "（ラオ・Ｇ）ファファファファ！"
	},
	{
		"id": 235,
		"start": 1100.6304760000007,
		"end": 1104.6304760000007,
		"text": "地翁拳　究極の秘技。"
	},
	{
		"id": 236,
		"start": 1107.6374760000008,
		"end": 1112.6424760000007,
		"text": "病や　ケガ　また<br />老いてしまったときのために→"
	},
	{
		"id": 237,
		"start": 1112.6424760000007,
		"end": 1118.6484760000008,
		"text": "若いころより　筋力を積み立て<br />保管しておく気功術！"
	},
	{
		"id": 238,
		"start": 1118.6484760000008,
		"end": 1123.6534760000009,
		"text": "技は熟練　力は全盛期！→"
	},
	{
		"id": 239,
		"start": 1123.6534760000009,
		"end": 1129.5934760000007,
		"text": "いったい　誰が<br />この　わしに勝てようか～！"
	},
	{
		"id": 240,
		"start": 1129.5934760000007,
		"end": 1131.5954760000009,
		"text": "これが　ラオ・Ｇさまの真の力！"
	},
	{
		"id": 241,
		"start": 1131.5954760000009,
		"end": 1135.5994760000008,
		"text": "ファファファ…！"
	},
	{
		"id": 242,
		"start": 1135.5994760000008,
		"end": 1137.6014760000007,
		"text": "ん？"
	},
	{
		"id": 243,
		"start": 1137.6014760000007,
		"end": 1141.6054760000009,
		"text": "（ラオ・Ｇ）花ノ国の猿が<br />わしに何の用じゃ！"
	},
	{
		"id": 244,
		"start": 1141.6054760000009,
		"end": 1144.6084760000008,
		"text": "よくも　ジジイを…。"
	},
	{
		"id": 245,
		"start": 1144.6084760000008,
		"end": 1146.6104760000007,
		"text": "許さねえ！"
	},
	{
		"id": 246,
		"start": 1146.6104760000007,
		"end": 1149.6134760000007,
		"text": "ファファファ　何を言うとる。"
	},
	{
		"id": 247,
		"start": 1149.6134760000007,
		"end": 1152.6164760000008,
		"text": "お前ごときが<br />わしに立ち向かうなど→"
	},
	{
		"id": 248,
		"start": 1152.6164760000008,
		"end": 1154.6184760000008,
		"text": "１００年　早いわ！"
	},
	{
		"id": 249,
		"start": 1154.6184760000008,
		"end": 1156.6204760000007,
		"text": "ジジイの敵…。"
	},
	{
		"id": 250,
		"start": 1156.6204760000007,
		"end": 1160.6244760000009,
		"text": "そして<br />麦わらたちへの恩返しやい！"
	},
	{
		"id": 251,
		"start": 1160.6244760000009,
		"end": 1162.6264760000008,
		"text": "バカな猿だ。"
	},
	{
		"id": 252,
		"start": 1162.6264760000008,
		"end": 1167.6314760000007,
		"text": "麦わらが　若を倒せると<br />本気で信じておるのか？"
	},
	{
		"id": 253,
		"start": 1167.6314760000007,
		"end": 1172.6364760000008,
		"text": "ああ！<br />（ラオ・Ｇ）ハダーッ！"
	},
	{
		"id": 254,
		"start": 1172.6364760000008,
		"end": 1176.6404760000007,
		"text": "いいか！<br />このドレスローザは　若のもの！"
	},
	{
		"id": 255,
		"start": 1176.6404760000007,
		"end": 1182.6464760000008,
		"text": "この国にいる　全ての者は<br />若の奴隷だー！"
	},
	{
		"id": 256,
		"start": 1182.6464760000008,
		"end": 1184.6484760000008,
		"text": "（ラオ・Ｇ）ハダッ！"
	},
	{
		"id": 257,
		"start": 1184.6484760000008,
		"end": 1186.6664760000008,
		"text": "やーっ！"
	},
	{
		"id": 258,
		"start": 1186.6664760000008,
		"end": 1190.5874760000008,
		"text": "ハッ！<br />いくら　麦わらを信じようと→"
	},
	{
		"id": 259,
		"start": 1190.5874760000008,
		"end": 1194.5914760000007,
		"text": "貴様らは<br />一生　この鳥カＧＯから→"
	},
	{
		"id": 260,
		"start": 1194.5914760000007,
		"end": 1196.5934760000007,
		"text": "出られやせんのじゃ！→"
	},
	{
		"id": 261,
		"start": 1196.5934760000007,
		"end": 1201.5984760000008,
		"text": "鳥カＧＯの「Ｇ」！"
	},
	{
		"id": 262,
		"start": 1201.5984760000008,
		"end": 1206.6034760000007,
		"text": "ふーむ。　すばしっこさだけは<br />認めてやる。→"
	},
	{
		"id": 263,
		"start": 1206.6034760000007,
		"end": 1213.6104760000007,
		"text": "じゃが　ベビー５とて<br />お前には渡さんぞ！"
	},
	{
		"id": 264,
		"start": 1213.6104760000007,
		"end": 1215.6124760000007,
		"text": "え？　ラオ・Ｇ！"
	},
	{
		"id": 265,
		"start": 1215.6124760000007,
		"end": 1217.6144760000006,
		"text": "（ラオ・Ｇ）待っておれ　ベビー５。"
	},
	{
		"id": 266,
		"start": 1217.6144760000006,
		"end": 1220.6174760000008,
		"text": "今　このバカを血祭りに<br />あＧＥて→"
	},
	{
		"id": 267,
		"start": 1220.6174760000008,
		"end": 1223.6204760000007,
		"text": "救い出してやるぞの「Ｇ」！"
	},
	{
		"id": 268,
		"start": 1223.6204760000007,
		"end": 1227.6244760000009,
		"text": "どうしましょう…<br />２人で　私の取り合いを…　キャ！"
	},
	{
		"id": 269,
		"start": 1227.6244760000009,
		"end": 1230.6274760000008,
		"text": "危ねえ！　どいてろ！<br />（ラオ・Ｇ）ハダーッ！"
	},
	{
		"id": 270,
		"start": 1230.6274760000008,
		"end": 1233.6304760000007,
		"text": "（ラオ・Ｇ）ファファファ…。→"
	},
	{
		"id": 271,
		"start": 1233.6304760000007,
		"end": 1235.6324760000007,
		"text": "しょせん　猿は猿じゃな！→"
	},
	{
		"id": 272,
		"start": 1235.6324760000007,
		"end": 1240.6374760000008,
		"text": "ベビー５を　盾にでも使えば<br />少しは優位に戦えたものを！"
	},
	{
		"id": 273,
		"start": 1240.6374760000008,
		"end": 1242.6394760000007,
		"text": "え？<br />（ラオ・Ｇ）ハダーッ！"
	},
	{
		"id": 274,
		"start": 1242.6394760000007,
		"end": 1245.6424760000007,
		"text": "うわ…　わああー！→"
	},
	{
		"id": 275,
		"start": 1245.6424760000007,
		"end": 1247.6424760000007,
		"text": "ああ…。"
	},
	{
		"id": 276,
		"start": 1249.5794760000008,
		"end": 1252.5824760000007,
		"text": "ファファファ…。"
	},
	{
		"id": 277,
		"start": 1252.5824760000007,
		"end": 1255.5854760000007,
		"text": "ベビー５は　われわれのものだ。"
	},
	{
		"id": 278,
		"start": 1255.5854760000007,
		"end": 1259.5894760000008,
		"text": "必要とされるだけで<br />命をも捨てる。"
	},
	{
		"id": 279,
		"start": 1259.5894760000008,
		"end": 1264.5944760000007,
		"text": "こんな便利な女が　他におるか？"
	},
	{
		"id": 280,
		"start": 1264.5944760000007,
		"end": 1266.5944760000007,
		"text": "（ベビー５）《便利…》"
	},
	{
		"id": 281,
		"start": 1275.6054760000009,
		"end": 1277.6074760000008,
		"text": "（サイ）このっ…！"
	},
	{
		"id": 282,
		"start": 1277.6074760000008,
		"end": 1282.6124760000007,
		"text": "（ラオ・Ｇ）最終奥義で<br />あの世に送ってやる！"
	},
	{
		"id": 283,
		"start": 1282.6124760000007,
		"end": 1290.6124760000007,
		"text": "ハア～！！"
	},
	{
		"id": 284,
		"start": 1295.6254760000008,
		"end": 1297.6274760000008,
		"text": "ハダ～！！"
	},
	{
		"id": 285,
		"start": 1297.6274760000008,
		"end": 1299.6294760000007,
		"text": "（サイ）くっ…。"
	},
	{
		"id": 286,
		"start": 1299.6294760000007,
		"end": 1302.6324760000007,
		"text": "これが　最ＧＯだの「Ｇ」！"
	},
	{
		"id": 287,
		"start": 1302.6324760000007,
		"end": 1305.6354760000008,
		"text": "おい　じゃあよ…。"
	},
	{
		"id": 288,
		"start": 1305.6354760000008,
		"end": 1307.5704760000008,
		"text": "あの女は…。"
	},
	{
		"id": 289,
		"start": 1307.5704760000008,
		"end": 1312.5754760000007,
		"text": "勝ったら　俺が　妻にもらうぞ～！！"
	},
	{
		"id": 290,
		"start": 1312.5754760000007,
		"end": 1315.5784760000008,
		"text": "えーっ！<br />ああ～　ズッキュン！"
	},
	{
		"id": 291,
		"start": 1315.5784760000008,
		"end": 1317.5804760000008,
		"text": "ハーッ！！"
	},
	{
		"id": 292,
		"start": 1317.5804760000008,
		"end": 1320.5834760000007,
		"text": "うわああー！！"
	},
	{
		"id": 293,
		"start": 1320.5834760000007,
		"end": 1322.5854760000007,
		"text": "おおっ！"
	},
	{
		"id": 294,
		"start": 1322.5854760000007,
		"end": 1327.5854760000007,
		"text": "（サイ）錐龍錐釘！"
	},
	{
		"id": 295,
		"start": 1330.5934760000007,
		"end": 1332.5954760000009,
		"text": "きっ…！"
	},
	{
		"id": 296,
		"start": 1332.5954760000009,
		"end": 1335.5984760000008,
		"text": "（ラオ・Ｇ）ぐぬぬ…。<br />（ベビー５）あっ。"
	},
	{
		"id": 297,
		"start": 1335.5984760000008,
		"end": 1339.6024760000007,
		"text": "ぐあああ！！"
	},
	{
		"id": 298,
		"start": 1339.6024760000007,
		"end": 1343.6024760000007,
		"text": "（ラオ・Ｇ）若…　すまぬ…。"
	},
	{
		"id": 299,
		"start": 1354.6174760000008,
		"end": 1358.6214760000007,
		"text": "Ｇ～！！→"
	},
	{
		"id": 300,
		"start": 1358.6214760000007,
		"end": 1360.6234760000007,
		"text": "くそ！　若猿！"
	},
	{
		"id": 301,
		"start": 1360.6234760000007,
		"end": 1364.6274760000008,
		"text": "バカ猿ＧＡの「Ｇ」！"
	},
	{
		"id": 302,
		"start": 1364.6274760000008,
		"end": 1368.6274760000008,
		"text": "（部下たち）うわあー！！"
	},
	{
		"id": 303,
		"start": 1371.5684760000008,
		"end": 1375.5724760000007,
		"text": "首領と呼べやい！"
	},
	{
		"id": 304,
		"start": 1375.5724760000007,
		"end": 1378.5754760000007,
		"text": "＜王宮のある台地　２段目＞"
	},
	{
		"id": 305,
		"start": 1378.5754760000007,
		"end": 1381.5754760000007,
		"text": "＜幹部戦…＞"
	},
	{
		"id": 306,
		"start": 1384.5814760000007,
		"end": 1388.5814760000007,
		"text": "スススス…。<br />（サイ）あ！？"
	},
	{
		"id": 307,
		"start": 1393.5904760000008,
		"end": 1396.5934760000007,
		"text": "うっ…。<br />（ベビー５）ふつつか者ですが→"
	},
	{
		"id": 308,
		"start": 1396.5934760000007,
		"end": 1399.5934760000007,
		"text": "よろしくお願いしま～す！"
	},
	{
		"id": 309,
		"start": 1404.6014760000007,
		"end": 1408.6054760000009,
		"text": "＜かつて　憧れたドフラミンゴに<br />裏切られ　操られ→"
	},
	{
		"id": 310,
		"start": 1408.6054760000009,
		"end": 1411.6084760000008,
		"text": "殺されかけて　なお<br />ルフィに立ち向かう　ベラミー＞"
	},
	{
		"id": 311,
		"start": 1411.6084760000008,
		"end": 1414.6114760000007,
		"text": "＜友情と　通すべき筋とで揺れる<br />ベラミーが→"
	},
	{
		"id": 312,
		"start": 1414.6114760000007,
		"end": 1416.6134760000007,
		"text": "最後の覚悟を決めたとき→"
	},
	{
		"id": 313,
		"start": 1416.6134760000007,
		"end": 1419.6164760000008,
		"text": "絶大な破壊力を持つ一撃が<br />ルフィを襲う！＞"
	},
	{
		"id": 314,
		"start": 1419.6164760000008,
		"end": 1421.6164760000008,
		"text": "次回　『ワンピース』"
	},
	{
		"id": 315,
		"start": 1424.6214760000007,
		"end": 1427.6214760000007,
		"text": "海賊王に　俺は　なる！"
	},
	{
		"id": 316,
		"start": 1438.8184760000008,
		"end": 1440.1704760000007,
		"text": "（佐々木・東野）<br />おはようございます。"
	},
	{
		"id": 317,
		"start": 1440.1704760000007,
		"end": 1442.1554760000008,
		"text": "（東野）『ワイドナショー』の<br />お時間でございます。"
	},
	{
		"id": 318,
		"start": 1442.1554760000008,
		"end": 1444.1574760000008,
		"text": "けさも　よろしくお願いします。<br />さあ　松本さん。"
	},
	{
		"id": 319,
		"start": 1444.1574760000008,
		"end": 1446.1094760000008,
		"text": "（松本）今日　何か面白くないですかこの集まりは　メンツは。"
	},
	{
		"id": 320,
		"start": 1446.1094760000008,
		"end": 1448.1784760000007,
		"text": "ヒロミさんは<br />よく　いらっしゃって→"
	},
	{
		"id": 321,
		"start": 1448.1784760000007,
		"end": 1450.1464760000008,
		"text": "古市さんも<br />よく　いらっしゃってまして。"
	},
	{
		"id": 322,
		"start": 1450.1464760000008,
		"end": 1452.0984760000008,
		"text": "初登場が蛭子さんでございまして<br />よろしくお願いします。"
	},
	{
		"id": 323,
		"start": 1452.0984760000008,
		"end": 1454.2174760000007,
		"text": "おはようございます。<br />大丈夫ですか？"
	},
	{
		"id": 324,
		"start": 1454.2174760000007,
		"end": 1456.4864760000007,
		"text": "ちょっと<br />死にかけの座敷犬みたいになって。"
	},
	{
		"id": 325,
		"start": 1456.4864760000007,
		"end": 1459.1054760000009,
		"text": "（蛭子）いや　全然　大丈夫です。<br />大丈夫ですか？"
	},
	{
		"id": 326,
		"start": 1459.1054760000009,
		"end": 1462.1084760000008,
		"text": "この番組は　もちろん<br />見たことないですよね？"
	}
]