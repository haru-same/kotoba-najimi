	{
		"id": 239,
		"start": 1649.722,
		"end": 1654.727,
		"text": "うっ…　あっ　ああ…。"
	},
	{
		"id": 240,
		"start": 1654.727,
		"end": 1659.732,
		"text": "（シスター）《ねっ　ロー君<br />この世に絶望などないのです》→"
	},
	{
		"id": 241,
		"start": 1659.732,
		"end": 1661.734,
		"text": "《慈悲深い救いの手は→"
	},
	{
		"id": 242,
		"start": 1661.734,
		"end": 1664.734,
		"text": "必ず　差し伸べられます》"
	},
	{
		"id": 243,
		"start": 1667.74,
		"end": 1670.71,
		"text": "（ロー）シスター！！→"
