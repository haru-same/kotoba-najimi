	{
		"id": 244,
		"start": 1670.71,
		"end": 1673.579,
		"text": "みんなー！！→"
	},
	{
		"id": 245,
		"start": 1673.579,
		"end": 1676.582,
		"text": "うわあー！！　ああー！！"
	},
	{
		"id": 246,
		"start": 1676.582,
		"end": 1681.587,
		"text": "うう…　ああ…。"
