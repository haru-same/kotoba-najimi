	{
		"id": 267,
		"start": 1786.726,
		"end": 1790.726,
		"text": "若さまに報告するだすやん！"
	},
	{
		"id": 268,
		"start": 1799.672,
		"end": 1801.674,
		"text": "＜この世の全てを支配する→"
	},
	{
		"id": 269,
		"start": 1801.674,
		"end": 1804.677,
		"text": "天竜人として生まれた<br />ドフラミンゴ＞"
	},
	{
		"id": 270,
		"start": 1804.677,
		"end": 1808.681,
		"text": "＜彼は　なぜ　その高貴な地位を<br />失ったのか？＞"
	},
	{
		"id": 271,
		"start": 1808.681,
		"end": 1811.684,
		"text": "＜想像を絶する　ドフラミンゴの過去が<br />明らかになったとき→"
	},
	{
		"id": 272,
		"start": 1811.684,
		"end": 1815.688,
		"text": "少年　ローの運命も<br />大きく動き始める＞"
	},
	{
		"id": 273,
		"start": 1815.688,
		"end": 1817.688,
		"text": "次回　『ワンピース』"
	},
	{
		"id": 274,
		"start": 1819.692,
		"end": 1822.692,
		"text": "海賊王に　俺は　なる！"
	},
	{
		"id": 275,
		"start": 1834.74,
		"end": 1835.758,
		"text": "（山﨑・東野）おはようございます。<br />（東野）さあ　『ワイドナショー』→"
	},
	{
		"id": 276,
		"start": 1835.758,
		"end": 1838.894,
		"text": "始まりましたけども<br />皆さん　よろしくお願いします。"
	},
	{
		"id": 277,
		"start": 1838.894,
		"end": 1841.681,
		"text": "（松本）よろしくお願いします。<br />前園さんでございますけれども→"
	},
	{
		"id": 278,
		"start": 1841.681,
		"end": 1843.683,
		"text": "前園さん。<br />（前園）はい。"
	},
	{
		"id": 279,
		"start": 1843.683,
		"end": 1846.752,
		"text": "あれ　どうされた？<br />いや　もう→"
	},
	{
		"id": 280,
		"start": 1846.752,
		"end": 1848.604,
		"text": "僕は　日本が勝つと<br />なでしこが勝つというふうに→"
