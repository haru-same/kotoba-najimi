	},
	{
		"id": 249,
		"start": 1697.603,
		"end": 1699.605,
		"text": "（ロー）病院が…。"
	},
	{
		"id": 250,
		"start": 1699.605,
		"end": 1704.61,
		"text": "あああ…　ああー！！"
	},
	{
		"id": 251,
		"start": 1704.61,
		"end": 1710.61,
		"text": "うっ　ぐっ…　ああー！！"
	},
	{
		"id": 252,
		"start": 1716.622,
		"end": 1718.624,
		"text": "ラミ～！！"
