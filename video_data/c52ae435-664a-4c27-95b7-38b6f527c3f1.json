[
	{
		"id": 1,
		"start": 14.08,
		"end": 20.086,
		"text": "♪～"
	},
	{
		"id": 2,
		"start": 70.37,
		"end": 74.674,
		"text": "（マスターの声）<br />１日が終わり<br />人々が家路へと急ぐ頃―"
	},
	{
		"id": 3,
		"start": 75.475,
		"end": 77.377,
		"text": "俺の１日は始まる―"
	},
	{
		"id": 4,
		"start": 101.267,
		"end": 105.271,
		"text": "営業時間は夜12時から<br />朝７時頃まで―"
	},
	{
		"id": 5,
		"start": 106.806,
		"end": 110.31,
		"text": "人は“深夜食堂”って<br />言ってるよ―"
	},
	{
		"id": 6,
		"start": 113.413,
		"end": 115.281,
		"text": "メニューは これだけ―"
	},
	{
		"id": 7,
		"start": 115.548,
		"end": 118.485,
		"text": "あとは<br />勝手に注文してくれりゃ―"
	},
	{
		"id": 8,
		"start": 118.585,
		"end": 120.72,
		"text": "できるもんなら作るよ<br />ってのが―"
	},
	{
		"id": 9,
		"start": 120.82,
		"end": 122.755,
		"text": "俺の営業方針さ―"
	},
	{
		"id": 10,
		"start": 127.527,
		"end": 129.329,
		"text": "客が来るかって？―"
	},
	{
		"id": 11,
		"start": 129.429,
		"end": 131.531,
		"text": "それが けっこう来るんだよ"
	},
	{
		"id": 12,
		"start": 132.499,
		"end": 134.167,
		"text": "（柱時計の時報）"
	},
	{
		"id": 13,
		"start": 163.763,
		"end": 166.833,
		"text": "（マスターの声）<br />うちでラーメンといえば<br />インスタントだけど―"
	},
	{
		"id": 14,
		"start": 167.267,
		"end": 169.702,
		"text": "タンメンは<br />生麺を使ってるんだ―"
	},
	{
		"id": 15,
		"start": 169.802,
		"end": 172.605,
		"text": "その方が<br />タンメンに合うからね"
	},
	{
		"id": 16,
		"start": 178.344,
		"end": 179.345,
		"text": "（島田(しまだ)和彦）う～ん"
	},
	{
		"id": 17,
		"start": 179.879,
		"end": 182.382,
		"text": "夜中のラーメンは<br />罪悪感があるけど―"
	},
	{
		"id": 18,
		"start": 182.482,
		"end": 184.884,
		"text": "タンメンなら<br />野菜が取れていいよね"
	},
	{
		"id": 19,
		"start": 185.184,
		"end": 186.686,
		"text": "（ルミ）また始まった"
	},
	{
		"id": 20,
		"start": 187.487,
		"end": 190.957,
		"text": "そんな言い訳するくらいなら<br />食べなきゃいいのに"
	},
	{
		"id": 21,
		"start": 191.257,
		"end": 192.492,
		"text": "人生と同じ"
	},
	{
		"id": 22,
		"start": 192.592,
		"end": 194.794,
		"text": "人は<br />そんな簡単に割り切れない"
	},
	{
		"id": 23,
		"start": 195.828,
		"end": 199.666,
		"text": "“気を付けろ<br />その一口が豚になる”"
	},
	{
		"id": 24,
		"start": 200.9,
		"end": 202.335,
		"text": "（マスターの声）<br />島田さん―"
	},
	{
		"id": 25,
		"start": 202.435,
		"end": 204.771,
		"text": "ラジオ局の<br />アナウンサーなんだけど―"
	},
	{
		"id": 26,
		"start": 205.238,
		"end": 206.806,
		"text": "健康診断で―"
	},
	{
		"id": 27,
		"start": 206.906,
		"end": 210.276,
		"text": "医者から 隠れ肥満の<br />宣告を受けたらしい"
	},
	{
		"id": 28,
		"start": 210.376,
		"end": 212.645,
		"text": "（戸が開く音）"
	},
	{
		"id": 29,
		"start": 213.98,
		"end": 214.881,
		"text": "（マスター）いらっしゃい"
	},
	{
		"id": 30,
		"start": 215.381,
		"end": 216.316,
		"text": "（木内晴美(きうちはるみ)）どうも"
	},
	{
		"id": 31,
		"start": 222.989,
		"end": 224.891,
		"text": "タンメン 麺抜きね"
	},
	{
		"id": 32,
		"start": 225.458,
		"end": 226.326,
		"text": "あいよ"
	},
	{
		"id": 33,
		"start": 232.966,
		"end": 235.835,
		"text": "（マスター）<br />はい お待ち 麺抜き"
	},
	{
		"id": 34,
		"start": 236.269,
		"end": 237.637,
		"text": "いただきます"
	},
	{
		"id": 35,
		"start": 253.553,
		"end": 254.721,
		"text": "あの…"
	},
	{
		"id": 36,
		"start": 255.688,
		"end": 257.79,
		"text": "そんなの ありですか？"
	},
	{
		"id": 37,
		"start": 258.825,
		"end": 263.263,
		"text": "あっ… 夜中に 炭水化物<br />食べると あれだから"
	},
	{
		"id": 38,
		"start": 263.83,
		"end": 267,
		"text": "それに<br />野菜 食べてるうちに"
	},
	{
		"id": 39,
		"start": 267.533,
		"end": 269.769,
		"text": "麺がスープ吸って どんどん<br />増えちゃうでしょう？"
	},
	{
		"id": 40,
		"start": 269.869,
		"end": 273.473,
		"text": "まあ 確かに<br />理にはかなっているけど"
	},
	{
		"id": 41,
		"start": 273.573,
		"end": 275.875,
		"text": "マスター お茶漬けやめて"
	},
	{
		"id": 42,
		"start": 275.975,
		"end": 278.611,
		"text": "今日の締めは<br />私もタンメン麺抜きで"
	},
	{
		"id": 43,
		"start": 278.711,
		"end": 279.612,
		"text": "（マスター）あいよ"
	},
	{
		"id": 44,
		"start": 282.348,
		"end": 284.484,
		"text": "仕事終わりですか？"
	},
	{
		"id": 45,
		"start": 287.587,
		"end": 289.789,
		"text": "朝まで もう一稼ぎしないと"
	},
	{
		"id": 46,
		"start": 290.423,
		"end": 291.991,
		"text": "すごいなあ"
	},
	{
		"id": 47,
		"start": 292.292,
		"end": 294.594,
		"text": "私 方向音痴だからなあ"
	},
	{
		"id": 48,
		"start": 294.994,
		"end": 297.63,
		"text": "やっぱり<br />ナビ使うんですよね？"
	},
	{
		"id": 49,
		"start": 298.598,
		"end": 301.601,
		"text": "都内の道は<br />基本 頭に入ってるから"
	},
	{
		"id": 50,
		"start": 303.703,
		"end": 305.972,
		"text": "麺抜きねえ"
	},
	{
		"id": 51,
		"start": 306.706,
		"end": 307.974,
		"text": "（マスターの声）<br />島田さん―"
	},
	{
		"id": 52,
		"start": 308.074,
		"end": 310.777,
		"text": "やたら感心してると<br />思ったら―"
	},
	{
		"id": 53,
		"start": 311.411,
		"end": 313.046,
		"text": "（島田）田端(たばた)の魔女さん―"
	},
	{
		"id": 54,
		"start": 313.346,
		"end": 315.782,
		"text": "私も いろんな方法<br />試しました"
	},
	{
		"id": 55,
		"start": 316.416,
		"end": 318.751,
		"text": "リンゴダイエットなんてのも<br />やりましたが―"
	},
	{
		"id": 56,
		"start": 318.851,
		"end": 321.321,
		"text": "あれじゃ<br />おなか膨れませんよ"
	},
	{
		"id": 57,
		"start": 321.421,
		"end": 322.088,
		"text": "しかたないから―"
	},
	{
		"id": 58,
		"start": 322.355,
		"end": 324.524,
		"text": "アップルパイでいいやって<br />食べ続けてたら―"
	},
	{
		"id": 59,
		"start": 324.624,
		"end": 326.359,
		"text": "余計<br />デブになっちゃいまして―"
	},
	{
		"id": 60,
		"start": 326.592,
		"end": 328.528,
		"text": "それも おなかだけ"
	},
	{
		"id": 61,
		"start": 330.029,
		"end": 332.665,
		"text": "ところで そば屋で<br />“抜き”といえば―"
	},
	{
		"id": 62,
		"start": 332.765,
		"end": 334.567,
		"text": "天ぷらそばの<br />そば抜きで―"
	},
	{
		"id": 63,
		"start": 334.667,
		"end": 336.369,
		"text": "これで<br />一杯やってる方を見ると―"
	},
	{
		"id": 64,
		"start": 336.502,
		"end": 339.639,
		"text": "“はぁ 通だね”なんて<br />思ったりしますが―"
	},
	{
		"id": 65,
		"start": 340.339,
		"end": 342.375,
		"text": "最近 行きつけの店で―"
	},
	{
		"id": 66,
		"start": 342.475,
		"end": 344.043,
		"text": "タンメンの麺抜き―"
	},
	{
		"id": 67,
		"start": 344.377,
		"end": 346.913,
		"text": "注文してる方が<br />いらっしゃったんですよ―"
	},
	{
		"id": 68,
		"start": 347.013,
		"end": 348.481,
		"text": "タンメンの麺抜きって―"
	},
	{
		"id": 69,
		"start": 348.581,
		"end": 352.785,
		"text": "これ よく考えたら 具の多い<br />野菜スープなんですけど―"
	},
	{
		"id": 70,
		"start": 352.885,
		"end": 355.054,
		"text": "これなら 体重を<br />気にしている方にも―"
	},
	{
		"id": 71,
		"start": 355.354,
		"end": 356.622,
		"text": "いいんじゃないかと<br />思いまして…"
	},
	{
		"id": 72,
		"start": 356.723,
		"end": 361.094,
		"text": "（客）へえ 変わった食い物(もん)<br />頼む人がいるんだね"
	},
	{
		"id": 73,
		"start": 362.795,
		"end": 364.931,
		"text": "ハハッ そうですね"
	},
	{
		"id": 74,
		"start": 369.535,
		"end": 372.371,
		"text": "（忠(ちゅう)さん）ゆんべの あれ<br />ここのことだったのかい？"
	},
	{
		"id": 75,
		"start": 372.472,
		"end": 375.441,
		"text": "そうなの<br />島田さんのラジオ番組"
	},
	{
		"id": 76,
		"start": 375.541,
		"end": 377.61,
		"text": "そしたら 麺抜きの話 してて"
	},
	{
		"id": 77,
		"start": 377.71,
		"end": 380.646,
		"text": "麺抜きって<br />あの 晴美さんのこと？"
	},
	{
		"id": 78,
		"start": 380.747,
		"end": 381.848,
		"text": "（マスター）ああ そう"
	},
	{
		"id": 79,
		"start": 381.948,
		"end": 383.649,
		"text": "（小道(こみち)）<br />誰なんです？ その女性って"
	},
	{
		"id": 80,
		"start": 383.75,
		"end": 387.386,
		"text": "（マスター）ほら<br />タクシーの運転手やってる"
	},
	{
		"id": 81,
		"start": 387.487,
		"end": 390.022,
		"text": "ああ あの年増の いい女？"
	},
	{
		"id": 82,
		"start": 390.123,
		"end": 391.924,
		"text": "年増って いかほど？"
	},
	{
		"id": 83,
		"start": 392.024,
		"end": 395.595,
		"text": "四十半ばごろじゃねえかな"
	},
	{
		"id": 84,
		"start": 395.695,
		"end": 397.029,
		"text": "あっ もう 全然オーケーです"
	},
	{
		"id": 85,
		"start": 397.13,
		"end": 400.032,
		"text": "いや むしろ<br />熟女としては まだ未熟かも"
	},
	{
		"id": 86,
		"start": 400.133,
		"end": 402.535,
		"text": "新陳代謝が 限りなく<br />スローな女性って―"
	},
	{
		"id": 87,
		"start": 402.635,
		"end": 403.903,
		"text": "そそるんすよね"
	},
	{
		"id": 88,
		"start": 404.003,
		"end": 406.706,
		"text": "性的リビドーが<br />こう ギンギンきます"
	},
	{
		"id": 89,
		"start": 407.14,
		"end": 409.108,
		"text": "長生きするよ お前は"
	},
	{
		"id": 90,
		"start": 409.709,
		"end": 412.879,
		"text": "マスター<br />俺もタンメンの麺抜きで"
	},
	{
		"id": 91,
		"start": 412.979,
		"end": 414.647,
		"text": "僕も お願いします"
	},
	{
		"id": 92,
		"start": 414.747,
		"end": 415.848,
		"text": "（マスター）あいよ"
	},
	{
		"id": 93,
		"start": 417.583,
		"end": 419.452,
		"text": "（小道）いいですか？<br />スローな感じ―"
	},
	{
		"id": 94,
		"start": 419.786,
		"end": 420.119,
		"text": "はい"
	},
	{
		"id": 95,
		"start": 420.119,
		"end": 420.653,
		"text": "はい"
	},
	{
		"id": 96,
		"start": 420.119,
		"end": 420.653,
		"text": "（シャッター音）"
	},
	{
		"id": 97,
		"start": 420.653,
		"end": 420.753,
		"text": "（シャッター音）"
	},
	{
		"id": 98,
		"start": 420.753,
		"end": 421.788,
		"text": "（シャッター音）"
	},
	{
		"id": 99,
		"start": 420.753,
		"end": 421.788,
		"text": "スロー？"
	},
	{
		"id": 100,
		"start": 422.121,
		"end": 422.989,
		"text": "（小道）<br />もう一枚 お願いします"
	},
	{
		"id": 101,
		"start": 423.089,
		"end": 424.59,
		"text": "うるさいよ"
	},
	{
		"id": 102,
		"start": 427.627,
		"end": 431.13,
		"text": "（マスターの声）それから<br />２週間後のことだった"
	},
	{
		"id": 103,
		"start": 436.669,
		"end": 439.605,
		"text": "やっぱり あれ<br />あたしのことだったんだ"
	},
	{
		"id": 104,
		"start": 439.906,
		"end": 441.04,
		"text": "すいません"
	},
	{
		"id": 105,
		"start": 441.14,
		"end": 443.676,
		"text": "勝手に<br />話のネタに使っちゃって"
	},
	{
		"id": 106,
		"start": 443.776,
		"end": 445.077,
		"text": "でも あなたが―"
	},
	{
		"id": 107,
		"start": 445.178,
		"end": 447.88,
		"text": "とても おいしそうに<br />召し上がってらしたから"
	},
	{
		"id": 108,
		"start": 449.081,
		"end": 451.217,
		"text": "昔のことを思い出しちゃった"
	},
	{
		"id": 109,
		"start": 451.784,
		"end": 453.82,
		"text": "電話で<br />ラジオにリクエストして"
	},
	{
		"id": 110,
		"start": 453.92,
		"end": 455.855,
		"text": "名前<br />呼ばれたことがあったから"
	},
	{
		"id": 111,
		"start": 456.022,
		"end": 457.623,
		"text": "電リク世代ですね"
	},
	{
		"id": 112,
		"start": 457.723,
		"end": 459.459,
		"text": "アハッ 年 バレちゃうな"
	},
	{
		"id": 113,
		"start": 459.559,
		"end": 460.493,
		"text": "フフッ"
	},
	{
		"id": 114,
		"start": 461.794,
		"end": 464.096,
		"text": "目指してたんですか？ ＤＪ"
	},
	{
		"id": 115,
		"start": 465.465,
		"end": 468.668,
		"text": "憧れてたんです<br />深夜放送のＤＪに"
	},
	{
		"id": 116,
		"start": 469.502,
		"end": 471.671,
		"text": "つまんないことで悩んだり"
	},
	{
		"id": 117,
		"start": 471.771,
		"end": 473.84,
		"text": "落ち込んで<br />やりきれないとき"
	},
	{
		"id": 118,
		"start": 473.94,
		"end": 477.143,
		"text": "ああ 独りぼっち<br />じゃないんだって"
	},
	{
		"id": 119,
		"start": 478.911,
		"end": 481.047,
		"text": "分かるなあ その感じ"
	},
	{
		"id": 120,
		"start": 481.881,
		"end": 482.915,
		"text": "（携帯電話の着信音）"
	},
	{
		"id": 121,
		"start": 482.915,
		"end": 483.783,
		"text": "（携帯電話の着信音）"
	},
	{
		"id": 122,
		"start": 482.915,
		"end": 483.783,
		"text": "あっ"
	},
	{
		"id": 123,
		"start": 483.783,
		"end": 484.584,
		"text": "（携帯電話の着信音）"
	},
	{
		"id": 124,
		"start": 488.087,
		"end": 489.055,
		"text": "はい"
	},
	{
		"id": 125,
		"start": 490.656,
		"end": 491.858,
		"text": "分かりました"
	},
	{
		"id": 126,
		"start": 491.958,
		"end": 494.594,
		"text": "すぐ近くにいますので"
	},
	{
		"id": 127,
		"start": 494.894,
		"end": 496.929,
		"text": "５分ほどしたら<br />お迎えにあがります"
	},
	{
		"id": 128,
		"start": 497.864,
		"end": 499.866,
		"text": "いつものところで待ってます"
	},
	{
		"id": 129,
		"start": 500.533,
		"end": 502.068,
		"text": "じゃ 後ほど"
	},
	{
		"id": 130,
		"start": 505.805,
		"end": 507.24,
		"text": "お得意様ですか？"
	},
	{
		"id": 131,
		"start": 507.874,
		"end": 509.842,
		"text": "レギュラーいると助かるのよ"
	},
	{
		"id": 132,
		"start": 509.942,
		"end": 512.979,
		"text": "今日みたいに流しても<br />お客さんがいないときに"
	},
	{
		"id": 133,
		"start": 514.247,
		"end": 516.516,
		"text": "マスター お勘定"
	},
	{
		"id": 134,
		"start": 518.818,
		"end": 519.719,
		"text": "ごちそうさま"
	},
	{
		"id": 135,
		"start": 519.819,
		"end": 520.953,
		"text": "（マスター）ありがとね"
	},
	{
		"id": 136,
		"start": 526.559,
		"end": 528.928,
		"text": "（戸が閉まる音）"
	},
	{
		"id": 137,
		"start": 531.964,
		"end": 535.568,
		"text": "どっかで会ったような気が<br />するんだよなあ"
	},
	{
		"id": 138,
		"start": 538.638,
		"end": 540.64,
		"text": "（島田）<br />ねっ 似てるでしょ？―"
	},
	{
		"id": 139,
		"start": 540.74,
		"end": 544.243,
		"text": "絶対 間違いないですよ<br />〝紅の楓(かえで)〞"
	},
	{
		"id": 140,
		"start": 544.877,
		"end": 548.848,
		"text": "（マスター）うーん…<br />確かに似てるけど―"
	},
	{
		"id": 141,
		"start": 548.948,
		"end": 550.283,
		"text": "これ 何だい？"
	},
	{
		"id": 142,
		"start": 550.783,
		"end": 552.852,
		"text": "紅の楓じゃないですか"
	},
	{
		"id": 143,
		"start": 552.952,
		"end": 555.688,
		"text": "あの伝説の<br />「忍者戦隊コーガ」の"
	},
	{
		"id": 144,
		"start": 555.821,
		"end": 556.689,
		"text": "知ってっか？"
	},
	{
		"id": 145,
		"start": 556.756,
		"end": 558.791,
		"text": "（小道）いや 戦隊もんって<br />たくさん あったんで―"
	},
	{
		"id": 146,
		"start": 558.891,
		"end": 560.326,
		"text": "もう どれが どれだか"
	},
	{
		"id": 147,
		"start": 561.761,
		"end": 562.628,
		"text": "はい"
	},
	{
		"id": 148,
		"start": 562.728,
		"end": 563.896,
		"text": "“漆黒のムササビ”―"
	},
	{
		"id": 149,
		"start": 563.996,
		"end": 565.097,
		"text": "〝群青のハヤテ〞―"
	},
	{
		"id": 150,
		"start": 565.197,
		"end": 566.265,
		"text": "〝萌黄(もえぎ)の米蔵(よねぞう)〞―"
	},
	{
		"id": 151,
		"start": 566.566,
		"end": 567.967,
		"text": "〝橙(だいだい)のタスケ〞―"
	},
	{
		"id": 152,
		"start": 568.067,
		"end": 569.902,
		"text": "〝紅の楓〞"
	},
	{
		"id": 153,
		"start": 572.271,
		"end": 577.143,
		"text": "“五人の忍びが そろうとき<br />誰にも知られず悪を断つ！”"
	},
	{
		"id": 154,
		"start": 579.645,
		"end": 580.78,
		"text": "子供のときに―"
	},
	{
		"id": 155,
		"start": 580.88,
		"end": 582.782,
		"text": "楓さんに ファンレター<br />送ったんですよ"
	},
	{
		"id": 156,
		"start": 582.882,
		"end": 585.318,
		"text": "そしたら このブロマイドが<br />届いたんですよ"
	},
	{
		"id": 157,
		"start": 587.019,
		"end": 589.589,
		"text": "覚えてるかな？ 僕のこと"
	},
	{
		"id": 158,
		"start": 594.06,
		"end": 595.995,
		"text": "（島田）中年ライダーさん"
	},
	{
		"id": 159,
		"start": 596.095,
		"end": 596.963,
		"text": "いくつになっても―"
	},
	{
		"id": 160,
		"start": 597.029,
		"end": 599.031,
		"text": "恋したって<br />いいじゃないですか"
	},
	{
		"id": 161,
		"start": 599.131,
		"end": 601.1,
		"text": "ただ ストーカーは<br />ダメですよ"
	},
	{
		"id": 162,
		"start": 601.2,
		"end": 603.636,
		"text": "相手の気持ちを<br />思いやんないと"
	},
	{
		"id": 163,
		"start": 604.637,
		"end": 608.975,
		"text": "恋といえば<br />僕 最近 初恋の女性に"
	},
	{
		"id": 164,
		"start": 609.075,
		"end": 611.811,
		"text": "たまたま ある場所で<br />再会したんですよ―"
	},
	{
		"id": 165,
		"start": 612.778,
		"end": 613.946,
		"text": "再会っていっても―"
	},
	{
		"id": 166,
		"start": 614.046,
		"end": 615.681,
		"text": "僕の片思いで 向こうは―"
	},
	{
		"id": 167,
		"start": 615.781,
		"end": 618.618,
		"text": "全く 僕のことなんか<br />知らないんですけどね―"
	},
	{
		"id": 168,
		"start": 618.851,
		"end": 621.354,
		"text": "ご存じのリスナーの方も<br />いると思うんですが―"
	},
	{
		"id": 169,
		"start": 621.654,
		"end": 622.922,
		"text": "僕が子供のころ―"
	},
	{
		"id": 170,
		"start": 623.022,
		"end": 626.392,
		"text": "「忍者戦隊コーガ」って<br />戦隊ものがありまして―"
	},
	{
		"id": 171,
		"start": 626.692,
		"end": 629.662,
		"text": "そのメンバーの一人<br />紅の楓さんが―"
	},
	{
		"id": 172,
		"start": 629.795,
		"end": 632.098,
		"text": "僕の<br />初恋の人だったんです―"
	},
	{
		"id": 173,
		"start": 632.198,
		"end": 634.1,
		"text": "楓さんだって<br />分かった瞬間―"
	},
	{
		"id": 174,
		"start": 634.2,
		"end": 637.103,
		"text": "僕の頭に<br />この曲が流れました―"
	},
	{
		"id": 175,
		"start": 637.203,
		"end": 642.074,
		"text": "「忍者戦隊コーガ」の主題歌<br />“刃の心で駆け抜けろ”です"
	},
	{
		"id": 176,
		"start": 646.178,
		"end": 649.348,
		"text": "（金本）へえ 懐かしいな"
	},
	{
		"id": 177,
		"start": 649.649,
		"end": 651.751,
		"text": "あのコーガの<br />ファンクラブ作るんすか？"
	},
	{
		"id": 178,
		"start": 651.851,
		"end": 652.718,
		"text": "ええ"
	},
	{
		"id": 179,
		"start": 652.818,
		"end": 653.786,
		"text": "まずは 様子見で―"
	},
	{
		"id": 180,
		"start": 653.886,
		"end": 655.921,
		"text": "番組のホームページで<br />発表したんです"
	},
	{
		"id": 181,
		"start": 656.022,
		"end": 657.823,
		"text": "そしたら 反響が すごくて"
	},
	{
		"id": 182,
		"start": 657.923,
		"end": 659.125,
		"text": "僕が<br />いちばん好きだったのは"
	},
	{
		"id": 183,
		"start": 659.225,
		"end": 660.96,
		"text": "あの… 抜け忍だった楓を"
	},
	{
		"id": 184,
		"start": 661.06,
		"end": 662.662,
		"text": "リーダーのムササビが<br />迎え入れる…"
	},
	{
		"id": 185,
		"start": 662.762,
		"end": 663.796,
		"text": "「秘すれば花」の回ですね"
	},
	{
		"id": 186,
		"start": 663.896,
		"end": 664.93,
		"text": "そう！ そうそう！"
	},
	{
		"id": 187,
		"start": 665.031,
		"end": 666.365,
		"text": "ムササビが<br />言うじゃないですか―"
	},
	{
		"id": 188,
		"start": 666.932,
		"end": 668.701,
		"text": "〝俺たちが<br />お前を守る〞―"
	},
	{
		"id": 189,
		"start": 668.801,
		"end": 670.336,
		"text": "〝だから逃げずに<br />戦え〞って―"
	},
	{
		"id": 190,
		"start": 671.037,
		"end": 672.938,
		"text": "子供ながらに<br />ぐっときたなあ"
	},
	{
		"id": 191,
		"start": 673.039,
		"end": 674.273,
		"text": "分かるなあ"
	},
	{
		"id": 192,
		"start": 674.407,
		"end": 677.009,
		"text": "（金本）ねえねえ あの…<br />タスケとハヤテってさ…"
	},
	{
		"id": 193,
		"start": 677.109,
		"end": 680.246,
		"text": "（小寿々(こすず)）ガキねえ 男って"
	},
	{
		"id": 194,
		"start": 681.38,
		"end": 685.151,
		"text": "（マスター）<br />お待たせ ジャンボ豚汁定食"
	},
	{
		"id": 195,
		"start": 685.251,
		"end": 688.687,
		"text": "（コウちゃん）<br />はい いただきます"
	},
	{
		"id": 196,
		"start": 689.255,
		"end": 692.958,
		"text": "あんたたち<br />盛り上がるのはいいけど"
	},
	{
		"id": 197,
		"start": 693.059,
		"end": 696.862,
		"text": "少しは その人の気持ち<br />思いやったら どうなの？"
	},
	{
		"id": 198,
		"start": 696.996,
		"end": 698.397,
		"text": "ねえ マスター"
	},
	{
		"id": 199,
		"start": 698.931,
		"end": 700.032,
		"text": "まあね"
	},
	{
		"id": 200,
		"start": 700.733,
		"end": 703.736,
		"text": "いろいろあって<br />今があるんだろうからね"
	},
	{
		"id": 201,
		"start": 704.27,
		"end": 705.771,
		"text": "それも含めて―"
	},
	{
		"id": 202,
		"start": 705.871,
		"end": 708.974,
		"text": "僕は 自分の番組に<br />出てもらおうと思ってます"
	},
	{
		"id": 203,
		"start": 709.408,
		"end": 710.443,
		"text": "あの人―"
	},
	{
		"id": 204,
		"start": 710.743,
		"end": 713.279,
		"text": "きちんと仕事している人<br />なんだろうなって―"
	},
	{
		"id": 205,
		"start": 713.379,
		"end": 716.449,
		"text": "ここで会って感じたんです<br />勘だけど"
	},
	{
		"id": 206,
		"start": 717.016,
		"end": 720.719,
		"text": "だから 自分の過去も 笑って<br />話せるんじゃないかって"
	},
	{
		"id": 207,
		"start": 720.82,
		"end": 723.355,
		"text": "そうですよ<br />とっくに吹っ切ってますよ"
	},
	{
		"id": 208,
		"start": 723.456,
		"end": 726.058,
		"text": "今じゃタクシードライバーで<br />頑張ってんだから"
	},
	{
		"id": 209,
		"start": 726.158,
		"end": 728.994,
		"text": "きっと いい思い出ですよ"
	},
	{
		"id": 210,
		"start": 731.363,
		"end": 733.766,
		"text": "（晴美）あなたには<br />いい思い出でも―"
	},
	{
		"id": 211,
		"start": 733.899,
		"end": 735.201,
		"text": "あたしには そうじゃない"
	},
	{
		"id": 212,
		"start": 737.47,
		"end": 739.939,
		"text": "もともと<br />好きでやった役じゃないし"
	},
	{
		"id": 213,
		"start": 741.74,
		"end": 745.044,
		"text": "あの後 何をやっても<br />“コーガの楓”って言われた"
	},
	{
		"id": 214,
		"start": 746.946,
		"end": 749.782,
		"text": "それがイヤで 役者<br />辞めたようなもんなのに"
	},
	{
		"id": 215,
		"start": 751.317,
		"end": 753.052,
		"text": "そうだったんですか"
	},
	{
		"id": 216,
		"start": 753.152,
		"end": 756.088,
		"text": "いろいろあって<br />今の生活があるわけだし"
	},
	{
		"id": 217,
		"start": 756.789,
		"end": 758.958,
		"text": "おもしろおかしく<br />騒ぎ立てられるのは…"
	},
	{
		"id": 218,
		"start": 759.058,
		"end": 760.359,
		"text": "そういうつもりじゃ…"
	},
	{
		"id": 219,
		"start": 760.926,
		"end": 764.029,
		"text": "いや 結果 そうですよね"
	},
	{
		"id": 220,
		"start": 764.897,
		"end": 766.298,
		"text": "すいませんでした"
	},
	{
		"id": 221,
		"start": 782.448,
		"end": 785.017,
		"text": "マスター お代わり"
	},
	{
		"id": 222,
		"start": 789.221,
		"end": 793.192,
		"text": "（マスター）<br />珍しいね うちで飲むなんて"
	},
	{
		"id": 223,
		"start": 794.293,
		"end": 797.463,
		"text": "うん 島田さん 来てる？"
	},
	{
		"id": 224,
		"start": 797.93,
		"end": 801.834,
		"text": "そういや<br />ここんとこ 顔 出さないな"
	},
	{
		"id": 225,
		"start": 805.938,
		"end": 810.976,
		"text": "あたしね ようやく 誰にでも<br />胸張って言えるくらい―"
	},
	{
		"id": 226,
		"start": 811.076,
		"end": 812.878,
		"text": "今の仕事 気に入ってるの"
	},
	{
		"id": 227,
		"start": 814.18,
		"end": 816.282,
		"text": "島田さんも そう言ってたよ"
	},
	{
		"id": 228,
		"start": 817.049,
		"end": 820.953,
		"text": "晴美さんは<br />きちんと仕事してるって"
	},
	{
		"id": 229,
		"start": 823.189,
		"end": 824.123,
		"text": "そう"
	},
	{
		"id": 230,
		"start": 828.394,
		"end": 831.463,
		"text": "マスター 麺抜き"
	},
	{
		"id": 231,
		"start": 832.298,
		"end": 833.365,
		"text": "あいよ"
	},
	{
		"id": 232,
		"start": 843.909,
		"end": 845.044,
		"text": "はぁ…"
	},
	{
		"id": 233,
		"start": 848.013,
		"end": 849.081,
		"text": "あのころ―"
	},
	{
		"id": 234,
		"start": 849.181,
		"end": 852.184,
		"text": "あたしが いちばん<br />芝居も下手くそだったのに"
	},
	{
		"id": 235,
		"start": 852.284,
		"end": 854.22,
		"text": "それ 認めたくなくて"
	},
	{
		"id": 236,
		"start": 855.12,
		"end": 859.391,
		"text": "他の人をけなして<br />周りに八つ当たりして"
	},
	{
		"id": 237,
		"start": 859.491,
		"end": 861.327,
		"text": "本当 最低だった"
	},
	{
		"id": 238,
		"start": 864.196,
		"end": 866.599,
		"text": "でも そんな あたしのこと"
	},
	{
		"id": 239,
		"start": 866.899,
		"end": 869.201,
		"text": "リーダーは<br />いつも かばってくれたんだ"
	},
	{
		"id": 240,
		"start": 871.036,
		"end": 873.305,
		"text": "リーダーの人<br />何ていったっけ？"
	},
	{
		"id": 241,
		"start": 874.874,
		"end": 875.875,
		"text": "ムササビ"
	},
	{
		"id": 242,
		"start": 876.308,
		"end": 877.943,
		"text": "ああ そうだった―"
	},
	{
		"id": 243,
		"start": 879.245,
		"end": 883.282,
		"text": "ムササビって<br />いい男だったんだ"
	},
	{
		"id": 244,
		"start": 885.084,
		"end": 886.051,
		"text": "うん"
	},
	{
		"id": 245,
		"start": 887.319,
		"end": 888.554,
		"text": "好きだった"
	},
	{
		"id": 246,
		"start": 891.59,
		"end": 892.925,
		"text": "でもね…"
	},
	{
		"id": 247,
		"start": 897.296,
		"end": 898.964,
		"text": "おはようございます"
	},
	{
		"id": 248,
		"start": 902.234,
		"end": 903.168,
		"text": "（晴美）あのとき―"
	},
	{
		"id": 249,
		"start": 903.269,
		"end": 905.938,
		"text": "いつもより早めに<br />スタジオ来たら…"
	},
	{
		"id": 250,
		"start": 917.349,
		"end": 918.25,
		"text": "（晴美）はっ！"
	},
	{
		"id": 251,
		"start": 919.084,
		"end": 920.185,
		"text": "（リーダー）いや 俺…―"
	},
	{
		"id": 252,
		"start": 922.488,
		"end": 923.589,
		"text": "ごめん"
	},
	{
		"id": 253,
		"start": 928.027,
		"end": 928.961,
		"text": "フフフ…"
	},
	{
		"id": 254,
		"start": 929.061,
		"end": 930.996,
		"text": "（リーダー）<br />今日さ 俺 おごるから"
	},
	{
		"id": 255,
		"start": 931.096,
		"end": 934.233,
		"text": "ええ～ いいよ 無理しなくて"
	},
	{
		"id": 256,
		"start": 934.333,
		"end": 935.534,
		"text": "（リーダー）いや さすがに"
	},
	{
		"id": 257,
		"start": 935.634,
		"end": 936.936,
		"text": "（晴美）ムササビ―"
	},
	{
		"id": 258,
		"start": 937.036,
		"end": 939.371,
		"text": "本当は 女の子に<br />なりたかったって―"
	},
	{
		"id": 259,
		"start": 939.471,
		"end": 941.473,
		"text": "あたしにだけ<br />話してくれたの―"
	},
	{
		"id": 260,
		"start": 943.943,
		"end": 946.679,
		"text": "おかげで 恋人には<br />なり損ねたけど―"
	},
	{
		"id": 261,
		"start": 946.979,
		"end": 949.982,
		"text": "あたしは 掛け替えのない<br />友達を手に入れた"
	},
	{
		"id": 262,
		"start": 954.386,
		"end": 955.254,
		"text": "（マスター）はい"
	},
	{
		"id": 263,
		"start": 958.49,
		"end": 960.225,
		"text": "（晴美）<br />２人とも お金なかったから"
	},
	{
		"id": 264,
		"start": 961.393,
		"end": 964.363,
		"text": "飲むときは これを<br />つまみ代わりにしてたんだ"
	},
	{
		"id": 265,
		"start": 968.267,
		"end": 970.035,
		"text": "ラジオで話題になって"
	},
	{
		"id": 266,
		"start": 970.135,
		"end": 973.973,
		"text": "昔のファンがムササビのこと<br />調べたりしたら ヤだなって"
	},
	{
		"id": 267,
		"start": 976.275,
		"end": 978.177,
		"text": "ずっと守ってもらってたから"
	},
	{
		"id": 268,
		"start": 978.277,
		"end": 980.546,
		"text": "今度は<br />あたしが守んなきゃって"
	},
	{
		"id": 269,
		"start": 985.217,
		"end": 987.987,
		"text": "でも<br />余計な お世話だったみたい"
	},
	{
		"id": 270,
		"start": 989.722,
		"end": 991.69,
		"text": "ムササビに聞いたのかい？"
	},
	{
		"id": 271,
		"start": 993.726,
		"end": 996.462,
		"text": "“あんたの人生は<br />あんたのもので”"
	},
	{
		"id": 272,
		"start": 996.562,
		"end": 999.064,
		"text": "“あたしの人生は<br />あたしのものなの”"
	},
	{
		"id": 273,
		"start": 999.431,
		"end": 1002.034,
		"text": "“お互い 違う道<br />歩いてるんだから”"
	},
	{
		"id": 274,
		"start": 1002.134,
		"end": 1003.669,
		"text": "“それでいいじゃない”って"
	},
	{
		"id": 275,
		"start": 1005.337,
		"end": 1009.174,
		"text": "で その彼<br />今は どうしてんだい？"
	},
	{
		"id": 276,
		"start": 1010.676,
		"end": 1011.577,
		"text": "フフッ"
	},
	{
		"id": 277,
		"start": 1022.321,
		"end": 1026.425,
		"text": "迷惑かけたのは<br />僕の方なのに 何で？"
	},
	{
		"id": 278,
		"start": 1027.226,
		"end": 1028.26,
		"text": "彼女も―"
	},
	{
		"id": 279,
		"start": 1028.36,
		"end": 1032.631,
		"text": "“少し言い過ぎたから<br />ちゃんと謝りたい”って"
	},
	{
		"id": 280,
		"start": 1033.165,
		"end": 1034.233,
		"text": "フフッ"
	},
	{
		"id": 281,
		"start": 1036.602,
		"end": 1037.736,
		"text": "こんばんは"
	},
	{
		"id": 282,
		"start": 1038.203,
		"end": 1039.371,
		"text": "いらっしゃい"
	},
	{
		"id": 283,
		"start": 1050.349,
		"end": 1054.386,
		"text": "あの… この度は<br />あなたの気持ちも考えず"
	},
	{
		"id": 284,
		"start": 1054.486,
		"end": 1056.789,
		"text": "軽率で<br />身勝手な行動をしてしまい…"
	},
	{
		"id": 285,
		"start": 1057.089,
		"end": 1059.658,
		"text": "（せき払い）"
	},
	{
		"id": 286,
		"start": 1060.759,
		"end": 1066.331,
		"text": "“俺たちが お前を守る<br />だから 逃げずに戦え”"
	},
	{
		"id": 287,
		"start": 1067.466,
		"end": 1068.333,
		"text": "（島田）えっ？"
	},
	{
		"id": 288,
		"start": 1069.701,
		"end": 1073.639,
		"text": "晴美 あたしが全部<br />自分で説明するから"
	},
	{
		"id": 289,
		"start": 1073.739,
		"end": 1074.706,
		"text": "いいわね？"
	},
	{
		"id": 290,
		"start": 1075.274,
		"end": 1077.576,
		"text": "どうぞ ご自由に<br />食べたら あたし 帰るから"
	},
	{
		"id": 291,
		"start": 1077.676,
		"end": 1080.079,
		"text": "あら つきあい悪い女"
	},
	{
		"id": 292,
		"start": 1080.179,
		"end": 1082.147,
		"text": "おせっかい おばさん<br />やめたの"
	},
	{
		"id": 293,
		"start": 1082.247,
		"end": 1083.282,
		"text": "（コウちゃん）ふ～ん"
	},
	{
		"id": 294,
		"start": 1084.716,
		"end": 1086.618,
		"text": "見覚えないかな？"
	},
	{
		"id": 295,
		"start": 1094.726,
		"end": 1096.528,
		"text": "紅の楓…"
	},
	{
		"id": 296,
		"start": 1098.664,
		"end": 1100.332,
		"text": "紅の楓―"
	},
	{
		"id": 297,
		"start": 1101.166,
		"end": 1102.234,
		"text": "ムササビ？―"
	},
	{
		"id": 298,
		"start": 1103.368,
		"end": 1104.47,
		"text": "ムササビ！？"
	},
	{
		"id": 299,
		"start": 1106.538,
		"end": 1109.808,
		"text": "あれ？ あれ？ ムササビ？"
	},
	{
		"id": 300,
		"start": 1110.109,
		"end": 1111.343,
		"text": "（マスター）水 飲むかい？"
	},
	{
		"id": 301,
		"start": 1116.415,
		"end": 1118.717,
		"text": "（島田のせき込み）"
	},
	{
		"id": 302,
		"start": 1118.817,
		"end": 1120.119,
		"text": "（島田）えっ？―"
	},
	{
		"id": 303,
		"start": 1120.486,
		"end": 1121.453,
		"text": "ムササビ…"
	},
	{
		"id": 304,
		"start": 1122.688,
		"end": 1124.323,
		"text": "（ディレクター）じゃ<br />よろしくお願いしまーす"
	},
	{
		"id": 305,
		"start": 1124.423,
		"end": 1126.792,
		"text": "生放送なんで<br />あの… お手柔らかに―"
	},
	{
		"id": 306,
		"start": 1127.826,
		"end": 1129.561,
		"text": "間もなく ＣＭ明けまーす"
	},
	{
		"id": 307,
		"start": 1129.661,
		"end": 1130.796,
		"text": "ＳＳ"
	},
	{
		"id": 308,
		"start": 1132.397,
		"end": 1134.867,
		"text": "島ちゃん どーん！"
	},
	{
		"id": 309,
		"start": 1135.734,
		"end": 1139.338,
		"text": "今夜も始まりました<br />「ミッドナイト島ちゃん」"
	},
	{
		"id": 310,
		"start": 1139.438,
		"end": 1142.875,
		"text": "何と 今日は<br />特別ゲストをお招きしました"
	},
	{
		"id": 311,
		"start": 1143.175,
		"end": 1143.842,
		"text": "というか―"
	},
	{
		"id": 312,
		"start": 1143.909,
		"end": 1145.777,
		"text": "こちらは 気を遣って<br />ご遠慮してたんですが…"
	},
	{
		"id": 313,
		"start": 1145.878,
		"end": 1147.813,
		"text": "（コウちゃん）ご無沙汰～！"
	},
	{
		"id": 314,
		"start": 1148.447,
		"end": 1151.483,
		"text": "忍者戦隊コーガのリーダー<br />漆黒のムササビよ"
	},
	{
		"id": 315,
		"start": 1151.583,
		"end": 1155.454,
		"text": "みんな覚えててくれて<br />すっごく うれしい～！"
	},
	{
		"id": 316,
		"start": 1161.293,
		"end": 1162.461,
		"text": "（せき払い）"
	},
	{
		"id": 317,
		"start": 1162.561,
		"end": 1164.763,
		"text": "今日は<br />あたし一人しかいないけど"
	},
	{
		"id": 318,
		"start": 1164.863,
		"end": 1166.598,
		"text": "いつもの いくわよ～！"
	},
	{
		"id": 319,
		"start": 1166.698,
		"end": 1171.637,
		"text": "“五人の忍びが そろうとき<br />誰にも知られず悪を断つ！”"
	},
	{
		"id": 320,
		"start": 1171.737,
		"end": 1176.175,
		"text": "“五つの鐘が鳴ったとき<br />悪の響きも聞き納め”"
	},
	{
		"id": 321,
		"start": 1176.275,
		"end": 1178.443,
		"text": "“コーガは<br />どこでも はせ 参上”"
	},
	{
		"id": 322,
		"start": 1178.544,
		"end": 1182.214,
		"text": "〝善悪の彼岸！<br />善悪の彼岸！〞"
	},
	{
		"id": 323,
		"start": 1188.187,
		"end": 1191.19,
		"text": "（島田）<br />あっという間に時間が<br />過ぎてしまいました―"
	},
	{
		"id": 324,
		"start": 1191.29,
		"end": 1192.191,
		"text": "ムササビさん―"
	},
	{
		"id": 325,
		"start": 1192.291,
		"end": 1196.328,
		"text": "お別れの前に 大切な友人に<br />メッセージを送りたいとか"
	},
	{
		"id": 326,
		"start": 1202.834,
		"end": 1205.704,
		"text": "（コウちゃん）公共の電波<br />使って 言っちゃうわ―"
	},
	{
		"id": 327,
		"start": 1206.972,
		"end": 1211.944,
		"text": "あたしは 今の自分も<br />昔の自分も大好き"
	},
	{
		"id": 328,
		"start": 1215.247,
		"end": 1218.784,
		"text": "ひとに何言われようが<br />あたしは あたし―"
	},
	{
		"id": 329,
		"start": 1219.985,
		"end": 1221.42,
		"text": "昔より―"
	},
	{
		"id": 330,
		"start": 1221.52,
		"end": 1223.855,
		"text": "ちょっと ずうずうしく<br />なったかもしれないけど"
	},
	{
		"id": 331,
		"start": 1227.259,
		"end": 1228.227,
		"text": "でも…"
	},
	{
		"id": 332,
		"start": 1230.429,
		"end": 1234.6,
		"text": "気にかけてくれて<br />ありがとね"
	},
	{
		"id": 333,
		"start": 1237.569,
		"end": 1240.806,
		"text": "ホントに ありがと"
	},
	{
		"id": 334,
		"start": 1246.945,
		"end": 1249.548,
		"text": "（島田）また来てくださいね<br />ムササビさん―"
	},
	{
		"id": 335,
		"start": 1250.282,
		"end": 1253.652,
		"text": "それでは また来週<br />おやすみなさい"
	},
	{
		"id": 336,
		"start": 1262.828,
		"end": 1263.762,
		"text": "（ノック）"
	},
	{
		"id": 337,
		"start": 1267.566,
		"end": 1269.534,
		"text": "すいません 気付かなくて"
	},
	{
		"id": 338,
		"start": 1269.968,
		"end": 1271.837,
		"text": "どちらまで ご利用ですか？"
	},
	{
		"id": 339,
		"start": 1284.616,
		"end": 1290.622,
		"text": "♪～"
	},
	{
		"id": 340,
		"start": 1303.302,
		"end": 1304.836,
		"text": "（マスター）はい お待ち"
	},
	{
		"id": 341,
		"start": 1307.606,
		"end": 1310.809,
		"text": "６人目の忍び<br />“純白の島ちゃん”です"
	},
	{
		"id": 342,
		"start": 1310.909,
		"end": 1313.912,
		"text": "僕のお薦めは<br />やっぱり麺入りです"
	},
	{
		"id": 343,
		"start": 1314.012,
		"end": 1316.348,
		"text": "新入りさん コソコソしない"
	},
	{
		"id": 344,
		"start": 1316.448,
		"end": 1317.549,
		"text": "かしこまり！"
	},
	{
		"id": 345,
		"start": 1318.617,
		"end": 1320.485,
		"text": "それでは 皆さん"
	},
	{
		"id": 346,
		"start": 1320.786,
		"end": 1322.721,
		"text": "（一同）おやすみなさい！"
	},
	{
		"id": 347,
		"start": 1324.489,
		"end": 1330.495,
		"text": "♪～"
	}
]