[
	{
		"id": 1,
		"start": 123.132,
		"end": 133.132,
		"text": "♪♪～"
	},
	{
		"id": 2,
		"start": 344.152,
		"end": 348.156,
		"text": "＜ワンピースを狙う<br />全ての海賊たちが目指す→"
	},
	{
		"id": 3,
		"start": 348.156,
		"end": 353.156,
		"text": "グランドライン後半の海<br />人呼んで　新世界＞"
	},
	{
		"id": 4,
		"start": 355.163,
		"end": 358.166,
		"text": "＜しかし<br />かつて　その海を制したのは→"
	},
	{
		"id": 5,
		"start": 358.166,
		"end": 363.171,
		"text": "海賊王　ゴールド・ロジャー<br />ただ　一人＞"
	},
	{
		"id": 6,
		"start": 363.171,
		"end": 370.178,
		"text": "＜新世界は　幾多の海賊たちの<br />夢と野望を阻んできた＞"
	},
	{
		"id": 7,
		"start": 370.178,
		"end": 374.182,
		"text": "＜その海に今　ゴムゴムの実を食べゴム人間となった少年→"
	},
	{
		"id": 8,
		"start": 374.182,
		"end": 378.186,
		"text": "モンキー・Ｄ・ルフィと<br />その仲間たちが挑む！＞"
	},
	{
		"id": 9,
		"start": 378.186,
		"end": 382.186,
		"text": "（ルフィ）海賊王に　俺は　なる！"
	},
	{
		"id": 10,
		"start": 385.193,
		"end": 389.197,
		"text": "＜新世界を支配する<br />四皇の一角を崩すため→"
	},
	{
		"id": 11,
		"start": 389.197,
		"end": 392.2,
		"text": "トラファルガー・ローと<br />海賊同盟を組んだ　ルフィたちは→"
	},
	{
		"id": 12,
		"start": 392.2,
		"end": 395.203,
		"text": "ドレスローザに乗り込むも→"
	},
	{
		"id": 13,
		"start": 395.203,
		"end": 398.206,
		"text": "現国王　ドフラミンゴの<br />計略によって→"
	},
	{
		"id": 14,
		"start": 398.206,
		"end": 400.208,
		"text": "島ごと閉じ込められてしまう＞"
	},
	{
		"id": 15,
		"start": 400.208,
		"end": 402.144,
		"text": "＜一味が<br />ドンキホーテファミリーと→"
	},
	{
		"id": 16,
		"start": 402.144,
		"end": 405.147,
		"text": "激しい戦いを繰り広げる中→"
	},
	{
		"id": 17,
		"start": 405.147,
		"end": 411.153,
		"text": "ルフィとローは　王宮で<br />ついに　ドフラミンゴと激突した＞"
	},
	{
		"id": 18,
		"start": 411.153,
		"end": 415.157,
		"text": "（セニョール）うっ　うっ…。"
	},
	{
		"id": 19,
		"start": 415.157,
		"end": 418.16,
		"text": "（セニョール）《すまない。<br />君を失いたくなくて→"
	},
	{
		"id": 20,
		"start": 418.16,
		"end": 421.16,
		"text": "俺は　嘘をついたんだ》"
	},
	{
		"id": 21,
		"start": 423.165,
		"end": 425.167,
		"text": "（セニョール）ルシアン。　ほら。"
	},
	{
		"id": 22,
		"start": 425.167,
		"end": 428.17,
		"text": "ギムレットと同じボンネットを<br />見つけたんだ。→"
	},
	{
		"id": 23,
		"start": 428.17,
		"end": 430.172,
		"text": "どうだ？　似てないか？→"
	},
	{
		"id": 24,
		"start": 430.172,
		"end": 432.174,
		"text": "あいつは　俺似だったから。"
	},
	{
		"id": 25,
		"start": 432.174,
		"end": 437.174,
		"text": "ハハハ…。"
	},
	{
		"id": 26,
		"start": 439.181,
		"end": 443.181,
		"text": "《もう一度　笑顔を…》"
	},
	{
		"id": 27,
		"start": 445.187,
		"end": 449.191,
		"text": "ごっ　ごめんよ。<br />冗談が過ぎたね。"
	},
	{
		"id": 28,
		"start": 449.191,
		"end": 452.194,
		"text": "こんな格好　人に見られたら…。"
	},
	{
		"id": 29,
		"start": 452.194,
		"end": 457.194,
		"text": "あっ　あっ　ああ…。"
	},
	{
		"id": 30,
		"start": 466.141,
		"end": 469.141,
		"text": "（看護師たち）ギャー！"
	},
	{
		"id": 31,
		"start": 471.146,
		"end": 474.149,
		"text": "（２人）ああ…。"
	},
	{
		"id": 32,
		"start": 474.149,
		"end": 486.161,
		"text": "♪♪～"
	},
	{
		"id": 33,
		"start": 486.161,
		"end": 506.181,
		"text": "（医療器具の音）"
	},
	{
		"id": 34,
		"start": 506.181,
		"end": 509.184,
		"text": "（セニョール）おはよう　ルシアン。"
	},
	{
		"id": 35,
		"start": 509.184,
		"end": 514.189,
		"text": "フッフフ…。"
	},
	{
		"id": 36,
		"start": 514.189,
		"end": 517.189,
		"text": "フフ…。"
	},
	{
		"id": 37,
		"start": 520.195,
		"end": 523.131,
		"text": "ん？　ああ…。"
	},
	{
		"id": 38,
		"start": 523.131,
		"end": 543.151,
		"text": "♪♪～"
	},
	{
		"id": 39,
		"start": 543.151,
		"end": 550.158,
		"text": "♪♪～"
	},
	{
		"id": 40,
		"start": 550.158,
		"end": 552.16,
		"text": "すげえな　おっさん。"
	},
	{
		"id": 41,
		"start": 552.16,
		"end": 555.163,
		"text": "息子も失って<br />奥さんも大事故に…。"
	},
	{
		"id": 42,
		"start": 555.163,
		"end": 559.167,
		"text": "（女性）そりゃあ<br />心も壊れるわよね。"
	},
	{
		"id": 43,
		"start": 559.167,
		"end": 563.171,
		"text": "（ディアマンテ）ウハハハ…！<br />（トレーボル）ベヘヘヘ…！"
	},
	{
		"id": 44,
		"start": 563.171,
		"end": 565.173,
		"text": "（ディアマンテ）ピンク！→"
	},
	{
		"id": 45,
		"start": 565.173,
		"end": 569.177,
		"text": "お前　服の趣味<br />どうしちまったんだ？　いったい。"
	},
	{
		"id": 46,
		"start": 569.177,
		"end": 573.181,
		"text": "（トレーボル）んねー<br />それ　ウケ狙いだよね？　んね？"
	},
	{
		"id": 47,
		"start": 573.181,
		"end": 575.183,
		"text": "（グラディウス）高級スーツはどうした？"
	},
	{
		"id": 48,
		"start": 575.183,
		"end": 578.186,
		"text": "（ラオ・Ｇ）見ＧＵるしいの「Ｇ」！"
	},
	{
		"id": 49,
		"start": 578.186,
		"end": 582.124,
		"text": "フン。　出掛けてくる。"
	},
	{
		"id": 50,
		"start": 582.124,
		"end": 584.126,
		"text": "見ろ！　変態だ！"
	},
	{
		"id": 51,
		"start": 584.126,
		"end": 588.13,
		"text": "変態が通るぞ！<br />（男の子）気持ち悪ぃ。"
	},
	{
		"id": 52,
		"start": 588.13,
		"end": 590.132,
		"text": "そんな格好で　うろつくな！"
	},
	{
		"id": 53,
		"start": 590.132,
		"end": 593.135,
		"text": "嫌ねぇ。　気味が悪い。"
	},
	{
		"id": 54,
		"start": 593.135,
		"end": 595.137,
		"text": "まだ　あの格好でいるのか。"
	},
	{
		"id": 55,
		"start": 595.137,
		"end": 599.137,
		"text": "うわー！　変な人だ！<br />（母親）こら！　よしなさい！"
	},
	{
		"id": 56,
		"start": 613.155,
		"end": 633.175,
		"text": "♪♪～"
	},
	{
		"id": 57,
		"start": 633.175,
		"end": 641.199,
		"text": "♪♪～"
	},
	{
		"id": 58,
		"start": 641.199,
		"end": 646.121,
		"text": "（セニョール）すまない　ルシアン。<br />遅くなっちまった。"
	},
	{
		"id": 59,
		"start": 646.121,
		"end": 649.124,
		"text": "ハァ　ルシアン…。→"
	},
	{
		"id": 60,
		"start": 649.124,
		"end": 651.124,
		"text": "あっ。"
	},
	{
		"id": 61,
		"start": 660.135,
		"end": 662.135,
		"text": "ん？"
	},
	{
		"id": 62,
		"start": 678.153,
		"end": 681.153,
		"text": "あっ…。"
	},
	{
		"id": 63,
		"start": 689.164,
		"end": 691.166,
		"text": "（ギムレットの泣き声）"
	},
	{
		"id": 64,
		"start": 691.166,
		"end": 694.169,
		"text": "（ルシアン）《ねえ　あなた。<br />この子の名前は？》"
	},
	{
		"id": 65,
		"start": 694.169,
		"end": 698.173,
		"text": "《そうだな…。　ギムレット》"
	},
	{
		"id": 66,
		"start": 698.173,
		"end": 700.173,
		"text": "《すてきな名前》"
	},
	{
		"id": 67,
		"start": 708.116,
		"end": 710.116,
		"text": "（セニョール）ルシアン。"
	},
	{
		"id": 68,
		"start": 713.121,
		"end": 718.126,
		"text": "俺にとっては<br />どんな高価なスーツより→"
	},
	{
		"id": 69,
		"start": 718.126,
		"end": 722.13,
		"text": "この服は　値打ちがあるんだよ。"
	},
	{
		"id": 70,
		"start": 722.13,
		"end": 733.141,
		"text": "♪♪～"
	},
	{
		"id": 71,
		"start": 733.141,
		"end": 736.141,
		"text": "（セニョール）<br />《この服を着てる間だけ…》"
	},
	{
		"id": 72,
		"start": 738.146,
		"end": 741.149,
		"text": "（セニョール）<br />《君が　ほほ笑んでくれるから》"
	},
	{
		"id": 73,
		"start": 741.149,
		"end": 750.158,
		"text": "♪♪～"
	},
	{
		"id": 74,
		"start": 750.158,
		"end": 754.162,
		"text": "（ルシアン）<br />《あ～　降られちゃいましたね》"
	},
	{
		"id": 75,
		"start": 754.162,
		"end": 756.164,
		"text": "《もう　びしゃびしゃ。　フフッ》"
	},
	{
		"id": 76,
		"start": 756.164,
		"end": 760.168,
		"text": "《そんなに　ずぶぬれで<br />何で笑ってる？》"
	},
	{
		"id": 77,
		"start": 760.168,
		"end": 764.105,
		"text": "《私　好きなんです　雨》→"
	},
	{
		"id": 78,
		"start": 764.105,
		"end": 769.105,
		"text": "《あなたも好き？　雨》"
	},
	{
		"id": 79,
		"start": 771.112,
		"end": 774.112,
		"text": "（フランキー）うおおー！！"
	},
	{
		"id": 80,
		"start": 778.119,
		"end": 783.124,
		"text": "うおおおー！！"
	},
	{
		"id": 81,
		"start": 783.124,
		"end": 787.124,
		"text": "うーっ！！　おっ！！"
	},
	{
		"id": 82,
		"start": 797.138,
		"end": 800.138,
		"text": "《君が好きだった雨が…》"
	},
	{
		"id": 83,
		"start": 802.143,
		"end": 805.146,
		"text": "（セニョール）《やんだみたいだ…》"
	},
	{
		"id": 84,
		"start": 805.146,
		"end": 816.146,
		"text": "♪♪～"
	},
	{
		"id": 85,
		"start": 824.099,
		"end": 826.099,
		"text": "（ルシアン）《あなた…》"
	},
	{
		"id": 86,
		"start": 837.112,
		"end": 839.114,
		"text": "（一同）セニョール！"
	},
	{
		"id": 87,
		"start": 839.114,
		"end": 842.117,
		"text": "（一同）フラランドー！"
	},
	{
		"id": 88,
		"start": 842.117,
		"end": 849.117,
		"text": "（フランキー）うっ…　ハァ…。"
	},
	{
		"id": 89,
		"start": 854.129,
		"end": 858.133,
		"text": "みっともねえな。"
	},
	{
		"id": 90,
		"start": 858.133,
		"end": 861.136,
		"text": "いってぇ　何を思い出して→"
	},
	{
		"id": 91,
		"start": 861.136,
		"end": 865.14,
		"text": "泣いてるんだよ…　きょうだい…。"
	},
	{
		"id": 92,
		"start": 865.14,
		"end": 882.09,
		"text": "♪♪～"
	},
	{
		"id": 93,
		"start": 882.09,
		"end": 887.095,
		"text": "（フランキー）いつか　また<br />どこかで会ったら→"
	},
	{
		"id": 94,
		"start": 887.095,
		"end": 893.101,
		"text": "酒を飲みながら<br />話してくれよ…。→"
	},
	{
		"id": 95,
		"start": 893.101,
		"end": 898.101,
		"text": "ルシアンって　女の話を。"
	},
	{
		"id": 96,
		"start": 906.114,
		"end": 924.132,
		"text": "♪♪～"
	},
	{
		"id": 97,
		"start": 924.132,
		"end": 927.135,
		"text": "さあ　野郎ども！"
	},
	{
		"id": 98,
		"start": 927.135,
		"end": 930.138,
		"text": "覚悟はいいか？"
	},
	{
		"id": 99,
		"start": 930.138,
		"end": 933.141,
		"text": "（一同）おーっ！"
	},
	{
		"id": 100,
		"start": 933.141,
		"end": 936.144,
		"text": "お前ら　トンタッタ族を苦しめた→"
	},
	{
		"id": 101,
		"start": 936.144,
		"end": 942.15,
		"text": "この　にっくき工場を<br />破壊しろー！！"
	},
	{
		"id": 102,
		"start": 942.15,
		"end": 945.153,
		"text": "（一同）おおー！！"
	},
	{
		"id": 103,
		"start": 945.153,
		"end": 947.155,
		"text": "（小人）行くれすー！"
	},
	{
		"id": 104,
		"start": 947.155,
		"end": 950.158,
		"text": "（小人）ありがとうれす！<br />フラランドー！"
	},
	{
		"id": 105,
		"start": 950.158,
		"end": 954.158,
		"text": "（フランキー）ハァ　ハァ…。"
	},
	{
		"id": 106,
		"start": 956.164,
		"end": 961.169,
		"text": "ハァ　ハァ…。"
	},
	{
		"id": 107,
		"start": 961.169,
		"end": 965.169,
		"text": "（フランキー）<br />《トンタッタ…　頑張れよ！》"
	},
	{
		"id": 108,
		"start": 968.176,
		"end": 975.183,
		"text": "（フランキー）<br />《後は　そっち頼むぜ　ルフィ》"
	},
	{
		"id": 109,
		"start": 975.183,
		"end": 990.183,
		"text": "♪♪～"
	},
	{
		"id": 110,
		"start": 993.201,
		"end": 996.204,
		"text": "ケガ人を助けるのが先だ！"
	},
	{
		"id": 111,
		"start": 996.204,
		"end": 998.204,
		"text": "救護班を呼べ！<br />（海兵）はっ！"
	},
	{
		"id": 112,
		"start": 1004.145,
		"end": 1008.145,
		"text": "（ディアマンテ）ショーターイム！"
	},
	{
		"id": 113,
		"start": 1013.154,
		"end": 1018.159,
		"text": "（ディアマンテ）さあ　一緒に<br />ショータイムしようぜぇ！"
	},
	{
		"id": 114,
		"start": 1018.159,
		"end": 1021.162,
		"text": "早く見せてくれよ！"
	},
	{
		"id": 115,
		"start": 1021.162,
		"end": 1028.162,
		"text": "親子揃って<br />見苦しく死んでいく姿をよ！"
	},
	{
		"id": 116,
		"start": 1031.172,
		"end": 1037.178,
		"text": "半月グレイブ！"
	},
	{
		"id": 117,
		"start": 1037.178,
		"end": 1040.181,
		"text": "（レベッカ）キャー！<br />（キュロス）うわ！"
	},
	{
		"id": 118,
		"start": 1040.181,
		"end": 1044.185,
		"text": "（ディアマンテ）ウハハハ…！<br />キュロス！→"
	},
	{
		"id": 119,
		"start": 1044.185,
		"end": 1051.192,
		"text": "お前　さっき　生意気に<br />怒ってたんじゃねえのか？→"
	},
	{
		"id": 120,
		"start": 1051.192,
		"end": 1055.196,
		"text": "俺が　ただ　ヒラヒラするだけの→"
	},
	{
		"id": 121,
		"start": 1055.196,
		"end": 1058.199,
		"text": "天才マタドールだとでも<br />思ったか？"
	},
	{
		"id": 122,
		"start": 1058.199,
		"end": 1064.138,
		"text": "いつまでも　てめえが<br />コロシアム史上最強だなんて思うな！→"
	},
	{
		"id": 123,
		"start": 1064.138,
		"end": 1067.141,
		"text": "俺には　確かな実力がある。"
	},
	{
		"id": 124,
		"start": 1067.141,
		"end": 1070.144,
		"text": "現コロシアムの英雄は→"
	},
	{
		"id": 125,
		"start": 1070.144,
		"end": 1074.144,
		"text": "この　ディアマンテさまよ！"
	},
	{
		"id": 126,
		"start": 1308.716,
		"end": 1311.719,
		"text": "現コロシアムの英雄は→"
	},
	{
		"id": 127,
		"start": 1311.719,
		"end": 1315.723,
		"text": "この　ディアマンテさまよ！"
	},
	{
		"id": 128,
		"start": 1315.723,
		"end": 1318.726,
		"text": "（キュロス）肩書など　どうでもいい！"
	},
	{
		"id": 129,
		"start": 1318.726,
		"end": 1323.731,
		"text": "だが　ヒラヒラもするのさ！<br />ウハハハ…！"
	},
	{
		"id": 130,
		"start": 1323.731,
		"end": 1326.734,
		"text": "やあー！"
	},
	{
		"id": 131,
		"start": 1326.734,
		"end": 1328.736,
		"text": "（ディアマンテ）陸軍旗！"
	},
	{
		"id": 132,
		"start": 1328.736,
		"end": 1331.739,
		"text": "（キュロス）くっ…　うわ！→"
	},
	{
		"id": 133,
		"start": 1331.739,
		"end": 1333.739,
		"text": "ああ…！"
	},
	{
		"id": 134,
		"start": 1335.743,
		"end": 1339.747,
		"text": "ウハハハハ！<br />（キュロス）あっ！"
	},
	{
		"id": 135,
		"start": 1339.747,
		"end": 1341.749,
		"text": "（レベッカ）お父さま！"
	},
	{
		"id": 136,
		"start": 1341.749,
		"end": 1345.753,
		"text": "惜しかったな。<br />もうちょっとだったのに。"
	},
	{
		"id": 137,
		"start": 1345.753,
		"end": 1348.756,
		"text": "ハァ　ハァ　くっ…　うっ！"
	},
	{
		"id": 138,
		"start": 1348.756,
		"end": 1351.759,
		"text": "ウハハハ！<br />そりゃそうだろうよ！"
	},
	{
		"id": 139,
		"start": 1351.759,
		"end": 1354.762,
		"text": "今日一日　国中を駆け回って→"
	},
	{
		"id": 140,
		"start": 1354.762,
		"end": 1359.767,
		"text": "この揺れる大地で<br />娘の命をかばいながら　戦闘。→"
	},
	{
		"id": 141,
		"start": 1359.767,
		"end": 1362.703,
		"text": "それを片足でやってんだ！→"
	},
	{
		"id": 142,
		"start": 1362.703,
		"end": 1366.707,
		"text": "常人なら<br />もう　とうに動けねえよ。"
	},
	{
		"id": 143,
		"start": 1366.707,
		"end": 1372.713,
		"text": "お父さま…。<br />（キュロス）ハァ　ハァ…。"
	},
	{
		"id": 144,
		"start": 1372.713,
		"end": 1375.716,
		"text": "（レパント）ヴィオラさま。<br />今　キュロスさまの様子は…。"
	},
	{
		"id": 145,
		"start": 1375.716,
		"end": 1379.72,
		"text": "（ヴィオラ）<br />かなり体力を消耗してるわ。"
	},
	{
		"id": 146,
		"start": 1379.72,
		"end": 1383.724,
		"text": "（ヴィオラ）キュロス義兄さまも<br />もう　限界に近い！"
	},
	{
		"id": 147,
		"start": 1383.724,
		"end": 1388.729,
		"text": "（レパント）どうか<br />スカーレットさまの敵を…。"
	},
	{
		"id": 148,
		"start": 1388.729,
		"end": 1390.731,
		"text": "（キュロス）ハァ　ハァ…。→"
	},
	{
		"id": 149,
		"start": 1390.731,
		"end": 1395.736,
		"text": "片足を気遣ってくれなくて<br />結構だ！"
	},
	{
		"id": 150,
		"start": 1395.736,
		"end": 1398.739,
		"text": "ほう？"
	},
	{
		"id": 151,
		"start": 1398.739,
		"end": 1402.743,
		"text": "それを言い訳にするくらいなら<br />戦場に立たん！"
	},
	{
		"id": 152,
		"start": 1402.743,
		"end": 1408.749,
		"text": "ない足は　大恩人に捧げた<br />私の誇りだ！"
	},
	{
		"id": 153,
		"start": 1408.749,
		"end": 1412.753,
		"text": "（ディアマンテ）そうか。<br />じゃあ安心して　弱点を突ける。"
	},
	{
		"id": 154,
		"start": 1412.753,
		"end": 1415.756,
		"text": "もう疲労困憊のお前に→"
	},
	{
		"id": 155,
		"start": 1415.756,
		"end": 1420.761,
		"text": "俺が　とどめを刺すのも<br />味気ねえ！"
	},
	{
		"id": 156,
		"start": 1420.761,
		"end": 1423.761,
		"text": "こういう遊びは　どうだ？"
	},
	{
		"id": 157,
		"start": 1427.702,
		"end": 1432.702,
		"text": "ラストショーターイム！"
	},
	{
		"id": 158,
		"start": 1436.711,
		"end": 1439.714,
		"text": "（ロビン）あれは…。<br />（レベッカ）紙吹雪？"
	},
	{
		"id": 159,
		"start": 1439.714,
		"end": 1442.717,
		"text": "（ディアマンテ）そう。<br />その紙っぺらに見えるのは→"
	},
	{
		"id": 160,
		"start": 1442.717,
		"end": 1446.721,
		"text": "俺が　ヒラヒラにした何かだ。"
	},
	{
		"id": 161,
		"start": 1446.721,
		"end": 1450.725,
		"text": "さあ　能力を解くぞ。→"
	},
	{
		"id": 162,
		"start": 1450.725,
		"end": 1454.729,
		"text": "解除！"
	},
	{
		"id": 163,
		"start": 1454.729,
		"end": 1456.731,
		"text": "棘の鉄球！"
	},
	{
		"id": 164,
		"start": 1456.731,
		"end": 1459.734,
		"text": "（ディアマンテ）見ろ！　美しい！→"
	},
	{
		"id": 165,
		"start": 1459.734,
		"end": 1462.737,
		"text": "まるで　星屑のようだ！"
	},
	{
		"id": 166,
		"start": 1462.737,
		"end": 1465.74,
		"text": "（レベッカ）ロビンさん！？<br />（ロビン）逃げ場はなさそうね。"
	},
	{
		"id": 167,
		"start": 1465.74,
		"end": 1468.743,
		"text": "（キュロス）ロビランド！<br />レベッカを頼むぞ！"
	},
	{
		"id": 168,
		"start": 1468.743,
		"end": 1470.745,
		"text": "（ロビン）隊長さん<br />あなたも危ないわ！"
	},
	{
		"id": 169,
		"start": 1470.745,
		"end": 1473.748,
		"text": "私なら　大丈夫だ！"
	},
	{
		"id": 170,
		"start": 1473.748,
		"end": 1477.752,
		"text": "慌ててるな？<br />察しがいい。→"
	},
	{
		"id": 171,
		"start": 1477.752,
		"end": 1482.69,
		"text": "そうさ。　こいつは<br />軍隊をも皆殺しにする星屑さ。→"
	},
	{
		"id": 172,
		"start": 1482.69,
		"end": 1486.694,
		"text": "生き残るのは至難の業だぞ。"
	},
	{
		"id": 173,
		"start": 1486.694,
		"end": 1488.696,
		"text": "ここが　ひまわり畑でよかった。"
	},
	{
		"id": 174,
		"start": 1488.696,
		"end": 1490.696,
		"text": "千紫万紅！"
	},
	{
		"id": 175,
		"start": 1493.701,
		"end": 1497.701,
		"text": "奇麗に咲いたところ<br />ごめんなさい！"
	},
	{
		"id": 176,
		"start": 1500.708,
		"end": 1504.712,
		"text": "せいぜい　もがけ！"
	},
	{
		"id": 177,
		"start": 1504.712,
		"end": 1506.714,
		"text": "アディオス！"
	},
	{
		"id": 178,
		"start": 1506.714,
		"end": 1509.714,
		"text": "死の星屑！"
	},
	{
		"id": 179,
		"start": 1513.721,
		"end": 1515.723,
		"text": "（レベッカ）あっ！"
	},
	{
		"id": 180,
		"start": 1515.723,
		"end": 1517.723,
		"text": "（ロビン）花傘！"
	},
	{
		"id": 181,
		"start": 1519.727,
		"end": 1521.729,
		"text": "危なかった…。"
	},
	{
		"id": 182,
		"start": 1521.729,
		"end": 1523.731,
		"text": "（ディアマンテ）ほう　うまくやりやがる。"
	},
	{
		"id": 183,
		"start": 1523.731,
		"end": 1525.733,
		"text": "≪（キュロス）よそ見するな！"
	},
	{
		"id": 184,
		"start": 1525.733,
		"end": 1527.733,
		"text": "（ディアマンテ）はぁ？"
	},
	{
		"id": 185,
		"start": 1529.737,
		"end": 1532.737,
		"text": "（ディアマンテ）キュロス！"
	},
	{
		"id": 186,
		"start": 1534.742,
		"end": 1537.745,
		"text": "こんな攻撃で<br />やられるような軍隊は→"
	},
	{
		"id": 187,
		"start": 1537.745,
		"end": 1539.747,
		"text": "高が知れている！"
	},
	{
		"id": 188,
		"start": 1539.747,
		"end": 1541.766,
		"text": "なんて　むちゃくちゃな！→"
	},
	{
		"id": 189,
		"start": 1541.766,
		"end": 1545.686,
		"text": "棘鉄球を　全て　はじいてやがる。"
	},
	{
		"id": 190,
		"start": 1545.686,
		"end": 1547.688,
		"text": "ぬああー！<br />（ディアマンテ）こいつ→"
	},
	{
		"id": 191,
		"start": 1547.688,
		"end": 1549.69,
		"text": "雨でも<br />ぬれずに進めるってのか！？"
	},
	{
		"id": 192,
		"start": 1549.69,
		"end": 1551.692,
		"text": "やああー！"
	},
	{
		"id": 193,
		"start": 1551.692,
		"end": 1553.694,
		"text": "くっそ…。"
	},
	{
		"id": 194,
		"start": 1553.694,
		"end": 1556.694,
		"text": "（キュロス）うわっ…。<br />（レベッカ）あっ！"
	},
	{
		"id": 195,
		"start": 1560.701,
		"end": 1562.703,
		"text": "あーっ！！"
	},
	{
		"id": 196,
		"start": 1562.703,
		"end": 1564.705,
		"text": "（キュロス）うああー！！"
	},
	{
		"id": 197,
		"start": 1564.705,
		"end": 1567.708,
		"text": "お父さまー！！"
	},
	{
		"id": 198,
		"start": 1567.708,
		"end": 1569.71,
		"text": "（ディアマンテ）ウハハハハ！→"
	},
	{
		"id": 199,
		"start": 1569.71,
		"end": 1571.712,
		"text": "上に気を付けろよ　キュロス！"
	},
	{
		"id": 200,
		"start": 1571.712,
		"end": 1573.714,
		"text": "ドジなやつだ！"
	},
	{
		"id": 201,
		"start": 1573.714,
		"end": 1576.717,
		"text": "こんなときに転んじまうなんて！"
	},
	{
		"id": 202,
		"start": 1576.717,
		"end": 1579.72,
		"text": "おのれ　ディアマンテ！<br />許さない！"
	},
	{
		"id": 203,
		"start": 1579.72,
		"end": 1582.723,
		"text": "駄目よ　レベッカ！<br />まだ鉄球が…。"
	},
	{
		"id": 204,
		"start": 1582.723,
		"end": 1586.727,
		"text": "（キュロス）ぐわああー！<br />（レベッカ）お父さまー！"
	},
	{
		"id": 205,
		"start": 1586.727,
		"end": 1589.73,
		"text": "うぐっ…。<br />（ディアマンテ）ウハハハ…。"
	},
	{
		"id": 206,
		"start": 1589.73,
		"end": 1595.736,
		"text": "お前の妻　スカーレットを<br />撃ったのも　この銃よ。"
	},
	{
		"id": 207,
		"start": 1595.736,
		"end": 1597.738,
		"text": "ぬう…！"
	},
	{
		"id": 208,
		"start": 1597.738,
		"end": 1599.74,
		"text": "（ディアマンテ）よかったな。→"
	},
	{
		"id": 209,
		"start": 1599.74,
		"end": 1603.677,
		"text": "夫婦揃って<br />俺の銃で死ねるんだからよ。"
	},
	{
		"id": 210,
		"start": 1603.677,
		"end": 1608.682,
		"text": "許さない！<br />許さない！　ディアマンテ！"
	},
	{
		"id": 211,
		"start": 1608.682,
		"end": 1613.687,
		"text": "ハハハハ。<br />そうだ　レベッカ　俺を許すな。"
	},
	{
		"id": 212,
		"start": 1613.687,
		"end": 1616.69,
		"text": "戦え。<br />この手にかけてやる。"
	},
	{
		"id": 213,
		"start": 1616.69,
		"end": 1619.693,
		"text": "母のようにな。"
	},
	{
		"id": 214,
		"start": 1619.693,
		"end": 1622.696,
		"text": "離して　ロビンさん！<br />私　あいつを…！"
	},
	{
		"id": 215,
		"start": 1622.696,
		"end": 1625.696,
		"text": "ウハハハ…！"
	},
	{
		"id": 216,
		"start": 1628.702,
		"end": 1630.704,
		"text": "（ロビン）《いつまで続くの…》→"
	},
	{
		"id": 217,
		"start": 1630.704,
		"end": 1632.704,
		"text": "《もう　花傘も限界…》"
	},
	{
		"id": 218,
		"start": 1634.708,
		"end": 1636.71,
		"text": "ぬう…！"
	},
	{
		"id": 219,
		"start": 1636.71,
		"end": 1640.714,
		"text": "ディアマンテー！！"
	},
	{
		"id": 220,
		"start": 1640.714,
		"end": 1642.714,
		"text": "（レベッカ）えっ…。"
	},
	{
		"id": 221,
		"start": 1648.722,
		"end": 1650.722,
		"text": "（レベッカ）嘘…。"
	},
	{
		"id": 222,
		"start": 1660.734,
		"end": 1663.671,
		"text": "私の戦いだ！"
	},
	{
		"id": 223,
		"start": 1663.671,
		"end": 1665.671,
		"text": "レベッカ！"
	},
	{
		"id": 224,
		"start": 1667.675,
		"end": 1671.675,
		"text": "剣を捨てろー！"
	},
	{
		"id": 225,
		"start": 1673.681,
		"end": 1677.685,
		"text": "お前は　せっかく　１０年もの間…。"
	},
	{
		"id": 226,
		"start": 1677.685,
		"end": 1681.689,
		"text": "お父さま！<br />傘の中へ！　死んじゃうよ！"
	},
	{
		"id": 227,
		"start": 1681.689,
		"end": 1687.695,
		"text": "母の願いを守り抜いたのだろう？"
	},
	{
		"id": 228,
		"start": 1687.695,
		"end": 1690.698,
		"text": "（レベッカ）《お母さまが<br />人を傷つけちゃ駄目だって》"
	},
	{
		"id": 229,
		"start": 1690.698,
		"end": 1693.698,
		"text": "（雷兵隊）《お前が<br />傷つかない方法を教えるのだ》"
	},
	{
		"id": 230,
		"start": 1696.704,
		"end": 1699.707,
		"text": "（観客）《戦え！　レベッカ！》"
	},
	{
		"id": 231,
		"start": 1699.707,
		"end": 1701.709,
		"text": "（ギャッツ）《誰一人<br />傷つけることなく→"
	},
	{
		"id": 232,
		"start": 1701.709,
		"end": 1707.715,
		"text": "全てを場外で勝利してきた<br />無敗の女！》"
	},
	{
		"id": 233,
		"start": 1707.715,
		"end": 1710.718,
		"text": "《敵も斬れず　何が勝者だ！》"
	},
	{
		"id": 234,
		"start": 1710.718,
		"end": 1713.718,
		"text": "（観客）《血を流せ　レベッカ！》"
	},
	{
		"id": 235,
		"start": 1715.723,
		"end": 1717.725,
		"text": "（観客）《この腰抜けがー！》"
	},
	{
		"id": 236,
		"start": 1717.725,
		"end": 1720.728,
		"text": "（スパルタン）《みっともねえ試合<br />しやがって》→"
	},
	{
		"id": 237,
		"start": 1720.728,
		"end": 1724.665,
		"text": "《剣闘士の恥さらしが！》"
	},
	{
		"id": 238,
		"start": 1724.665,
		"end": 1726.667,
		"text": "（観客）《くたばれ　レベッカ！》"
	},
	{
		"id": 239,
		"start": 1726.667,
		"end": 1728.669,
		"text": "（観客）《人でなしの一族！》"
	},
	{
		"id": 240,
		"start": 1728.669,
		"end": 1731.669,
		"text": "（観客）《汚れた血の女だ！》"
	},
	{
		"id": 241,
		"start": 1735.676,
		"end": 1742.683,
		"text": "あのコロシアムに立って　なお<br />人を傷つけなかった！"
	},
	{
		"id": 242,
		"start": 1742.683,
		"end": 1746.683,
		"text": "お前の手は　まだ　美しい！"
	},
	{
		"id": 243,
		"start": 1749.69,
		"end": 1751.69,
		"text": "（キュロス）その手こそ…。"
	},
	{
		"id": 244,
		"start": 1753.694,
		"end": 1757.694,
		"text": "母　スカーレットの宿る手だ！"
	},
	{
		"id": 245,
		"start": 1762.703,
		"end": 1764.705,
		"text": "《お母さま…》"
	},
	{
		"id": 246,
		"start": 1764.705,
		"end": 1767.708,
		"text": "こんな外道の血で→"
	},
	{
		"id": 247,
		"start": 1767.708,
		"end": 1771.708,
		"text": "いまさら<br />その手を汚すことはない！"
	},
	{
		"id": 248,
		"start": 1779.72,
		"end": 1781.739,
		"text": "レベッカ！"
	},
	{
		"id": 249,
		"start": 1781.739,
		"end": 1785.739,
		"text": "お前に　剣は振らせない！"
	},
	{
		"id": 250,
		"start": 1788.662,
		"end": 1792.662,
		"text": "うおおおー！！"
	},
	{
		"id": 251,
		"start": 1799.673,
		"end": 1802.676,
		"text": "＜無数の死の鉄球を<br />全身に受け→"
	},
	{
		"id": 252,
		"start": 1802.676,
		"end": 1804.678,
		"text": "限界を迎える　キュロス＞"
	},
	{
		"id": 253,
		"start": 1804.678,
		"end": 1806.68,
		"text": "＜だが　ドフラミンゴから受けた→"
	},
	{
		"id": 254,
		"start": 1806.68,
		"end": 1811.685,
		"text": "国民たちの苦痛と悲しみを思い<br />彼の怒りが頂点に達したとき→"
	},
	{
		"id": 255,
		"start": 1811.685,
		"end": 1816.69,
		"text": "ドレスローザの因縁を切り裂く<br />全身全霊の一撃が放たれる！＞"
	},
	{
		"id": 256,
		"start": 1816.69,
		"end": 1818.69,
		"text": "次回　『ワンピース』"
	},
	{
		"id": 257,
		"start": 1820.694,
		"end": 1822.694,
		"text": "海賊王に　俺は　なる！"
	},
	{
		"id": 258,
		"start": 1834.591,
		"end": 1835.859,
		"text": "（東野）初登場でございます。<br />メンタリストのＤａｉＧｏさんです。"
	},
	{
		"id": 259,
		"start": 1835.859,
		"end": 1837.711,
		"text": "よろしくお願いします。<br />（松本）北川　景子じゃない方の→"
	},
	{
		"id": 260,
		"start": 1837.711,
		"end": 1839.73,
		"text": "ＤａｉＧｏですよね。<br />（ＤａｉＧｏ）そうですね。"
	},
	{
		"id": 261,
		"start": 1839.73,
		"end": 1842.733,
		"text": "間違えられますけれども<br />メンタリストのＤａｉＧｏさんで。"
	},
	{
		"id": 262,
		"start": 1842.733,
		"end": 1844.718,
		"text": "最近　あんまり　メンタリスト<br />やってないんですね。"
	},
	{
		"id": 263,
		"start": 1844.718,
		"end": 1846.72,
		"text": "（ＤａｉＧｏ）そうですね。<br />その辺　色々あるみたいなんです。"
	},
	{
		"id": 264,
		"start": 1846.72,
		"end": 1849.723,
		"text": "（東野）ご本人的にも。<br />（ＤａｉＧｏ）最近　違う仕事とかを→"
	},
	{
		"id": 265,
		"start": 1849.723,
		"end": 1851.725,
		"text": "たくさん　やっていて。<br />企業のアドバイザーとかを→"
	},
	{
		"id": 266,
		"start": 1851.725,
		"end": 1854.945,
		"text": "やってるんで　あんまり　テレビ<br />そこそこみたいな感じで。"
	},
	{
		"id": 267,
		"start": 1854.945,
		"end": 1856.647,
		"text": "いっとき<br />すごかったじゃないですか。"
	},
	{
		"id": 268,
		"start": 1856.647,
		"end": 1858.649,
		"text": "（松本）メンタリストの。<br />その辺は→"
	},
	{
		"id": 269,
		"start": 1858.649,
		"end": 1860.651,
		"text": "結局　あれは<br />いんちきやったってことですか？"
	}
]