	{
		"id": 187,
		"start": 1446.483,
		"end": 1450.487,
		"text": "みんな急に　町の方　見てよ。"
	},
	{
		"id": 188,
		"start": 1450.487,
		"end": 1454.491,
		"text": "（フラッパー）<br />わっ　分かんないれすけど→"
	},
	{
		"id": 189,
		"start": 1454.491,
		"end": 1461.498,
		"text": "何か　胸が　すっとすくような…。"
	},
	{
		"id": 190,
		"start": 1461.498,
		"end": 1463.5,
		"text": "ハァ。"
	},
	{
		"id": 191,
		"start": 1463.5,
		"end": 1468.505,
		"text": "なっ…！<br />おい　すいてる場合じゃねえだろ！"
	},
	{
		"id": 192,
		"start": 1468.505,
		"end": 1471.525,
		"text": "（フランキー）工場破壊は完了した。"
	},
	{
		"id": 193,
		"start": 1471.525,
		"end": 1474.525,
