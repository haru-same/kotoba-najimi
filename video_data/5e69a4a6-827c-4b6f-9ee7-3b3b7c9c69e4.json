[
	{
		"id": 1,
		"start": 122.764,
		"end": 132.764,
		"text": "♪♪～"
	},
	{
		"id": 2,
		"start": 343.718,
		"end": 347.722,
		"text": "＜ワンピースを狙う<br />全ての海賊たちが目指す→"
	},
	{
		"id": 3,
		"start": 347.722,
		"end": 352.722,
		"text": "グランドライン後半の海<br />人呼んで　新世界＞"
	},
	{
		"id": 4,
		"start": 354.729,
		"end": 357.732,
		"text": "＜しかし<br />かつて　その海を制したのは→"
	},
	{
		"id": 5,
		"start": 357.732,
		"end": 362.737,
		"text": "海賊王　ゴールド・ロジャー<br />ただ　一人＞"
	},
	{
		"id": 6,
		"start": 362.737,
		"end": 369.744,
		"text": "＜新世界は　幾多の海賊たちの<br />夢と野望を阻んできた＞"
	},
	{
		"id": 7,
		"start": 369.744,
		"end": 373.748,
		"text": "＜その海に今　ゴムゴムの実を食べゴム人間となった少年→"
	},
	{
		"id": 8,
		"start": 373.748,
		"end": 377.752,
		"text": "モンキー・Ｄ・ルフィと<br />その仲間たちが挑む！＞"
	},
	{
		"id": 9,
		"start": 377.752,
		"end": 382.752,
		"text": "（ルフィ）海賊王に　俺は　なる！"
	},
	{
		"id": 10,
		"start": 385.76,
		"end": 388.763,
		"text": "＜新世界を支配する<br />四皇の一角を崩すため→"
	},
	{
		"id": 11,
		"start": 388.763,
		"end": 391.766,
		"text": "トラファルガー・ローと<br />海賊同盟を組んだ　ルフィたちは→"
	},
	{
		"id": 12,
		"start": 391.766,
		"end": 394.769,
		"text": "ドレスローザに乗り込むも→"
	},
	{
		"id": 13,
		"start": 394.769,
		"end": 397.772,
		"text": "現国王　ドフラミンゴの<br />計略によって→"
	},
	{
		"id": 14,
		"start": 397.772,
		"end": 401.709,
		"text": "島ごと閉じ込められてしまう＞"
	},
	{
		"id": 15,
		"start": 401.709,
		"end": 405.713,
		"text": "＜一味が　ドンキホーテファミリーと<br />激しい戦いを繰り広げる中→"
	},
	{
		"id": 16,
		"start": 405.713,
		"end": 410.713,
		"text": "ルフィとローは　王宮で<br />ついに　ドフラミンゴと激突＞"
	},
	{
		"id": 17,
		"start": 412.72,
		"end": 417.725,
		"text": "＜そして　ゾロは<br />リク王に迫る　ピーカに…＞"
	},
	{
		"id": 18,
		"start": 417.725,
		"end": 426.725,
		"text": "（オオロンブス）提督キラーボウリング！"
	},
	{
		"id": 19,
		"start": 429.737,
		"end": 433.741,
		"text": "（ゾロ）九山八海　一世界。"
	},
	{
		"id": 20,
		"start": 433.741,
		"end": 436.744,
		"text": "千　集まって　小千世界。"
	},
	{
		"id": 21,
		"start": 436.744,
		"end": 442.744,
		"text": "三乗　結んで　斬れぬ物なし！"
	},
	{
		"id": 22,
		"start": 444.752,
		"end": 447.752,
		"text": "三刀流奥義！"
	},
	{
		"id": 23,
		"start": 451.759,
		"end": 458.759,
		"text": "一大・三千・大千・世界！"
	},
	{
		"id": 24,
		"start": 464.706,
		"end": 467.709,
		"text": "（ピーカ）ピッキャー！"
	},
	{
		"id": 25,
		"start": 467.709,
		"end": 471.713,
		"text": "九山八海　斬れぬ物なし。"
	},
	{
		"id": 26,
		"start": 471.713,
		"end": 473.715,
		"text": "（ピーカ）死ね～！"
	},
	{
		"id": 27,
		"start": 473.715,
		"end": 475.715,
		"text": "三・千・世・界！"
	},
	{
		"id": 28,
		"start": 478.72,
		"end": 480.722,
		"text": "（ウソップ）ぶぇ～！→"
	},
	{
		"id": 29,
		"start": 480.722,
		"end": 484.726,
		"text": "で　結局　落ちてくる！"
	},
	{
		"id": 30,
		"start": 484.726,
		"end": 489.726,
		"text": "（エリザベロー）キング・パンチ！！"
	},
	{
		"id": 31,
		"start": 498.74,
		"end": 500.74,
		"text": "上出来だ。"
	},
	{
		"id": 32,
		"start": 536.711,
		"end": 539.714,
		"text": "（ミホーク）<br />《刀を折られたことはあるか？》"
	},
	{
		"id": 33,
		"start": 539.714,
		"end": 542.717,
		"text": "《あ？》"
	},
	{
		"id": 34,
		"start": 542.717,
		"end": 545.72,
		"text": "《まあ　何度かな》"
	},
	{
		"id": 35,
		"start": 545.72,
		"end": 547.722,
		"text": "《お前にも折られたよ》"
	},
	{
		"id": 36,
		"start": 547.722,
		"end": 549.724,
		"text": "（ミホーク）《覇気をまとえば→"
	},
	{
		"id": 37,
		"start": 549.724,
		"end": 553.728,
		"text": "同じ刀でも<br />折れることはなかった》"
	},
	{
		"id": 38,
		"start": 553.728,
		"end": 556.731,
		"text": "（ミホーク）《刃こぼれすら<br />おのれの恥と思え》→"
	},
	{
		"id": 39,
		"start": 556.731,
		"end": 560.735,
		"text": "《全ての刀剣は　黒刀になり得る》"
	},
	{
		"id": 40,
		"start": 560.735,
		"end": 564.739,
		"text": "《黒刀》<br />《それを体得するまで　禁酒だ》"
	},
	{
		"id": 41,
		"start": 564.739,
		"end": 566.739,
		"text": "《あぁ！？》"
	},
	{
		"id": 42,
		"start": 569.744,
		"end": 574.744,
		"text": "ハァ　ハァ…。"
	},
	{
		"id": 43,
		"start": 591.699,
		"end": 593.699,
		"text": "刃こぼれなし。"
	},
	{
		"id": 44,
		"start": 595.703,
		"end": 597.703,
		"text": "ヘヘ。"
	},
	{
		"id": 45,
		"start": 599.707,
		"end": 603.711,
		"text": "≪（ウソップ）ゾロ～！<br />ゾロ　無事か～！？"
	},
	{
		"id": 46,
		"start": 603.711,
		"end": 607.711,
		"text": "ヘッ　当たり前だ。"
	},
	{
		"id": 47,
		"start": 620.728,
		"end": 623.731,
		"text": "（藤虎）ケガ人は　ござんせんか？"
	},
	{
		"id": 48,
		"start": 623.731,
		"end": 626.734,
		"text": "（海兵）ええ。<br />イッショウさんのおかげです。"
	},
	{
		"id": 49,
		"start": 626.734,
		"end": 631.734,
		"text": "《キング・パンチ！！》"
	},
	{
		"id": 50,
		"start": 640.765,
		"end": 642.683,
		"text": "（海兵たち）<br />《残骸が落ちてくるぞ～！》→"
	},
	{
		"id": 51,
		"start": 642.683,
		"end": 645.686,
		"text": "《早く逃げろ！　走れ～！》"
	},
	{
		"id": 52,
		"start": 645.686,
		"end": 665.706,
		"text": "♪♪～"
	},
	{
		"id": 53,
		"start": 665.706,
		"end": 674.715,
		"text": "♪♪～"
	},
	{
		"id": 54,
		"start": 674.715,
		"end": 678.719,
		"text": "《イッショウさん…》<br />（藤虎）《さあ　早く　皆さんを》"
	},
	{
		"id": 55,
		"start": 678.719,
		"end": 680.721,
		"text": "（海兵）《はっ　はい！》<br />（海兵）《逃げろ！》"
	},
	{
		"id": 56,
		"start": 680.721,
		"end": 684.725,
		"text": "《今のうちに早く！》<br />（海兵）《逃げるんだ！》"
	},
	{
		"id": 57,
		"start": 684.725,
		"end": 686.725,
		"text": "《ああ…》"
	},
	{
		"id": 58,
		"start": 692.733,
		"end": 695.733,
		"text": "（海兵）《全員退避！》"
	},
	{
		"id": 59,
		"start": 702.677,
		"end": 704.679,
		"text": "（海兵）《イッショウさん<br />避難　完了です！》"
	},
	{
		"id": 60,
		"start": 704.679,
		"end": 706.681,
		"text": "《あい》"
	},
	{
		"id": 61,
		"start": 706.681,
		"end": 721.696,
		"text": "♪♪～"
	},
	{
		"id": 62,
		"start": 721.696,
		"end": 723.698,
		"text": "（海兵）イッショウさんが<br />いっとき→"
	},
	{
		"id": 63,
		"start": 723.698,
		"end": 727.702,
		"text": "あれを浮かせてくれたおかげで<br />一人も死傷者は出ていません。"
	},
	{
		"id": 64,
		"start": 727.702,
		"end": 732.707,
		"text": "（海兵）幸い　中心街は<br />避難区域にもしていましたので。"
	},
	{
		"id": 65,
		"start": 732.707,
		"end": 734.709,
		"text": "よござんした。"
	},
	{
		"id": 66,
		"start": 734.709,
		"end": 738.713,
		"text": "（市民）見たか！？<br />あの巨大な石像が　真っ二つに！"
	},
	{
		"id": 67,
		"start": 738.713,
		"end": 741.716,
		"text": "（市民）最高幹部がやられた<br />ってことか？"
	},
	{
		"id": 68,
		"start": 741.716,
		"end": 745.72,
		"text": "（部下）ピーカさまが　やられた～！"
	},
	{
		"id": 69,
		"start": 745.72,
		"end": 749.724,
		"text": "（サイ）この実力で<br />大会に出場しなかったとは→"
	},
	{
		"id": 70,
		"start": 749.724,
		"end": 751.726,
		"text": "謙虚なやつやい。<br />（チンジャオ）あれで→"
	},
	{
		"id": 71,
		"start": 751.726,
		"end": 754.729,
		"text": "ガープの孫の子分か。"
	},
	{
		"id": 72,
		"start": 754.729,
		"end": 757.732,
		"text": "ヘヘヘ　やりよる。"
	},
	{
		"id": 73,
		"start": 757.732,
		"end": 762.67,
		"text": "結果　リク王を守れたから<br />よしとするが…。"
	},
	{
		"id": 74,
		"start": 762.67,
		"end": 767.675,
		"text": "（バルトロメオ）ふぇ～！<br />ゾドゼンバビ～！"
	},
	{
		"id": 75,
		"start": 767.675,
		"end": 771.675,
		"text": "（キュロス）リク王さま！<br />（レベッカ）よかった！"
	},
	{
		"id": 76,
		"start": 775.683,
		"end": 777.685,
		"text": "え…　工場が！？"
	},
	{
		"id": 77,
		"start": 777.685,
		"end": 781.689,
		"text": "（市民）地下から出てきた工場が<br />爆発したぞ！"
	},
	{
		"id": 78,
		"start": 781.689,
		"end": 783.689,
		"text": "（市民）何が起きてんだ！？"
	},
	{
		"id": 79,
		"start": 789.697,
		"end": 792.7,
		"text": "（インヘル）もう一発いくれすよ！<br />（小人たち）おーっ！→"
	},
	{
		"id": 80,
		"start": 792.7,
		"end": 795.7,
		"text": "それーっ！！"
	},
	{
		"id": 81,
		"start": 798.706,
		"end": 800.708,
		"text": "（小人たち）やったー！！"
	},
	{
		"id": 82,
		"start": 800.708,
		"end": 803.711,
		"text": "（小人）吹き飛べ！<br />僕らを苦しめた工場！"
	},
	{
		"id": 83,
		"start": 803.711,
		"end": 808.711,
		"text": "もう　だまされて<br />こき使われることはないんだ！"
	},
	{
		"id": 84,
		"start": 815.723,
		"end": 818.726,
		"text": "（トレーボル）んね　んねー<br />ピーカもやられ→"
	},
	{
		"id": 85,
		"start": 818.726,
		"end": 823.664,
		"text": "工場も吹き飛んだぞ　ドフィ。<br />ベヘヘヘヘ。→"
	},
	{
		"id": 86,
		"start": 823.664,
		"end": 828.669,
		"text": "だが　敗者も復活する！<br />工場も再生する！→"
	},
	{
		"id": 87,
		"start": 828.669,
		"end": 830.671,
		"text": "ぬか喜んでいろ！→"
	},
	{
		"id": 88,
		"start": 830.671,
		"end": 837.678,
		"text": "こっちには　チユチユの実の能力者マンシェリーがいるのだ！"
	},
	{
		"id": 89,
		"start": 837.678,
		"end": 840.678,
		"text": "（トレーボル）ベヘヘヘ…！"
	},
	{
		"id": 90,
		"start": 849.69,
		"end": 853.694,
		"text": "≪（レオ）レベッカさま～！<br />ロビランド～！→"
	},
	{
		"id": 91,
		"start": 853.694,
		"end": 856.694,
		"text": "ご無事れすか～！？"
	},
	{
		"id": 92,
		"start": 864.705,
		"end": 866.707,
		"text": "（レオ）はっ！　隊長！→"
	},
	{
		"id": 93,
		"start": 866.707,
		"end": 869.71,
		"text": "キュロス隊長なんれすね！？<br />（キュロス）ああ。"
	},
	{
		"id": 94,
		"start": 869.71,
		"end": 871.712,
		"text": "姫を救えたんだな！　レオ！"
	},
	{
		"id": 95,
		"start": 871.712,
		"end": 875.716,
		"text": "（レオ）何て　りりしいお姿！<br />銅像に　そっくりれす！"
	},
	{
		"id": 96,
		"start": 875.716,
		"end": 878.719,
		"text": "ハッ　逆だが。"
	},
	{
		"id": 97,
		"start": 878.719,
		"end": 880.738,
		"text": "（マンシェリー）まあ　ひどい　おケガ！"
	},
	{
		"id": 98,
		"start": 880.738,
		"end": 883.657,
		"text": "すぐに治してさしあげます！"
	},
	{
		"id": 99,
		"start": 883.657,
		"end": 885.659,
		"text": "（キュロス）いや　待ってくれ。"
	},
	{
		"id": 100,
		"start": 885.659,
		"end": 887.661,
		"text": "君が　マンシェリー姫だな。"
	},
	{
		"id": 101,
		"start": 887.661,
		"end": 890.664,
		"text": "君の能力の話は<br />よく聞いていたが→"
	},
	{
		"id": 102,
		"start": 890.664,
		"end": 894.668,
		"text": "制限があるはず。<br />私には使わないでくれ。"
	},
	{
		"id": 103,
		"start": 894.668,
		"end": 898.672,
		"text": "えっ…。<br />ですけれど　全身から血が…。"
	},
	{
		"id": 104,
		"start": 898.672,
		"end": 900.674,
		"text": "（キュロス）大丈夫。　意識もある。→"
	},
	{
		"id": 105,
		"start": 900.674,
		"end": 906.68,
		"text": "まず　君が無事で　よかった。<br />つらかったろう。→"
	},
	{
		"id": 106,
		"start": 906.68,
		"end": 908.682,
		"text": "能力を使ってくれる気なら→"
	},
	{
		"id": 107,
		"start": 908.682,
		"end": 914.688,
		"text": "それを必要とする人たちは<br />国中にいる！"
	},
	{
		"id": 108,
		"start": 914.688,
		"end": 916.69,
		"text": "えっ！　これは…。"
	},
	{
		"id": 109,
		"start": 916.69,
		"end": 934.708,
		"text": "♪♪～"
	},
	{
		"id": 110,
		"start": 934.708,
		"end": 937.711,
		"text": "これが　ドレスローザ…？"
	},
	{
		"id": 111,
		"start": 937.711,
		"end": 940.714,
		"text": "状況は　レオたちに<br />色々　聞きましたけど→"
	},
	{
		"id": 112,
		"start": 940.714,
		"end": 942.716,
		"text": "こんなに　ひどいなんて…。"
	},
	{
		"id": 113,
		"start": 942.716,
		"end": 946.72,
		"text": "だが　工場の破壊にも<br />成功したようだ。"
	},
	{
		"id": 114,
		"start": 946.72,
		"end": 948.722,
		"text": "戦いは　もう正念場！"
	},
	{
		"id": 115,
		"start": 948.722,
		"end": 952.726,
		"text": "インヘルたち<br />うまくやったみたいれすね！"
	},
	{
		"id": 116,
		"start": 952.726,
		"end": 956.73,
		"text": "（キュロス）ドフラミンゴのわなによって<br />奪われた　この国を→"
	},
	{
		"id": 117,
		"start": 956.73,
		"end": 959.733,
		"text": "リク王の手に取り戻せるか→"
	},
	{
		"id": 118,
		"start": 959.733,
		"end": 962.733,
		"text": "再び　絶望の国へと転落するか…。"
	},
	{
		"id": 119,
		"start": 965.739,
		"end": 969.743,
		"text": "（キュロス）希望は　今…<br />ある海賊たちの手に→"
	},
	{
		"id": 120,
		"start": 969.743,
		"end": 971.745,
		"text": "委ねられている！"
	},
	{
		"id": 121,
		"start": 971.745,
		"end": 973.745,
		"text": "海賊？"
	},
	{
		"id": 122,
		"start": 977.751,
		"end": 980.751,
		"text": "（キュロス）あの王宮の…。"
	},
	{
		"id": 123,
		"start": 982.756,
		"end": 985.756,
		"text": "（キュロス）最上階で！"
	},
	{
		"id": 124,
		"start": 990.764,
		"end": 994.768,
		"text": "ルフィランド…　ローランド…。"
	},
	{
		"id": 125,
		"start": 994.768,
		"end": 997.771,
		"text": "（カブ）あの二人が　頼みの綱だ。"
	},
	{
		"id": 126,
		"start": 997.771,
		"end": 1001.709,
		"text": "（ロビン）ルフィ…　トラ男君…。"
	},
	{
		"id": 127,
		"start": 1001.709,
		"end": 1003.711,
		"text": "ルーシー…。"
	},
	{
		"id": 128,
		"start": 1003.711,
		"end": 1015.711,
		"text": "♪♪～"
	},
	{
		"id": 129,
		"start": 1250.324,
		"end": 1270.344,
		"text": "♪♪～"
	},
	{
		"id": 130,
		"start": 1270.344,
		"end": 1273.344,
		"text": "うがっ…！"
	},
	{
		"id": 131,
		"start": 1278.352,
		"end": 1282.356,
		"text": "（コアラ）今　外壁塔の中に<br />入ったところよ。"
	},
	{
		"id": 132,
		"start": 1282.356,
		"end": 1285.359,
		"text": "（コアラ）ちょっと　まずいことが<br />起きそうなの。"
	},
	{
		"id": 133,
		"start": 1285.359,
		"end": 1287.361,
		"text": "サボ君　来られない？　今どこ？"
	},
	{
		"id": 134,
		"start": 1287.361,
		"end": 1289.363,
		"text": "[TEL]（サボ）コロシアムの<br />逃げ遅れた囚人たちを→"
	},
	{
		"id": 135,
		"start": 1289.363,
		"end": 1293.367,
		"text": "逃がしているとこだ。<br />そっちまでは遠過ぎるよ。"
	},
	{
		"id": 136,
		"start": 1293.367,
		"end": 1296.37,
		"text": "（サボ）お前一人で<br />何とかならねえことなんて→"
	},
	{
		"id": 137,
		"start": 1296.37,
		"end": 1298.372,
		"text": "そうそうないだろ？<br />[TEL]（コアラ）できそうなら→"
	},
	{
		"id": 138,
		"start": 1298.372,
		"end": 1302.376,
		"text": "連絡しないよ！<br />ルフィ君に被害が及ぶかも。"
	},
	{
		"id": 139,
		"start": 1302.376,
		"end": 1305.376,
		"text": "（サボ）すぐ行く！<br />[TEL]（コアラ）もう！"
	},
	{
		"id": 140,
		"start": 1319.326,
		"end": 1323.326,
		"text": "≪（ウソップ）ゾロ～！　大丈夫か～！？"
	},
	{
		"id": 141,
		"start": 1326.333,
		"end": 1330.337,
		"text": "（ウソップ）ゾロ！　手を貸そうか？"
	},
	{
		"id": 142,
		"start": 1330.337,
		"end": 1332.339,
		"text": "≪大丈夫。　傷は受けてねえ。"
	},
	{
		"id": 143,
		"start": 1332.339,
		"end": 1335.339,
		"text": "よかった…。"
	},
	{
		"id": 144,
		"start": 1340.347,
		"end": 1347.347,
		"text": "（島民たち）わあー！！"
	},
	{
		"id": 145,
		"start": 1355.362,
		"end": 1358.362,
		"text": "ハァ　ハァ…。"
	},
	{
		"id": 146,
		"start": 1360.367,
		"end": 1363.387,
		"text": "ゾロ！　ありがどう！"
	},
	{
		"id": 147,
		"start": 1363.387,
		"end": 1366.306,
		"text": "もう駄目がど思っだ～！"
	},
	{
		"id": 148,
		"start": 1366.306,
		"end": 1369.309,
		"text": "（錦えもん）ゾロ殿！<br />あっぱれでござった！"
	},
	{
		"id": 149,
		"start": 1369.309,
		"end": 1371.311,
		"text": "海賊！　ありがとう！"
	},
	{
		"id": 150,
		"start": 1371.311,
		"end": 1376.316,
		"text": "あんな巨像を刀で斬るなんて<br />目を疑った！"
	},
	{
		"id": 151,
		"start": 1376.316,
		"end": 1379.319,
		"text": "待て待て　お前ら！"
	},
	{
		"id": 152,
		"start": 1379.319,
		"end": 1382.322,
		"text": "喜んでいいのは<br />この息の詰まりそうなカゴが→"
	},
	{
		"id": 153,
		"start": 1382.322,
		"end": 1384.324,
		"text": "消えたときだろ！"
	},
	{
		"id": 154,
		"start": 1384.324,
		"end": 1387.324,
		"text": "（一同）あっ…。"
	},
	{
		"id": 155,
		"start": 1397.337,
		"end": 1403.343,
		"text": "まだ　あるってことは<br />肝心の男が倒せてねえ証拠。"
	},
	{
		"id": 156,
		"start": 1403.343,
		"end": 1423.33,
		"text": "♪♪～"
	},
	{
		"id": 157,
		"start": 1423.33,
		"end": 1426.199,
		"text": "♪♪～"
	},
	{
		"id": 158,
		"start": 1426.199,
		"end": 1429.202,
		"text": "やめろ！　やめろ　ベラミー！"
	},
	{
		"id": 159,
		"start": 1429.202,
		"end": 1431.204,
		"text": "やめろー！！"
	},
	{
		"id": 160,
		"start": 1431.204,
		"end": 1434.204,
		"text": "（ベラミー）てやっ！<br />うわっ…！"
	},
	{
		"id": 161,
		"start": 1453.226,
		"end": 1457.23,
		"text": "ベラミー　何で…。"
	},
	{
		"id": 162,
		"start": 1457.23,
		"end": 1459.232,
		"text": "何で　そこまでする！？"
	},
	{
		"id": 163,
		"start": 1459.232,
		"end": 1461.234,
		"text": "お前…。"
	},
	{
		"id": 164,
		"start": 1461.234,
		"end": 1466.239,
		"text": "お前　ミンゴに　だまされて<br />利用されてんだぞ！"
	},
	{
		"id": 165,
		"start": 1466.239,
		"end": 1470.243,
		"text": "お前を自由にしてやるって<br />言ったけど…。"
	},
	{
		"id": 166,
		"start": 1470.243,
		"end": 1473.246,
		"text": "（ドフラミンゴ）《もう　じゅうぶんだ<br />ベラミー》→"
	},
	{
		"id": 167,
		"start": 1473.246,
		"end": 1479.252,
		"text": "《自由にしてやるよ。<br />今までどおりな》"
	},
	{
		"id": 168,
		"start": 1479.252,
		"end": 1484.252,
		"text": "お前が逃げずに　俺と戦って<br />死のうとすること…。"
	},
	{
		"id": 169,
		"start": 1486.293,
		"end": 1491.298,
		"text": "あいつ<br />分かってたんじゃねえのか？"
	},
	{
		"id": 170,
		"start": 1491.298,
		"end": 1493.3,
		"text": "（ベラミー）ハハハ！　だろうな！→"
	},
	{
		"id": 171,
		"start": 1493.3,
		"end": 1496.303,
		"text": "それが　どうした！→"
	},
	{
		"id": 172,
		"start": 1496.303,
		"end": 1498.305,
		"text": "俺は　あの人を尊敬している！→"
	},
	{
		"id": 173,
		"start": 1498.305,
		"end": 1502.309,
		"text": "死に方だけでも<br />認めてもらえるんなら→"
	},
	{
		"id": 174,
		"start": 1502.309,
		"end": 1504.311,
		"text": "それで本望だ！"
	},
	{
		"id": 175,
		"start": 1504.311,
		"end": 1508.315,
		"text": "嘘つけ！！<br />こうするしかねえんだろ！！"
	},
	{
		"id": 176,
		"start": 1508.315,
		"end": 1511.315,
		"text": "男だもんな！！"
	},
	{
		"id": 177,
		"start": 1514.321,
		"end": 1518.325,
		"text": "（ベラミー）そこまで分かってんなら<br />黙って戦え！"
	},
	{
		"id": 178,
		"start": 1518.325,
		"end": 1520.325,
		"text": "でも　無駄死にだ！"
	},
	{
		"id": 179,
		"start": 1523.33,
		"end": 1527.334,
		"text": "《いいや　本望さ》→"
	},
	{
		"id": 180,
		"start": 1527.334,
		"end": 1532.339,
		"text": "《もう一人だけ<br />俺が憧れた男に→"
	},
	{
		"id": 181,
		"start": 1532.339,
		"end": 1536.343,
		"text": "引導を渡されるんならな！》"
	},
	{
		"id": 182,
		"start": 1536.343,
		"end": 1556.296,
		"text": "♪♪～"
	},
	{
		"id": 183,
		"start": 1556.296,
		"end": 1567.307,
		"text": "♪♪～"
	},
	{
		"id": 184,
		"start": 1567.307,
		"end": 1569.309,
		"text": "（ベラミー）《麦わら…》"
	},
	{
		"id": 185,
		"start": 1569.309,
		"end": 1587.327,
		"text": "♪♪～"
	},
	{
		"id": 186,
		"start": 1587.327,
		"end": 1593.333,
		"text": "（鐘の音）"
	},
	{
		"id": 187,
		"start": 1593.333,
		"end": 1596.336,
		"text": "（サーキース）《おい　麦わら！<br />どこ行くんだ！》"
	},
	{
		"id": 188,
		"start": 1596.336,
		"end": 1600.336,
		"text": "《空》"
	},
	{
		"id": 189,
		"start": 1603.36,
		"end": 1606.279,
		"text": "《何　言ってんだ！<br />ベラミーは変わった！》"
	},
	{
		"id": 190,
		"start": 1606.279,
		"end": 1608.281,
		"text": "《変わったよ　ベラミー》"
	},
	{
		"id": 191,
		"start": 1608.281,
		"end": 1610.281,
		"text": "《友達だ！》"
	},
	{
		"id": 192,
		"start": 1612.285,
		"end": 1616.285,
		"text": "（ベラミー）《ありがとよ　麦わら》"
	},
	{
		"id": 193,
		"start": 1618.291,
		"end": 1621.294,
		"text": "（ベラミー）麦わらぁ！"
	},
	{
		"id": 194,
		"start": 1621.294,
		"end": 1624.297,
		"text": "俺のケンカを買う気が<br />ねえんなら→"
	},
	{
		"id": 195,
		"start": 1624.297,
		"end": 1631.304,
		"text": "俺が　お前を殺すまでだ～！"
	},
	{
		"id": 196,
		"start": 1631.304,
		"end": 1635.308,
		"text": "お前を殺す！"
	},
	{
		"id": 197,
		"start": 1635.308,
		"end": 1637.308,
		"text": "ぐっ…。"
	},
	{
		"id": 198,
		"start": 1641.314,
		"end": 1645.318,
		"text": "覚悟するんだな　麦わらぁ！"
	},
	{
		"id": 199,
		"start": 1645.318,
		"end": 1648.321,
		"text": "この一撃で終わりにするぞ！"
	},
	{
		"id": 200,
		"start": 1648.321,
		"end": 1652.325,
		"text": "何　突っ立ってる！<br />構えろ　臆病者！"
	},
	{
		"id": 201,
		"start": 1652.325,
		"end": 1658.325,
		"text": "お前が　もし　パンチの打ち方を<br />知ってるんならな！"
	},
	{
		"id": 202,
		"start": 1663.303,
		"end": 1665.172,
		"text": "ミンゴ～！！"
	},
	{
		"id": 203,
		"start": 1665.172,
		"end": 1667.174,
		"text": "麦わらぁ！"
	},
	{
		"id": 204,
		"start": 1667.174,
		"end": 1670.174,
		"text": "死ね～！！"
	},
	{
		"id": 205,
		"start": 1673.18,
		"end": 1677.184,
		"text": "あばよ　麦わらぁ！"
	},
	{
		"id": 206,
		"start": 1677.184,
		"end": 1681.184,
		"text": "《ありがとよ！<br />友達と呼んでくれて！》"
	},
	{
		"id": 207,
		"start": 1711.218,
		"end": 1721.228,
		"text": "ハァ　ハァ…。"
	},
	{
		"id": 208,
		"start": 1721.228,
		"end": 1723.23,
		"text": "（ベラミー）《分かってんだ！》→"
	},
	{
		"id": 209,
		"start": 1723.23,
		"end": 1729.23,
		"text": "《憧れる男を間違えたことも！<br />おのれの惨めさも！》"
	},
	{
		"id": 210,
		"start": 1732.272,
		"end": 1735.275,
		"text": "（ベラミー）<br />《まぬけには　まぬけなりの→"
	},
	{
		"id": 211,
		"start": 1735.275,
		"end": 1739.275,
		"text": "貫くべき筋ってもんが<br />あんだよ！》"
	},
	{
		"id": 212,
		"start": 1744.284,
		"end": 1749.289,
		"text": "うおおお…！"
	},
	{
		"id": 213,
		"start": 1749.289,
		"end": 1753.293,
		"text": "うおおお～！！"
	},
	{
		"id": 214,
		"start": 1753.293,
		"end": 1758.293,
		"text": "ドフラミンゴ～！！"
	},
	{
		"id": 215,
		"start": 1761.301,
		"end": 1764.304,
		"text": "（ドフラミンゴ）フフフフ…。→"
	},
	{
		"id": 216,
		"start": 1764.304,
		"end": 1769.309,
		"text": "猛獣が吠えてやがる。→"
	},
	{
		"id": 217,
		"start": 1769.309,
		"end": 1774.314,
		"text": "さっさと上がってくるがいい。→"
	},
	{
		"id": 218,
		"start": 1774.314,
		"end": 1778.318,
		"text": "驚くだろうな　ロー。→"
	},
	{
		"id": 219,
		"start": 1778.318,
		"end": 1784.318,
		"text": "あいつが<br />お前の死体を見ちまったら。"
	},
	{
		"id": 220,
		"start": 1796.269,
		"end": 1799.272,
		"text": "＜ローの壮絶な最期に<br />ルフィが怒りの咆哮を上げ→"
	},
	{
		"id": 221,
		"start": 1799.272,
		"end": 1802.275,
		"text": "ドレスローザの最終決戦が<br />始まる！＞"
	},
	{
		"id": 222,
		"start": 1802.275,
		"end": 1804.275,
		"text": "次回　『ワンピース』"
	},
	{
		"id": 223,
		"start": 1806.279,
		"end": 1809.279,
		"text": "海賊王に　俺は　なる！"
	},
	{
		"id": 224,
		"start": 1812.285,
		"end": 1814.287,
		"text": "＜完全新作！＞"
	},
	{
		"id": 225,
		"start": 1814.287,
		"end": 1816.289,
		"text": "＜かつてない<br />麦わらの一味の大冒険が→"
	},
	{
		"id": 226,
		"start": 1816.289,
		"end": 1818.289,
		"text": "今　ここに始まる！＞"
	},
	{
		"id": 227,
		"start": 1833.957,
		"end": 1835.342,
		"text": "（山﨑・東野）おはようございます。"
	},
	{
		"id": 228,
		"start": 1835.342,
		"end": 1837.31,
		"text": "（東野）さあ　『ワイドナショー』<br />でございますけども→"
	},
	{
		"id": 229,
		"start": 1837.31,
		"end": 1839.312,
		"text": "さあ　松本さん。<br />けさの『ワイドナショー』は→"
	},
	{
		"id": 230,
		"start": 1839.312,
		"end": 1841.431,
		"text": "初登場の方がいらっしゃいまして。<br />古市さんと→"
	},
	{
		"id": 231,
		"start": 1841.431,
		"end": 1845.335,
		"text": "丸坊主のヒロミさんの間にですね。（松本）丸坊主じゃないな。"
	},
	{
		"id": 232,
		"start": 1845.335,
		"end": 1848.421,
		"text": "何か　丸坊主になったことに<br />なってるよね　ニュースで。"
	},
	{
		"id": 233,
		"start": 1848.421,
		"end": 1850.623,
		"text": "（ヒロミ）何か　ニュースで<br />そういうふうになってたんでしょ。"
	},
	{
		"id": 234,
		"start": 1850.623,
		"end": 1852.258,
		"text": "申し訳ない　ホントに。<br />こんなもんでした　すいません。"
	},
	{
		"id": 235,
		"start": 1852.258,
		"end": 1856.329,
		"text": "ヒロミさんが<br />おしゃれ丸刈りヘアになって→"
	},
	{
		"id": 236,
		"start": 1856.329,
		"end": 1858.331,
		"text": "何かあったの？<br />って驚きの声が上がってるという。"
	}
]