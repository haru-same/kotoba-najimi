[
	{
		"id": 1,
		"start": 94.719,
		"end": 97.847,
		"text": "（学校のチャイム）"
	},
	{
		"id": 2,
		"start": 103.102,
		"end": 105.855,
		"text": "（阿良々木(あららぎ)の荒い息）"
	},
	{
		"id": 3,
		"start": 112.529,
		"end": 113.822,
		"text": "（戦場ヶ原(せんじょうがはら)）ハッ"
	},
	{
		"id": 4,
		"start": 117.659,
		"end": 118.493,
		"text": "（阿良々木）あっ"
	},
	{
		"id": 5,
		"start": 121.121,
		"end": 121.955,
		"text": "ん？"
	},
	{
		"id": 6,
		"start": 136.761,
		"end": 139.764,
		"text": "（阿良々木）<br />よけるより正しい判断だったろう"
	},
	{
		"id": 7,
		"start": 141.975,
		"end": 144.477,
		"text": "いや 間違っていたのかもしれない"
	},
	{
		"id": 8,
		"start": 145.228,
		"end": 147.897,
		"text": "なぜなら<br />彼女の体が とても"
	},
	{
		"id": 9,
		"start": 148.189,
		"end": 150.441,
		"text": "とてつもなく<br />軽かったからだ"
	},
	{
		"id": 10,
		"start": 151.234,
		"end": 154.445,
		"text": "そう 彼女 戦場ヶ原には"
	},
	{
		"id": 11,
		"start": 154.737,
		"end": 157.615,
		"text": "およそ体重と呼べるものが<br />全くと言っていいほど"
	},
	{
		"id": 12,
		"start": 157.824,
		"end": 159.075,
		"text": "なかったのである"
	},
	{
		"id": 13,
		"start": 163.079,
		"end": 169.085,
		"text": "♪〜"
	},
	{
		"id": 14,
		"start": 246.246,
		"end": 252.252,
		"text": "〜♪"
	},
	{
		"id": 15,
		"start": 261.01,
		"end": 264.639,
		"text": "（羽川(はねかわ)）<br />文化祭といっても<br />私たち もう３年生だからね"
	},
	{
		"id": 16,
		"start": 265.223,
		"end": 269.352,
		"text": "さしてすることも ないんだけれど<br />受験勉強のほうが大事だし"
	},
	{
		"id": 17,
		"start": 270.52,
		"end": 273.231,
		"text": "あらかじめ私たちで候補を絞って"
	},
	{
		"id": 18,
		"start": 273.314,
		"end": 277.36,
		"text": "その中から みんなの投票で<br />決定するっていうので いいかな"
	},
	{
		"id": 19,
		"start": 277.777,
		"end": 281.197,
		"text": "（阿良々木）<br />いいんじゃないのか<br />一見 民主主義っぽくて"
	},
	{
		"id": 20,
		"start": 281.572,
		"end": 284.951,
		"text": "（羽川）<br />相変わらず嫌な言い方するよね<br />阿良々木くんは"
	},
	{
		"id": 21,
		"start": 285.493,
		"end": 288.121,
		"text": "ひねてるっていうか フフッ"
	},
	{
		"id": 22,
		"start": 293.876,
		"end": 296.754,
		"text": "（阿良々木）<br />羽川翼(つばさ) クラスの委員長"
	},
	{
		"id": 23,
		"start": 297.171,
		"end": 299.173,
		"text": "規律正しく 折り目正しく"
	},
	{
		"id": 24,
		"start": 299.257,
		"end": 301.551,
		"text": "恐ろしく真面目で<br />教師受けもいいという"
	},
	{
		"id": 25,
		"start": 301.968,
		"end": 303.678,
		"text": "委員長の中の委員長である"
	},
	{
		"id": 26,
		"start": 305.346,
		"end": 307.598,
		"text": "（羽川）<br />参考までに 阿良々木くんは"
	},
	{
		"id": 27,
		"start": 307.682,
		"end": 311.06,
		"text": "去年 おととし<br />文化祭の出し物 何だった？"
	},
	{
		"id": 28,
		"start": 311.561,
		"end": 313.187,
		"text": "（阿良々木）<br />お化け屋敷と喫茶店"
	},
	{
		"id": 29,
		"start": 313.646,
		"end": 314.605,
		"text": "ふむ ふむ"
	},
	{
		"id": 30,
		"start": 315.148,
		"end": 319.277,
		"text": "（阿良々木）<br />そして 副委員長である僕は<br />現在 文化祭の計画を"
	},
	{
		"id": 31,
		"start": 319.36,
		"end": 321.738,
		"text": "羽川と２人で練っている<br />というわけだった"
	},
	{
		"id": 32,
		"start": 323.406,
		"end": 326.701,
		"text": "（阿良々木）<br />なあ うちのクラスに<br />戦場ヶ原っているだろ"
	},
	{
		"id": 33,
		"start": 327.16,
		"end": 331.539,
		"text": "戦場ヶ原さん？<br />戦場ヶ原さんが どうかしたの？"
	},
	{
		"id": 34,
		"start": 331.789,
		"end": 333.624,
		"text": "（阿良々木）まあ 何か気になって"
	},
	{
		"id": 35,
		"start": 333.958,
		"end": 336.836,
		"text": "あいつ 病弱だからって<br />よく学校を休むだろ"
	},
	{
		"id": 36,
		"start": 336.961,
		"end": 339.38,
		"text": "文化祭には参加するのかなって"
	},
	{
		"id": 37,
		"start": 339.63,
		"end": 340.631,
		"text": "（羽川）ふうん"
	},
	{
		"id": 38,
		"start": 340.715,
		"end": 344.343,
		"text": "（阿良々木）<br />いや それに ほら<br />何か“戦場ヶ原ひたぎ”なんて"
	},
	{
		"id": 39,
		"start": 344.427,
		"end": 346.012,
		"text": "変わった名前で面白いじゃん"
	},
	{
		"id": 40,
		"start": 346.095,
		"end": 348.931,
		"text": "“戦場ヶ原”って地名姓だよ"
	},
	{
		"id": 41,
		"start": 349.015,
		"end": 350.892,
		"text": "（阿良々木）<br />ああ えっと そうじゃなくて"
	},
	{
		"id": 42,
		"start": 351.1,
		"end": 353.853,
		"text": "僕が言ってるのは ほら<br />下の名前のほうだから"
	},
	{
		"id": 43,
		"start": 354.062,
		"end": 357.732,
		"text": "（羽川）<br />戦場ヶ原さんの下の名前って<br />“ひたぎ”でしょう？"
	},
	{
		"id": 44,
		"start": 357.815,
		"end": 359.817,
		"text": "そんな変わってるかな"
	},
	{
		"id": 45,
		"start": 359.901,
		"end": 363.738,
		"text": "ひたぎって 確か<br />土木関係の用語じゃなかったっけ"
	},
	{
		"id": 46,
		"start": 363.905,
		"end": 365.782,
		"text": "（阿良々木）<br />お前は何でも知ってるな"
	},
	{
		"id": 47,
		"start": 365.865,
		"end": 369.118,
		"text": "何でもは知らないわよ<br />知ってることだけ"
	},
	{
		"id": 48,
		"start": 370.62,
		"end": 371.454,
		"text": "（阿良々木）ん…"
	},
	{
		"id": 49,
		"start": 377.668,
		"end": 380.421,
		"text": "（阿良々木）う〜ん…"
	},
	{
		"id": 50,
		"start": 381.881,
		"end": 383.841,
		"text": "（羽川）珍しいね<br />（阿良々木）え？"
	},
	{
		"id": 51,
		"start": 384.258,
		"end": 387.303,
		"text": "阿良々木くんが<br />他人に興味を持つなんて"
	},
	{
		"id": 52,
		"start": 387.762,
		"end": 389.764,
		"text": "（阿良々木）<br />いや そんなんじゃないけど"
	},
	{
		"id": 53,
		"start": 389.847,
		"end": 392.975,
		"text": "（羽川）<br />病弱な女の子 男子は好きだもんね"
	},
	{
		"id": 54,
		"start": 393.059,
		"end": 395.812,
		"text": "あ〜 やだやだ<br />汚らわしい 汚らわしい"
	},
	{
		"id": 55,
		"start": 397.146,
		"end": 398.981,
		"text": "病弱 ねえ…"
	},
	{
		"id": 56,
		"start": 400.441,
		"end": 403.861,
		"text": "（阿良々木）<br />いや しかし<br />あれは病気なのだろうか"
	},
	{
		"id": 57,
		"start": 405.154,
		"end": 407.907,
		"text": "（阿良々木）<br />とにかくさ 戦場ヶ原って<br />どんなヤツなんだ？"
	},
	{
		"id": 58,
		"start": 408.366,
		"end": 410.034,
		"text": "（羽川）戦場ヶ原さんのことなら"
	},
	{
		"id": 59,
		"start": 410.118,
		"end": 412.995,
		"text": "阿良々木くんのほうが<br />よく知ってるんじゃないのかな"
	},
	{
		"id": 60,
		"start": 413.079,
		"end": 415.748,
		"text": "３年連続で<br />同じクラスだったんでしょ？"
	},
	{
		"id": 61,
		"start": 416.332,
		"end": 419.252,
		"text": "（阿良々木）<br />そう言われりゃ<br />確かにそうなんだが"
	},
	{
		"id": 62,
		"start": 421.838,
		"end": 423.047,
		"text": "（羽川）フーン"
	},
	{
		"id": 63,
		"start": 425.174,
		"end": 430.138,
		"text": "まあ 何の問題もない優等生だよ<br />成績だって良いしね"
	},
	{
		"id": 64,
		"start": 430.43,
		"end": 431.389,
		"text": "（阿良々木）だろうな"
	},
	{
		"id": 65,
		"start": 431.722,
		"end": 435.601,
		"text": "（羽川）<br />でも 同じクラスになって まだ<br />ちょうど １か月くらいだしね"
	},
	{
		"id": 66,
		"start": 435.726,
		"end": 437.812,
		"text": "分かんないってのが やっぱりかな"
	},
	{
		"id": 67,
		"start": 437.937,
		"end": 440.022,
		"text": "ゴールデンウイーク<br />挟んじゃってるし"
	},
	{
		"id": 68,
		"start": 440.356,
		"end": 442.15,
		"text": "（阿良々木）ゴールデンウイークね"
	},
	{
		"id": 69,
		"start": 442.233,
		"end": 445.194,
		"text": "（羽川）<br />ん？ ゴールデンウイークが<br />どうかした？"
	},
	{
		"id": 70,
		"start": 445.278,
		"end": 446.988,
		"text": "（阿良々木）どうもしない 続けて"
	},
	{
		"id": 71,
		"start": 447.28,
		"end": 449.282,
		"text": "ああ そうだね…"
	},
	{
		"id": 72,
		"start": 449.782,
		"end": 453.077,
		"text": "戦場ヶ原さん<br />言葉数も多いほうじゃないし"
	},
	{
		"id": 73,
		"start": 453.161,
		"end": 454.787,
		"text": "友達も全然いないみたい"
	},
	{
		"id": 74,
		"start": 455.413,
		"end": 457.665,
		"text": "やっぱり病気のせいなのかな"
	},
	{
		"id": 75,
		"start": 458.166,
		"end": 461.127,
		"text": "中学生のときは<br />もっと元気いっぱいで"
	},
	{
		"id": 76,
		"start": 461.21,
		"end": 462.837,
		"text": "明るい子だったんだけどね"
	},
	{
		"id": 77,
		"start": 463.504,
		"end": 468.05,
		"text": "（阿良々木）<br />中学生のときって 羽川<br />戦場ヶ原と同じ中学だったのか？"
	},
	{
		"id": 78,
		"start": 468.134,
		"end": 468.968,
		"text": "うん"
	},
	{
		"id": 79,
		"start": 470.595,
		"end": 473.639,
		"text": "戦場ヶ原さん<br />すごくきれいだったし"
	},
	{
		"id": 80,
		"start": 473.764,
		"end": 477.351,
		"text": "運動もできたから<br />陸上部のスターだったんだよ"
	},
	{
		"id": 81,
		"start": 478.853,
		"end": 482.482,
		"text": "だから 話だけなら<br />いろいろ聞いたもんだったよ"
	},
	{
		"id": 82,
		"start": 482.565,
		"end": 483.399,
		"text": "（阿良々木）話って？"
	},
	{
		"id": 83,
		"start": 484.275,
		"end": 487.612,
		"text": "（羽川）<br />すごく人当たりのいい<br />いい人だって話"
	},
	{
		"id": 84,
		"start": 488.237,
		"end": 491.908,
		"text": "何かお父さんが外資系の企業の<br />お偉いさんらしくて"
	},
	{
		"id": 85,
		"start": 491.991,
		"end": 493.784,
		"text": "お家(うち)も すごい豪邸で"
	},
	{
		"id": 86,
		"start": 493.868,
		"end": 496.996,
		"text": "それでも 全然気取ったところが<br />ないんだって話"
	},
	{
		"id": 87,
		"start": 497.079,
		"end": 498.998,
		"text": "（阿良々木）超人みたいな話だな"
	},
	{
		"id": 88,
		"start": 499.165,
		"end": 503.544,
		"text": "（羽川）<br />でも こんなことを<br />言っちゃいけないんだろうけど"
	},
	{
		"id": 89,
		"start": 504.837,
		"end": 506.047,
		"text": "戦場ヶ原さん…"
	},
	{
		"id": 90,
		"start": 506.672,
		"end": 507.507,
		"text": "（阿良々木）なに？"
	},
	{
		"id": 91,
		"start": 508.049,
		"end": 509.008,
		"text": "今のほうが"
	},
	{
		"id": 92,
		"start": 510.635,
		"end": 513.513,
		"text": "昔より ずっときれいなんだよね"
	},
	{
		"id": 93,
		"start": 516.724,
		"end": 520.186,
		"text": "存在が とても はかなげで"
	},
	{
		"id": 94,
		"start": 522.48,
		"end": 524.524,
		"text": "（阿良々木）<br />存在が はかなげ…"
	},
	{
		"id": 95,
		"start": 529.946,
		"end": 534.242,
		"text": "（阿良々木）<br />あ〜 そうだ 思い出した<br />僕 忍野(おしの)に呼ばれてるんだった"
	},
	{
		"id": 96,
		"start": 534.325,
		"end": 536.744,
		"text": "（羽川）忍野さんに？ なんで？"
	},
	{
		"id": 97,
		"start": 536.827,
		"end": 538.746,
		"text": "（阿良々木）<br />ちょっと まあ 仕事の手伝いを"
	},
	{
		"id": 98,
		"start": 539.247,
		"end": 540.122,
		"text": "（羽川）ふうん"
	},
	{
		"id": 99,
		"start": 540.206,
		"end": 542.375,
		"text": "（阿良々木）<br />羽川 あと 任せていいか？"
	},
	{
		"id": 100,
		"start": 542.625,
		"end": 546.17,
		"text": "埋め合わせすると約束できるなら<br />今日は いいわ"
	},
	{
		"id": 101,
		"start": 546.42,
		"end": 548.839,
		"text": "忍野さんを待たせても悪いしね"
	},
	{
		"id": 102,
		"start": 549.006,
		"end": 549.882,
		"text": "（阿良々木）ああ 悪いな"
	},
	{
		"id": 103,
		"start": 551.926,
		"end": 553.469,
		"text": "（羽川）忍野さんによろしくね"
	},
	{
		"id": 104,
		"start": 558.266,
		"end": 561.102,
		"text": "（戦場ヶ原）<br />羽川さんと何を話していたの？"
	},
	{
		"id": 105,
		"start": 561.978,
		"end": 562.812,
		"text": "（阿良々木）ん？"
	},
	{
		"id": 106,
		"start": 562.895,
		"end": 563.98,
		"text": "あっ ああ…"
	},
	{
		"id": 107,
		"start": 565.439,
		"end": 566.274,
		"text": "動かないで"
	},
	{
		"id": 108,
		"start": 566.566,
		"end": 567.4,
		"text": "（阿良々木）あ…"
	},
	{
		"id": 109,
		"start": 569.652,
		"end": 571.153,
		"text": "ああ 違うわ"
	},
	{
		"id": 110,
		"start": 571.487,
		"end": 575.7,
		"text": "動いてもいいけれど とても危険よ<br />と言うのが正しかったわね"
	},
	{
		"id": 111,
		"start": 576.242,
		"end": 579.579,
		"text": "好奇心というのは<br />全くゴキブリみたいね"
	},
	{
		"id": 112,
		"start": 579.745,
		"end": 583.165,
		"text": "人の触れられたくない秘密ばかりに<br />こぞって寄ってくる"
	},
	{
		"id": 113,
		"start": 583.457,
		"end": 584.834,
		"text": "うっとうしくて たまらないわ"
	},
	{
		"id": 114,
		"start": 584.959,
		"end": 585.793,
		"text": "あっ あ…"
	},
	{
		"id": 115,
		"start": 587.336,
		"end": 588.17,
		"text": "おい…"
	},
	{
		"id": 116,
		"start": 588.671,
		"end": 591.173,
		"text": "何よ 右側が寂しいの？"
	},
	{
		"id": 117,
		"start": 591.549,
		"end": 593.759,
		"text": "だったら<br />そう言ってくれればいいのに"
	},
	{
		"id": 118,
		"start": 595.344,
		"end": 596.178,
		"text": "あっ ぐ…"
	},
	{
		"id": 119,
		"start": 596.846,
		"end": 598.973,
		"text": "まったく 私も うかつだったわ"
	},
	{
		"id": 120,
		"start": 599.223,
		"end": 600.349,
		"text": "まさか あんな所に"
	},
	{
		"id": 121,
		"start": 601.809,
		"end": 605.73,
		"text": "バナナの皮が落ちているなんて<br />思いもしなかったわ"
	},
	{
		"id": 122,
		"start": 606.897,
		"end": 608.316,
		"text": "気付いているんでしょう？"
	},
	{
		"id": 123,
		"start": 608.941,
		"end": 611.569,
		"text": "そう 私には重さが"
	},
	{
		"id": 124,
		"start": 612.737,
		"end": 613.571,
		"text": "ない"
	},
	{
		"id": 125,
		"start": 613.946,
		"end": 614.78,
		"text": "（阿良々木）ハッ"
	},
	{
		"id": 126,
		"start": 615.74,
		"end": 618.909,
		"text": "（戦場ヶ原）<br />といっても全くない<br />というわけでは ないのよ"
	},
	{
		"id": 127,
		"start": 619.452,
		"end": 621.37,
		"text": "私の身長 体格だと"
	},
	{
		"id": 128,
		"start": 621.454,
		"end": 625.374,
		"text": "平均体重は40キロ後半強<br />というところ らしいのだけれど"
	},
	{
		"id": 129,
		"start": 626.334,
		"end": 627.71,
		"text": "実際の体重は"
	},
	{
		"id": 130,
		"start": 630.004,
		"end": 630.838,
		"text": "５キロ"
	},
	{
		"id": 131,
		"start": 631.047,
		"end": 631.881,
		"text": "（阿良々木）ハッ"
	},
	{
		"id": 132,
		"start": 632.465,
		"end": 635.885,
		"text": "中学校を卒業して<br />この高校に入る前のことよ"
	},
	{
		"id": 133,
		"start": 637.511,
		"end": 642.35,
		"text": "１匹の蟹(かに)に出会って<br />重さを根こそぎ持っていかれたわ"
	},
	{
		"id": 134,
		"start": 642.642,
		"end": 643.476,
		"text": "（阿良々木）あ…"
	},
	{
		"id": 135,
		"start": 643.893,
		"end": 646.646,
		"text": "（戦場ヶ原）<br />ああ 別に理解しなくていいのよ"
	},
	{
		"id": 136,
		"start": 647.229,
		"end": 649.482,
		"text": "これ以上 嗅ぎまわられたら<br />すごく迷惑だから—"
	},
	{
		"id": 137,
		"start": 649.565,
		"end": 651.942,
		"text": "しゃべっただけだから<br />阿良々木くん"
	},
	{
		"id": 138,
		"start": 652.86,
		"end": 653.944,
		"text": "阿良々木くん"
	},
	{
		"id": 139,
		"start": 655.112,
		"end": 658.24,
		"text": "ねえ 阿良々木暦(こよみ)くん"
	},
	{
		"id": 140,
		"start": 659.075,
		"end": 661.202,
		"text": "さて 私は あなたに"
	},
	{
		"id": 141,
		"start": 661.327,
		"end": 664.08,
		"text": "私の秘密を<br />黙っていてもらうために"
	},
	{
		"id": 142,
		"start": 664.163,
		"end": 665.79,
		"text": "何をすればいいのかしら"
	},
	{
		"id": 143,
		"start": 666.207,
		"end": 669.585,
		"text": "私は私のために何をすべきかしら"
	},
	{
		"id": 144,
		"start": 670.378,
		"end": 672.254,
		"text": "口が裂けても しゃべらないと"
	},
	{
		"id": 145,
		"start": 672.338,
		"end": 674.59,
		"text": "阿良々木くんに<br />誓ってもらうためには"
	},
	{
		"id": 146,
		"start": 674.882,
		"end": 676.967,
		"text": "どうやって<br />口を封じればいいかしら"
	},
	{
		"id": 147,
		"start": 677.259,
		"end": 678.094,
		"text": "（阿良々木）う…"
	},
	{
		"id": 148,
		"start": 678.26,
		"end": 681.68,
		"text": "（戦場ヶ原）<br />とにかく 私が欲しいのは<br />沈黙と無関心だけ"
	},
	{
		"id": 149,
		"start": 682.598,
		"end": 685.226,
		"text": "沈黙と無関心を<br />約束してくれるのなら"
	},
	{
		"id": 150,
		"start": 685.601,
		"end": 688.312,
		"text": "２回 うなずいてちょうだい<br />阿良々木くん"
	},
	{
		"id": 151,
		"start": 688.938,
		"end": 692.316,
		"text": "それ以外の動作は停止であれ<br />敵対行為とみなして"
	},
	{
		"id": 152,
		"start": 693.234,
		"end": 694.902,
		"text": "即座に攻撃に移るわ"
	},
	{
		"id": 153,
		"start": 695.277,
		"end": 696.237,
		"text": "（阿良々木）うっ うっ"
	},
	{
		"id": 154,
		"start": 697.113,
		"end": 699.115,
		"text": "そう ありがとう"
	},
	{
		"id": 155,
		"start": 704.995,
		"end": 705.955,
		"text": "（ガチャン）<br />（阿良々木のうめき声）"
	},
	{
		"id": 156,
		"start": 706.33,
		"end": 707.915,
		"text": "（阿良々木）ぐ… ぐぐ…"
	},
	{
		"id": 157,
		"start": 710,
		"end": 712.711,
		"text": "悲鳴を上げないのね 立派だわ"
	},
	{
		"id": 158,
		"start": 713.712,
		"end": 715.881,
		"text": "今回は これで勘弁してあげる"
	},
	{
		"id": 159,
		"start": 716.465,
		"end": 717.466,
		"text": "（阿良々木）お前…"
	},
	{
		"id": 160,
		"start": 717.591,
		"end": 718.509,
		"text": "くっ"
	},
	{
		"id": 161,
		"start": 719.718,
		"end": 721.137,
		"text": "（戦場ヶ原）<br />それじゃ 阿良々木くん"
	},
	{
		"id": 162,
		"start": 721.303,
		"end": 723.681,
		"text": "明日からは ちゃんと<br />私のこと 無視してね"
	},
	{
		"id": 163,
		"start": 724.098,
		"end": 725.307,
		"text": "よろしくさん"
	},
	{
		"id": 164,
		"start": 732.565,
		"end": 734.4,
		"text": "（阿良々木）悪魔みたいな女だ"
	},
	{
		"id": 165,
		"start": 735.151,
		"end": 735.985,
		"text": "痛(い)てっ"
	},
	{
		"id": 166,
		"start": 740.281,
		"end": 744.201,
		"text": "（阿良々木）<br />でも 大丈夫<br />この程度なら僕は大丈夫"
	},
	{
		"id": 167,
		"start": 744.452,
		"end": 747.204,
		"text": "（ドアが開く音）<br />（羽川）あれ 阿良々木くん？"
	},
	{
		"id": 168,
		"start": 747.746,
		"end": 748.581,
		"text": "（阿良々木）羽川"
	},
	{
		"id": 169,
		"start": 748.664,
		"end": 749.915,
		"text": "（羽川）まだ いたの？"
	},
	{
		"id": 170,
		"start": 750.04,
		"end": 753.002,
		"text": "忍野さんの所<br />早く行かなくていいの？"
	},
	{
		"id": 171,
		"start": 753.085,
		"end": 755.296,
		"text": "（阿良々木）<br />羽川 お前 バナナ好きか？"
	},
	{
		"id": 172,
		"start": 755.379,
		"end": 758.716,
		"text": "（羽川）<br />え？ まあ 別に嫌いじゃないけれど"
	},
	{
		"id": 173,
		"start": 758.799,
		"end": 761.51,
		"text": "栄養価 高いし<br />好きか嫌いかで言えば"
	},
	{
		"id": 174,
		"start": 761.594,
		"end": 763.179,
		"text": "うん 好きかな"
	},
	{
		"id": 175,
		"start": 763.262,
		"end": 765.973,
		"text": "（阿良々木）<br />どんなに好きでも<br />校内では絶対に食べるなよ"
	},
	{
		"id": 176,
		"start": 766.39,
		"end": 767.475,
		"text": "はっ はあ？"
	},
	{
		"id": 177,
		"start": 768.017,
		"end": 771.77,
		"text": "（阿良々木）<br />食べるだけなら まだいい<br />残った皮を階段にポイ捨てしてみろ"
	},
	{
		"id": 178,
		"start": 772.146,
		"end": 773.856,
		"text": "僕は お前を絶対許さない！"
	},
	{
		"id": 179,
		"start": 773.939,
		"end": 776.275,
		"text": "（羽川）<br />一体 何を言っているの<br />阿良々木くん"
	},
	{
		"id": 180,
		"start": 776.358,
		"end": 777.193,
		"text": "（阿良々木）くっ"
	},
	{
		"id": 181,
		"start": 777.276,
		"end": 778.527,
		"text": "（羽川）あ〜 こら 阿良々木くん！"
	},
	{
		"id": 182,
		"start": 778.611,
		"end": 780.154,
		"text": "廊下を走っちゃダメ！"
	},
	{
		"id": 183,
		"start": 781.405,
		"end": 783.24,
		"text": "ん？ ああ…"
	},
	{
		"id": 184,
		"start": 783.365,
		"end": 785.659,
		"text": "先生に言いつけちゃうよ"
	},
	{
		"id": 185,
		"start": 785.868,
		"end": 788.829,
		"text": "（阿良々木の荒い息）"
	},
	{
		"id": 186,
		"start": 797.922,
		"end": 802.635,
		"text": "あきれたわ　いえ<br />ここは素直に驚いたと言うべきね"
	},
	{
		"id": 187,
		"start": 803.135,
		"end": 804.929,
		"text": "あれだけのことを されておいて"
	},
	{
		"id": 188,
		"start": 805.012,
		"end": 808.265,
		"text": "すぐに反抗精神を<br />立ち上げることが できたなんて"
	},
	{
		"id": 189,
		"start": 808.807,
		"end": 809.767,
		"text": "（阿良々木）戦場ヶ原"
	},
	{
		"id": 190,
		"start": 809.85,
		"end": 811.018,
		"text": "いいわ 分かった"
	},
	{
		"id": 191,
		"start": 811.31,
		"end": 812.686,
		"text": "分かりました 阿良々木くん"
	},
	{
		"id": 192,
		"start": 816.023,
		"end": 817.733,
		"text": "戦争をしましょう"
	},
	{
		"id": 193,
		"start": 818.192,
		"end": 820.903,
		"text": "（阿良々木）<br />ちっ 違う 違う　戦争は しない"
	},
	{
		"id": 194,
		"start": 821.32,
		"end": 823.489,
		"text": "しないの？ な〜んだ"
	},
	{
		"id": 195,
		"start": 825.074,
		"end": 826.7,
		"text": "じゃあ 何の用よ"
	},
	{
		"id": 196,
		"start": 826.992,
		"end": 828.536,
		"text": "（阿良々木）<br />ひょっとしたら なんだけど"
	},
	{
		"id": 197,
		"start": 828.869,
		"end": 831.247,
		"text": "お前の力に<br />なれるかもしれないと思って"
	},
	{
		"id": 198,
		"start": 831.789,
		"end": 832.623,
		"text": "（戦場ヶ原）力に？"
	},
	{
		"id": 199,
		"start": 834.291,
		"end": 837.67,
		"text": "ふざけないで<br />あなたに何ができるっていうのよ"
	},
	{
		"id": 200,
		"start": 838.17,
		"end": 840.965,
		"text": "黙って気を払わないで<br />いてくれたら それでいいの"
	},
	{
		"id": 201,
		"start": 841.465,
		"end": 843.926,
		"text": "優しさも敵対行為と見なすわよ"
	},
	{
		"id": 202,
		"start": 846.554,
		"end": 847.388,
		"text": "あっ"
	},
	{
		"id": 203,
		"start": 856.522,
		"end": 857.481,
		"text": "傷が…"
	},
	{
		"id": 204,
		"start": 859.483,
		"end": 860.317,
		"text": "ない"
	},
	{
		"id": 205,
		"start": 861.36,
		"end": 864.363,
		"text": "あなた それって どういう…"
	},
	{
		"id": 206,
		"start": 865.781,
		"end": 868.033,
		"text": "忍野… 忍野さん？"
	},
	{
		"id": 207,
		"start": 868.701,
		"end": 870.119,
		"text": "（阿良々木）そう 忍野メメ"
	},
	{
		"id": 208,
		"start": 871.161,
		"end": 874.123,
		"text": "忍野のおかげで<br />僕は吸血鬼から人間に戻れたんだ"
	},
	{
		"id": 209,
		"start": 875.249,
		"end": 878.21,
		"text": "不死身だったころの後遺症が<br />少し残っているけど"
	},
	{
		"id": 210,
		"start": 878.752,
		"end": 880.921,
		"text": "（戦場ヶ原）ふうん 忍野メメね"
	},
	{
		"id": 211,
		"start": 881.255,
		"end": 883.966,
		"text": "なんだか さぞかし<br />よく萌えそうな名前じゃないの"
	},
	{
		"id": 212,
		"start": 884.466,
		"end": 886.76,
		"text": "（阿良々木）<br />その手の期待は するだけムダだぞ"
	},
	{
		"id": 213,
		"start": 887.261,
		"end": 889.555,
		"text": "30過ぎの<br />年季の入った中年だからな"
	},
	{
		"id": 214,
		"start": 890.014,
		"end": 890.89,
		"text": "（戦場ヶ原）あっそ"
	},
	{
		"id": 215,
		"start": 891.015,
		"end": 894.31,
		"text": "でも 子供のころは さぞかし<br />萌えキャラだったのでしょうね"
	},
	{
		"id": 216,
		"start": 894.81,
		"end": 897.229,
		"text": "（阿良々木）<br />そういう目で生身の人物を見るな"
	},
	{
		"id": 217,
		"start": 897.73,
		"end": 900.566,
		"text": "ていうか お前 萌えとかキャラとか<br />分かるヤツなのか？"
	},
	{
		"id": 218,
		"start": 901.233,
		"end": 903.444,
		"text": "これしき 一般教養の部類よ"
	},
	{
		"id": 219,
		"start": 903.777,
		"end": 906.947,
		"text": "私みたいなキャラのことを<br />ツンデレっていうんでしょう？"
	},
	{
		"id": 220,
		"start": 908.198,
		"end": 910.951,
		"text": "（阿良々木）<br />お前みたいなキャラは<br />ツンドラって感じだ"
	},
	{
		"id": 221,
		"start": 921.879,
		"end": 926.425,
		"text": "（戦場ヶ原）<br />お尻が痛いわ ジンジンする<br />スカートにしわが寄っちゃったし"
	},
	{
		"id": 222,
		"start": 926.55,
		"end": 927.718,
		"text": "（阿良々木）僕の責任じゃない"
	},
	{
		"id": 223,
		"start": 928.385,
		"end": 930.721,
		"text": "言い逃れは やめなさい<br />切り落とすわよ"
	},
	{
		"id": 224,
		"start": 931.472,
		"end": 932.89,
		"text": "どの部位をですか？"
	},
	{
		"id": 225,
		"start": 938.854,
		"end": 943.025,
		"text": "そういえば お前 自分のかばんは<br />どうしたんだ？ 手ぶらだけど"
	},
	{
		"id": 226,
		"start": 943.192,
		"end": 945.819,
		"text": "（戦場ヶ原）<br />教科書は学校のロッカーに<br />置きっぱなし"
	},
	{
		"id": 227,
		"start": 945.945,
		"end": 948.822,
		"text": "それに 体中に<br />文房具を仕込んでおけば"
	},
	{
		"id": 228,
		"start": 948.906,
		"end": 950.115,
		"text": "かばんは不要よ"
	},
	{
		"id": 229,
		"start": 950.199,
		"end": 951.825,
		"text": "（阿良々木）ああ なるほど"
	},
	{
		"id": 230,
		"start": 951.951,
		"end": 953.911,
		"text": "（戦場ヶ原）<br />両手が自由になっていないと"
	},
	{
		"id": 231,
		"start": 953.994,
		"end": 956.288,
		"text": "いざっていうときに<br />どうしたって戦いにくいもの"
	},
	{
		"id": 232,
		"start": 957.957,
		"end": 960.292,
		"text": "（阿良々木）<br />その文房具とやら 僕が預かる"
	},
	{
		"id": 233,
		"start": 960.376,
		"end": 961.21,
		"text": "（戦場ヶ原）え？"
	},
	{
		"id": 234,
		"start": 961.335,
		"end": 962.294,
		"text": "（阿良々木）預かるから出せ"
	},
	{
		"id": 235,
		"start": 962.67,
		"end": 963.879,
		"text": "あ… え？"
	},
	{
		"id": 236,
		"start": 964.421,
		"end": 966.84,
		"text": "（阿良々木）<br />忍野は何というか<br />変なおっさんだけど"
	},
	{
		"id": 237,
		"start": 967.132,
		"end": 968.842,
		"text": "一応 僕の恩人なんだ"
	},
	{
		"id": 238,
		"start": 969.343,
		"end": 972.763,
		"text": "その恩人に危険人物を<br />会わせるわけには いかないから"
	},
	{
		"id": 239,
		"start": 972.846,
		"end": 974.098,
		"text": "文房具は僕が預かる"
	},
	{
		"id": 240,
		"start": 975.599,
		"end": 978.018,
		"text": "ここにきて<br />そんなことを言うなんて"
	},
	{
		"id": 241,
		"start": 978.811,
		"end": 980.854,
		"text": "あなた 私をはめたわね"
	},
	{
		"id": 242,
		"start": 986.318,
		"end": 988.779,
		"text": "了承したわ　受け取りなさい"
	},
	{
		"id": 243,
		"start": 991.949,
		"end": 992.783,
		"text": "あ…"
	},
	{
		"id": 244,
		"start": 994.743,
		"end": 995.786,
		"text": "お前な"
	},
	{
		"id": 245,
		"start": 996.203,
		"end": 997.663,
		"text": "勘違いしないでね"
	},
	{
		"id": 246,
		"start": 997.997,
		"end": 1001.166,
		"text": "別に私は あなたに<br />気を許したというわけではないのよ"
	},
	{
		"id": 247,
		"start": 1002.001,
		"end": 1003.961,
		"text": "（阿良々木）<br />気を許したわけではないって…"
	},
	{
		"id": 248,
		"start": 1004.211,
		"end": 1008.215,
		"text": "（戦場ヶ原）<br />いいこと？ もしも 私から<br />１分おきに連絡が なかったら"
	},
	{
		"id": 249,
		"start": 1008.298,
		"end": 1010.175,
		"text": "５千人の むくつけき仲間が"
	},
	{
		"id": 250,
		"start": 1010.259,
		"end": 1012.886,
		"text": "あなたの家族を<br />襲撃することになっているわ"
	},
	{
		"id": 251,
		"start": 1013.012,
		"end": 1015.806,
		"text": "（阿良々木）<br />大丈夫だって　余計な心配するな"
	},
	{
		"id": 252,
		"start": 1016.098,
		"end": 1018.183,
		"text": "（戦場ヶ原）<br />１分あれば事が足りるというの？"
	},
	{
		"id": 253,
		"start": 1018.267,
		"end": 1019.727,
		"text": "（阿良々木）<br />僕は どこかのボクサーかよ"
	},
	{
		"id": 254,
		"start": 1021.437,
		"end": 1024.815,
		"text": "（戦場ヶ原）<br />妹さん ２人とも<br />まだ中学生なんですってね"
	},
	{
		"id": 255,
		"start": 1038.537,
		"end": 1039.371,
		"text": "（阿良々木）こっちだよ"
	},
	{
		"id": 256,
		"start": 1039.455,
		"end": 1042.041,
		"text": "（戦場ヶ原）<br />何よ 感謝するなんて思わないでね"
	},
	{
		"id": 257,
		"start": 1042.499,
		"end": 1043.667,
		"text": "（阿良々木）<br />分かってるよ"
	},
	{
		"id": 258,
		"start": 1044.043,
		"end": 1045.753,
		"text": "（戦場ヶ原）<br />むしろ あなたが感謝しなさい"
	},
	{
		"id": 259,
		"start": 1045.836,
		"end": 1047.087,
		"text": "（阿良々木）<br />分からねえぞ"
	},
	{
		"id": 260,
		"start": 1047.254,
		"end": 1050.382,
		"text": "（戦場ヶ原）<br />あのホッチキス<br />傷が目立たないようにと思って"
	},
	{
		"id": 261,
		"start": 1050.466,
		"end": 1054.428,
		"text": "わざと外側じゃなくて 内側に<br />針が刺さるようにしてあげたのよ"
	},
	{
		"id": 262,
		"start": 1054.511,
		"end": 1057.639,
		"text": "（阿良々木）<br />それは顔が目立つから<br />腹を殴れみたいな"
	},
	{
		"id": 263,
		"start": 1057.723,
		"end": 1059.475,
		"text": "加害者側からの都合だろ？"
	},
	{
		"id": 264,
		"start": 1059.558,
		"end": 1063.145,
		"text": "（戦場ヶ原）<br />まあ どのみち 全然<br />ムダな気遣いだったわけだけれど"
	},
	{
		"id": 265,
		"start": 1063.437,
		"end": 1064.271,
		"text": "（阿良々木）<br />だな"
	},
	{
		"id": 266,
		"start": 1064.772,
		"end": 1067.941,
		"text": "（戦場ヶ原）<br />不死身って便利そうね<br />って言われたら傷つく？"
	},
	{
		"id": 267,
		"start": 1068.233,
		"end": 1070.027,
		"text": "（阿良々木）<br />今は そうでもない"
	},
	{
		"id": 268,
		"start": 1070.319,
		"end": 1071.612,
		"text": "（戦場ヶ原）<br />あら そう"
	},
	{
		"id": 269,
		"start": 1071.737,
		"end": 1073.238,
		"text": "（阿良々木）<br />それに もう不死身じゃない"
	},
	{
		"id": 270,
		"start": 1073.614,
		"end": 1075.866,
		"text": "傷の治りが<br />ちっとばかし早いというだけで"
	},
	{
		"id": 271,
		"start": 1075.991,
		"end": 1077.409,
		"text": "他は普通の人間だ"
	},
	{
		"id": 272,
		"start": 1079.036,
		"end": 1081.663,
		"text": "（戦場ヶ原）<br />阿良々木くん<br />最後に言っておくけれど"
	},
	{
		"id": 273,
		"start": 1081.872,
		"end": 1082.706,
		"text": "（阿良々木）何だよ"
	},
	{
		"id": 274,
		"start": 1082.956,
		"end": 1085.918,
		"text": "（戦場ヶ原）<br />服の上からだと そうは<br />見えないかもしれないけれど"
	},
	{
		"id": 275,
		"start": 1086.085,
		"end": 1087.795,
		"text": "私の肉体は 案外"
	},
	{
		"id": 276,
		"start": 1087.92,
		"end": 1090.672,
		"text": "法を犯してまで手に入れる価値は<br />ないかもしれないわよ"
	},
	{
		"id": 277,
		"start": 1092.174,
		"end": 1096.136,
		"text": "（阿良々木）<br />何か お前 全般的に見て<br />自意識過剰っつうか"
	},
	{
		"id": 278,
		"start": 1096.303,
		"end": 1098.263,
		"text": "ちょっと被害妄想<br />強すぎるんじゃないか？"
	},
	{
		"id": 279,
		"start": 1098.806,
		"end": 1100.724,
		"text": "嫌だわ 本当のことでも"
	},
	{
		"id": 280,
		"start": 1100.808,
		"end": 1102.601,
		"text": "言っていいことと<br />悪いことが あるでしょう"
	},
	{
		"id": 281,
		"start": 1102.893,
		"end": 1104.269,
		"text": "自覚している"
	},
	{
		"id": 282,
		"start": 1105.312,
		"end": 1107.064,
		"text": "（戦場ヶ原）<br />それにしても よく こんな"
	},
	{
		"id": 283,
		"start": 1107.481,
		"end": 1111.401,
		"text": "今にも壊れそうなビルに<br />住んでいるわね その忍野って人"
	},
	{
		"id": 284,
		"start": 1111.485,
		"end": 1114.363,
		"text": "（阿良々木）<br />ああ 随分な変わり者でね"
	},
	{
		"id": 285,
		"start": 1114.78,
		"end": 1118.575,
		"text": "でも 僕や戦場ヶ原みたいなのを<br />専門にしているんだって"
	},
	{
		"id": 286,
		"start": 1120.911,
		"end": 1123.539,
		"text": "（忍野）<br />おお 阿良々木くん やっと来たのか"
	},
	{
		"id": 287,
		"start": 1127.543,
		"end": 1128.961,
		"text": "何だい 阿良々木くん"
	},
	{
		"id": 288,
		"start": 1129.294,
		"end": 1131.839,
		"text": "今日は また違う女の子を<br />連れているんだね"
	},
	{
		"id": 289,
		"start": 1132.256,
		"end": 1134.925,
		"text": "やめろ 人を<br />そんな安いキャラ設定にするな"
	},
	{
		"id": 290,
		"start": 1135.384,
		"end": 1139.304,
		"text": "（忍野）<br />うん 初めまして お嬢さん<br />忍野です"
	},
	{
		"id": 291,
		"start": 1139.763,
		"end": 1142.558,
		"text": "初めまして　戦場ヶ原ひたぎです"
	},
	{
		"id": 292,
		"start": 1143.725,
		"end": 1145.644,
		"text": "阿良々木くんとはクラスメートで"
	},
	{
		"id": 293,
		"start": 1145.727,
		"end": 1147.896,
		"text": "忍野さんの話を<br />教えてもらいました"
	},
	{
		"id": 294,
		"start": 1148.355,
		"end": 1150.023,
		"text": "ああ そう"
	},
	{
		"id": 295,
		"start": 1150.566,
		"end": 1152.651,
		"text": "忍野 こいつが２年前くらいに…"
	},
	{
		"id": 296,
		"start": 1152.734,
		"end": 1154.486,
		"text": "こいつ呼ばわりは やめて"
	},
	{
		"id": 297,
		"start": 1154.695,
		"end": 1156.405,
		"text": "じゃあ 何て呼べばいいんだよ"
	},
	{
		"id": 298,
		"start": 1156.738,
		"end": 1158.157,
		"text": "戦場ヶ原様"
	},
	{
		"id": 299,
		"start": 1158.49,
		"end": 1161.118,
		"text": "う… センジョーガハラサマ"
	},
	{
		"id": 300,
		"start": 1161.618,
		"end": 1165.08,
		"text": "カタカナ発音は いただけないわ<br />ちゃんと言いなさい"
	},
	{
		"id": 301,
		"start": 1165.622,
		"end": 1166.957,
		"text": "戦場ヶ原ちゃん"
	},
	{
		"id": 302,
		"start": 1167.207,
		"end": 1168.041,
		"text": "（ブスッ）"
	},
	{
		"id": 303,
		"start": 1168.125,
		"end": 1170.544,
		"text": "痛(い)って！ 失明するだろうが！"
	},
	{
		"id": 304,
		"start": 1170.627,
		"end": 1171.712,
		"text": "（戦場ヶ原）失言するからよ"
	},
	{
		"id": 305,
		"start": 1172.004,
		"end": 1173.547,
		"text": "（阿良々木）何だ その等価交換は"
	},
	{
		"id": 306,
		"start": 1173.672,
		"end": 1175.883,
		"text": "銅40グラム 亜鉛25グラム"
	},
	{
		"id": 307,
		"start": 1175.966,
		"end": 1178.677,
		"text": "ニッケル15グラム<br />照れ隠し５グラムに"
	},
	{
		"id": 308,
		"start": 1178.969,
		"end": 1182.139,
		"text": "悪意97キロで<br />私の暴言は錬成されているわ"
	},
	{
		"id": 309,
		"start": 1182.431,
		"end": 1183.765,
		"text": "（阿良々木）<br />ほとんど悪意じゃねえかよ"
	},
	{
		"id": 310,
		"start": 1184.016,
		"end": 1185.976,
		"text": "（戦場ヶ原）<br />ちなみに<br />照れ隠しというのは ウソよ"
	},
	{
		"id": 311,
		"start": 1186.393,
		"end": 1188.437,
		"text": "（阿良々木）<br />一番 抜いちゃいけない要素が<br />抜けちゃった！"
	},
	{
		"id": 312,
		"start": 1188.896,
		"end": 1193.692,
		"text": "（戦場ヶ原）<br />それより 何よりも まず<br />あの子は 一体 何？"
	},
	{
		"id": 313,
		"start": 1194.234,
		"end": 1196.737,
		"text": "（阿良々木）<br />ああ あれは<br />気にしなくても いいよ"
	},
	{
		"id": 314,
		"start": 1196.945,
		"end": 1199.823,
		"text": "影も形もない<br />名前もなければ存在もない"
	},
	{
		"id": 315,
		"start": 1199.907,
		"end": 1200.741,
		"text": "そういう子供"
	},
	{
		"id": 316,
		"start": 1201.2,
		"end": 1202.492,
		"text": "（忍野）いやいや 阿良々木くん"
	},
	{
		"id": 317,
		"start": 1202.868,
		"end": 1204.745,
		"text": "名前は昨日 付けてやったんだ"
	},
	{
		"id": 318,
		"start": 1204.912,
		"end": 1207.289,
		"text": "ゴールデンウイークには<br />よく働いてくれたしね"
	},
	{
		"id": 319,
		"start": 1207.789,
		"end": 1210.5,
		"text": "（阿良々木）<br />へえ 名前をね　何て名前？"
	},
	{
		"id": 320,
		"start": 1211.084,
		"end": 1213.086,
		"text": "“忍野忍(しのぶ)”と名付けてみた"
	},
	{
		"id": 321,
		"start": 1213.962,
		"end": 1217.382,
		"text": "“刃(やいば)の下に心あり”<br />彼女らしい いい名前だろ？"
	},
	{
		"id": 322,
		"start": 1217.841,
		"end": 1220.093,
		"text": "名字は僕のを<br />そのまま流用させてもらった"
	},
	{
		"id": 323,
		"start": 1220.886,
		"end": 1222.054,
		"text": "（阿良々木）いいんじゃないのか"
	},
	{
		"id": 324,
		"start": 1222.179,
		"end": 1224.681,
		"text": "（戦場ヶ原）<br />だから あの子は 一体 何なのよ"
	},
	{
		"id": 325,
		"start": 1224.765,
		"end": 1226.975,
		"text": "（阿良々木）<br />だから 何でもないってば"
	},
	{
		"id": 326,
		"start": 1227.434,
		"end": 1231.104,
		"text": "（阿良々木）<br />吸血鬼のなれの果て<br />美しき鬼の搾りかす"
	},
	{
		"id": 327,
		"start": 1231.355,
		"end": 1233.815,
		"text": "なんて言っても<br />そんなの しかたがないだろ"
	},
	{
		"id": 328,
		"start": 1234.274,
		"end": 1237.444,
		"text": "そう 何でもないの ならいいわ"
	},
	{
		"id": 329,
		"start": 1238.153,
		"end": 1241.031,
		"text": "私を助けてくださるって<br />聞いたのですけれど"
	},
	{
		"id": 330,
		"start": 1241.531,
		"end": 1243.659,
		"text": "助ける？ そりゃ無理だ"
	},
	{
		"id": 331,
		"start": 1244.076,
		"end": 1246.703,
		"text": "君が勝手に<br />１人で助かるだけだよ お嬢ちゃん"
	},
	{
		"id": 332,
		"start": 1248.163,
		"end": 1251.625,
		"text": "（戦場ヶ原）<br />私に向かって同じようなセリフを<br />吐いた人が５人いるわ"
	},
	{
		"id": 333,
		"start": 1251.833,
		"end": 1253.418,
		"text": "その全員が詐欺師だった"
	},
	{
		"id": 334,
		"start": 1254.211,
		"end": 1257.089,
		"text": "あなたもその部類なのかしら<br />忍野さん"
	},
	{
		"id": 335,
		"start": 1257.589,
		"end": 1261.468,
		"text": "ハハハッ お嬢ちゃん<br />随分と元気いいね"
	},
	{
		"id": 336,
		"start": 1261.551,
		"end": 1263.428,
		"text": "何か いいことでも あったのかい？"
	},
	{
		"id": 337,
		"start": 1263.971,
		"end": 1268.016,
		"text": "まあ 何にせよ話してくれないと<br />話は先に進まないかな"
	},
	{
		"id": 338,
		"start": 1268.225,
		"end": 1270.06,
		"text": "秘密は厳守するから 平気 平気"
	},
	{
		"id": 339,
		"start": 1270.519,
		"end": 1273.939,
		"text": "（阿良々木）<br />あっ ああ まず<br />僕が簡単に説明すると"
	},
	{
		"id": 340,
		"start": 1274.022,
		"end": 1276.942,
		"text": "（戦場ヶ原）<br />いいわ 阿良々木くん<br />自分でするから"
	},
	{
		"id": 341,
		"start": 1277.276,
		"end": 1278.443,
		"text": "（阿良々木）戦場ヶ原"
	},
	{
		"id": 342,
		"start": 1278.944,
		"end": 1280.821,
		"text": "自分で できるから"
	},
	{
		"id": 343,
		"start": 1284.7,
		"end": 1285.909,
		"text": "（忍野）“おもし蟹(かに)”"
	},
	{
		"id": 344,
		"start": 1286.66,
		"end": 1288.328,
		"text": "おもし… 蟹"
	},
	{
		"id": 345,
		"start": 1288.578,
		"end": 1292.499,
		"text": "うん 九州の山あい辺りでの<br />民間伝承だよ"
	},
	{
		"id": 346,
		"start": 1293,
		"end": 1297.129,
		"text": "地域によって おもし蟹だったり<br />思いし蟹 重石蟹(おもしがに)"
	},
	{
		"id": 347,
		"start": 1297.337,
		"end": 1299.423,
		"text": "それに<br />“おもいし神”ってのもある"
	},
	{
		"id": 348,
		"start": 1299.881,
		"end": 1301.591,
		"text": "“蟹”と“神”が<br />かかっているわけだ"
	},
	{
		"id": 349,
		"start": 1302.092,
		"end": 1304.678,
		"text": "でも九州って<br />どうして そんな所の？"
	},
	{
		"id": 350,
		"start": 1305.178,
		"end": 1307.806,
		"text": "（忍野）<br />場所そのものに<br />意味があるんじゃないからね"
	},
	{
		"id": 351,
		"start": 1308.098,
		"end": 1311.393,
		"text": "そういう状況があれば<br />そこに生じる それだけだ"
	},
	{
		"id": 352,
		"start": 1311.727,
		"end": 1312.769,
		"text": "（阿良々木）ふうん"
	},
	{
		"id": 353,
		"start": 1312.978,
		"end": 1317.232,
		"text": "（忍野）<br />この場合 別に蟹じゃなくてもいい<br />兎(うさぎ)だって話もあるし"
	},
	{
		"id": 354,
		"start": 1317.357,
		"end": 1321.57,
		"text": "それに忍ちゃんじゃないけれど<br />美しい女の人だって話もある"
	},
	{
		"id": 355,
		"start": 1321.945,
		"end": 1324.281,
		"text": "（阿良々木）<br />ふうん 月の模様みたいだな"
	},
	{
		"id": 356,
		"start": 1324.698,
		"end": 1327.909,
		"text": "まっ お嬢ちゃんが行き遭ったのが<br />蟹だっていうんなら"
	},
	{
		"id": 357,
		"start": 1328.035,
		"end": 1329.161,
		"text": "今回は蟹なんだろう"
	},
	{
		"id": 358,
		"start": 1329.995,
		"end": 1331.121,
		"text": "（戦場ヶ原）何なんですか それは"
	},
	{
		"id": 359,
		"start": 1331.955,
		"end": 1334.75,
		"text": "名前なんて そんなのは<br />何だって構いませんけれど"
	},
	{
		"id": 360,
		"start": 1335.25,
		"end": 1336.668,
		"text": "（忍野）名前は重要だよ"
	},
	{
		"id": 361,
		"start": 1337.085,
		"end": 1339.046,
		"text": "さっきも言ったけど 蟹じゃなくて"
	},
	{
		"id": 362,
		"start": 1339.129,
		"end": 1340.881,
		"text": "元は神なのかもしれない"
	},
	{
		"id": 363,
		"start": 1341.131,
		"end": 1344.176,
		"text": "おもいし神から おもし蟹へ<br />派生したってことだね"
	},
	{
		"id": 364,
		"start": 1344.801,
		"end": 1348.013,
		"text": "まあ お嬢ちゃんは<br />運の悪い中じゃ 運のいい部類だよ"
	},
	{
		"id": 365,
		"start": 1348.597,
		"end": 1349.765,
		"text": "どうしてですか"
	},
	{
		"id": 366,
		"start": 1350.223,
		"end": 1352.601,
		"text": "（忍野）<br />神様なんてのは<br />どこにでもいるからさ"
	},
	{
		"id": 367,
		"start": 1352.934,
		"end": 1355.062,
		"text": "どこにでもいるし どこにもいない"
	},
	{
		"id": 368,
		"start": 1355.479,
		"end": 1358.482,
		"text": "お嬢ちゃんが そうなる前から<br />お嬢ちゃんの周りには あったし"
	},
	{
		"id": 369,
		"start": 1358.607,
		"end": 1360.275,
		"text": "あるいは なかったとも言える"
	},
	{
		"id": 370,
		"start": 1360.901,
		"end": 1363.904,
		"text": "（戦場ヶ原）<br />フン 禅問答ですね まるで"
	},
	{
		"id": 371,
		"start": 1363.987,
		"end": 1366.239,
		"text": "（忍野）<br />神道だよ　ああ 修験道かな"
	},
	{
		"id": 372,
		"start": 1366.74,
		"end": 1368.283,
		"text": "勘違いするなよ お嬢ちゃん"
	},
	{
		"id": 373,
		"start": 1368.492,
		"end": 1370.744,
		"text": "君は何かのせいで<br />そうなったわけじゃない"
	},
	{
		"id": 374,
		"start": 1370.911,
		"end": 1372.579,
		"text": "ちょっと視点が変わっただけだ"
	},
	{
		"id": 375,
		"start": 1373.163,
		"end": 1375.749,
		"text": "視点が？ 何が言いたいんですか"
	},
	{
		"id": 376,
		"start": 1376.208,
		"end": 1379.252,
		"text": "被害者づらが気に食わねえ<br />っつってんだよ お嬢ちゃん"
	},
	{
		"id": 377,
		"start": 1385.884,
		"end": 1388.136,
		"text": "へえ なかなか どうして"
	},
	{
		"id": 378,
		"start": 1388.303,
		"end": 1391.014,
		"text": "てっきり ただのワガママな<br />お嬢ちゃんかと思ったけど"
	},
	{
		"id": 379,
		"start": 1391.181,
		"end": 1393.391,
		"text": "（戦場ヶ原）<br />どうして そう思ったんですか"
	},
	{
		"id": 380,
		"start": 1393.683,
		"end": 1397.354,
		"text": "（忍野）<br />おもし蟹に遭うような人間は<br />大抵 そうだからだよ"
	},
	{
		"id": 381,
		"start": 1397.854,
		"end": 1399.815,
		"text": "遭おうと思って<br />遭えるもんじゃないし"
	},
	{
		"id": 382,
		"start": 1399.898,
		"end": 1401.358,
		"text": "通常 障るような神でもない"
	},
	{
		"id": 383,
		"start": 1402.651,
		"end": 1404.111,
		"text": "障らない…"
	},
	{
		"id": 384,
		"start": 1405.779,
		"end": 1407.155,
		"text": "（忍野）ただ そこにいるだけで"
	},
	{
		"id": 385,
		"start": 1407.28,
		"end": 1409.908,
		"text": "お嬢ちゃんが何かを望まない限り<br />実現しないんだ"
	},
	{
		"id": 386,
		"start": 1411.034,
		"end": 1414.996,
		"text": "いや もっとも そこまで事情に<br />深入りするつもりは ないけれどね"
	},
	{
		"id": 387,
		"start": 1415.58,
		"end": 1417.874,
		"text": "とにかく いいよ 分かった"
	},
	{
		"id": 388,
		"start": 1418.125,
		"end": 1421.086,
		"text": "体重を取り戻したいと言うのなら<br />力になるさ"
	},
	{
		"id": 389,
		"start": 1421.878,
		"end": 1423.505,
		"text": "阿良々木くんの紹介だしね"
	},
	{
		"id": 390,
		"start": 1425.924,
		"end": 1427.592,
		"text": "（戦場ヶ原）<br />助けてくれるんですか？"
	},
	{
		"id": 391,
		"start": 1435.225,
		"end": 1441.231,
		"text": "♪〜"
	},
	{
		"id": 392,
		"start": 1518.016,
		"end": 1524.022,
		"text": "〜♪"
	},
	{
		"id": 393,
		"start": 1525.065,
		"end": 1525.899,
		"text": "（火憐(かれん)）<br />火憐だぜ"
	},
	{
		"id": 394,
		"start": 1525.982,
		"end": 1526.816,
		"text": "（月火(つきひ)）<br />月火だよ"
	},
	{
		"id": 395,
		"start": 1526.942,
		"end": 1528.318,
		"text": "（火憐）<br />働きアリって言うけどさ"
	},
	{
		"id": 396,
		"start": 1528.443,
		"end": 1529.277,
		"text": "（月火）<br />言っちゃってるよね"
	},
	{
		"id": 397,
		"start": 1529.361,
		"end": 1531.154,
		"text": "（火憐）<br />あれ全体の２割しか<br />働いてねえんだってよ"
	},
	{
		"id": 398,
		"start": 1531.238,
		"end": 1532.822,
		"text": "（月火）<br />残りの８割 働いてないんだ"
	},
	{
		"id": 399,
		"start": 1532.906,
		"end": 1533.907,
		"text": "（火憐）<br />だけど 私は物申す！"
	},
	{
		"id": 400,
		"start": 1533.99,
		"end": 1534.824,
		"text": "（月火）<br />申しちゃう！"
	},
	{
		"id": 401,
		"start": 1534.908,
		"end": 1537.786,
		"text": "（火憐）<br />働きアリなんて名前が そもそも<br />押し付けがましいイメージ操作だ！"
	},
	{
		"id": 402,
		"start": 1537.911,
		"end": 1539.496,
		"text": "（月火）<br />でも さぼりアリとは言えないよね"
	},
	{
		"id": 403,
		"start": 1539.579,
		"end": 1540.413,
		"text": "（火憐）<br />では ここで予告編クイズ"
	},
	{
		"id": 404,
		"start": 1540.497,
		"end": 1541.331,
		"text": "（月火）<br />クイズ！"
	},
	{
		"id": 405,
		"start": 1541.456,
		"end": 1542.874,
		"text": "（火憐）<br />ヒート＆ビートと言えば 私ですが"
	},
	{
		"id": 406,
		"start": 1542.999,
		"end": 1543.833,
		"text": "（月火）<br />火憐ちゃんですが"
	},
	{
		"id": 407,
		"start": 1543.917,
		"end": 1545.752,
		"text": "（火憐）<br />このアンドを示す記号<br />一体 何と読むでしょう"
	},
	{
		"id": 408,
		"start": 1545.835,
		"end": 1547.629,
		"text": "（月火）<br />問題じゃなくて<br />問題文が分かりづらい"
	},
	{
		"id": 409,
		"start": 1547.712,
		"end": 1549.881,
		"text": "（火憐）<br />正解はアンパサンドに<br />決まってんだろ！"
	},
	{
		"id": 410,
		"start": 1550.09,
		"end": 1551.132,
		"text": "（月火）<br />ぴんと こないな！"
	},
	{
		"id": 411,
		"start": 1551.383,
		"end": 1552.884,
		"text": "（火憐･月火）<br />次回「ひたぎクラブ<br />其ノ貳(そのに)」"
	},
	{
		"id": 412,
		"start": 1553.134,
		"end": 1554.511,
		"text": "（火憐）<br />正解者にだけ見せてやる"
	},
	{
		"id": 413,
		"start": 1554.594,
		"end": 1555.845,
		"text": "（月火）<br />誰も見れないよ"
	}
]