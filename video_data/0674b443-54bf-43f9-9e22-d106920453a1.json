[
	{
		"id": 1,
		"start": 1.7189999999999994,
		"end": 3.4699999999999998,
		"text": "（くしゃみ）"
	},
	{
		"id": 2,
		"start": 3.804000000000001,
		"end": 7.057000000000002,
		"text": "（アクア）ううう… 寒う"
	},
	{
		"id": 3,
		"start": 7.182000000000002,
		"end": 10.936000000000002,
		"text": "コートも質屋に入れちゃったし<br />寒いよお"
	},
	{
		"id": 4,
		"start": 12.146000000000003,
		"end": 13.814000000000002,
		"text": "ああっ ひ… 火が"
	},
	{
		"id": 5,
		"start": 15.107000000000005,
		"end": 16.10799999999999,
		"text": "よっ"
	},
	{
		"id": 6,
		"start": 18.56899999999999,
		"end": 20.11199999999999,
		"text": "（佐藤和真(さとうカズマ)）<br />…って 何やってんだ お前！"
	},
	{
		"id": 7,
		"start": 20.23699999999999,
		"end": 21.77999999999999,
		"text": "ふざけんな"
	},
	{
		"id": 8,
		"start": 22.03099999999999,
		"end": 25.07499999999999,
		"text": "俺の唯一の日本の思い出の品に<br />なんてことすんだ！"
	},
	{
		"id": 9,
		"start": 25.19999999999999,
		"end": 28.16199999999999,
		"text": "しかたないでしょ<br />ほかに燃やす物 ないんだから"
	},
	{
		"id": 10,
		"start": 28.28699999999999,
		"end": 30.12199999999999,
		"text": "（カズマ）<br />だったら その羽衣よこせ"
	},
	{
		"id": 11,
		"start": 30.24699999999999,
		"end": 32.45799999999999,
		"text": "ジャージより<br />よく燃えそうじゃねえか"
	},
	{
		"id": 12,
		"start": 32.70799999999999,
		"end": 34.50099999999999,
		"text": "（アクア）<br />放しなさいよ ヒキニート"
	},
	{
		"id": 13,
		"start": 35.00199999999999,
		"end": 37.62899999999999,
		"text": "暖(あった)めてよ もっと私を暖めてよ！"
	},
	{
		"id": 14,
		"start": 37.75499999999999,
		"end": 40.04899999999999,
		"text": "（風の音）"
	},
	{
		"id": 15,
		"start": 42.42599999999999,
		"end": 44.26099999999999,
		"text": "ああーっ！"
	},
	{
		"id": 16,
		"start": 44.38599999999999,
		"end": 45.38699999999999,
		"text": "ちょ… タンマ！"
	},
	{
		"id": 17,
		"start": 46.05499999999999,
		"end": 49.43299999999999,
		"text": "（吹く音）"
	},
	{
		"id": 18,
		"start": 49.55799999999999,
		"end": 50.55899999999999,
		"text": "あっ"
	},
	{
		"id": 19,
		"start": 51.43499999999999,
		"end": 53.14499999999999,
		"text": "あっ あ…"
	},
	{
		"id": 20,
		"start": 53.26999999999999,
		"end": 55.02199999999999,
		"text": "（泣き声）"
	},
	{
		"id": 21,
		"start": 55.43899999999999,
		"end": 59.35999999999999,
		"text": "（カズマ）このままじゃダメだ<br />早く何とかしないと"
	},
	{
		"id": 22,
		"start": 61.69499999999999,
		"end": 68.70200000000004,
		"text": "♪～"
	},
	{
		"id": 23,
		"start": 143.94400000000005,
		"end": 150.95100000000005,
		"text": "～♪"
	},
	{
		"id": 24,
		"start": 153.66200000000003,
		"end": 155.95600000000005,
		"text": "（カズマ）俺たちのパーティーは<br />バランスが悪い"
	},
	{
		"id": 25,
		"start": 156.49800000000005,
		"end": 160.50200000000004,
		"text": "もう少しこう メインの武器になる<br />スキルが欲しいところだ"
	},
	{
		"id": 26,
		"start": 163.42200000000005,
		"end": 164.63100000000003,
		"text": "（カズマ）おし 着いたぞ"
	},
	{
		"id": 27,
		"start": 164.75600000000003,
		"end": 165.59100000000004,
		"text": "いいか アクア"
	},
	{
		"id": 28,
		"start": 165.71600000000004,
		"end": 168.71900000000005,
		"text": "今のうちに言っとくが<br />絶対に暴れるなよ　分かったか？"
	},
	{
		"id": 29,
		"start": 168.96900000000005,
		"end": 172.97300000000004,
		"text": "ちょっと 何言ってんの？<br />カズマって私を何だと思ってんの？"
	},
	{
		"id": 30,
		"start": 173.09800000000004,
		"end": 177.31100000000004,
		"text": "私 チンピラや無法者じゃないのよ<br />女神よ 神様なのよ"
	},
	{
		"id": 31,
		"start": 177.43600000000004,
		"end": 179.56300000000005,
		"text": "…って ちょっと<br />話 聞いてんの？"
	},
	{
		"id": 32,
		"start": 180.02200000000005,
		"end": 181.27300000000005,
		"text": "待ってよ！ 女神の話は…"
	},
	{
		"id": 33,
		"start": 181.39800000000005,
		"end": 182.77400000000003,
		"text": "（ウィズ）いらっしゃ…"
	},
	{
		"id": 34,
		"start": 183.06600000000003,
		"end": 184.02600000000004,
		"text": "ああっ！"
	},
	{
		"id": 35,
		"start": 184.77600000000004,
		"end": 186.52800000000005,
		"text": "ん…　ああっ！"
	},
	{
		"id": 36,
		"start": 186.65300000000005,
		"end": 190.65700000000004,
		"text": "出たわね このクソアンデッド<br />こんな所で店なんて出してたの？"
	},
	{
		"id": 37,
		"start": 191.40800000000004,
		"end": 193.49300000000005,
		"text": "私が馬小屋で<br />寝泊まりしてるってのに―"
	},
	{
		"id": 38,
		"start": 193.61900000000003,
		"end": 196.24600000000004,
		"text": "あんたは<br />お店の経営者ってわけ？"
	},
	{
		"id": 39,
		"start": 196.37100000000004,
		"end": 198.54000000000005,
		"text": "リッチーのくせに生意気よ！"
	},
	{
		"id": 40,
		"start": 198.66500000000005,
		"end": 200.58400000000003,
		"text": "こんな店 神の名のもとに<br />燃やして…"
	},
	{
		"id": 41,
		"start": 200.70900000000003,
		"end": 201.79300000000003,
		"text": "（なぐる音）<br />（アクア）痛い！"
	},
	{
		"id": 42,
		"start": 201.91900000000004,
		"end": 202.79400000000004,
		"text": "あっ"
	},
	{
		"id": 43,
		"start": 203.29500000000004,
		"end": 204.54600000000005,
		"text": "カズマさん"
	},
	{
		"id": 44,
		"start": 205.38000000000005,
		"end": 207.04900000000004,
		"text": "よう ウィズ　久しぶり"
	},
	{
		"id": 45,
		"start": 211.88700000000003,
		"end": 214.22300000000004,
		"text": "お茶も出ないのかしら？<br />このお店は"
	},
	{
		"id": 46,
		"start": 214.34800000000004,
		"end": 217.80900000000003,
		"text": "あっ す… すみません<br />今すぐ持ってきます"
	},
	{
		"id": 47,
		"start": 220.89600000000004,
		"end": 222.23100000000005,
		"text": "（割れる音）<br />（ウィズ）ああっ！"
	},
	{
		"id": 48,
		"start": 222.35600000000005,
		"end": 224.10700000000003,
		"text": "相変わらずだな"
	},
	{
		"id": 49,
		"start": 227.48600000000005,
		"end": 229.07100000000003,
		"text": "（カズマ）彼女の名前はウィズ"
	},
	{
		"id": 50,
		"start": 229.53000000000003,
		"end": 232.99100000000004,
		"text": "こう見えて<br />普通の人間ではなくリッチー"
	},
	{
		"id": 51,
		"start": 233.11600000000004,
		"end": 235.82700000000003,
		"text": "ノーライフキングと呼ばれる<br />アンデッドの王だ"
	},
	{
		"id": 52,
		"start": 236.95400000000004,
		"end": 239.33100000000005,
		"text": "ウィズはリッチーなのに<br />優しい子で―"
	},
	{
		"id": 53,
		"start": 239.45600000000005,
		"end": 243.08500000000004,
		"text": "毎夜 墓地をさまよう魂を<br />天に返してあげていた"
	},
	{
		"id": 54,
		"start": 243.50200000000004,
		"end": 247.92300000000003,
		"text": "いろいろあって 俺たちは<br />その仕事を代わりに引き受けて―"
	},
	{
		"id": 55,
		"start": 248.04800000000003,
		"end": 251.09300000000005,
		"text": "彼女を見逃してあげることに<br />したんだが…"
	},
	{
		"id": 56,
		"start": 252.71900000000005,
		"end": 254.51300000000003,
		"text": "ど… どうぞ"
	},
	{
		"id": 57,
		"start": 258.34999999999985,
		"end": 260.22699999999986,
		"text": "（アクア）う… おいしい"
	},
	{
		"id": 58,
		"start": 260.68599999999986,
		"end": 263.2719999999998,
		"text": "ふん　アンデッドのくせに<br />店なんて構えて"
	},
	{
		"id": 59,
		"start": 263.3969999999998,
		"end": 266.02399999999983,
		"text": "アンデッドのくせに暖かいお茶とか<br />作っちゃったりして"
	},
	{
		"id": 60,
		"start": 266.14899999999983,
		"end": 267.48399999999987,
		"text": "すいません すいません"
	},
	{
		"id": 61,
		"start": 267.60899999999987,
		"end": 269.77799999999985,
		"text": "私なんかが<br />こんな ぜいたくしちゃって"
	},
	{
		"id": 62,
		"start": 269.90299999999985,
		"end": 271.86299999999983,
		"text": "小姑(こじゅうと)みたいな嫌みは やめろ"
	},
	{
		"id": 63,
		"start": 272.48899999999986,
		"end": 273.61499999999984,
		"text": "フウ"
	},
	{
		"id": 64,
		"start": 275.11699999999985,
		"end": 277.61899999999986,
		"text": "ウィズ<br />ポイントに余裕が出来たから―"
	},
	{
		"id": 65,
		"start": 277.74399999999986,
		"end": 279.12099999999987,
		"text": "スキルを何か<br />教えてくれないか？"
	},
	{
		"id": 66,
		"start": 280.87199999999984,
		"end": 283.1659999999998,
		"text": "ちょっと！<br />何 考えてんのよ カズマ"
	},
	{
		"id": 67,
		"start": 283.29199999999986,
		"end": 285.37699999999984,
		"text": "女神の従者がリッチーのスキルを<br />覚えるなんて―"
	},
	{
		"id": 68,
		"start": 285.50199999999984,
		"end": 287.12899999999985,
		"text": "見過ごすわけには<br />いかないんですけど！"
	},
	{
		"id": 69,
		"start": 287.25399999999985,
		"end": 288.54699999999985,
		"text": "誰が従者だ 誰が！"
	},
	{
		"id": 70,
		"start": 289.25599999999986,
		"end": 291.42499999999984,
		"text": "いい？ リッチーってのはね―"
	},
	{
		"id": 71,
		"start": 291.54999999999984,
		"end": 293.80199999999985,
		"text": "薄暗くてジメジメした所が<br />大好きな―"
	},
	{
		"id": 72,
		"start": 293.92699999999985,
		"end": 296.88799999999986,
		"text": "言ってみればナメクジの親戚<br />みたいな連中なの"
	},
	{
		"id": 73,
		"start": 297.01399999999984,
		"end": 298.0559999999998,
		"text": "ひ… ひどい"
	},
	{
		"id": 74,
		"start": 298.1809999999998,
		"end": 302.05999999999983,
		"text": "いや リッチーのスキルなんて<br />普通は覚えられないだろ"
	},
	{
		"id": 75,
		"start": 302.18499999999983,
		"end": 303.93699999999984,
		"text": "そんなスキルを覚えられたら―"
	},
	{
		"id": 76,
		"start": 304.06199999999984,
		"end": 306.4399999999998,
		"text": "結構な戦力に<br />なるんじゃないかと思ってな"
	},
	{
		"id": 77,
		"start": 306.89799999999985,
		"end": 308.35799999999983,
		"text": "（ウィズ）あの…<br />（カズマ）ん？"
	},
	{
		"id": 78,
		"start": 308.48299999999983,
		"end": 310.44399999999985,
		"text": "さっき“女神の従者”って"
	},
	{
		"id": 79,
		"start": 310.56899999999985,
		"end": 312.0699999999998,
		"text": "（カズマ）あっ<br />（アクア）まあね"
	},
	{
		"id": 80,
		"start": 312.1949999999998,
		"end": 313.61399999999986,
		"text": "私はアクア"
	},
	{
		"id": 81,
		"start": 313.73899999999986,
		"end": 317.4089999999998,
		"text": "そう アクシズ教団で<br />あがめられている女神アクアよ"
	},
	{
		"id": 82,
		"start": 317.7839999999998,
		"end": 319.32799999999986,
		"text": "ひかえなさい リッチー！"
	},
	{
		"id": 83,
		"start": 319.45299999999986,
		"end": 321.16299999999984,
		"text": "ヒイイッ"
	},
	{
		"id": 84,
		"start": 321.28799999999984,
		"end": 324.0819999999998,
		"text": "おい ウィズ<br />そんなに おびえなくても"
	},
	{
		"id": 85,
		"start": 324.2069999999998,
		"end": 325.33399999999983,
		"text": "いえ その…"
	},
	{
		"id": 86,
		"start": 325.45899999999983,
		"end": 328.79499999999985,
		"text": "アクシズ教団の人は<br />頭のおかしい人が多く―"
	},
	{
		"id": 87,
		"start": 328.91999999999985,
		"end": 332.59099999999984,
		"text": "関わり合いにならないほうがいい<br />というのが世間の常識なので"
	},
	{
		"id": 88,
		"start": 332.71599999999984,
		"end": 336.1779999999998,
		"text": "アクシズ教団の元締めの<br />女神様と聞いて…"
	},
	{
		"id": 89,
		"start": 336.51099999999985,
		"end": 338.34699999999987,
		"text": "うっ うう…"
	},
	{
		"id": 90,
		"start": 338.47199999999987,
		"end": 340.1399999999998,
		"text": "何ですってえ？"
	},
	{
		"id": 91,
		"start": 340.2649999999998,
		"end": 342.5169999999998,
		"text": "この この！<br />こんな物 こうして…"
	},
	{
		"id": 92,
		"start": 342.6419999999998,
		"end": 344.89499999999987,
		"text": "（カズマ）話が進まねえ"
	},
	{
		"id": 93,
		"start": 350.14999999999986,
		"end": 351.40099999999984,
		"text": "う～ん"
	},
	{
		"id": 94,
		"start": 352.23499999999984,
		"end": 355.61399999999986,
		"text": "そういえば あのベルディアさんを<br />倒されたそうで"
	},
	{
		"id": 95,
		"start": 356.73999999999984,
		"end": 359.07599999999985,
		"text": "あの方は幹部の中でも―"
	},
	{
		"id": 96,
		"start": 359.20099999999985,
		"end": 362.82899999999984,
		"text": "剣の腕に関しては相当なものだった<br />はずなのですが"
	},
	{
		"id": 97,
		"start": 362.95399999999984,
		"end": 364.45599999999985,
		"text": "すごいですね"
	},
	{
		"id": 98,
		"start": 364.95599999999985,
		"end": 368.08499999999987,
		"text": "なんか ベルディアを<br />知ってるみたいな口ぶりだな"
	},
	{
		"id": 99,
		"start": 368.50199999999984,
		"end": 369.3779999999998,
		"text": "ああ"
	},
	{
		"id": 100,
		"start": 369.79499999999985,
		"end": 372.92299999999983,
		"text": "私 魔王軍の８人の幹部の<br />１人ですから"
	},
	{
		"id": 101,
		"start": 373.04799999999983,
		"end": 374.50799999999987,
		"text": "（アクア）確保！"
	},
	{
		"id": 102,
		"start": 375.5919999999998,
		"end": 378.3029999999998,
		"text": "（ウィズ）待って！<br />待ってください アクア様"
	},
	{
		"id": 103,
		"start": 378.4279999999998,
		"end": 381.5149999999998,
		"text": "お願いします 話を聞いてください"
	},
	{
		"id": 104,
		"start": 382.18199999999985,
		"end": 383.3079999999998,
		"text": "フウーッ"
	},
	{
		"id": 105,
		"start": 383.4329999999998,
		"end": 386.31099999999986,
		"text": "やったわね カズマ<br />これで借金なんてチャラよ チャラ"
	},
	{
		"id": 106,
		"start": 386.76999999999987,
		"end": 388.5219999999998,
		"text": "それどころか お釣りが来るわ"
	},
	{
		"id": 107,
		"start": 388.6469999999998,
		"end": 391.56599999999986,
		"text": "おい アクア<br />一応 事情は聞いてやれよ"
	},
	{
		"id": 108,
		"start": 393.02599999999984,
		"end": 398.73999999999984,
		"text": "えっと さすがに魔王軍の幹部だと<br />冒険者な手前 見逃すわけにも…"
	},
	{
		"id": 109,
		"start": 398.86499999999984,
		"end": 403.28599999999983,
		"text": "違うんです　魔王城を守る結界の<br />維持のために頼まれたんです"
	},
	{
		"id": 110,
		"start": 403.41199999999986,
		"end": 406.4979999999998,
		"text": "もちろん 今まで人に危害を<br />加えたことなんて ないですし―"
	},
	{
		"id": 111,
		"start": 406.6229999999998,
		"end": 410.83599999999984,
		"text": "私を倒したところで そもそも<br />賞金も懸かっていませんから"
	},
	{
		"id": 112,
		"start": 411.58599999999984,
		"end": 412.5869999999998,
		"text": "（２人）あ…"
	},
	{
		"id": 113,
		"start": 413.00399999999985,
		"end": 414.2139999999998,
		"text": "んん？"
	},
	{
		"id": 114,
		"start": 414.3389999999998,
		"end": 417.5919999999998,
		"text": "よく分かんないけど<br />念のために退治しておくわね"
	},
	{
		"id": 115,
		"start": 417.7169999999998,
		"end": 419.84499999999986,
		"text": "待ってください アクア様！"
	},
	{
		"id": 116,
		"start": 422.51399999999984,
		"end": 424.18199999999985,
		"text": "えっと つまり―"
	},
	{
		"id": 117,
		"start": 424.30699999999985,
		"end": 428.72799999999984,
		"text": "幹部を全部 倒すと魔王の城への<br />道が開けるとか そんな感じか？"
	},
	{
		"id": 118,
		"start": 430.0629999999998,
		"end": 433.39999999999986,
		"text": "そういうことです<br />魔王さんに頼まれたんです"
	},
	{
		"id": 119,
		"start": 433.52499999999986,
		"end": 435.60999999999984,
		"text": "“人里で<br />お店を経営しながら―”"
	},
	{
		"id": 120,
		"start": 435.7359999999998,
		"end": 437.61199999999985,
		"text": "“のんびり暮らすのは<br />止めないから―”"
	},
	{
		"id": 121,
		"start": 437.73799999999983,
		"end": 441.1159999999998,
		"text": "“幹部として結界の維持だけ<br />頼めないか”って"
	},
	{
		"id": 122,
		"start": 441.2409999999998,
		"end": 442.95099999999985,
		"text": "つまり あんたがいるかぎり―"
	},
	{
		"id": 123,
		"start": 443.07599999999985,
		"end": 446.07899999999984,
		"text": "人類は魔王城には<br />攻め込めないってことね"
	},
	{
		"id": 124,
		"start": 447.2049999999998,
		"end": 449.12399999999985,
		"text": "カズマ 退治しときましょう"
	},
	{
		"id": 125,
		"start": 449.24899999999985,
		"end": 451.08399999999983,
		"text": "待って！ 待ってください"
	},
	{
		"id": 126,
		"start": 451.20899999999983,
		"end": 453.6279999999998,
		"text": "せめて もう少しだけ<br />生かしておいてください"
	},
	{
		"id": 127,
		"start": 454.83799999999985,
		"end": 457.92399999999986,
		"text": "私には まだ<br />やるべきことがあるんです"
	},
	{
		"id": 128,
		"start": 458.79999999999984,
		"end": 462.13699999999983,
		"text": "ふん　私だったら<br />結界くらい破れるけどね"
	},
	{
		"id": 129,
		"start": 462.88799999999986,
		"end": 464.5559999999998,
		"text": "リッチーだし退治しましょう"
	},
	{
		"id": 130,
		"start": 464.6809999999998,
		"end": 466.01599999999985,
		"text": "（ウィズ）待ってください！"
	},
	{
		"id": 131,
		"start": 466.14099999999985,
		"end": 469.60299999999984,
		"text": "待て待て　どうせ今すぐ<br />結界を解いたところで―"
	},
	{
		"id": 132,
		"start": 469.72799999999984,
		"end": 472.1469999999998,
		"text": "今の俺たちのレベルじゃ<br />魔王は倒せないんだし"
	},
	{
		"id": 133,
		"start": 473.02299999999985,
		"end": 474.81599999999986,
		"text": "首ちょんぱされるのがオチだ"
	},
	{
		"id": 134,
		"start": 474.94099999999986,
		"end": 476.6929999999998,
		"text": "ふん 分かったわよ"
	},
	{
		"id": 135,
		"start": 476.8179999999998,
		"end": 478.61199999999985,
		"text": "あ… ありがとうございます"
	},
	{
		"id": 136,
		"start": 478.73699999999985,
		"end": 482.57399999999984,
		"text": "（カズマ）<br />でもいいのか？ ベルディアを<br />倒した俺たちに恨みとか…"
	},
	{
		"id": 137,
		"start": 483.3249999999998,
		"end": 486.20299999999986,
		"text": "ベルディアさんとは<br />特に仲がよかったとか―"
	},
	{
		"id": 138,
		"start": 486.32799999999986,
		"end": 488.3299999999998,
		"text": "そんなことも<br />なかったですからね"
	},
	{
		"id": 139,
		"start": 491.4159999999998,
		"end": 492.91799999999984,
		"text": "私が歩いていると…"
	},
	{
		"id": 140,
		"start": 493.04299999999984,
		"end": 494.83599999999984,
		"text": "（デュラハン）おおっと～！"
	},
	{
		"id": 141,
		"start": 495.50399999999985,
		"end": 499.21599999999984,
		"text": "ハア ハア<br />フハハッ ハハッ…"
	},
	{
		"id": 142,
		"start": 499.84099999999984,
		"end": 502.05199999999985,
		"text": "手がすべった～！"
	},
	{
		"id": 143,
		"start": 502.5939999999998,
		"end": 503.51199999999983,
		"text": "ハハハハッ"
	},
	{
		"id": 144,
		"start": 503.63699999999983,
		"end": 506.8899999999998,
		"text": "（ウィズ）よく 足元に<br />自分の首を転がしてきて"
	},
	{
		"id": 145,
		"start": 507.0149999999998,
		"end": 509.93499999999983,
		"text": "スカートの中を<br />のぞこうとする人でした"
	},
	{
		"id": 146,
		"start": 510.10099999999983,
		"end": 513.0209999999998,
		"text": "（デュラハン）ストラーイク！<br />いろんな意味で ドストラーイク！"
	},
	{
		"id": 147,
		"start": 514.0639999999999,
		"end": 515.3979999999998,
		"text": "それに―"
	},
	{
		"id": 148,
		"start": 515.6899999999998,
		"end": 519.6529999999998,
		"text": "今でも心だけは<br />人間のつもりですしね"
	},
	{
		"id": 149,
		"start": 520.1109999999999,
		"end": 522.2389999999998,
		"text": "いくぞ　オリャッ"
	},
	{
		"id": 150,
		"start": 522.7389999999998,
		"end": 525.4919999999998,
		"text": "それでは私のスキルを<br />お教えしますね"
	},
	{
		"id": 151,
		"start": 526.2839999999999,
		"end": 530.7469999999998,
		"text": "以前 私を見逃してくれたことへの<br />せめてもの恩返し…"
	},
	{
		"id": 152,
		"start": 530.8719999999998,
		"end": 531.9979999999998,
		"text": "どうした？"
	},
	{
		"id": 153,
		"start": 532.1229999999998,
		"end": 533.2499999999999,
		"text": "あの…"
	},
	{
		"id": 154,
		"start": 533.3749999999999,
		"end": 537.3789999999998,
		"text": "私のスキルは相手がいないと<br />使えないものばかりでして"
	},
	{
		"id": 155,
		"start": 537.5039999999998,
		"end": 540.0059999999999,
		"text": "アクア 悪いけど頼めないか？"
	},
	{
		"id": 156,
		"start": 540.5489999999999,
		"end": 545.3449999999998,
		"text": "ほう？ アンデッドが この私に<br />何のスキルを使おうっての？"
	},
	{
		"id": 157,
		"start": 545.4699999999998,
		"end": 547.2219999999999,
		"text": "そ… その"
	},
	{
		"id": 158,
		"start": 547.3469999999999,
		"end": 549.4319999999998,
		"text": "ドレインタッチなんて<br />どうでしょう？"
	},
	{
		"id": 159,
		"start": 549.5579999999999,
		"end": 551.8099999999998,
		"text": "相手の体力や魔力を<br />吸い取ったり―"
	},
	{
		"id": 160,
		"start": 551.9349999999998,
		"end": 554.8549999999998,
		"text": "逆に相手に分け与えたりできる<br />スキルです"
	},
	{
		"id": 161,
		"start": 554.9799999999998,
		"end": 557.1899999999998,
		"text": "なるほど<br />使い方によっては―"
	},
	{
		"id": 162,
		"start": 557.3149999999998,
		"end": 560.8609999999999,
		"text": "俺たちのパーティーの火力不足を<br />補うことができるかもな"
	},
	{
		"id": 163,
		"start": 560.9859999999999,
		"end": 564.6979999999999,
		"text": "も… もちろん ほんの<br />ちょっぴりしか吸いませんので"
	},
	{
		"id": 164,
		"start": 564.8229999999999,
		"end": 566.1989999999998,
		"text": "（アクア）ふーん"
	},
	{
		"id": 165,
		"start": 568.2849999999999,
		"end": 570.6619999999998,
		"text": "いいわよ<br />いくらでも吸ってちょうだい"
	},
	{
		"id": 166,
		"start": 571.0789999999998,
		"end": 572.5389999999999,
		"text": "さあ どうぞ"
	},
	{
		"id": 167,
		"start": 573.5399999999998,
		"end": 577.6689999999999,
		"text": "（カズマ）どちらがリッチーで<br />どちらが女神だか分からなくなるな"
	},
	{
		"id": 168,
		"start": 577.7939999999999,
		"end": 581.0889999999998,
		"text": "で… では 失礼します"
	},
	{
		"id": 169,
		"start": 584.9679999999998,
		"end": 586.3439999999998,
		"text": "あれ？　あ… あれ？"
	},
	{
		"id": 170,
		"start": 586.4689999999998,
		"end": 588.1379999999998,
		"text": "ほらほら どうしたの？"
	},
	{
		"id": 171,
		"start": 588.2629999999998,
		"end": 590.4729999999998,
		"text": "私から魔力や体力を<br />吸うんじゃないの？"
	},
	{
		"id": 172,
		"start": 590.5989999999998,
		"end": 592.7669999999998,
		"text": "あ… あれ～？"
	},
	{
		"id": 173,
		"start": 593.1429999999998,
		"end": 593.9769999999999,
		"text": "（アクア）痛い！"
	},
	{
		"id": 174,
		"start": 594.1019999999999,
		"end": 598.4809999999999,
		"text": "ちょっとカズマ 邪魔しないでよ<br />これはリッチーと女神の戦いなのよ"
	},
	{
		"id": 175,
		"start": 598.6069999999999,
		"end": 600.6919999999999,
		"text": "そうやすやすと吸われて<br />たまるもんですか！"
	},
	{
		"id": 176,
		"start": 600.8169999999999,
		"end": 603.7369999999999,
		"text": "いいや 話進まんから<br />吸わせてやれよ"
	},
	{
		"id": 177,
		"start": 607.0319999999998,
		"end": 608.1579999999998,
		"text": "（カズマ）…っし"
	},
	{
		"id": 178,
		"start": 625.5079999999998,
		"end": 626.7179999999998,
		"text": "習得完了"
	},
	{
		"id": 179,
		"start": 626.8429999999998,
		"end": 629.0539999999999,
		"text": "（ウィズ）あ… あの アクア様？"
	},
	{
		"id": 180,
		"start": 629.5539999999999,
		"end": 632.5569999999998,
		"text": "もう大丈夫ですよ<br />手を離していただいても"
	},
	{
		"id": 181,
		"start": 632.6819999999998,
		"end": 636.5189999999999,
		"text": "…というかアクア様に触れていると<br />なんだか手がピリピリするので―"
	},
	{
		"id": 182,
		"start": 636.6449999999999,
		"end": 638.9799999999998,
		"text": "そろそろ離してほしいのですが"
	},
	{
		"id": 183,
		"start": 639.1889999999999,
		"end": 640.1059999999999,
		"text": "（アクア）ウフッ"
	},
	{
		"id": 184,
		"start": 640.6899999999998,
		"end": 642.2749999999999,
		"text": "（ウィズ）ア… アクア様？"
	},
	{
		"id": 185,
		"start": 642.9429999999999,
		"end": 645.4449999999998,
		"text": "あの 手が熱くなってきたんですが"
	},
	{
		"id": 186,
		"start": 645.5699999999998,
		"end": 649.2819999999998,
		"text": "アクア様 消えちゃう消えちゃう<br />私 消えちゃいます！"
	},
	{
		"id": 187,
		"start": 649.6579999999998,
		"end": 650.4919999999998,
		"text": "（アクア）痛い！"
	},
	{
		"id": 188,
		"start": 650.6169999999998,
		"end": 652.8269999999999,
		"text": "どさくさに紛れて何してんだ"
	},
	{
		"id": 189,
		"start": 652.9529999999999,
		"end": 655.5799999999998,
		"text": "うう… ひどいです"
	},
	{
		"id": 190,
		"start": 655.7049999999998,
		"end": 659.7929999999999,
		"text": "（カズマ）ウィズが なんだか<br />薄くなってるのは気のせいだろうか"
	},
	{
		"id": 191,
		"start": 660.1259999999999,
		"end": 661.5859999999998,
		"text": "（不動産屋）ごめんください"
	},
	{
		"id": 192,
		"start": 662.3369999999999,
		"end": 664.4219999999998,
		"text": "ウィズさんは<br />いらっしゃいますか？"
	},
	{
		"id": 193,
		"start": 672.8469999999999,
		"end": 673.8479999999998,
		"text": "ここか"
	},
	{
		"id": 194,
		"start": 673.9729999999998,
		"end": 676.9759999999999,
		"text": "悪くないわね<br />ええ 悪くないわ"
	},
	{
		"id": 195,
		"start": 677.2679999999998,
		"end": 679.4369999999998,
		"text": "この私が住むのに<br />ふさわしいんじゃないかしら"
	},
	{
		"id": 196,
		"start": 680.0629999999999,
		"end": 683.6499999999999,
		"text": "（めぐみん）元は とある貴族の<br />別荘だったらしいですね"
	},
	{
		"id": 197,
		"start": 683.7749999999999,
		"end": 687.1529999999998,
		"text": "（ダクネス）<br />しかし除霊の報酬として<br />ここに住んでいいとは―"
	},
	{
		"id": 198,
		"start": 687.2779999999998,
		"end": 688.7379999999998,
		"text": "太っ腹な大家さんだな"
	},
	{
		"id": 199,
		"start": 689.2389999999998,
		"end": 692.7419999999998,
		"text": "ウィズは街では高名な魔法使いで<br />通ってるらしくてな"
	},
	{
		"id": 200,
		"start": 693.7019999999999,
		"end": 696.7459999999999,
		"text": "それで よく この手の案件が<br />持ち込まれるんだそうだ"
	},
	{
		"id": 201,
		"start": 697.3719999999998,
		"end": 700.4999999999999,
		"text": "これで安心して冬を越すことが<br />できるってもんよ"
	},
	{
		"id": 202,
		"start": 700.6249999999999,
		"end": 704.5459999999998,
		"text": "災い転じて福と成すとは<br />よく言ったもんね　さっすが私！"
	},
	{
		"id": 203,
		"start": 704.6709999999998,
		"end": 706.0469999999998,
		"text": "でも大家さんが言うには―"
	},
	{
		"id": 204,
		"start": 706.1719999999998,
		"end": 709.8429999999998,
		"text": "はらっても はらっても<br />すぐにまた霊が現れるらしいぞ"
	},
	{
		"id": 205,
		"start": 709.9679999999998,
		"end": 711.1359999999999,
		"text": "任せなさいよ"
	},
	{
		"id": 206,
		"start": 711.2609999999999,
		"end": 713.9299999999998,
		"text": "何せ私は 女神にして<br />アークプリースト"
	},
	{
		"id": 207,
		"start": 714.0549999999998,
		"end": 716.6409999999998,
		"text": "言ってみれば 対アンデッドの<br />エキスパートよ"
	},
	{
		"id": 208,
		"start": 718.7269999999999,
		"end": 719.6439999999999,
		"text": "んっ"
	},
	{
		"id": 209,
		"start": 719.7689999999999,
		"end": 721.5629999999999,
		"text": "（３人）おお～"
	},
	{
		"id": 210,
		"start": 721.6879999999999,
		"end": 724.1899999999998,
		"text": "（アクア）見える　見えるわ"
	},
	{
		"id": 211,
		"start": 724.3149999999998,
		"end": 725.3579999999998,
		"text": "この屋敷には―"
	},
	{
		"id": 212,
		"start": 725.4829999999998,
		"end": 729.1539999999999,
		"text": "貴族が遊び半分で手を出した<br />メイドとのあいだに出来た子ども"
	},
	{
		"id": 213,
		"start": 729.2789999999999,
		"end": 731.8649999999999,
		"text": "その隠し子が<br />幽閉されていたようね"
	},
	{
		"id": 214,
		"start": 731.9899999999999,
		"end": 734.4509999999998,
		"text": "もともと体の弱かった<br />貴族の男は病死"
	},
	{
		"id": 215,
		"start": 735.1179999999998,
		"end": 737.6199999999999,
		"text": "隠し子の母親のメイドも<br />行方知れず"
	},
	{
		"id": 216,
		"start": 737.7459999999999,
		"end": 739.6639999999999,
		"text": "この屋敷に<br />一人残された少女は―"
	},
	{
		"id": 217,
		"start": 739.7889999999999,
		"end": 742.2079999999999,
		"text": "やがて 若くして<br />父親と同じ病に伏して…"
	},
	{
		"id": 218,
		"start": 742.3329999999999,
		"end": 746.2119999999999,
		"text": "なんで そんな よけいな設定まで<br />分かるんだってツッコみたいんだが"
	},
	{
		"id": 219,
		"start": 746.3369999999999,
		"end": 749.4659999999999,
		"text": "（フクロウの鳴き声）"
	},
	{
		"id": 220,
		"start": 750.7589999999998,
		"end": 751.9679999999998,
		"text": "フウ"
	},
	{
		"id": 221,
		"start": 752.8859999999999,
		"end": 754.8039999999999,
		"text": "これで ひととおり<br />掃除も終わったな"
	},
	{
		"id": 222,
		"start": 754.9289999999999,
		"end": 757.9739999999998,
		"text": "部屋割りも決めたし<br />荷物も運び込んだし―"
	},
	{
		"id": 223,
		"start": 758.0989999999998,
		"end": 759.7259999999999,
		"text": "あとは夜を待つだけだ"
	},
	{
		"id": 224,
		"start": 759.8509999999999,
		"end": 761.4779999999998,
		"text": "（カズマ）さすがに ちょっと<br />ホコリっぽいな"
	},
	{
		"id": 225,
		"start": 761.6029999999998,
		"end": 763.8129999999999,
		"text": "（アクア）少女の名は<br />アンナ･フィランテ･エステロイド"
	},
	{
		"id": 226,
		"start": 763.9379999999999,
		"end": 768.4849999999998,
		"text": "好きな物は ぬいぐるみ人形<br />そして冒険者たちの冒険話"
	},
	{
		"id": 227,
		"start": 769.0679999999999,
		"end": 771.7799999999999,
		"text": "でも安心して<br />この霊は悪い子じゃないわ"
	},
	{
		"id": 228,
		"start": 771.9049999999999,
		"end": 773.5729999999999,
		"text": "おっと<br />でも 子どもながらに―"
	},
	{
		"id": 229,
		"start": 773.6979999999999,
		"end": 776.1169999999998,
		"text": "ちょっぴり大人ぶったことが<br />好きなようね"
	},
	{
		"id": 230,
		"start": 776.2419999999998,
		"end": 777.4939999999998,
		"text": "甘いお酒を飲んだりしてた…"
	},
	{
		"id": 231,
		"start": 778.8279999999999,
		"end": 781.4979999999998,
		"text": "んじゃ これから<br />自由行動ってことで"
	},
	{
		"id": 232,
		"start": 781.6229999999998,
		"end": 783.7919999999998,
		"text": "悪霊が出たら<br />すぐに連絡すること"
	},
	{
		"id": 233,
		"start": 784.7929999999999,
		"end": 785.9189999999999,
		"text": "解散"
	},
	{
		"id": 234,
		"start": 788.6299999999999,
		"end": 790.2979999999999,
		"text": "ハア～"
	},
	{
		"id": 235,
		"start": 791.1739999999999,
		"end": 794.9279999999999,
		"text": "いいなあ 自分の部屋があるって<br />すばらしいなあ"
	},
	{
		"id": 236,
		"start": 795.0529999999999,
		"end": 798.0139999999999,
		"text": "（カズマ）<br />朝までには悪霊のたぐいも<br />出ていってくれるだろう"
	},
	{
		"id": 237,
		"start": 798.1389999999999,
		"end": 799.3489999999998,
		"text": "アクアのことだ"
	},
	{
		"id": 238,
		"start": 799.4739999999998,
		"end": 802.1429999999998,
		"text": "自分の家が悪霊なんぞに<br />好き勝手にやられるのを―"
	},
	{
		"id": 239,
		"start": 802.2679999999998,
		"end": 804.0199999999999,
		"text": "放置しておくような<br />やつではない"
	},
	{
		"id": 240,
		"start": 804.5209999999998,
		"end": 807.4819999999999,
		"text": "一応 アークプリーストにして<br />女神だしな"
	},
	{
		"id": 241,
		"start": 808.1489999999999,
		"end": 809.8179999999999,
		"text": "（アクアの叫び声）"
	},
	{
		"id": 242,
		"start": 809.9429999999999,
		"end": 810.7769999999998,
		"text": "何だ？"
	},
	{
		"id": 243,
		"start": 812.1949999999998,
		"end": 814.7809999999998,
		"text": "どうした？ おい アクア<br />何があった 大丈夫か？"
	},
	{
		"id": 244,
		"start": 814.9059999999998,
		"end": 816.8249999999998,
		"text": "（アクア）うっ うう…"
	},
	{
		"id": 245,
		"start": 817.0749999999998,
		"end": 818.7849999999999,
		"text": "カ… カズマ"
	},
	{
		"id": 246,
		"start": 819.3269999999999,
		"end": 820.1609999999998,
		"text": "おい"
	},
	{
		"id": 247,
		"start": 821.1619999999998,
		"end": 824.5409999999998,
		"text": "これは大事に取っておいた<br />すごく高いお酒なのよ"
	},
	{
		"id": 248,
		"start": 824.6659999999998,
		"end": 827.3769999999998,
		"text": "お風呂から上がったら ゆっくり<br />チビチビ大事に飲もうと―"
	},
	{
		"id": 249,
		"start": 827.5019999999998,
		"end": 829.1279999999998,
		"text": "楽しみにしてたのに"
	},
	{
		"id": 250,
		"start": 829.2539999999998,
		"end": 833.7579999999998,
		"text": "それが 私が部屋に帰ってきたら<br />見てのとおり空(から)だったのよ～！"
	},
	{
		"id": 251,
		"start": 833.8829999999998,
		"end": 835.9269999999998,
		"text": "そうか　じゃあおやすみ<br />またあしたな"
	},
	{
		"id": 252,
		"start": 836.0519999999998,
		"end": 837.4279999999999,
		"text": "（アクア）これは悪霊の仕業よ"
	},
	{
		"id": 253,
		"start": 837.5539999999999,
		"end": 842.4329999999999,
		"text": "ちょっと私 屋敷の中を探索して<br />目につく霊をしばき回してくるわ！"
	},
	{
		"id": 254,
		"start": 842.5589999999999,
		"end": 844.9359999999998,
		"text": "オリャアアーッ！"
	},
	{
		"id": 255,
		"start": 845.0609999999998,
		"end": 848.7309999999999,
		"text": "出てこいやあーっ！"
	},
	{
		"id": 256,
		"start": 848.8569999999999,
		"end": 849.9409999999998,
		"text": "（あくび）"
	},
	{
		"id": 257,
		"start": 850.0659999999998,
		"end": 852.1099999999998,
		"text": "（アクア）ターンアンデッド！<br />ターンアンデッド！"
	},
	{
		"id": 258,
		"start": 852.2349999999998,
		"end": 853.9869999999999,
		"text": "ターンアンデッド！<br />ターンアンデッド！"
	},
	{
		"id": 259,
		"start": 854.1119999999999,
		"end": 855.0289999999999,
		"text": "ターンアンデッド！"
	},
	{
		"id": 260,
		"start": 855.1539999999999,
		"end": 856.1969999999999,
		"text": "花鳥風月"
	},
	{
		"id": 261,
		"start": 856.3219999999999,
		"end": 857.9489999999998,
		"text": "ターンアンデッド！<br />ターンアンデッド！"
	},
	{
		"id": 262,
		"start": 858.0739999999998,
		"end": 859.9509999999998,
		"text": "ターンアンデッド！"
	},
	{
		"id": 263,
		"start": 860.0759999999998,
		"end": 863.2039999999998,
		"text": "（鳴き声）"
	},
	{
		"id": 264,
		"start": 866.4569999999999,
		"end": 867.7089999999998,
		"text": "（カズマ）ん…"
	},
	{
		"id": 265,
		"start": 869.7939999999999,
		"end": 871.4209999999998,
		"text": "（カズマ）トイレ 行きたい"
	},
	{
		"id": 266,
		"start": 871.9629999999999,
		"end": 873.2139999999998,
		"text": "（物音）<br />（カズマ）ん？"
	},
	{
		"id": 267,
		"start": 876.4259999999998,
		"end": 877.3429999999998,
		"text": "（カズマ）怖っ！"
	},
	{
		"id": 268,
		"start": 877.8439999999998,
		"end": 880.5129999999998,
		"text": "は？ あんな人形<br />この部屋にあったっけ？"
	},
	{
		"id": 269,
		"start": 880.9719999999999,
		"end": 882.5989999999998,
		"text": "ないわ～！"
	},
	{
		"id": 270,
		"start": 882.7239999999998,
		"end": 886.5609999999998,
		"text": "こんなベタな展開 ないわ～！"
	},
	{
		"id": 271,
		"start": 895.5699999999998,
		"end": 898.7809999999998,
		"text": "ヒッ！　う… くうっ"
	},
	{
		"id": 272,
		"start": 900.1159999999999,
		"end": 900.9919999999998,
		"text": "うっ"
	},
	{
		"id": 273,
		"start": 901.1169999999998,
		"end": 902.7439999999998,
		"text": "はああっ！"
	},
	{
		"id": 274,
		"start": 903.1609999999998,
		"end": 905.7469999999998,
		"text": "（叫び声）"
	},
	{
		"id": 275,
		"start": 906.2059999999998,
		"end": 908.7499999999999,
		"text": "（カズマ）アクア！<br />アクア様あーっ！"
	},
	{
		"id": 276,
		"start": 909.2499999999999,
		"end": 911.2519999999998,
		"text": "怖い　怖い　超怖い！<br />何 これ？"
	},
	{
		"id": 277,
		"start": 911.3769999999998,
		"end": 913.0039999999998,
		"text": "（人形の笑い声）"
	},
	{
		"id": 278,
		"start": 913.4629999999999,
		"end": 914.9229999999999,
		"text": "（カズマ）アクアーッ！"
	},
	{
		"id": 279,
		"start": 915.0479999999999,
		"end": 915.8819999999998,
		"text": "助け…"
	},
	{
		"id": 280,
		"start": 917.3829999999998,
		"end": 918.6349999999999,
		"text": "ギイヤーッ！"
	},
	{
		"id": 281,
		"start": 918.7599999999999,
		"end": 921.2209999999999,
		"text": "ワアアーッ！"
	},
	{
		"id": 282,
		"start": 922.3879999999998,
		"end": 926.0589999999999,
		"text": "お… 脅かすなよ めぐみん<br />危うく漏らすところだったぞ"
	},
	{
		"id": 283,
		"start": 926.1839999999999,
		"end": 927.8939999999999,
		"text": "それは こっちのセリフです！"
	},
	{
		"id": 284,
		"start": 928.0189999999999,
		"end": 931.1889999999999,
		"text": "なぜカズマが この部屋に<br />飛び込んでくるんですか？"
	},
	{
		"id": 285,
		"start": 931.3139999999999,
		"end": 933.5239999999999,
		"text": "アクアが戻ってきたかと<br />思ったのに"
	},
	{
		"id": 286,
		"start": 933.6499999999999,
		"end": 935.8179999999999,
		"text": "めぐみんこそ<br />なんでアクアの部屋に？"
	},
	{
		"id": 287,
		"start": 935.9439999999998,
		"end": 936.7779999999998,
		"text": "（めぐみん）うっ"
	},
	{
		"id": 288,
		"start": 937.1949999999998,
		"end": 941.0739999999998,
		"text": "いや… 人形があちこちで<br />動いておりまして"
	},
	{
		"id": 289,
		"start": 941.1989999999998,
		"end": 945.5359999999998,
		"text": "その アクアに身の安全を<br />守ってもらうのと…"
	},
	{
		"id": 290,
		"start": 946.6629999999999,
		"end": 947.7889999999999,
		"text": "一緒に―"
	},
	{
		"id": 291,
		"start": 947.9139999999999,
		"end": 950.7919999999998,
		"text": "トイレにと… 思いまして"
	},
	{
		"id": 292,
		"start": 950.9169999999998,
		"end": 952.2519999999998,
		"text": "（カズマ）お前もか"
	},
	{
		"id": 293,
		"start": 952.7519999999998,
		"end": 956.3809999999999,
		"text": "（カズマ）<br />多分アクアは まだ屋敷内の除霊に<br />行ってるんじゃないか"
	},
	{
		"id": 294,
		"start": 956.5059999999999,
		"end": 957.6739999999999,
		"text": "そうですか"
	},
	{
		"id": 295,
		"start": 957.7989999999999,
		"end": 959.8839999999998,
		"text": "では きっと<br />ダクネスも一緒ですね"
	},
	{
		"id": 296,
		"start": 960.0089999999998,
		"end": 961.2609999999999,
		"text": "ハア…"
	},
	{
		"id": 297,
		"start": 961.3859999999999,
		"end": 964.0969999999999,
		"text": "ああ あいつ あれで<br />クルセイダーだったな"
	},
	{
		"id": 298,
		"start": 964.2219999999999,
		"end": 965.2649999999999,
		"text": "ううっ"
	},
	{
		"id": 299,
		"start": 965.8069999999998,
		"end": 967.1829999999999,
		"text": "う… うう"
	},
	{
		"id": 300,
		"start": 967.3079999999999,
		"end": 968.7679999999998,
		"text": "なあ めぐみん"
	},
	{
		"id": 301,
		"start": 969.1019999999999,
		"end": 971.5209999999998,
		"text": "しばらく あっち向いて<br />耳塞いでてくれ"
	},
	{
		"id": 302,
		"start": 971.6459999999998,
		"end": 973.5229999999998,
		"text": "ちょっと失礼してベランダから…"
	},
	{
		"id": 303,
		"start": 974.2319999999999,
		"end": 976.9009999999998,
		"text": "おい 何してんだよ？<br />離してくれ"
	},
	{
		"id": 304,
		"start": 977.1509999999998,
		"end": 979.0699999999998,
		"text": "さもないと 俺のズボンと<br />この部屋のじゅうたんが―"
	},
	{
		"id": 305,
		"start": 979.1949999999998,
		"end": 980.5299999999999,
		"text": "大変なことになる"
	},
	{
		"id": 306,
		"start": 981.0719999999999,
		"end": 982.4069999999998,
		"text": "行かせませんよ"
	},
	{
		"id": 307,
		"start": 982.5319999999998,
		"end": 985.0339999999999,
		"text": "何 一人でスッキリしようと<br />してるんですか"
	},
	{
		"id": 308,
		"start": 985.5349999999999,
		"end": 988.1619999999998,
		"text": "私たちは仲間じゃないですか"
	},
	{
		"id": 309,
		"start": 988.2879999999999,
		"end": 990.2899999999998,
		"text": "トイレだろうと どこだろうと―"
	},
	{
		"id": 310,
		"start": 991.2069999999999,
		"end": 992.4169999999998,
		"text": "逝くときは 一緒です"
	},
	{
		"id": 311,
		"start": 992.5419999999998,
		"end": 996.7539999999998,
		"text": "ええい 離せ！ こんなときだけ<br />仲間の絆を主張するな"
	},
	{
		"id": 312,
		"start": 996.8789999999998,
		"end": 999.8819999999998,
		"text": "お前“紅魔族(こうまぞく)はトイレ行かない”<br />とか言ってたじゃねえか"
	},
	{
		"id": 313,
		"start": 1000.0079999999998,
		"end": 1002.6769999999998,
		"text": "なんなら そこに空いた酒瓶が<br />転がってるから！"
	},
	{
		"id": 314,
		"start": 1003.2189999999998,
		"end": 1005.5549999999998,
		"text": "今 とんでもないことを<br />口走りましたね！"
	},
	{
		"id": 315,
		"start": 1005.6799999999998,
		"end": 1008.1819999999998,
		"text": "その空いた酒瓶で<br />私に何をしろと？"
	},
	{
		"id": 316,
		"start": 1008.3079999999999,
		"end": 1011.4359999999998,
		"text": "させませんよ  私もカズマが<br />用を足そうとしてるところを―"
	},
	{
		"id": 317,
		"start": 1011.8529999999998,
		"end": 1013.7299999999998,
		"text": "後ろから<br />揺らしてやるぐらいは―"
	},
	{
		"id": 318,
		"start": 1013.8549999999998,
		"end": 1015.7319999999999,
		"text": "できますか… ら…"
	},
	{
		"id": 319,
		"start": 1018.1509999999998,
		"end": 1018.9849999999998,
		"text": "ん？"
	},
	{
		"id": 320,
		"start": 1024.3240000000005,
		"end": 1026.8260000000007,
		"text": "（２人の叫び声）"
	},
	{
		"id": 321,
		"start": 1028.9950000000006,
		"end": 1033.0820000000006,
		"text": "（荒い息）"
	},
	{
		"id": 322,
		"start": 1033.2070000000006,
		"end": 1035.4600000000007,
		"text": "（めぐみん）うう…<br />カズマ いますか？"
	},
	{
		"id": 323,
		"start": 1035.9180000000006,
		"end": 1036.9610000000007,
		"text": "いるよ"
	},
	{
		"id": 324,
		"start": 1038.2120000000007,
		"end": 1039.7970000000007,
		"text": "（めぐみん）本当にいますか？"
	},
	{
		"id": 325,
		"start": 1039.9220000000007,
		"end": 1040.7570000000007,
		"text": "いるって"
	},
	{
		"id": 326,
		"start": 1040.8820000000007,
		"end": 1043.5510000000006,
		"text": "（めぐみん）本当に本当に<br />ちゃんと いてくれますか？"
	},
	{
		"id": 327,
		"start": 1043.6760000000006,
		"end": 1045.0940000000007,
		"text": "いるって！"
	},
	{
		"id": 328,
		"start": 1045.2190000000007,
		"end": 1047.9300000000007,
		"text": "ちゃんといるし 置いてったり<br />しないから早くしてくれ"
	},
	{
		"id": 329,
		"start": 1048.6810000000007,
		"end": 1049.8490000000006,
		"text": "（めぐみん）あの…"
	},
	{
		"id": 330,
		"start": 1049.9740000000006,
		"end": 1052.1430000000007,
		"text": "さすがに ちょっと<br />恥ずかしいので―"
	},
	{
		"id": 331,
		"start": 1052.2680000000007,
		"end": 1054.6870000000006,
		"text": "大きめの声で歌でも<br />歌ってくれません？"
	},
	{
		"id": 332,
		"start": 1054.8120000000006,
		"end": 1056.1050000000007,
		"text": "何が悲しくて―"
	},
	{
		"id": 333,
		"start": 1056.2300000000007,
		"end": 1059.4000000000005,
		"text": "こんな夜中にトイレの前で<br />歌わなきゃならないんだよ"
	},
	{
		"id": 334,
		"start": 1059.5250000000005,
		"end": 1061.0690000000006,
		"text": "（物音）<br />（カズマ）うっ"
	},
	{
		"id": 335,
		"start": 1063.5710000000006,
		"end": 1064.9060000000006,
		"text": "（カズマ）ヒイイッ！"
	},
	{
		"id": 336,
		"start": 1065.0310000000006,
		"end": 1066.2400000000007,
		"text": "（ノック）<br />（カズマ）おい めぐみん　急げ！"
	},
	{
		"id": 337,
		"start": 1066.3660000000007,
		"end": 1068.4510000000007,
		"text": "（めぐみん）そんなに<br />急がせないでくださいよ！"
	},
	{
		"id": 338,
		"start": 1068.5760000000007,
		"end": 1070.6610000000007,
		"text": "カズマもピンチなのは<br />分かってますから"
	},
	{
		"id": 339,
		"start": 1070.7870000000007,
		"end": 1072.9140000000007,
		"text": "（カズマ）確かに俺とお前の<br />膀胱(ぼうこう)もピンチだが―"
	},
	{
		"id": 340,
		"start": 1073.0390000000007,
		"end": 1075.0830000000005,
		"text": "もっとヤバいものが<br />近づいてきてんだよ！"
	},
	{
		"id": 341,
		"start": 1075.2080000000005,
		"end": 1078.5440000000006,
		"text": "そんなに せかされたら<br />出るものも出ませんよ"
	},
	{
		"id": 342,
		"start": 1078.6690000000006,
		"end": 1080.0880000000006,
		"text": "あああーっ ダメだ"
	},
	{
		"id": 343,
		"start": 1080.5460000000007,
		"end": 1082.7570000000007,
		"text": "もう待てねえ！"
	},
	{
		"id": 344,
		"start": 1084.1330000000007,
		"end": 1086.8860000000006,
		"text": "アアアーッ！"
	},
	{
		"id": 345,
		"start": 1087.0110000000006,
		"end": 1088.1370000000006,
		"text": "急げ！"
	},
	{
		"id": 346,
		"start": 1090.7650000000006,
		"end": 1092.8920000000007,
		"text": "（めぐみん）<br />ここまで非常識な人とは！"
	},
	{
		"id": 347,
		"start": 1093.0170000000007,
		"end": 1095.8120000000006,
		"text": "お前は半分でも出せたんだから<br />いいだろ！"
	},
	{
		"id": 348,
		"start": 1095.9370000000006,
		"end": 1099.1480000000006,
		"text": "何の話ですか？ 紅魔族は<br />トイレになんか行きませんよ"
	},
	{
		"id": 349,
		"start": 1099.5650000000007,
		"end": 1103.5280000000007,
		"text": "今さら それ言うか？<br />俺のほうは もう限界だってのに"
	},
	{
		"id": 350,
		"start": 1103.6530000000007,
		"end": 1106.1970000000006,
		"text": "そんなこともあろうかと<br />持ってきました"
	},
	{
		"id": 351,
		"start": 1106.4890000000007,
		"end": 1107.6570000000006,
		"text": "使ってください"
	},
	{
		"id": 352,
		"start": 1107.7820000000006,
		"end": 1109.1580000000006,
		"text": "要らんわ！"
	},
	{
		"id": 353,
		"start": 1109.2830000000006,
		"end": 1113.1620000000007,
		"text": "（鳴き声）"
	},
	{
		"id": 354,
		"start": 1115.0390000000007,
		"end": 1117.4170000000006,
		"text": "（人形たちの声）"
	},
	{
		"id": 355,
		"start": 1117.9590000000007,
		"end": 1119.6690000000006,
		"text": "もう限界だ"
	},
	{
		"id": 356,
		"start": 1119.9610000000007,
		"end": 1121.6290000000006,
		"text": "このまま いっそ花瓶に…"
	},
	{
		"id": 357,
		"start": 1121.7540000000006,
		"end": 1125.6330000000007,
		"text": "（めぐみん）<br />黒より黒く 闇より暗き漆黒に―"
	},
	{
		"id": 358,
		"start": 1125.7580000000007,
		"end": 1128.2190000000007,
		"text": "我が真紅(しんく)の混交を望みたもう"
	},
	{
		"id": 359,
		"start": 1128.3440000000007,
		"end": 1130.8880000000006,
		"text": "何 唱えてんだ<br />この屋敷ごと吹き飛ばす気か！"
	},
	{
		"id": 360,
		"start": 1131.0130000000006,
		"end": 1132.2230000000006,
		"text": "（物音）"
	},
	{
		"id": 361,
		"start": 1132.8490000000006,
		"end": 1136.2270000000005,
		"text": "（ドアに ぶつかる音）"
	},
	{
		"id": 362,
		"start": 1136.3520000000005,
		"end": 1138.2290000000007,
		"text": "クソッ やるしかないか"
	},
	{
		"id": 363,
		"start": 1138.7710000000006,
		"end": 1141.1900000000007,
		"text": "めぐみん<br />ドアを開けたら お前は走れ"
	},
	{
		"id": 364,
		"start": 1141.3150000000007,
		"end": 1142.8590000000006,
		"text": "俺は覚えたての<br />ドレインタッチで―"
	},
	{
		"id": 365,
		"start": 1142.9840000000006,
		"end": 1145.8620000000005,
		"text": "人形から魔力の１つでも<br />吸い取ってやる！"
	},
	{
		"id": 366,
		"start": 1146.9880000000007,
		"end": 1149.5740000000005,
		"text": "オラア かかってこいやーっ！"
	},
	{
		"id": 367,
		"start": 1149.6990000000005,
		"end": 1150.7410000000007,
		"text": "この悪霊が"
	},
	{
		"id": 368,
		"start": 1150.8670000000006,
		"end": 1153.3690000000006,
		"text": "あとで うちの狂犬女神<br />けしかけてやんぞ コラーッ！"
	},
	{
		"id": 369,
		"start": 1153.8700000000006,
		"end": 1155.1210000000005,
		"text": "（ぶつかる音）"
	},
	{
		"id": 370,
		"start": 1156.4970000000005,
		"end": 1157.3730000000007,
		"text": "（２人）あ？"
	},
	{
		"id": 371,
		"start": 1157.9150000000006,
		"end": 1159.6250000000007,
		"text": "（アクア）あ… ああ…"
	},
	{
		"id": 372,
		"start": 1159.7500000000007,
		"end": 1162.5870000000007,
		"text": "お… おい アクア<br />大丈夫か？"
	},
	{
		"id": 373,
		"start": 1166.0070000000007,
		"end": 1170.0110000000006,
		"text": "（ルナ）<br />悪霊を退治したということで<br />臨時報酬が出てますよ"
	},
	{
		"id": 374,
		"start": 1170.1360000000006,
		"end": 1173.1810000000007,
		"text": "ギルドでも討伐クエストを<br />設定したんですが―"
	},
	{
		"id": 375,
		"start": 1173.3060000000007,
		"end": 1176.6420000000007,
		"text": "いくら退治しても すぐに<br />新しい悪霊が住み着いて―"
	},
	{
		"id": 376,
		"start": 1176.7670000000007,
		"end": 1178.1020000000005,
		"text": "困っていたんです"
	},
	{
		"id": 377,
		"start": 1178.2270000000005,
		"end": 1181.8980000000006,
		"text": "らしいですね　なんでそんなに<br />悪霊が集まってきてたんだろ？"
	},
	{
		"id": 378,
		"start": 1182.0230000000006,
		"end": 1183.8160000000007,
		"text": "それなんですが―"
	},
	{
		"id": 379,
		"start": 1183.9410000000007,
		"end": 1187.4870000000005,
		"text": "あの屋敷の近くに<br />共同墓地があるじゃないですか"
	},
	{
		"id": 380,
		"start": 1187.7370000000005,
		"end": 1189.5720000000006,
		"text": "共同墓地って…"
	},
	{
		"id": 381,
		"start": 1189.6970000000006,
		"end": 1191.7820000000006,
		"text": "ウィズと初めて会ったとこだな"
	},
	{
		"id": 382,
		"start": 1191.9080000000006,
		"end": 1196.5370000000007,
		"text": "（ルナ）誰かが いたずらか何かで<br />巨大な結界を張ったようなんですよ"
	},
	{
		"id": 383,
		"start": 1197.2050000000006,
		"end": 1201.5420000000006,
		"text": "それで行(ゆ)き場(ば)をなくした霊が<br />あの空き家に住み着いてたみたいで"
	},
	{
		"id": 384,
		"start": 1201.6670000000006,
		"end": 1202.9600000000007,
		"text": "（カズマ）うっ…"
	},
	{
		"id": 385,
		"start": 1203.2520000000006,
		"end": 1204.4620000000007,
		"text": "ちょっと失礼"
	},
	{
		"id": 386,
		"start": 1206.8810000000005,
		"end": 1207.7980000000007,
		"text": "（ルナ）うん？"
	},
	{
		"id": 387,
		"start": 1208.1740000000007,
		"end": 1210.8010000000006,
		"text": "（カズマ）<br />おい 心当たりがあるな？<br />言え"
	},
	{
		"id": 388,
		"start": 1211.4690000000007,
		"end": 1212.3030000000006,
		"text": "はい"
	},
	{
		"id": 389,
		"start": 1212.4280000000006,
		"end": 1216.4740000000006,
		"text": "以前ウィズの代わりに墓地の除霊を<br />引き受けたじゃないですか"
	},
	{
		"id": 390,
		"start": 1216.5990000000006,
		"end": 1218.2670000000007,
		"text": "でも しょっちゅう<br />墓地まで行くのって―"
	},
	{
		"id": 391,
		"start": 1218.3920000000007,
		"end": 1219.8940000000007,
		"text": "面倒くさいじゃないですか"
	},
	{
		"id": 392,
		"start": 1220.0190000000007,
		"end": 1222.5630000000006,
		"text": "それで いっそ墓地に<br />霊の住み場所をなくしてやれば―"
	},
	{
		"id": 393,
		"start": 1223.0640000000005,
		"end": 1226.4840000000006,
		"text": "そのうち適当に散って<br />いなくなるかなって思って"
	},
	{
		"id": 394,
		"start": 1226.7760000000007,
		"end": 1229.0700000000006,
		"text": "やり… ま… した"
	},
	{
		"id": 395,
		"start": 1230.0710000000006,
		"end": 1234.3250000000007,
		"text": "（カズマ）なんというマッチポンプ<br />これは どう考えてもダメだろ"
	},
	{
		"id": 396,
		"start": 1235.0340000000006,
		"end": 1238.8710000000005,
		"text": "（カズマ）<br />ギルドからの臨時報酬は<br />受け取らない　いいな？"
	},
	{
		"id": 397,
		"start": 1238.9960000000005,
		"end": 1239.8300000000006,
		"text": "（アクア）はい…"
	},
	{
		"id": 398,
		"start": 1243.6260000000007,
		"end": 1244.7100000000007,
		"text": "んっ"
	},
	{
		"id": 399,
		"start": 1245.2110000000007,
		"end": 1250.1320000000007,
		"text": "てっきりアクアの妄想かと思ったら<br />まさか本当の話だったとはな"
	},
	{
		"id": 400,
		"start": 1250.3830000000007,
		"end": 1251.3840000000007,
		"text": "（ウィズ）カズマさん"
	},
	{
		"id": 401,
		"start": 1251.8010000000006,
		"end": 1254.5120000000006,
		"text": "こんにちは お墓の掃除ですか？"
	},
	{
		"id": 402,
		"start": 1254.6370000000006,
		"end": 1258.2660000000005,
		"text": "よかったですね<br />ここに住んでいいことになって"
	},
	{
		"id": 403,
		"start": 1258.8080000000007,
		"end": 1261.8940000000007,
		"text": "ホント　大家さんがいい人で<br />助かったよ"
	},
	{
		"id": 404,
		"start": 1262.0190000000007,
		"end": 1263.7290000000007,
		"text": "ウィズにも迷惑かけちまったな"
	},
	{
		"id": 405,
		"start": 1263.8540000000007,
		"end": 1267.7750000000005,
		"text": "（ウィズ）<br />いえいえ　むしろ私としては<br />これでよかったと思ってますから"
	},
	{
		"id": 406,
		"start": 1268.7340000000006,
		"end": 1271.4870000000005,
		"text": "これなら きっと<br />寂しくないでしょうし"
	},
	{
		"id": 407,
		"start": 1271.6120000000005,
		"end": 1272.5720000000006,
		"text": "フフッ"
	},
	{
		"id": 408,
		"start": 1273.6140000000007,
		"end": 1274.7400000000007,
		"text": "…だな"
	},
	{
		"id": 409,
		"start": 1275.1570000000006,
		"end": 1278.0350000000005,
		"text": "頼むから もう<br />いたずらしないでくれよ"
	},
	{
		"id": 410,
		"start": 1278.1600000000005,
		"end": 1281.5810000000006,
		"text": "（ウィズ）<br />では店番があるので<br />今日は帰りますね"
	},
	{
		"id": 411,
		"start": 1281.7060000000006,
		"end": 1283.4160000000006,
		"text": "皆さんにも よろしく"
	},
	{
		"id": 412,
		"start": 1283.5410000000006,
		"end": 1285.6680000000006,
		"text": "（カズマ）ああ<br />来てくれてありがとうな"
	},
	{
		"id": 413,
		"start": 1289.2130000000006,
		"end": 1290.2980000000007,
		"text": "フウ…"
	},
	{
		"id": 414,
		"start": 1290.7560000000005,
		"end": 1294.3020000000006,
		"text": "（アクア）カズマ！<br />ごはん出来たから早く来て"
	},
	{
		"id": 415,
		"start": 1294.7190000000007,
		"end": 1298.5560000000007,
		"text": "早く来ないと せっかくのお昼が<br />冷めちゃうんですけど！"
	},
	{
		"id": 416,
		"start": 1298.6810000000007,
		"end": 1300.9330000000007,
		"text": "（カズマ）<br />これで ようやく念願の"
	},
	{
		"id": 417,
		"start": 1301.0580000000007,
		"end": 1304.1450000000007,
		"text": "地に足の着いた生活が<br />手に入ったというわけだ"
	},
	{
		"id": 418,
		"start": 1304.6870000000006,
		"end": 1307.0640000000005,
		"text": "分かったよ  今 行く！"
	},
	{
		"id": 419,
		"start": 1307.2730000000006,
		"end": 1310.4430000000007,
		"text": "（ダクネス）アクア<br />暖炉のまきが もうないんだが"
	},
	{
		"id": 420,
		"start": 1310.5680000000007,
		"end": 1311.9440000000006,
		"text": "（アクア）<br />ああ じゃあ そこの―"
	},
	{
		"id": 421,
		"start": 1312.0690000000006,
		"end": 1314.1960000000006,
		"text": "カズマのジャージでも<br />くべておいて"
	},
	{
		"id": 422,
		"start": 1314.3220000000006,
		"end": 1315.7810000000006,
		"text": "（カズマ）やめろお！"
	},
	{
		"id": 423,
		"start": 1315.9060000000006,
		"end": 1320.2020000000007,
		"text": "だから俺の大事な思い出を<br />燃やそうとすんなーっ！"
	},
	{
		"id": 424,
		"start": 1320.7450000000006,
		"end": 1327.4600000000007,
		"text": "♪～"
	},
	{
		"id": 425,
		"start": 1402.9930000000006,
		"end": 1410.0000000000007,
		"text": "～♪"
	}
]