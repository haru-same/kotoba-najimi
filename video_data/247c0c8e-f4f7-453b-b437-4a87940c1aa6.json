[
	{
		"id": 1,
		"start": 2.669,
		"end": 4.045,
		"text": "（電話:忍野(おしの)）<br />“迷(まよ)い牛(うし)”"
	},
	{
		"id": 2,
		"start": 5.255,
		"end": 6.798,
		"text": "迷い牛だろ そりゃ"
	},
	{
		"id": 3,
		"start": 7.298,
		"end": 10.468,
		"text": "カタツムリを漢字で書きゃ<br />牛って入っているでしょうが"
	},
	{
		"id": 4,
		"start": 10.593,
		"end": 12.887,
		"text": "渦巻の渦の<br />さんずいを虫偏に変えて"
	},
	{
		"id": 5,
		"start": 12.971,
		"end": 15.265,
		"text": "それで牛 蝸牛(かたつむり)だよ"
	},
	{
		"id": 6,
		"start": 15.932,
		"end": 17.767,
		"text": "人を迷わせる類いの怪異(かいい)は"
	},
	{
		"id": 7,
		"start": 17.851,
		"end": 20.603,
		"text": "そりゃ数えきれないくらいに<br />いっぱい いるけれど"
	},
	{
		"id": 8,
		"start": 20.854,
		"end": 25.442,
		"text": "そのタイプで蝸牛だっていうなら<br />迷い牛で間違いないでしょ"
	},
	{
		"id": 9,
		"start": 25.942,
		"end": 28.57,
		"text": "（あくび）"
	},
	{
		"id": 10,
		"start": 30.071,
		"end": 36.077,
		"text": "♪〜"
	},
	{
		"id": 11,
		"start": 113.321,
		"end": 119.327,
		"text": "〜♪"
	},
	{
		"id": 12,
		"start": 160.785,
		"end": 161.619,
		"text": "（戦場ヶ原(せんじょうがはら)）この辺り"
	},
	{
		"id": 13,
		"start": 161.744,
		"end": 162.745,
		"text": "（阿良々木(あららぎ)）は？"
	},
	{
		"id": 14,
		"start": 163.329,
		"end": 166.332,
		"text": "（戦場ヶ原）<br />この辺りだったのよ<br />私の住んでた家"
	},
	{
		"id": 15,
		"start": 167.625,
		"end": 170.67,
		"text": "（阿良々木）家って… 道じゃん"
	},
	{
		"id": 16,
		"start": 171.129,
		"end": 172.505,
		"text": "（戦場ヶ原）区画整理ね"
	},
	{
		"id": 17,
		"start": 172.797,
		"end": 176.05,
		"text": "たかだか１年足らずだというのに<br />何てことかしら"
	},
	{
		"id": 18,
		"start": 178.177,
		"end": 179.012,
		"text": "（八九寺(はちくじ)）うっ"
	},
	{
		"id": 19,
		"start": 179.095,
		"end": 180.346,
		"text": "（戦場ヶ原）売っちゃったわけだし"
	},
	{
		"id": 20,
		"start": 180.43,
		"end": 183.182,
		"text": "家が残っているとは<br />思っていなかったけれど"
	},
	{
		"id": 21,
		"start": 183.349,
		"end": 185.101,
		"text": "これは さすがに結構ブルーだわ"
	},
	{
		"id": 22,
		"start": 185.56,
		"end": 187.061,
		"text": "（阿良々木）<br />まあ そりゃ そうだよな"
	},
	{
		"id": 23,
		"start": 187.604,
		"end": 188.438,
		"text": "（戦場ヶ原）ハア"
	},
	{
		"id": 24,
		"start": 189.522,
		"end": 192.734,
		"text": "どうしようもないことで<br />時間を取らせてしまったわね"
	},
	{
		"id": 25,
		"start": 192.817,
		"end": 194.694,
		"text": "行きましょうか 阿良々木くん"
	},
	{
		"id": 26,
		"start": 194.777,
		"end": 195.987,
		"text": "（阿良々木）え… もう いいのか？"
	},
	{
		"id": 27,
		"start": 196.279,
		"end": 197.238,
		"text": "（戦場ヶ原）いいのよ"
	},
	{
		"id": 28,
		"start": 197.78,
		"end": 200.408,
		"text": "あっそ　じゃ 行くぞ 八九寺"
	},
	{
		"id": 29,
		"start": 200.491,
		"end": 201.326,
		"text": "うん"
	},
	{
		"id": 30,
		"start": 212.253,
		"end": 215.215,
		"text": "（阿良々木）<br />いい加減に僕の足から離れろよ<br />八九寺"
	},
	{
		"id": 31,
		"start": 215.298,
		"end": 216.507,
		"text": "歩きにくいんだよ"
	},
	{
		"id": 32,
		"start": 217.842,
		"end": 219.719,
		"text": "何か言えよ 黙ってないで"
	},
	{
		"id": 33,
		"start": 219.802,
		"end": 222.43,
		"text": "（八九寺）<br />私だって 阿良々木さんの<br />いかつい脚になんて"
	},
	{
		"id": 34,
		"start": 222.513,
		"end": 224.182,
		"text": "しがみつきたくなんか ありません"
	},
	{
		"id": 35,
		"start": 224.307,
		"end": 226.434,
		"text": "ひどいです！ ＰＴＡに訴えます！"
	},
	{
		"id": 36,
		"start": 226.768,
		"end": 228.519,
		"text": "へえ ＰＴＡに…"
	},
	{
		"id": 37,
		"start": 228.645,
		"end": 231.147,
		"text": "ＰＴＡは<br />ものすごい組織なんですよ"
	},
	{
		"id": 38,
		"start": 231.439,
		"end": 232.357,
		"text": "阿良々木さんみたいな"
	},
	{
		"id": 39,
		"start": 232.44,
		"end": 235.86,
		"text": "何の権力も持たない<br />未成年の 一市民なんて"
	},
	{
		"id": 40,
		"start": 236.11,
		"end": 238.363,
		"text": "指先ひとつで ポポイのポイです！"
	},
	{
		"id": 41,
		"start": 239.53,
		"end": 242.242,
		"text": "指先ひとつか　そりゃ 怖いな"
	},
	{
		"id": 42,
		"start": 242.367,
		"end": 245.536,
		"text": "ところで 八九寺<br />ＰＴＡとは何の略なんだ？"
	},
	{
		"id": 43,
		"start": 245.87,
		"end": 247.163,
		"text": "え？ それは…"
	},
	{
		"id": 44,
		"start": 247.497,
		"end": 248.873,
		"text": "（戦場ヶ原）ＰＴＡというのは"
	},
	{
		"id": 45,
		"start": 249.207,
		"end": 251.459,
		"text": "Parent Teacher Associationの略よ"
	},
	{
		"id": 46,
		"start": 252.293,
		"end": 254.545,
		"text": "親と教師の会という意味ね"
	},
	{
		"id": 47,
		"start": 255.296,
		"end": 260.009,
		"text": "経皮経管(けいひけいかん)的血管形成術という<br />医学用語の略でもあるけれど"
	},
	{
		"id": 48,
		"start": 261.219,
		"end": 264.43,
		"text": "阿良々木くんが そんなものを<br />求めているとは思えないから"
	},
	{
		"id": 49,
		"start": 264.722,
		"end": 267.558,
		"text": "この場合は<br />親と教師の会で正解でしょう"
	},
	{
		"id": 50,
		"start": 267.767,
		"end": 270.77,
		"text": "へえ 戦場ヶ原 さすがに博学だな"
	},
	{
		"id": 51,
		"start": 271.229,
		"end": 273.815,
		"text": "（戦場ヶ原）<br />あなたが浅学非才なだけよ<br />阿良々木くん"
	},
	{
		"id": 52,
		"start": 274.399,
		"end": 277.485,
		"text": "語呂がいいから<br />浅学と言われるのには文句はないが"
	},
	{
		"id": 53,
		"start": 277.777,
		"end": 280.071,
		"text": "しかし 非才は<br />この場合 余計では？"
	},
	{
		"id": 54,
		"start": 280.405,
		"end": 283.616,
		"text": "（戦場ヶ原）<br />そうかしら じゃあ<br />悲惨と言いかえておくわ"
	},
	{
		"id": 55,
		"start": 292.875,
		"end": 294.127,
		"text": "時に八九寺"
	},
	{
		"id": 56,
		"start": 294.21,
		"end": 295.92,
		"text": "何でしょう 阿良々木さん"
	},
	{
		"id": 57,
		"start": 296.17,
		"end": 298.548,
		"text": "（阿良々木）<br />この住所の場所には<br />一体 何があるんだ？"
	},
	{
		"id": 58,
		"start": 299.966,
		"end": 300.8,
		"text": "（八九寺）フーンだ"
	},
	{
		"id": 59,
		"start": 301.092,
		"end": 303.303,
		"text": "話しません　黙秘権を行使します"
	},
	{
		"id": 60,
		"start": 303.428,
		"end": 304.262,
		"text": "く…"
	},
	{
		"id": 61,
		"start": 304.846,
		"end": 306.889,
		"text": "教えねえと連れてってやんねえぞ"
	},
	{
		"id": 62,
		"start": 306.973,
		"end": 309.475,
		"text": "（八九寺）<br />別に頼んでません<br />１人で行けます"
	},
	{
		"id": 63,
		"start": 309.559,
		"end": 310.977,
		"text": "（阿良々木）でも お前 迷子だろ？"
	},
	{
		"id": 64,
		"start": 311.06,
		"end": 312.186,
		"text": "（八九寺）だったら何ですか！"
	},
	{
		"id": 65,
		"start": 312.312,
		"end": 313.688,
		"text": "う〜"
	},
	{
		"id": 66,
		"start": 314.063,
		"end": 318.735,
		"text": "いや 八九寺 あのな<br />後学のために教えてやるけれど"
	},
	{
		"id": 67,
		"start": 318.818,
		"end": 320.82,
		"text": "そういうときは<br />誰かを頼ればいいんだよ"
	},
	{
		"id": 68,
		"start": 321.321,
		"end": 323.74,
		"text": "自分に自信が持てない<br />阿良々木さん辺りは"
	},
	{
		"id": 69,
		"start": 323.823,
		"end": 324.866,
		"text": "そうすればいいです"
	},
	{
		"id": 70,
		"start": 325.366,
		"end": 327.535,
		"text": "気の済むまで<br />他人を頼ってください"
	},
	{
		"id": 71,
		"start": 327.618,
		"end": 330.455,
		"text": "でも私は そんなことを<br />する必要が ないんです"
	},
	{
		"id": 72,
		"start": 330.538,
		"end": 331.914,
		"text": "私にとっては この程度"
	},
	{
		"id": 73,
		"start": 332.832,
		"end": 334.5,
		"text": "日常自販機なんですから！"
	},
	{
		"id": 74,
		"start": 334.917,
		"end": 336.878,
		"text": "へえ 定価販売なんだな"
	},
	{
		"id": 75,
		"start": 338.254,
		"end": 340.256,
		"text": "分かりましたよ お嬢様"
	},
	{
		"id": 76,
		"start": 340.506,
		"end": 343.176,
		"text": "お願いです この住所の場所に<br />一体 何があるのか"
	},
	{
		"id": 77,
		"start": 343.259,
		"end": 345.47,
		"text": "どうか 私めに教えてくださいませ"
	},
	{
		"id": 78,
		"start": 345.803,
		"end": 347.472,
		"text": "言葉に誠意が こもってません"
	},
	{
		"id": 79,
		"start": 347.597,
		"end": 348.473,
		"text": "（阿良々木）くっ"
	},
	{
		"id": 80,
		"start": 350.058,
		"end": 353.603,
		"text": "（阿良々木）<br />妹なら２人とも<br />この手で確実に落ちるんだが"
	},
	{
		"id": 81,
		"start": 353.978,
		"end": 355.229,
		"text": "八九寺はバカな子供を"
	},
	{
		"id": 82,
		"start": 355.313,
		"end": 357.607,
		"text": "あしらうようには いかない<br />というわけか"
	},
	{
		"id": 83,
		"start": 357.732,
		"end": 359.609,
		"text": "まったく どうしたものだろう"
	},
	{
		"id": 84,
		"start": 360.985,
		"end": 362.904,
		"text": "（阿良々木）<br />お嬢ちゃん お小遣いをあげよう"
	},
	{
		"id": 85,
		"start": 362.987,
		"end": 364.947,
		"text": "（八九寺）<br />きゃっほ〜 何でも話します！"
	},
	{
		"id": 86,
		"start": 365.281,
		"end": 366.157,
		"text": "（阿良々木）<br />バカな子供だった"
	},
	{
		"id": 87,
		"start": 366.449,
		"end": 367.283,
		"text": "ヘヘヘッ"
	},
	{
		"id": 88,
		"start": 368.785,
		"end": 371.954,
		"text": "その住所には<br />綱手(つなで)さんという方が住んでいます"
	},
	{
		"id": 89,
		"start": 372.038,
		"end": 372.872,
		"text": "（阿良々木）ふ〜ん"
	},
	{
		"id": 90,
		"start": 374.499,
		"end": 376.626,
		"text": "で どういう知り合いなんだ？"
	},
	{
		"id": 91,
		"start": 376.751,
		"end": 377.71,
		"text": "（八九寺）親戚です"
	},
	{
		"id": 92,
		"start": 377.877,
		"end": 379.212,
		"text": "（阿良々木）親戚ね…"
	},
	{
		"id": 93,
		"start": 379.67,
		"end": 380.546,
		"text": "母の日くらい"
	},
	{
		"id": 94,
		"start": 380.63,
		"end": 382.381,
		"text": "うちで親孝行<br />してればいいのに"
	},
	{
		"id": 95,
		"start": 382.507,
		"end": 384.467,
		"text": "（火憐(かれん)）<br />兄ちゃんは<br />そんなことだから"
	},
	{
		"id": 96,
		"start": 384.55,
		"end": 386.552,
		"text": "そんなことだから<br />いつまで たっても…"
	},
	{
		"id": 97,
		"start": 386.636,
		"end": 388.471,
		"text": "阿良々木さんに<br />言われたくは ありません"
	},
	{
		"id": 98,
		"start": 388.846,
		"end": 390.681,
		"text": "いや お前が何を知ってるんだよ"
	},
	{
		"id": 99,
		"start": 391.057,
		"end": 391.891,
		"text": "何となくです"
	},
	{
		"id": 100,
		"start": 392.35,
		"end": 394.477,
		"text": "阿良々木さんこそ 日曜日の朝から"
	},
	{
		"id": 101,
		"start": 394.56,
		"end": 396.729,
		"text": "公園のベンチで<br />ボーッとしているなんて"
	},
	{
		"id": 102,
		"start": 396.813,
		"end": 399.065,
		"text": "まともな人間のやることとは<br />思えませんが"
	},
	{
		"id": 103,
		"start": 399.148,
		"end": 399.982,
		"text": "（阿良々木）別に ただの…"
	},
	{
		"id": 104,
		"start": 401.317,
		"end": 402.151,
		"text": "ツーリングだよ"
	},
	{
		"id": 105,
		"start": 402.276,
		"end": 404.654,
		"text": "（八九寺）<br />ツーリングですか<br />かっこいいです！"
	},
	{
		"id": 106,
		"start": 406.072,
		"end": 407.99,
		"text": "（阿良々木）<br />まっ 自転車でだけどな"
	},
	{
		"id": 107,
		"start": 408.074,
		"end": 411.953,
		"text": "（八九寺）<br />そうですか　ツーリングと言えば<br />やはりバイクですよね"
	},
	{
		"id": 108,
		"start": 412.036,
		"end": 413.538,
		"text": "とても惜しい感じです"
	},
	{
		"id": 109,
		"start": 413.621,
		"end": 417.25,
		"text": "（阿良々木）<br />学校が校則で免許の取得を<br />禁止しているからな"
	},
	{
		"id": 110,
		"start": 417.917,
		"end": 420.086,
		"text": "でも どっちみち<br />バイクは危ないからな"
	},
	{
		"id": 111,
		"start": 420.169,
		"end": 421.754,
		"text": "僕は車のほうが いいや"
	},
	{
		"id": 112,
		"start": 422.255,
		"end": 423.089,
		"text": "エヘッ"
	},
	{
		"id": 113,
		"start": 423.214,
		"end": 425.842,
		"text": "そうですか　でも それですと"
	},
	{
		"id": 114,
		"start": 425.925,
		"end": 427.844,
		"text": "フォーリングに<br />なってしまいますよね"
	},
	{
		"id": 115,
		"start": 428.386,
		"end": 430.096,
		"text": "（阿良々木）<br />この子 ツーリングのスペルを"
	},
	{
		"id": 116,
		"start": 430.263,
		"end": 432.14,
		"text": "かなり面白く勘違いしている"
	},
	{
		"id": 117,
		"start": 433.266,
		"end": 434.1,
		"text": "フッ"
	},
	{
		"id": 118,
		"start": 438.104,
		"end": 440.773,
		"text": "でも 本当に ややこしいな<br />この辺の道"
	},
	{
		"id": 119,
		"start": 441.19,
		"end": 444.36,
		"text": "お前 よくこんな所を<br />１人で来ようと思ったもんだよ"
	},
	{
		"id": 120,
		"start": 444.902,
		"end": 446.863,
		"text": "（八九寺）<br />別に初めてでは ないですから"
	},
	{
		"id": 121,
		"start": 447.321,
		"end": 450.116,
		"text": "（阿良々木）<br />そうなのか？<br />じゃあ なんで迷うんだよ"
	},
	{
		"id": 122,
		"start": 450.283,
		"end": 453.077,
		"text": "う… 久しぶりだからです"
	},
	{
		"id": 123,
		"start": 453.911,
		"end": 456.497,
		"text": "そういえば あらららぎさんは"
	},
	{
		"id": 124,
		"start": 456.789,
		"end": 457.79,
		"text": "“ら”が１個 多いぞ"
	},
	{
		"id": 125,
		"start": 458.166,
		"end": 459.5,
		"text": "（八九寺）失礼 かみました"
	},
	{
		"id": 126,
		"start": 459.917,
		"end": 461.711,
		"text": "気分の悪い かみ方<br />してんじゃねえよ"
	},
	{
		"id": 127,
		"start": 463.212,
		"end": 464.547,
		"text": "（八九寺）しかたがありません"
	},
	{
		"id": 128,
		"start": 464.63,
		"end": 467.216,
		"text": "誰だって言い間違いを<br />することくらいは あります"
	},
	{
		"id": 129,
		"start": 467.467,
		"end": 468.426,
		"text": "それとも阿良々木さんは"
	},
	{
		"id": 130,
		"start": 468.926,
		"end": 471.596,
		"text": "生まれてから 一度も<br />かんだことがないと言うのですか"
	},
	{
		"id": 131,
		"start": 471.971,
		"end": 473.139,
		"text": "（阿良々木）ないとは言わないが"
	},
	{
		"id": 132,
		"start": 473.598,
		"end": 476.1,
		"text": "少なくとも<br />人の名前をかんだりはしないよ"
	},
	{
		"id": 133,
		"start": 476.267,
		"end": 477.101,
		"text": "（八九寺）では"
	},
	{
		"id": 134,
		"start": 478.978,
		"end": 481.397,
		"text": "バスガス爆発と<br />３回言ってください"
	},
	{
		"id": 135,
		"start": 481.814,
		"end": 483.232,
		"text": "（阿良々木）<br />それ 人の名前じゃないじゃん"
	},
	{
		"id": 136,
		"start": 483.649,
		"end": 486.903,
		"text": "いえ 人の名前です<br />知り合いに３人ほどいます"
	},
	{
		"id": 137,
		"start": 487.361,
		"end": 490.823,
		"text": "ですから むしろ かなり<br />一般的な名前であると思われます"
	},
	{
		"id": 138,
		"start": 491.365,
		"end": 494.202,
		"text": "（阿良々木）<br />バスガス爆発 バスガス爆発<br />バスガス爆発"
	},
	{
		"id": 139,
		"start": 494.327,
		"end": 495.703,
		"text": "夢を食べる動物は？"
	},
	{
		"id": 140,
		"start": 497.205,
		"end": 498.039,
		"text": "バク？"
	},
	{
		"id": 141,
		"start": 498.122,
		"end": 499.624,
		"text": "ブブー はずれです"
	},
	{
		"id": 142,
		"start": 500.208,
		"end": 502.084,
		"text": "夢を食べる動物 それは"
	},
	{
		"id": 143,
		"start": 503.669,
		"end": 504.67,
		"text": "人間ですよ"
	},
	{
		"id": 144,
		"start": 504.754,
		"end": 506.38,
		"text": "うまいこと言ってんじゃねえよ"
	},
	{
		"id": 145,
		"start": 508.925,
		"end": 510.468,
		"text": "（戦場ヶ原）ねえ 阿良々木くん"
	},
	{
		"id": 146,
		"start": 510.843,
		"end": 511.677,
		"text": "（阿良々木）ん？"
	},
	{
		"id": 147,
		"start": 511.844,
		"end": 514.055,
		"text": "（戦場ヶ原）<br />もう一度 住所 教えてくれる？"
	},
	{
		"id": 148,
		"start": 514.305,
		"end": 515.348,
		"text": "（阿良々木）<br />まあ いいけど　えっと"
	},
	{
		"id": 149,
		"start": 515.473,
		"end": 516.599,
		"text": "（早送りされた声）"
	},
	{
		"id": 150,
		"start": 517.767,
		"end": 519.727,
		"text": "どうも行き過ぎてしまったようね"
	},
	{
		"id": 151,
		"start": 520.019,
		"end": 521.354,
		"text": "（阿良々木）え… そうなの？"
	},
	{
		"id": 152,
		"start": 521.437,
		"end": 523.564,
		"text": "（戦場ヶ原）<br />責めたければ<br />好きなだけ責めなさい"
	},
	{
		"id": 153,
		"start": 523.648,
		"end": 526.234,
		"text": "（阿良々木）<br />いや この程度のことで<br />責めたりは しないよ"
	},
	{
		"id": 154,
		"start": 526.317,
		"end": 527.151,
		"text": "（戦場ヶ原）そう"
	},
	{
		"id": 155,
		"start": 537.078,
		"end": 539.956,
		"text": "（阿良々木）<br />お前 なんで そこまで<br />戦場ヶ原のことビビッてんだ？"
	},
	{
		"id": 156,
		"start": 541.165,
		"end": 543.668,
		"text": "（八九寺）<br />どうも 私は嫌われているようです"
	},
	{
		"id": 157,
		"start": 543.793,
		"end": 547.255,
		"text": "邪魔だ どこかに消えろという<br />強い意思を感じます"
	},
	{
		"id": 158,
		"start": 547.713,
		"end": 549.966,
		"text": "（阿良々木）<br />さすがに そんなことは<br />ないとは思うが"
	},
	{
		"id": 159,
		"start": 550.466,
		"end": 551.425,
		"text": "う〜ん"
	},
	{
		"id": 160,
		"start": 551.884,
		"end": 553.427,
		"text": "（阿良々木）なあ 戦場ヶ原"
	},
	{
		"id": 161,
		"start": 553.594,
		"end": 554.47,
		"text": "（戦場ヶ原）何よ"
	},
	{
		"id": 162,
		"start": 554.595,
		"end": 556.305,
		"text": "（阿良々木）<br />お前って 子供 嫌いなの？"
	},
	{
		"id": 163,
		"start": 556.389,
		"end": 557.848,
		"text": "（戦場ヶ原）嫌いね 大嫌い"
	},
	{
		"id": 164,
		"start": 558.266,
		"end": 559.809,
		"text": "１人残らず<br />死ねばいいのに"
	},
	{
		"id": 165,
		"start": 560.184,
		"end": 561.018,
		"text": "（八九寺）ひっ"
	},
	{
		"id": 166,
		"start": 561.185,
		"end": 563.688,
		"text": "（戦場ヶ原）<br />どう接していいのか<br />全く分からないもの"
	},
	{
		"id": 167,
		"start": 563.98,
		"end": 567.608,
		"text": "中学生のときだったかしらね<br />デパートで買い物をしていたら"
	},
	{
		"id": 168,
		"start": 567.984,
		"end": 570.82,
		"text": "私 ７歳くらいの子供に<br />ぶつかってしまったの"
	},
	{
		"id": 169,
		"start": 571.279,
		"end": 573.072,
		"text": "（阿良々木）<br />ああ それで泣かれちゃったとか？"
	},
	{
		"id": 170,
		"start": 573.489,
		"end": 574.782,
		"text": "（戦場ヶ原）<br />いえ そうじゃなくてね"
	},
	{
		"id": 171,
		"start": 575.449,
		"end": 578.286,
		"text": "私 そのとき<br />その７歳くらいの子供に対して"
	},
	{
		"id": 172,
		"start": 578.369,
		"end": 579.245,
		"text": "こう言っちゃったのよ"
	},
	{
		"id": 173,
		"start": 579.954,
		"end": 582.456,
		"text": "“大丈夫ですか<br />ケガは ありませんか”"
	},
	{
		"id": 174,
		"start": 582.54,
		"end": 585.209,
		"text": "“ごめんなさい<br />申し訳ありません”って"
	},
	{
		"id": 175,
		"start": 585.876,
		"end": 588.546,
		"text": "子供相手に どうしていいのか<br />分からなくなって"
	},
	{
		"id": 176,
		"start": 588.629,
		"end": 590.172,
		"text": "気が動転してしまったのね"
	},
	{
		"id": 177,
		"start": 590.84,
		"end": 594.594,
		"text": "だけど だからって 私が<br />あんなに下手(したて)に出てしまうだなんて"
	},
	{
		"id": 178,
		"start": 595.344,
		"end": 597.388,
		"text": "それが 私はショックでショックで"
	},
	{
		"id": 179,
		"start": 598.055,
		"end": 600.099,
		"text": "以来 子供と呼ばれるものは"
	},
	{
		"id": 180,
		"start": 600.766,
		"end": 602.268,
		"text": "人間であれ何であれ"
	},
	{
		"id": 181,
		"start": 602.351,
		"end": 604.812,
		"text": "憎しみをもって向かうように<br />心掛けているわ"
	},
	{
		"id": 182,
		"start": 604.937,
		"end": 606.272,
		"text": "わあああ〜"
	},
	{
		"id": 183,
		"start": 609.233,
		"end": 610.443,
		"text": "ところで 阿良々木くん"
	},
	{
		"id": 184,
		"start": 610.818,
		"end": 611.777,
		"text": "（阿良々木）どうした？"
	},
	{
		"id": 185,
		"start": 612.111,
		"end": 614.363,
		"text": "（戦場ヶ原）<br />どうも また<br />行き過ぎてしまったみたい"
	},
	{
		"id": 186,
		"start": 614.447,
		"end": 617.033,
		"text": "（阿良々木）<br />えっ 住所を？ ２回目だぞ おい"
	},
	{
		"id": 187,
		"start": 617.533,
		"end": 620.119,
		"text": "責められるものなら<br />いくらでも責めてみなさい"
	},
	{
		"id": 188,
		"start": 620.202,
		"end": 622.246,
		"text": "（阿良々木）<br />いや この程度のことで<br />責めたり…"
	},
	{
		"id": 189,
		"start": 622.622,
		"end": 623.664,
		"text": "って あれ？"
	},
	{
		"id": 190,
		"start": 624.123,
		"end": 627.918,
		"text": "戦場ヶ原 何か さっきと<br />セリフが微妙に変わってないか？"
	},
	{
		"id": 191,
		"start": 628.085,
		"end": 629.42,
		"text": "（戦場ヶ原）あら そうかしら"
	},
	{
		"id": 192,
		"start": 629.503,
		"end": 631.505,
		"text": "（阿良々木）何だよ あっ そっか"
	},
	{
		"id": 193,
		"start": 631.589,
		"end": 634.133,
		"text": "区画整理が どうとかって<br />言ってたな　そのせいか"
	},
	{
		"id": 194,
		"start": 634.55,
		"end": 636.594,
		"text": "（戦場ヶ原）<br />いえ そういうことでは ないのよ"
	},
	{
		"id": 195,
		"start": 637.178,
		"end": 639.764,
		"text": "昔の道が完全になくなっている<br />わけではないから"
	},
	{
		"id": 196,
		"start": 640.306,
		"end": 642.016,
		"text": "構造的に迷うわけが ないのよ"
	},
	{
		"id": 197,
		"start": 642.099,
		"end": 642.933,
		"text": "（ピッ）"
	},
	{
		"id": 198,
		"start": 643.893,
		"end": 645.144,
		"text": "（阿良々木）何をしているんだ？"
	},
	{
		"id": 199,
		"start": 645.353,
		"end": 648.606,
		"text": "ＧＰＳのナビゲーションシステムを<br />使ってみようと思うの"
	},
	{
		"id": 200,
		"start": 650.191,
		"end": 651.275,
		"text": "（阿良々木）<br />ふ〜ん"
	},
	{
		"id": 201,
		"start": 652.693,
		"end": 654.111,
		"text": "（ピピッ ピピッ）<br />（戦場ヶ原）え…"
	},
	{
		"id": 202,
		"start": 654.487,
		"end": 655.321,
		"text": "（阿良々木）どうした？"
	},
	{
		"id": 203,
		"start": 657.698,
		"end": 659.742,
		"text": "（戦場ヶ原）<br />急に圏外になってしまったわ"
	},
	{
		"id": 204,
		"start": 663.371,
		"end": 664.455,
		"text": "これは…"
	},
	{
		"id": 205,
		"start": 667.958,
		"end": 669.46,
		"text": "（八九寺）<br />多分 無理だと思います"
	},
	{
		"id": 206,
		"start": 669.794,
		"end": 670.628,
		"text": "（阿良々木）<br />え？"
	},
	{
		"id": 207,
		"start": 671.253,
		"end": 673.339,
		"text": "（八九寺）<br />何度 行っても<br />たどり着けないんですから"
	},
	{
		"id": 208,
		"start": 674.757,
		"end": 675.591,
		"text": "（阿良々木）<br />八九寺？"
	},
	{
		"id": 209,
		"start": 677.134,
		"end": 679.553,
		"text": "（八九寺）<br />私は いつまでも<br />たどり着けないんです"
	},
	{
		"id": 210,
		"start": 680.179,
		"end": 683.057,
		"text": "お母さんの所には<br />たどり着けません"
	},
	{
		"id": 211,
		"start": 684.475,
		"end": 688.27,
		"text": "私は蝸牛の迷子ですから"
	},
	{
		"id": 212,
		"start": 694.36,
		"end": 697.279,
		"text": "（阿良々木）<br />どうしても目的地に<br />たどり着けない僕らは"
	},
	{
		"id": 213,
		"start": 697.363,
		"end": 699.115,
		"text": "忍野に相談することにした"
	},
	{
		"id": 214,
		"start": 700.866,
		"end": 704.245,
		"text": "場所は分かるのか？<br />何なら やっぱり僕が行こうか"
	},
	{
		"id": 215,
		"start": 705.538,
		"end": 709.708,
		"text": "（戦場ヶ原）<br />阿良々木くんの お粗末な記憶力と<br />同じレベルで心配してもらっても"
	},
	{
		"id": 216,
		"start": 709.834,
		"end": 711.961,
		"text": "私はうれしくも何ともないわ"
	},
	{
		"id": 217,
		"start": 712.169,
		"end": 714.004,
		"text": "むしろ悲しくなってくる<br />くらいだもの"
	},
	{
		"id": 218,
		"start": 714.922,
		"end": 715.965,
		"text": "そうですか"
	},
	{
		"id": 219,
		"start": 717.174,
		"end": 718.008,
		"text": "（戦場ヶ原）ていうか"
	},
	{
		"id": 220,
		"start": 719.677,
		"end": 722.513,
		"text": "その子と<br />２人きりになんて しないで"
	},
	{
		"id": 221,
		"start": 733.858,
		"end": 736.777,
		"text": "（阿良々木）<br />僕と戦場ヶ原が遭った<br />怪異の話をしてから"
	},
	{
		"id": 222,
		"start": 736.986,
		"end": 739.864,
		"text": "何だか また<br />警戒されちゃってるんだよな"
	},
	{
		"id": 223,
		"start": 742.074,
		"end": 745.327,
		"text": "お前さ あのとき<br />お母さんって言ってたよな"
	},
	{
		"id": 224,
		"start": 745.828,
		"end": 746.662,
		"text": "綱手っていうのは"
	},
	{
		"id": 225,
		"start": 746.829,
		"end": 748.372,
		"text": "親戚の家じゃ<br />なかったのか？"
	},
	{
		"id": 226,
		"start": 750.374,
		"end": 753.878,
		"text": "八九寺ちゃん 今度 アイスクリーム<br />食べさせてあげるから"
	},
	{
		"id": 227,
		"start": 753.961,
		"end": 755.796,
		"text": "もうちょっと<br />こっちに近づいてこない？"
	},
	{
		"id": 228,
		"start": 758.048,
		"end": 759.008,
		"text": "（八九寺）行きます！"
	},
	{
		"id": 229,
		"start": 759.842,
		"end": 763.596,
		"text": "（阿良々木）<br />それで さっきの話だけれど<br />お母さんって？"
	},
	{
		"id": 230,
		"start": 765.264,
		"end": 767.141,
		"text": "母親だって親戚のうちでしょう"
	},
	{
		"id": 231,
		"start": 767.558,
		"end": 769.518,
		"text": "いや そりゃ そうだけどさ"
	},
	{
		"id": 232,
		"start": 771.061,
		"end": 773.522,
		"text": "それに お母さんといっても"
	},
	{
		"id": 233,
		"start": 773.606,
		"end": 775.941,
		"text": "残念なことに もう<br />お母さんじゃありませんから"
	},
	{
		"id": 234,
		"start": 777.193,
		"end": 778.027,
		"text": "ああ…"
	},
	{
		"id": 235,
		"start": 778.527,
		"end": 782.198,
		"text": "（八九寺）<br />綱手というのは３年生までの<br />私の名前でした"
	},
	{
		"id": 236,
		"start": 782.364,
		"end": 783.949,
		"text": "お父さんに引き取られて"
	},
	{
		"id": 237,
		"start": 784.033,
		"end": 786.285,
		"text": "八九寺へと名字が<br />変わっちゃいましたけれど"
	},
	{
		"id": 238,
		"start": 786.41,
		"end": 787.745,
		"text": "あっちゃ〜"
	},
	{
		"id": 239,
		"start": 788.329,
		"end": 789.747,
		"text": "偉そうに年上ぶって"
	},
	{
		"id": 240,
		"start": 789.83,
		"end": 792.291,
		"text": "親孝行しろよとか<br />言っちゃったよ 僕"
	},
	{
		"id": 241,
		"start": 792.791,
		"end": 796.67,
		"text": "（八九寺）<br />いえ 別に今日が母の日だから<br />ということでは ないです"
	},
	{
		"id": 242,
		"start": 796.754,
		"end": 800.549,
		"text": "お母さんの家は機会さえあれば<br />いつでも来たい場所ですから"
	},
	{
		"id": 243,
		"start": 801.717,
		"end": 802.635,
		"text": "なるほどね"
	},
	{
		"id": 244,
		"start": 803.135,
		"end": 806.847,
		"text": "だけど 何度訪ねても<br />いつでも たどり着けませんけれど"
	},
	{
		"id": 245,
		"start": 807.306,
		"end": 810.226,
		"text": "（阿良々木）<br />そのとき蝸牛に遭ったってわけか"
	},
	{
		"id": 246,
		"start": 810.518,
		"end": 812.853,
		"text": "（八九寺）<br />遭ったというか<br />よく分かりませんが"
	},
	{
		"id": 247,
		"start": 814.647,
		"end": 817.9,
		"text": "（阿良々木）<br />お前も いろいろ大変だったんだな"
	},
	{
		"id": 248,
		"start": 818.734,
		"end": 819.568,
		"text": "ガウ！"
	},
	{
		"id": 249,
		"start": 820.152,
		"end": 820.986,
		"text": "（阿良々木）痛(い)って！"
	},
	{
		"id": 250,
		"start": 821.07,
		"end": 822.488,
		"text": "いきなり何すんだ このガキ"
	},
	{
		"id": 251,
		"start": 822.571,
		"end": 823.531,
		"text": "（八九寺）う〜"
	},
	{
		"id": 252,
		"start": 823.697,
		"end": 824.99,
		"text": "（阿良々木）痛い！ 痛い 痛い"
	},
	{
		"id": 253,
		"start": 825.074,
		"end": 826.617,
		"text": "痛いって！"
	},
	{
		"id": 254,
		"start": 829.286,
		"end": 830.12,
		"text": "てい！"
	},
	{
		"id": 255,
		"start": 831.622,
		"end": 832.456,
		"text": "（八九寺）ぐっ"
	},
	{
		"id": 256,
		"start": 839.255,
		"end": 840.631,
		"text": "う… うう…"
	},
	{
		"id": 257,
		"start": 843.008,
		"end": 843.842,
		"text": "うう…"
	},
	{
		"id": 258,
		"start": 849.139,
		"end": 849.974,
		"text": "フッ"
	},
	{
		"id": 259,
		"start": 850.558,
		"end": 852.434,
		"text": "いや 笑えないな"
	},
	{
		"id": 260,
		"start": 853.185,
		"end": 854.853,
		"text": "２回目以降となっちまうと"
	},
	{
		"id": 261,
		"start": 855.062,
		"end": 857.982,
		"text": "ただ むなしいだけのもんなんだな<br />勝利なんて"
	},
	{
		"id": 262,
		"start": 859.358,
		"end": 863.529,
		"text": "（阿良々木）<br />小学生女子を２回も殴りつけ<br />失神させた挙句に"
	},
	{
		"id": 263,
		"start": 863.863,
		"end": 868.576,
		"text": "ニヒルを気取って たそがれている<br />男子高校生の姿が そこにはあった"
	},
	{
		"id": 264,
		"start": 871.662,
		"end": 873.247,
		"text": "ていうか それも僕だった"
	},
	{
		"id": 265,
		"start": 876.625,
		"end": 878.919,
		"text": "（羽川(はねかわ)）<br />怖い顔になってるよ 阿良々木くん"
	},
	{
		"id": 266,
		"start": 879.42,
		"end": 881.839,
		"text": "（阿良々木）あ… はっ 羽川？"
	},
	{
		"id": 267,
		"start": 882.298,
		"end": 884.633,
		"text": "（羽川）<br />びっくりしたみたいな顔になったね"
	},
	{
		"id": 268,
		"start": 884.758,
		"end": 887.553,
		"text": "うん まあ<br />そっちのほうが いいかな"
	},
	{
		"id": 269,
		"start": 888.22,
		"end": 889.054,
		"text": "フッ"
	},
	{
		"id": 270,
		"start": 889.555,
		"end": 892.057,
		"text": "どうしたの？<br />何やってるの こんなとこで"
	},
	{
		"id": 271,
		"start": 892.641,
		"end": 894.977,
		"text": "ちょっと まあ<br />暇つぶしっていうか"
	},
	{
		"id": 272,
		"start": 895.394,
		"end": 897.521,
		"text": "ああ そうなんだ"
	},
	{
		"id": 273,
		"start": 898.105,
		"end": 899.982,
		"text": "私も暇つぶしかな"
	},
	{
		"id": 274,
		"start": 900.19,
		"end": 904.32,
		"text": "私 家 いづらいからさ<br />日曜日は散歩の日なのよ"
	},
	{
		"id": 275,
		"start": 906.113,
		"end": 908.24,
		"text": "（阿良々木）<br />気 遣い過ぎだと思うけどな"
	},
	{
		"id": 276,
		"start": 908.365,
		"end": 909.7,
		"text": "（羽川）フフッ"
	},
	{
		"id": 277,
		"start": 910.326,
		"end": 914.079,
		"text": "ねえ ところで阿良々木くん<br />その子 妹さん？"
	},
	{
		"id": 278,
		"start": 914.163,
		"end": 917.958,
		"text": "（阿良々木）<br />えっと 迷子の子だよ<br />八九寺真宵(まよい)って名前"
	},
	{
		"id": 279,
		"start": 918.667,
		"end": 922.588,
		"text": "八九寺か<br />関西圏じゃ よく聞く言葉だね"
	},
	{
		"id": 280,
		"start": 922.963,
		"end": 923.797,
		"text": "そういえば"
	},
	{
		"id": 281,
		"start": 924.214,
		"end": 925.424,
		"text": "「東雲(しののめ)物語」に"
	},
	{
		"id": 282,
		"start": 925.507,
		"end": 927.092,
		"text": "出てくる お寺って<br />確か…"
	},
	{
		"id": 283,
		"start": 927.509,
		"end": 929.72,
		"text": "あっ あれは でも<br />漢字が違ったっけ"
	},
	{
		"id": 284,
		"start": 930.554,
		"end": 932.097,
		"text": "（阿良々木）<br />お前は何でも知ってるな"
	},
	{
		"id": 285,
		"start": 932.556,
		"end": 935.476,
		"text": "何でもは知らないわよ<br />知ってることだけ"
	},
	{
		"id": 286,
		"start": 935.851,
		"end": 937.227,
		"text": "（八九寺）んん…<br />（羽川）ん？"
	},
	{
		"id": 287,
		"start": 939.146,
		"end": 941.065,
		"text": "（八九寺）ハア…"
	},
	{
		"id": 288,
		"start": 941.398,
		"end": 943.233,
		"text": "こんにちは 真宵ちゃん"
	},
	{
		"id": 289,
		"start": 943.484,
		"end": 947.446,
		"text": "私 このお兄ちゃんのお友達で<br />羽川翼(つばさ)っていうんだよ"
	},
	{
		"id": 290,
		"start": 948.656,
		"end": 951.742,
		"text": "話しかけないでください<br />あなたのことが嫌いです"
	},
	{
		"id": 291,
		"start": 952.117,
		"end": 955.245,
		"text": "あれ〜 嫌われるようなこと<br />しちゃったかな"
	},
	{
		"id": 292,
		"start": 955.704,
		"end": 956.538,
		"text": "うり うり"
	},
	{
		"id": 293,
		"start": 956.83,
		"end": 957.998,
		"text": "う… うう…"
	},
	{
		"id": 294,
		"start": 960.125,
		"end": 962.127,
		"text": "かわいいね 真宵ちゃん"
	},
	{
		"id": 295,
		"start": 962.211,
		"end": 965.422,
		"text": "いや〜ん ホント<br />食べちゃいたいくらい ウフッ"
	},
	{
		"id": 296,
		"start": 965.965,
		"end": 967.174,
		"text": "あっ でもね"
	},
	{
		"id": 297,
		"start": 967.257,
		"end": 970.052,
		"text": "お兄ちゃんの手 いきなり<br />かんだりしちゃダメじゃない"
	},
	{
		"id": 298,
		"start": 970.511,
		"end": 971.345,
		"text": "めっ！"
	},
	{
		"id": 299,
		"start": 972.596,
		"end": 974.306,
		"text": "あ… ああ…"
	},
	{
		"id": 300,
		"start": 974.723,
		"end": 976.6,
		"text": "ほら ごめんなさいは？"
	},
	{
		"id": 301,
		"start": 979.019,
		"end": 981.063,
		"text": "ごっ ごめんなさいでした<br />阿良々木さん"
	},
	{
		"id": 302,
		"start": 981.522,
		"end": 982.356,
		"text": "（阿良々木）え？"
	},
	{
		"id": 303,
		"start": 983.899,
		"end": 986.151,
		"text": "（羽川）<br />それに 阿良々木くんもダメだよ"
	},
	{
		"id": 304,
		"start": 986.235,
		"end": 988.779,
		"text": "（阿良々木）<br />ダメだよって やっぱ暴力？"
	},
	{
		"id": 305,
		"start": 989.113,
		"end": 991.657,
		"text": "（羽川）<br />もちろん暴力もダメだけれどね"
	},
	{
		"id": 306,
		"start": 991.907,
		"end": 993.826,
		"text": "でも 子供を たたいたら"
	},
	{
		"id": 307,
		"start": 993.909,
		"end": 996.412,
		"text": "たたかれたことを<br />納得できるだけの理由を"
	},
	{
		"id": 308,
		"start": 996.495,
		"end": 998.247,
		"text": "話してあげなくちゃダメ"
	},
	{
		"id": 309,
		"start": 999.832,
		"end": 1002.459,
		"text": "お前と話すとホント勉強になるな"
	},
	{
		"id": 310,
		"start": 1002.96,
		"end": 1004.878,
		"text": "フフフッ"
	},
	{
		"id": 311,
		"start": 1005.254,
		"end": 1008.59,
		"text": "で 迷子なんだっけ？<br />この辺なの？"
	},
	{
		"id": 312,
		"start": 1008.674,
		"end": 1010.801,
		"text": "何なら<br />案内できると思うけど"
	},
	{
		"id": 313,
		"start": 1010.884,
		"end": 1012.886,
		"text": "（阿良々木）えっと いや 今"
	},
	{
		"id": 314,
		"start": 1012.97,
		"end": 1015.764,
		"text": "戦場ヶ原に人を呼びにいって<br />もらってるところだから"
	},
	{
		"id": 315,
		"start": 1015.848,
		"end": 1019.143,
		"text": "（羽川）<br />あれ 阿良々木くん<br />戦場ヶ原さんと 一緒だったの？"
	},
	{
		"id": 316,
		"start": 1019.768,
		"end": 1021.061,
		"text": "ん？ あっ"
	},
	{
		"id": 317,
		"start": 1021.77,
		"end": 1023.313,
		"text": "この前 阿良々木くん"
	},
	{
		"id": 318,
		"start": 1023.397,
		"end": 1027.776,
		"text": "戦場ヶ原さんのことについて<br />私に あれこれ聞いてきたよね"
	},
	{
		"id": 319,
		"start": 1028.11,
		"end": 1028.986,
		"text": "ん？"
	},
	{
		"id": 320,
		"start": 1029.361,
		"end": 1030.195,
		"text": "ん…"
	},
	{
		"id": 321,
		"start": 1031.697,
		"end": 1032.531,
		"text": "（手を打った音）"
	},
	{
		"id": 322,
		"start": 1032.698,
		"end": 1034.867,
		"text": "ああ そっか そういうこと"
	},
	{
		"id": 323,
		"start": 1035.451,
		"end": 1037.286,
		"text": "お前の そういう妄想力って"
	},
	{
		"id": 324,
		"start": 1037.369,
		"end": 1039.413,
		"text": "やおい好きの女子も<br />そこのけだよな"
	},
	{
		"id": 325,
		"start": 1039.747,
		"end": 1041.123,
		"text": "やおい？ 何それ"
	},
	{
		"id": 326,
		"start": 1041.582,
		"end": 1044.46,
		"text": "（阿良々木）<br />“やまナシ おちナシ<br />意味深長”の頭文字だ"
	},
	{
		"id": 327,
		"start": 1044.752,
		"end": 1048.172,
		"text": "（羽川）<br />ウソっぽいね<br />いいわ 今度 調べてみる"
	},
	{
		"id": 328,
		"start": 1048.255,
		"end": 1049.381,
		"text": "（阿良々木）真面目だな"
	},
	{
		"id": 329,
		"start": 1049.465,
		"end": 1052.718,
		"text": "（羽川）<br />じゃ 邪魔するのもなんだし<br />私 もう行くね"
	},
	{
		"id": 330,
		"start": 1053.218,
		"end": 1056.305,
		"text": "お邪魔しました<br />戦場ヶ原さんに よろしくね"
	},
	{
		"id": 331,
		"start": 1056.472,
		"end": 1058.432,
		"text": "（阿良々木）だから 誤解してるぞ"
	},
	{
		"id": 332,
		"start": 1058.932,
		"end": 1062.644,
		"text": "あっ そうだ 羽川<br />一応１つだけ聞かせてくれるか"
	},
	{
		"id": 333,
		"start": 1063.145,
		"end": 1065.564,
		"text": "この辺で綱手さんっていう家<br />知ってる？"
	},
	{
		"id": 334,
		"start": 1065.647,
		"end": 1066.899,
		"text": "綱手さん？"
	},
	{
		"id": 335,
		"start": 1067.524,
		"end": 1069.109,
		"text": "う〜ん えっと"
	},
	{
		"id": 336,
		"start": 1071.195,
		"end": 1072.821,
		"text": "（羽川）<br />いや 知らないわ"
	},
	{
		"id": 337,
		"start": 1073.28,
		"end": 1075.699,
		"text": "（阿良々木）<br />羽川でも<br />知らないことが あるんだな"
	},
	{
		"id": 338,
		"start": 1075.783,
		"end": 1076.95,
		"text": "（羽川）言ったでしょ？"
	},
	{
		"id": 339,
		"start": 1077.034,
		"end": 1079.87,
		"text": "私が知ってるのは<br />私が知っていることだけなの"
	},
	{
		"id": 340,
		"start": 1080.412,
		"end": 1081.872,
		"text": "他のことは からきしね"
	},
	{
		"id": 341,
		"start": 1082.247,
		"end": 1083.082,
		"text": "ああ そう"
	},
	{
		"id": 342,
		"start": 1083.582,
		"end": 1085.501,
		"text": "期待に添えず申し訳ないね"
	},
	{
		"id": 343,
		"start": 1086.001,
		"end": 1086.835,
		"text": "（阿良々木）いやいや"
	},
	{
		"id": 344,
		"start": 1086.919,
		"end": 1088.879,
		"text": "（羽川）じゃ 今度こそ バイバイ"
	},
	{
		"id": 345,
		"start": 1090.547,
		"end": 1091.673,
		"text": "（着信音）"
	},
	{
		"id": 346,
		"start": 1091.757,
		"end": 1092.591,
		"text": "（阿良々木）おっ"
	},
	{
		"id": 347,
		"start": 1092.674,
		"end": 1095.969,
		"text": "（着信音）"
	},
	{
		"id": 348,
		"start": 1096.095,
		"end": 1099.973,
		"text": "（阿良々木）<br />５月14日 日曜日<br />14時15分30秒—"
	},
	{
		"id": 349,
		"start": 1100.099,
		"end": 1103.185,
		"text": "戦場ヶ原の携帯番号を<br />入手した瞬間だった"
	},
	{
		"id": 350,
		"start": 1110.692,
		"end": 1114.279,
		"text": "（忍野）<br />まったく 日曜日の朝の惰眠を<br />邪魔するなんて"
	},
	{
		"id": 351,
		"start": 1114.571,
		"end": 1116.198,
		"text": "ツンデレちゃんは ひどい子だよ"
	},
	{
		"id": 352,
		"start": 1116.573,
		"end": 1119.785,
		"text": "（阿良々木）<br />ツンデレちゃん？<br />ああ 戦場ヶ原のことか"
	},
	{
		"id": 353,
		"start": 1119.868,
		"end": 1122.496,
		"text": "（電話:忍野）<br />しっかし よくもまあ<br />こんな短期間に"
	},
	{
		"id": 354,
		"start": 1122.579,
		"end": 1125.958,
		"text": "そうも いろいろな怪異に<br />出遭えるもんだよ 阿良々木くんは"
	},
	{
		"id": 355,
		"start": 1126.375,
		"end": 1128.085,
		"text": "行き遭ったのは僕じゃないよ"
	},
	{
		"id": 356,
		"start": 1128.544,
		"end": 1130.379,
		"text": "（電話:忍野）<br />ん？ そうなのかい？"
	},
	{
		"id": 357,
		"start": 1130.587,
		"end": 1133.674,
		"text": "（阿良々木）<br />蝸牛に行き遭ったのは<br />八九寺真宵って名前の子供だよ"
	},
	{
		"id": 358,
		"start": 1135.092,
		"end": 1137.177,
		"text": "（電話:忍野）<br />ああ 八九寺真宵か"
	},
	{
		"id": 359,
		"start": 1137.427,
		"end": 1140.639,
		"text": "ハハッ なるほどね<br />見えてきた 見えてきた"
	},
	{
		"id": 360,
		"start": 1141.306,
		"end": 1145.144,
		"text": "八九寺つったら あれ<br />「東雲物語」の第５節"
	},
	{
		"id": 361,
		"start": 1145.644,
		"end": 1147.604,
		"text": "羽川も そんなこと言ってたな"
	},
	{
		"id": 362,
		"start": 1148.522,
		"end": 1149.857,
		"text": "（電話:忍野）<br />ん？ 何だい<br />ツンデレちゃん"
	},
	{
		"id": 363,
		"start": 1150.691,
		"end": 1151.525,
		"text": "どうした 忍野？"
	},
	{
		"id": 364,
		"start": 1151.942,
		"end": 1154.736,
		"text": "（電話:忍野）<br />あ〜 ふ〜ん…"
	},
	{
		"id": 365,
		"start": 1155.154,
		"end": 1155.988,
		"text": "（阿良々木）はあ？"
	},
	{
		"id": 366,
		"start": 1157.781,
		"end": 1161.827,
		"text": "（電話:忍野）<br />ハア〜 阿良々木くんは<br />本当に甲斐性なしだな"
	},
	{
		"id": 367,
		"start": 1162.035,
		"end": 1163.745,
		"text": "あ？ 何だよ いきなり"
	},
	{
		"id": 368,
		"start": 1164.163,
		"end": 1166.832,
		"text": "（電話:忍野）<br />ツンデレちゃんに<br />こんなに気を遣わせちゃって"
	},
	{
		"id": 369,
		"start": 1167.124,
		"end": 1169.459,
		"text": "ツンデレちゃんが<br />責任を感じちゃってるじゃないか"
	},
	{
		"id": 370,
		"start": 1169.793,
		"end": 1174.006,
		"text": "ああ いや 戦場ヶ原を<br />巻き込んだのは悪かったと思ってる"
	},
	{
		"id": 371,
		"start": 1174.423,
		"end": 1176.383,
		"text": "（電話:忍野）<br />そういう意味じゃないよ ったく"
	},
	{
		"id": 372,
		"start": 1177.009,
		"end": 1180.846,
		"text": "阿良々木くん 自分のことと<br />委員長ちゃんとツンデレちゃんと"
	},
	{
		"id": 373,
		"start": 1180.971,
		"end": 1183.64,
		"text": "３つ 立て続けに怪異を<br />解決しちゃったもんだから"
	},
	{
		"id": 374,
		"start": 1183.724,
		"end": 1185.601,
		"text": "ちょっと<br />調子こいちゃったんじゃないの？"
	},
	{
		"id": 375,
		"start": 1185.726,
		"end": 1187.728,
		"text": "（阿良々木）<br />別に そんなつもりはないよ"
	},
	{
		"id": 376,
		"start": 1187.978,
		"end": 1189.897,
		"text": "（電話:忍野）<br />それに怪異に遭ったとき"
	},
	{
		"id": 377,
		"start": 1190.022,
		"end": 1191.773,
		"text": "僕みたいなのが<br />そばにいるケースなんて"
	},
	{
		"id": 378,
		"start": 1191.857,
		"end": 1193.525,
		"text": "普通は ありえないんだから"
	},
	{
		"id": 379,
		"start": 1193.734,
		"end": 1195.944,
		"text": "まして 電話で<br />要件を済まされるとは"
	},
	{
		"id": 380,
		"start": 1196.028,
		"end": 1197.237,
		"text": "思いもよらなかったよ"
	},
	{
		"id": 381,
		"start": 1198.28,
		"end": 1200.199,
		"text": "（阿良々木）<br />まあ 安易だったとは思うよ"
	},
	{
		"id": 382,
		"start": 1200.616,
		"end": 1202.993,
		"text": "（電話:忍野）<br />あと 年頃の女の子を１人で"
	},
	{
		"id": 383,
		"start": 1203.076,
		"end": 1207.456,
		"text": "怪しい男が寝泊まりしている場所に<br />送り込むなんてのも 感心しないよ"
	},
	{
		"id": 384,
		"start": 1207.873,
		"end": 1210.417,
		"text": "でも お前は別に何もしないだろ？"
	},
	{
		"id": 385,
		"start": 1210.751,
		"end": 1212.92,
		"text": "（電話:忍野）<br />信頼は普通に ありがたいけれど"
	},
	{
		"id": 386,
		"start": 1213.295,
		"end": 1216.215,
		"text": "阿良々木くんが<br />僕を信頼してくれているほどには"
	},
	{
		"id": 387,
		"start": 1216.298,
		"end": 1217.216,
		"text": "ツンデレちゃんは僕を"
	},
	{
		"id": 388,
		"start": 1217.299,
		"end": 1219.718,
		"text": "信頼してくれているわけじゃ<br />ないんだから"
	},
	{
		"id": 389,
		"start": 1220.26,
		"end": 1221.261,
		"text": "うわ！"
	},
	{
		"id": 390,
		"start": 1221.345,
		"end": 1223.096,
		"text": "何もしない 何もしないって！"
	},
	{
		"id": 391,
		"start": 1223.18,
		"end": 1225.182,
		"text": "ホッチキスを構えるのを<br />やめてくれ ツンデレちゃん"
	},
	{
		"id": 392,
		"start": 1225.265,
		"end": 1226.308,
		"text": "ホッチキス？"
	},
	{
		"id": 393,
		"start": 1226.767,
		"end": 1228.977,
		"text": "（電話:忍野）<br />ハア びっくりした"
	},
	{
		"id": 394,
		"start": 1229.269,
		"end": 1231.897,
		"text": "ツンデレちゃんは<br />怖いツンデレちゃんだったんだな"
	},
	{
		"id": 395,
		"start": 1232.147,
		"end": 1233.69,
		"text": "えっと じゃあ…"
	},
	{
		"id": 396,
		"start": 1234.024,
		"end": 1237.11,
		"text": "ああ もう やっぱ<br />電話は苦手だな 話しにくいや"
	},
	{
		"id": 397,
		"start": 1237.319,
		"end": 1239.071,
		"text": "話しにくいって そんな…"
	},
	{
		"id": 398,
		"start": 1239.655,
		"end": 1240.781,
		"text": "（電話:忍野）<br />じゃあ こうしよう"
	},
	{
		"id": 399,
		"start": 1241.24,
		"end": 1244.243,
		"text": "迷い牛対策は<br />ツンデレちゃんに伝えておくからさ"
	},
	{
		"id": 400,
		"start": 1244.326,
		"end": 1246.495,
		"text": "阿良々木くんは<br />そこでそのまま じっとしててよ"
	},
	{
		"id": 401,
		"start": 1247.079,
		"end": 1249.998,
		"text": "対策って 人づてで大丈夫なのか"
	},
	{
		"id": 402,
		"start": 1250.207,
		"end": 1254.461,
		"text": "あの戦場ヶ原のときの<br />あの儀式みたいなのは必要ないのか"
	},
	{
		"id": 403,
		"start": 1254.795,
		"end": 1257.673,
		"text": "（電話:忍野）<br />ないよ　蝸牛は蟹(かに)ほど厄介じゃない"
	},
	{
		"id": 404,
		"start": 1257.839,
		"end": 1259.299,
		"text": "だって神様じゃないもん"
	},
	{
		"id": 405,
		"start": 1259.675,
		"end": 1263.553,
		"text": "お化けだな どっちかって言うと<br />幽霊とか そっちの類"
	},
	{
		"id": 406,
		"start": 1263.804,
		"end": 1264.638,
		"text": "（阿良々木）幽霊？"
	},
	{
		"id": 407,
		"start": 1264.721,
		"end": 1267.557,
		"text": "（電話:忍野）<br />ああ それから<br />阿良々木くんは知ってるかな"
	},
	{
		"id": 408,
		"start": 1267.724,
		"end": 1270.936,
		"text": "四国八十八か所とか<br />西国三十三所とか"
	},
	{
		"id": 409,
		"start": 1271.395,
		"end": 1274.147,
		"text": "（阿良々木）<br />ああ まあ<br />それくらいなら さすがに"
	},
	{
		"id": 410,
		"start": 1274.523,
		"end": 1277.734,
		"text": "（電話:忍野）<br />まあ 八九寺ってのも<br />いわば その 一種だね"
	},
	{
		"id": 411,
		"start": 1277.818,
		"end": 1278.86,
		"text": "（阿良々木）ふ〜ん"
	},
	{
		"id": 412,
		"start": 1278.986,
		"end": 1282.322,
		"text": "（電話:忍野）<br />ただ ほらさ<br />八九って〝やく〞"
	},
	{
		"id": 413,
		"start": 1282.406,
		"end": 1283.824,
		"text": "つまり〝厄〞に<br />通じちゃうところが"
	},
	{
		"id": 414,
		"start": 1283.907,
		"end": 1284.741,
		"text": "あるじゃない"
	},
	{
		"id": 415,
		"start": 1285.075,
		"end": 1286.827,
		"text": "そういう解釈が<br />広がっちゃって"
	},
	{
		"id": 416,
		"start": 1286.91,
		"end": 1290.08,
		"text": "八九寺というくくりは<br />そのうちに廃れちゃった"
	},
	{
		"id": 417,
		"start": 1290.497,
		"end": 1291.331,
		"text": "（阿良々木）うん"
	},
	{
		"id": 418,
		"start": 1291.999,
		"end": 1295.043,
		"text": "（電話:忍野）<br />で そういう経緯<br />歴史を理解したうえで"
	},
	{
		"id": 419,
		"start": 1295.127,
		"end": 1298.297,
		"text": "改めて八九寺真宵って名前を<br />見るとどうだろう"
	},
	{
		"id": 420,
		"start": 1298.672,
		"end": 1301.508,
		"text": "妙に意味ありげで<br />困っちゃうよね 普通は"
	},
	{
		"id": 421,
		"start": 1301.925,
		"end": 1303.01,
		"text": "意味ありげ？"
	},
	{
		"id": 422,
		"start": 1303.093,
		"end": 1304.845,
		"text": "（電話:忍野）<br />阿良々木くんが最初の段階で"
	},
	{
		"id": 423,
		"start": 1304.928,
		"end": 1307.931,
		"text": "それを感じてくれていたなら<br />よかったんだけれどね"
	},
	{
		"id": 424,
		"start": 1308.015,
		"end": 1309.725,
		"text": "よかったって 何がだよ"
	},
	{
		"id": 425,
		"start": 1310.058,
		"end": 1312.686,
		"text": "（電話:忍野）<br />じゃ 詳細はツンデレちゃんに<br />言っとくから"
	},
	{
		"id": 426,
		"start": 1312.769,
		"end": 1314.187,
		"text": "（阿良々木）あ… ああ"
	},
	{
		"id": 427,
		"start": 1314.396,
		"end": 1315.981,
		"text": "（電話:忍野）<br />もっとも ツンデレちゃんが"
	},
	{
		"id": 428,
		"start": 1316.064,
		"end": 1318.317,
		"text": "それを素直に教えてくれれば<br />いいけどね"
	},
	{
		"id": 429,
		"start": 1318.442,
		"end": 1319.318,
		"text": "え？"
	},
	{
		"id": 430,
		"start": 1319.443,
		"end": 1322.404,
		"text": "（不通音）"
	},
	{
		"id": 431,
		"start": 1322.904,
		"end": 1324.281,
		"text": "（阿良々木）どういうことだよ"
	},
	{
		"id": 432,
		"start": 1326.366,
		"end": 1330.412,
		"text": "ハア というわけだ 八九寺<br />何とか なりそうだぞ"
	},
	{
		"id": 433,
		"start": 1330.662,
		"end": 1331.955,
		"text": "（八九寺）印象としては"
	},
	{
		"id": 434,
		"start": 1332.039,
		"end": 1335.25,
		"text": "あまり何とか なりそうな<br />会話では ありませんでしたが"
	},
	{
		"id": 435,
		"start": 1335.876,
		"end": 1337.502,
		"text": "それは ともかく阿良々木さん"
	},
	{
		"id": 436,
		"start": 1337.919,
		"end": 1338.879,
		"text": "何だよ"
	},
	{
		"id": 437,
		"start": 1340.172,
		"end": 1342.215,
		"text": "私は おなかが すいていますよ"
	},
	{
		"id": 438,
		"start": 1345.302,
		"end": 1351.308,
		"text": "♪〜"
	},
	{
		"id": 439,
		"start": 1428.426,
		"end": 1434.432,
		"text": "〜♪"
	},
	{
		"id": 440,
		"start": 1435.142,
		"end": 1437.31,
		"text": "（火憐）火憐だぜ<br />（月火(つきひ)）月火だよ"
	},
	{
		"id": 441,
		"start": 1437.435,
		"end": 1439.938,
		"text": "火憐ちゃん 剣道の試合じゃ<br />ガッツポーズ禁止って本当？"
	},
	{
		"id": 442,
		"start": 1440.021,
		"end": 1441.398,
		"text": "（火憐）<br />そうらしいぜ　応援も禁止だ"
	},
	{
		"id": 443,
		"start": 1441.481,
		"end": 1442.44,
		"text": "（月火）<br />握手するのも禁止？"
	},
	{
		"id": 444,
		"start": 1442.524,
		"end": 1444.317,
		"text": "（火憐）<br />禁止 禁止<br />試合後のハグなんて ありえねえ"
	},
	{
		"id": 445,
		"start": 1444.401,
		"end": 1445.235,
		"text": "（月火）<br />厳しいね"
	},
	{
		"id": 446,
		"start": 1445.318,
		"end": 1446.528,
		"text": "（火憐）<br />まあ 武器持っちゃってるしな"
	},
	{
		"id": 447,
		"start": 1446.611,
		"end": 1449.364,
		"text": "（月火）<br />だけど 私に言わせれば<br />武器よりも防具のほうが怖いよ"
	},
	{
		"id": 448,
		"start": 1449.447,
		"end": 1450.282,
		"text": "（火憐）<br />にゃ？"
	},
	{
		"id": 449,
		"start": 1450.407,
		"end": 1452.868,
		"text": "（月火）<br />面 胴 小手<br />あそこまで守りを固めなくとも"
	},
	{
		"id": 450,
		"start": 1452.951,
		"end": 1454.286,
		"text": "（火憐）<br />守りすぎて 逆に怖い？"
	},
	{
		"id": 451,
		"start": 1454.369,
		"end": 1456.413,
		"text": "（月火）<br />では ここで<br />初めての予告編クイズ！"
	},
	{
		"id": 452,
		"start": 1456.496,
		"end": 1457.33,
		"text": "（火憐）<br />４回目だぜ？"
	},
	{
		"id": 453,
		"start": 1457.414,
		"end": 1458.623,
		"text": "（月火）<br />ケータリングといえば出前ですが"
	},
	{
		"id": 454,
		"start": 1458.707,
		"end": 1459.541,
		"text": "（火憐）<br />出前だな"
	},
	{
		"id": 455,
		"start": 1459.624,
		"end": 1461.418,
		"text": "（月火）これ 携帯電話っぽいよね<br />（火憐）んん？"
	},
	{
		"id": 456,
		"start": 1461.501,
		"end": 1463.336,
		"text": "（月火）<br />そこで 私は携帯電話の使用を"
	},
	{
		"id": 457,
		"start": 1463.42,
		"end": 1464.963,
		"text": "ケータリングと名付けることを<br />提唱します"
	},
	{
		"id": 458,
		"start": 1465.046,
		"end": 1465.881,
		"text": "（火憐）<br />んんん？"
	},
	{
		"id": 459,
		"start": 1465.964,
		"end": 1467.716,
		"text": "（月火）<br />コラ 電車の中では<br />ケータリング禁止！"
	},
	{
		"id": 460,
		"start": 1467.799,
		"end": 1468.633,
		"text": "（火憐）<br />そりゃ 禁止だろ"
	},
	{
		"id": 461,
		"start": 1468.717,
		"end": 1470.927,
		"text": "（月火）<br />いつまでもケータリングしてないで<br />お風呂 入っちゃいなさい"
	},
	{
		"id": 462,
		"start": 1471.011,
		"end": 1472.304,
		"text": "（火憐）<br />クイズ 出しちゃいなさい"
	},
	{
		"id": 463,
		"start": 1472.429,
		"end": 1474.139,
		"text": "（火憐･月火）<br />次回「まよいマイマイ<br />其ノ参(そのさん)」"
	},
	{
		"id": 464,
		"start": 1474.431,
		"end": 1475.807,
		"text": "（月火）<br />ケータリングでも見れるかな"
	},
	{
		"id": 465,
		"start": 1475.891,
		"end": 1476.975,
		"text": "（火憐）<br />ワンセグもなんだ"
	}
]